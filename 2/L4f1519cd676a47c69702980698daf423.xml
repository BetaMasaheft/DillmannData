                        <quote>locus quo quis se convertit</quote>
                     </cit> vel <cit type="translation" xml:lang="la">
                        <quote>recipit</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>refugium</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>receptus</quote>
