               <sense xml:lang="la" source="#dillmann" xml:id="Dmain">, rarius <foreign xml:lang="gez">የርባሕ፡</foreign> (<ref cRef="Num." loc="13,33">Num. 13,33</ref> FH), plerumque
                     <foreign xml:lang="gez">ያርብሕ፡</foreign>, <gramGrp>
                     <pos expand="Substantivum">subst.</pos>
                  </gramGrp> (<bibl>
                     <ptr target="bm:Dillmann1857Gramm"/>gramm.</bibl> § 113), <lbl expand="singularis">sing.</lbl> et <lbl expand="collatio, -is vel collectivum, -e">coll.</lbl>, <lbl expand="pluralis">Pl.</lbl>
                  <foreign xml:lang="gez">እለ፡ ያርብሕ፡</foreign> (<bibl>
                     <ptr target="bm:Dillmann1857Gramm"/>gramm.</bibl> § 140 s. f.), <cit type="translation" xml:lang="la">
                     <quote>gigas</quote>
                  </cit>, <cit type="translation" xml:lang="la">
                     <quote>gigantes</quote>
                  </cit>: <foreign xml:lang="grc">γίγας</foreign>
                  <ref cRef="Ps." loc="18,6">Ps. 18,6</ref>; <ref cRef="Ps. " loc="32,16">Ps.
                     32,16</ref>; <ref cRef="Jes." loc="3,2">Jes. 3,2</ref>; <ref cRef="Jes." loc="49,24">Jes. 49,24</ref>; <ref cRef="Hez." loc="39,20">Hez. 39,20</ref>;
                     <foreign xml:lang="grc">γίγαντες </foreign>
                  <foreign xml:lang="gez">ሥጋ፡ ያርብሕ፡ ትብልዑ፡</foreign>
                  <ref cRef="Hez." loc="39,18">Hez. 39,18</ref>; <ref cRef="Kuf." loc="7">Kuf.
                     7</ref>; in <gramGrp>
                     <case value="accusativus">Acc.</case>
                  </gramGrp> vel <foreign xml:lang="gez">ያርብሐ፡</foreign> ut <ref cRef="Gen." loc="10,8">Gen. 10,8</ref> (G); <ref cRef="Sir." loc="47,4">Sir. 47,4</ref>
                  <lbl expand="varia lectio">var.</lbl>, vel <foreign xml:lang="gez">ይርባሕ፡</foreign>
                  sive <foreign xml:lang="gez">ያርብሕ፡ </foreign> (tanquam <gramGrp>
                     <pos expand="nomen proprium">n. pr.</pos>
                  </gramGrp> indeclinabile) <ref cRef="Gen." loc="10,8">Gen. 10,8</ref> FH; <foreign xml:lang="gez">በንእሱ፡ ቀተለ፡ ይርባሕ፡</foreign>
                  <ref cRef="Sir." loc="47,4">Sir. 47,4</ref>; <foreign xml:lang="gez">እለ፡
                     ያርብሕ፡</foreign> vel <foreign xml:lang="gez">ይርባሕ፡</foreign>
                  <ref cRef="Gen." loc="6,4">Gen. 6,4</ref>; <ref cRef="Gen." loc="14,5">Gen.
                     14,5</ref>; <ref cRef="Num." loc="13,33">Num. 13,33</ref>; <ref cRef="Deut." loc="1,28">Deut. 1,28</ref>; <ref cRef="Jos." loc="12,4">Jos. 12,4</ref>; <ref cRef="2 Reg." loc="21,22">2 Reg. 21,22</ref>; <ref cRef="Jes." loc="13,3">Jes.
                     13,3</ref>; <ref cRef="Jes." loc="14,9">Jes. 14,9</ref>; <ref cRef="Hez." loc="32,12">Hez. 32,12</ref>; <ref cRef="Hez." loc="32,17">Hez. 32,17</ref>
                  <lbl expand="sequens">seq.</lbl>; <ref cRef="Job" loc="26,5">Job 26,5</ref>; <ref cRef="Judith" loc="16,6">Judith 16,6</ref>; <ref cRef="Sir." loc="16,7">Sir.
                     16,7</ref>; <ref cRef="Hen." loc="106,17">Hen. 106,17</ref>. – <lbl expand="Vocabularium Aethiopicum, -a -a.">Voc. Ae.</lbl>: <foreign xml:lang="gez">ያርብሕ፡ ብ፡ አርበኛ፡</foreign>; <cit type="translation" xml:lang="la">
                     <quote>aliud</quote>
                  </cit>: <foreign xml:lang="gez">ያርብሕ፡ ዘ፡ ኀይለኛ፡ አዛልሂ፡ ውእቱ።</foreign>
