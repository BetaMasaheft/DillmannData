            <entry xml:id="L5ef682b3417d4dcf99be99d8b5fc41f3" n="4691">
               <form>
                  <foreign xml:lang="gez">ተኀጥአ</foreign>
               </form>
               <sense xml:lang="la" source="#dillmann" xml:id="Dmain"> <subc>III,1</subc> reflex. <sense n="1" xml:id="D1"> <cit type="translation" xml:lang="la">
                                <quote> se subducere </quote>
                            </cit> <sense n="a" xml:id="D1a"> <cit type="translation" xml:lang="la">
                                    <quote> evanescere </quote>
                                </cit>, <cit type="translation" xml:lang="la">
                                    <quote> sensim discedere </quote>
                                </cit>: <foreign xml:lang="gez">ተኀጥአ፡ ሲሳይ፡</foreign> <cit type="translation" xml:lang="la">
                                    <quote> annonae penuria exstitit </quote>
                                </cit>, <ref cRef="Jsp." loc="364">Jsp. p. 364</ref>; in sepulchro post triennium <foreign xml:lang="gez">ይትኀጣእ፡ ሥጋ፡ ወአምትንት፡ እምነ፡ ምውት፡</foreign> <ref cRef="Phlx. " loc="121.">Phlx.  121.</ref> </sense> <sense n="b" xml:id="D1b"> <cit type="translation" xml:lang="la">
                                    <quote> aufugere </quote>
                                </cit>, <cit type="translation" xml:lang="la">
                                    <quote> fugitivum fieri </quote>
                                </cit>: <foreign xml:lang="gez">ተኀጥአ፡ ወአምሠጠ፡</foreign> <ref cRef="1 Reg." loc="19,12">1 Reg. 19,12</ref>; <ref cRef="1 Reg." loc="19,18">1 Reg. 19,18</ref>; <ref cRef="2 Reg." loc="13,34">2 Reg. 13,34</ref>; <ref cRef="Hez." loc="34,4">Hez. 34,4</ref>; <ref cRef="2 Esr." loc="4,19">2 Esr. 4,19</ref>; <ref cRef="Act. " loc="7,29">Act.  7,29</ref>; <lbl expand="cum">c.</lbl> <foreign xml:lang="gez">እምነ፡</foreign>, ut: <foreign xml:lang="gez">አግብርት፡ እለ፡ ተኀጥኡ፡ እምአጋእዝቲሆሙ፡</foreign> <ref cRef="1 Reg." loc="25,10">1 Reg. 25,10</ref>; <ref cRef="1 Reg." loc="20,1">1 Reg. 20,1</ref>; <ref cRef="Gen." loc="16,6">Gen. 16,6</ref>; <ref cRef="Gen." loc="16,8">Gen. 16,8</ref>; <ref cRef="2 Par." loc="10,2">2 Par. 10,2</ref>; <foreign xml:lang="gez">ተኀጥአት፡ ጽድቅ፡ እምፍኖትነ፡</foreign> <ref cRef="Jes." loc="59,14">Jes. 59,14</ref>; <foreign xml:lang="gez">ተኀጥአት፡ ምክር፡ እምጠቢባን፡</foreign> <ref cRef="Jer. " loc="29,8">Jer.  29,8</ref>; <ref cRef="Judith" loc="10,12">Judith 10,12</ref>; <ref cRef="Judith" loc="11,3">Judith 11,3</ref>; <ref cRef="Apoc." loc="9,6">Apoc. 9,6</ref>; <lbl expand="cum">c.</lbl> <foreign xml:lang="gez">ውስተ፡</foreign> vel <gramGrp> <case value="accusativus">Acc.</case> </gramGrp> loci <ref cRef="2 Esr." loc="23,10">2 Esr. 23,10</ref>; <ref cRef="Jon." loc="1,3">Jon. 1,3</ref>; <ref cRef="Asc. Jes." loc="3,3">Asc. Jes. 3,3</ref>; <ref cRef="Asc. Jes." loc="3,5">Asc. Jes. 3,5</ref>; de brutis: <foreign xml:lang="gez">በግዕ፡ ዘተኀጥአ፡</foreign> <foreign xml:lang="grc">πρόβατον πλανώμενον</foreign> <ref cRef="Jer." loc="13,14">Jer. 13,14</ref>; etiam <lbl expand="cum">c.</lbl> <gramGrp> <case value="accusativus">Acc.</case> </gramGrp> <lbl expand="persona">pers.</lbl>, cui aliquis se subducit: <foreign xml:lang="gez">ተኀጥአከ፡</foreign> <ref cRef="Sir." loc="30,40">Sir. 30,40</ref>.</sense> </sense> <sense n="2" xml:id="D2"> <cit type="translation" xml:lang="la">
                                <quote> abesse </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> deesse </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> deficere </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> non reperiri </quote>
                            </cit>: <foreign xml:lang="gez">ኢትትኀጣእ፡ አመ፡ ዕለተ፡ ትፍሥሕት፡</foreign> <ref cRef="Sir." loc="14,14">Sir. 14,14</ref>; <foreign xml:lang="gez">ናሁ፡ ተኀጥአ፡ ዘይጌሥጾ፡ ለኢዮብ፡ እምኔክሙ፡</foreign> <ref cRef="Job" loc="32,12">Job 32,12</ref>; <foreign xml:lang="gez">በተኀጥኦተ፡ መምህር፡</foreign> <ref cRef="Prov." loc="29,18">Prov. 29,18</ref>; (<foreign xml:lang="gez">ሰማይ፡ ውእተ፡</foreign>) <foreign xml:lang="gez">መካን፡ ኀበ፡ ተኀጥአ፡ ሐዘን፡ ወናእክ፡ ወገዓር፡</foreign> <ref cRef="Genz." loc="65">Genz. f. 65</ref>; <foreign xml:lang="gez">በተኀጥኦቶሙ፡ ዝናም፡</foreign> <ref cRef="Sx. Mij." loc="24 Enc.">Sx. Mij. 24 Enc.</ref>; absens <foreign xml:lang="gez">ዘተኀጥአ፡ ዜናሁ፡</foreign> (<cit type="translation" xml:lang="de">
                                <quote>ein Verschollener</quote>
                            </cit>) <ref cRef="F.N." loc=" 42,13">F.N.  42,13</ref>.</sense> <sense n="3" xml:id="D3"> <cit type="translation" xml:lang="la">
                                <quote> amissum esse </quote>
                            </cit>: <foreign xml:lang="gez">በእንተ፡ ዘይትረከብ፡ እምዘተኀጥአ፡</foreign> <ref cRef="F.N." loc=" 40">F.N.  40</ref>.</sense> </sense>
               
