               <sense xml:lang="la" source="#dillmann" xml:id="Dmain"> <subc>II,2</subc> <sense n="1" xml:id="D1">
                     <cit type="translation" xml:lang="la">
                        <quote>memorem facere</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>in memoriam revocare</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>monere</quote>
                     </cit>, <lbl expand="cum">c.</lbl>
                     <gramGrp>
                        <case value="accusativus">Acc.</case>
                     </gramGrp>
                     <lbl expand="persona">pers.</lbl>: <foreign xml:lang="gez">አዘክሩ፡ ሕዝበ፡</foreign>
                     <ref cRef="Jer." loc="4,16">Jer. 4,16</ref>; <lbl expand="cum">c.</lbl>
                     <gramGrp>
                        <case value="accusativus">Acc.</case>
                     </gramGrp>
                     <lbl expand="persona">pers.</lbl> et <foreign xml:lang="gez">በእንተ፡</foreign>
                     rei <ref cRef="Gen." loc="40,14">Gen. 40,14</ref>; <lbl expand="cum">c.</lbl>
                     <gramGrp>
                        <case value="accusativus">Acc.</case>
                     </gramGrp> rei: <ref cRef="Num." loc="5,15">Num. 5,15</ref>; <ref cRef="Hez." loc="21,23">Hez. 21,23</ref>; <ref cRef="Hez." loc="21,24">Hez. 21,24</ref>;
                        <ref cRef="Lit." loc="173,4">Lit. 173,4</ref>; <lbl expand="cum">c.</lbl>
                     <gramGrp>
                        <case value="accusativus">Acc.</case>
                     </gramGrp> rei et <foreign xml:lang="gez">ለ፡ </foreign> pers.: <foreign xml:lang="gez">በቃለ፡ ዚአሁ፡ በዘአዘከረ፡ ሎቱ፡ መሐላ፡ አበው፡ </foreign>
                     <ref cRef="Sap." loc="18,22">Sap. 18,22</ref>; <foreign xml:lang="gez">አዘክሪ፡</foreign> (o Maria!) <foreign xml:lang="gez">ሊተ፡ ተዝካረ፡
                        ኪዳንኪ፡</foreign> I <ref cRef="Sal. Mar." loc="14">Sal. Mar. 14</ref>; <lbl expand="cum">c.</lbl>
                     <lbl expand="duplex">dupl.</lbl>
                     <gramGrp>
                        <case value="accusativus">Acc.</case>
                     </gramGrp>: <ref cRef="1 Cor. " loc="4,17">1 Cor. 4,17</ref>; <ref cRef="2 Petr." loc="1,12">2 Petr. 1,12</ref> Platt; <ref cRef="Joh." loc="14,26">Joh. 14,26</ref>; <ref cRef="Clem." loc="81">Clem. f. 81</ref>;
                        <foreign xml:lang="gez">አዘከራ፡ እሳተ፡ ገሀነም፡</foreign>
                     <ref cRef="Sx. Jac." loc="3">Sx. Jac. 3</ref>; <lbl expand="cum">c.</lbl>
                     <foreign xml:lang="gez">ኀበ፡</foreign> pers.: <ref cRef="Lit." loc="173,4">Lit.
                        173,4</ref>.</sense>
                  <sense n="2" xml:id="D2">
                     <cit type="translation" xml:lang="la">
                        <quote>memorare</quote>
                     </cit>, <sense n="a" xml:id="D2a">
                        <cit type="translation" xml:lang="la">
                           <quote>memoriae prodere</quote>
                        </cit>: <foreign xml:lang="gez">ወነዋ፡ አዘከረ፡ ዮሴፍ፡ ወልደ፡ ከርዮን፡ ከመ፡ </foreign> …
                           <ref cRef="Abush." loc="20.">Abush. 20.</ref>
                     </sense>
                     <sense n="b" xml:id="D2b">
                        <cit type="translation" xml:lang="la">
                           <quote>mentionem facere</quote>
                        </cit>, <lbl expand="cum">c.</lbl>
                        <gramGrp>
                           <case value="accusativus">Acc.</case>
                        </gramGrp> rei, <ref cRef="Abush." loc="30.">Abush. 30.</ref>
                     </sense>
                  </sense>
               </sense>
               
