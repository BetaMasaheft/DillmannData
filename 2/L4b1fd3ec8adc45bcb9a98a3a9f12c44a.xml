               <sense xml:id="Dmain" xml:lang="la" source="#dillmann"> <subc>I,1</subc> [<foreign xml:lang="he">יָלַד</foreign> <foreign xml:lang="syr">ܝܻܠܶܕ</foreign> <foreign xml:lang="ar">وَلَدَ</foreign>] <gramGrp> <pos expand="Subjunctivus">Subj.</pos> </gramGrp> <foreign xml:lang="gez">ይለድ፡</foreign>, Imperat. <foreign xml:lang="gez">ለድ፡</foreign>, <sense n="1" xml:id="D1"> <cit type="translation" xml:lang="la"><t:quote xmlns:t="http://www.tei-c.org/ns/1.0"> parĕre </t:quote></cit> <sense n="a" xml:id="D1a"> de feminis (hominibus), <gramGrp> <pos expand="absolute">abs.</pos> </gramGrp>: <foreign xml:lang="gez">ወሊድ፡</foreign> <ref cRef="Koh." loc="3,2">Koh. 3,2</ref>; <ref cRef="Gen." loc="3,16">Gen. 3,16</ref>; <ref cRef="Gen." loc="17,17">Gen. 17,17</ref>, <ref cRef="Gen." loc="18,13">Gen. 18,13</ref>, <ref cRef="Gen." loc="20,17">Gen. 20,17</ref>, <ref cRef="Gen." loc="30,3">Gen. 30,3</ref>; <ref cRef="Jud." loc="13,2">Jud. 13,2</ref>; <foreign xml:lang="gez">እንተ፡ ትወልድ፡</foreign> <cit type="translation" xml:lang="la"><t:quote xmlns:t="http://www.tei-c.org/ns/1.0"> parturiens </t:quote></cit>, <cit type="translation" xml:lang="la"><t:quote xmlns:t="http://www.tei-c.org/ns/1.0"> puerpera </t:quote></cit> <ref cRef="Lev." loc="12,7">Lev. 12,7</ref>; <ref cRef="Kuf." loc="3">Kuf. 3</ref>; <ref cRef="Jes." loc="21,3">Jes. 21,3</ref>; <ref cRef="Jer." loc="6,24">Jer. 6,24</ref>; <ref cRef="Jer." loc="8,21">Jer. 8,21</ref>; <ref cRef="Sir. " loc="48,19">Sir.  48,19</ref>; <foreign xml:lang="gez">ዕለተ፡ ወሊዶታ፡</foreign> <ref cRef="Luc." loc="2,6">Luc. 2,6</ref>; <foreign xml:lang="gez">ሶበ፡ ታለጽቅ፡ ትለድ፡</foreign> <ref cRef="Joh." loc="16,21">Joh. 16,21</ref>; <ref cRef="Luc." loc="1,57">Luc. 1,57</ref>; plerumque <lbl expand="sequens">seq.</lbl> <gramGrp> <case value="accusativus">Acc.</case> </gramGrp>: <ref cRef="Gen." loc="4,1">Gen. 4,1</ref>; <ref cRef="Gen." loc="4,2">Gen. 4,2</ref>; <ref cRef="Gen." loc="16,11">Gen. 16,11</ref>; <foreign xml:lang="gez">ዘወለደት፡ እምከርሣ፡</foreign> <ref cRef="Jes." loc="49,15">Jes. 49,15</ref>; <ref cRef="Matth." loc="1,21">Matth. 1,21</ref>; <ref cRef="Matth." loc="1,23">Matth. 1,23</ref>; <ref cRef="Matth." loc="1,25">Matth. 1,25</ref>; <ref cRef="Luc." loc="1,31">Luc. 1,31</ref>; <ref cRef="Luc. " loc="1,57">Luc.  1,57</ref>; <ref cRef="Luc." loc="2,7">Luc. 2,7</ref> al. crebro; <lbl expand="cum">c.</lbl> <foreign xml:lang="gez">ለ፡</foreign> patris (<cit type="translation" xml:lang="la"><t:quote xmlns:t="http://www.tei-c.org/ns/1.0"> cui </t:quote></cit> filius natus est): <foreign xml:lang="gez">ወለደት፡ ሎቱ ወልደ፡</foreign> <ref cRef="Hos." loc="1,3">Hos. 1,3</ref>; <ref cRef="Kuf." loc="4">Kuf. 4</ref>; <ref cRef="Gen." loc="6,4">Gen. 6,4</ref>; <ref cRef="Gen." loc="17,21">Gen. 17,21</ref>; <ref cRef="Gen." loc="21,3">Gen. 21,3</ref>; <ref cRef="Gen." loc="29,34">Gen. 29,34</ref>; <foreign xml:lang="gez">ኢወለደት፡ ሎቱ፡</foreign> <ref cRef="Gen." loc="16,1">Gen. 16,1</ref>; <foreign xml:lang="gez">በዲበ፡</foreign> auctoris (patris): <foreign xml:lang="gez">ብእሲት፡ እንተ፡ ትወልድ፡ ወራሴ፡ በዲበ፡ መሐዛ፡</foreign> <ref cRef="Sir." loc="23,22">Sir. 23,22</ref> vel <lbl expand="cum">c.</lbl> <foreign xml:lang="gez">በላዕለ፡</foreign> <ref cRef="Sir." loc="23,23">Sir. 23,23</ref>.
</sense> <sense n="b" xml:id="D1b"> de bestiis: <ref cRef="Gen." loc="30,39">Gen. 30,39</ref>; <ref cRef="Gen." loc="31,8">Gen. 31,8</ref>; <ref cRef="Jer." loc="14,5">Jer. 14,5</ref>.
</sense> <sense n="c" xml:id="D1c"> de avibus: <foreign xml:lang="gez">አስተጋብአት፡ ዘኢወለደት፡</foreign> <ref cRef="Jer. " loc="17,11">Jer.  17,11</ref>.
</sense> <sense n="d" xml:id="D1d"> <lbl expand="translate">transl.</lbl> <foreign xml:lang="gez">ወለድክሙ፡ ዐመፃ፡</foreign> <ref cRef="Jes." loc="59,4">Jes. 59,4</ref>; <ref cRef="Ps. " loc="7,15">Ps.  7,15</ref>; <foreign xml:lang="gez">ኢታአምር፡ ዘትወልድ፡ ሳኒታ፡</foreign> <ref cRef="Prov." loc="3,28">Prov. 3,28</ref>; <ref cRef="Jac. " loc="1,15">Jac.  1,15</ref>.
</sense> 
</sense> <sense n="2" xml:id="D2"> <cit type="translation" xml:lang="la"><t:quote xmlns:t="http://www.tei-c.org/ns/1.0"> gignere </t:quote></cit>, <cit type="translation" xml:lang="la"><t:quote xmlns:t="http://www.tei-c.org/ns/1.0"> procreare </t:quote></cit>, <cit type="translation" xml:lang="la"><t:quote xmlns:t="http://www.tei-c.org/ns/1.0"> generare </t:quote></cit>, <cit type="translation" xml:lang="la"><t:quote xmlns:t="http://www.tei-c.org/ns/1.0"> progignere </t:quote></cit>: <sense n="a" xml:id="D2a"> de viris (hominibus): <foreign xml:lang="gez">ኢወሊድ፡</foreign> <foreign xml:lang="grc">ἀτεκνία</foreign> <ref cRef="Sap." loc="4,1">Sap. 4,1</ref>; <foreign xml:lang="gez">ኢይወልዱ፡ ለመርገም፡</foreign> <foreign xml:lang="grc">οὐδὲ τεκνοποιήσουσιν εἰς κατάραν</foreign> <ref cRef="Jes." loc="65,23">Jes. 65,23</ref>; <foreign xml:lang="gez">ለዱ፡ ደቂቀ፡ ወአዋልደ፡</foreign> <ref cRef="Jer. " loc="36,6">Jer.  36,6</ref>; <ref cRef="Gen." loc="4,18">Gen. 4,18</ref>; <ref cRef="Gen." loc="5,4">Gen. 5,4</ref> <lbl expand="sequens">seq.</lbl>; <ref cRef="3 Reg." loc="1,6">3 Reg. 1,6</ref>; <ref cRef="Jes." loc="1,2">Jes. 1,2</ref>; <ref cRef="Jer." loc="16,2">Jer. 16,2</ref>; <ref cRef="Sir." loc="41,9">Sir. 41,9</ref>; <ref cRef="Hen. " loc="6,2">Hen.  6,2</ref>; <ref cRef="Matth." loc="1,2">Matth. 1,2</ref> <lbl expand="sequens">seq.</lbl>; <lbl expand="cum">c.</lbl> <foreign xml:lang="gez">በኀበ፡</foreign> mulieris <foreign xml:lang="gez">ይወልድ፡ በኀበ፡ ዘማት፡</foreign> <ref cRef="Clem." loc="133.">Clem. f. 133.</ref> 
</sense> <sense n="b" xml:id="D2b"> de Deo: <ref cRef="Prov." loc="8,25">Prov. 8,25</ref>; <ref cRef="Ps." loc="2,7">Ps. 2,7</ref>; <ref cRef="Deut." loc="32,18">Deut. 32,18</ref>.
</sense> <sense n="c" xml:id="D2c"> spiritualiter: <cit type="translation" xml:lang="la"><t:quote xmlns:t="http://www.tei-c.org/ns/1.0"> regenerare </t:quote></cit>, sive de Deo (qui homines regenerat) <ref cRef="1 Petr." loc="1,3">1 Petr. 1,3</ref>; sive de hominibus (qui alios regenerant): <ref cRef="Philem." loc="10">Philem. 10</ref>; <ref cRef="1 Cor." loc="4,15">1 Cor. 4,15</ref>; addito <foreign xml:lang="gez">ዳግመ፡</foreign>, ut: sacerdotes <foreign xml:lang="gez">ዳግመ፡ ወለዱክሙ፡ እማይ፡ ወእመንፈስ፡ ቅዱስ፡</foreign> <ref cRef="Clem." loc="228.">Clem. f. 228.</ref> 
</sense> <sense n="d" xml:id="D2d"> de terra, plantis: <foreign xml:lang="gez">ምድር፡ ትወልድ፡ ሣዕረ፡</foreign> <ref cRef="Hebr." loc="6,7">Hebr. 6,7</ref>; <foreign xml:lang="gez">ወርቀ፡ ወብሩረ፡</foreign> <ref cRef="4 Esr." loc="6,28">4 Esr. 6,28</ref>; <ref cRef="Hen." loc="65,8">Hen. 65,8</ref>; <ref cRef="4 Esr." loc="2,39">4 Esr. 2,39</ref> <lbl expand="varia lectio">var.</lbl> 
</sense> <sense n="e" xml:id="D2e"> <lbl expand="translate">transl.</lbl>: <foreign xml:lang="gez">ቍጥዓ፡ ወመዐት፡ ይወልዱ፡ ክሒደ፡</foreign> <ref cRef="Chrys. ho." loc="6">Chrys. ho. 6</ref>; <ref cRef="Chrys. Ta." loc="29 ">Chrys. Ta. 29 </ref>(<lbl expand="videas">vid. sub</lbl> <foreign xml:lang="gez">ዕሩቅ፡</foreign>).</sense>
</sense> </sense>

