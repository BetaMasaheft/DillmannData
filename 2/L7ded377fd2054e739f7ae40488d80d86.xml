         <div>
            <entry xml:id="L7ded377fd2054e739f7ae40488d80d86" n="6613">
               <form>
                  <foreign xml:lang="gez">ተዋቀሠ</foreign>
               </form>
               <sense xml:lang="la" source="#dillmann" xml:id="Dmain"> <subc>III,3</subc> <cit type="translation" xml:lang="la">
                     <quote>litigare</quote>
                  </cit>, <cit type="translation" xml:lang="la">
                     <quote>disputare</quote>
                  </cit>, <cit type="translation" xml:lang="la">
                     <quote>altercari</quote>
                  </cit>, <cit type="translation" xml:lang="la">
                     <quote>contradicere litigando</quote>
                  </cit>: <foreign xml:lang="gez">ምንተ፡ ትብል፡ ተዋቂሠከ፡ </foreign>
                  <ref cRef="Job" loc="35,2">Job 35,2</ref>; <foreign xml:lang="grc">ἀνταπεκρίθη</foreign>
                  <ref cRef="Job" loc="16,8">Job 16,8</ref>; <foreign xml:lang="grc">κρίθητι</foreign>
                  <foreign xml:lang="gez">ተዋቀሥ፡</foreign>
                  <ref cRef="Job" loc="35,14">Job 35,14</ref>; <foreign xml:lang="gez">ንዑ፡
                     ንትዋቀሥ፡</foreign>
                  <foreign xml:lang="grc">διελεγχθῶμεν </foreign>
                  <ref cRef="Jes." loc="1,18">Jes. 1,18</ref>; <foreign xml:lang="grc">κριθῶμεν</foreign>
                  <ref cRef="Jes." loc="43,26">Jes. 43,26</ref>; <ref cRef="Ps." loc="36,35">Ps.
                     36,35</ref>; <ref cRef="Num." loc="29,13">Num. 29,13</ref>; <lbl expand="cum">c.</lbl>
                  <foreign xml:lang="gez">ምስለ፡</foreign> pers. <ref cRef="Job" loc="39,32">Job
                     39,32</ref>; <lbl expand="cum">c.</lbl>
                  <gramGrp>
                     <case value="accusativus">Acc.</case>
                  </gramGrp>
                  <lbl expand="persona">pers.</lbl>: <ref cRef="Act." loc="9,29">Act. 9,29</ref>
                  Platt; <ref cRef="Act." loc="13,8">Act. 13,8</ref>; <ref cRef="Act." loc="15,2">Act. 15,2</ref>; <ref cRef="Act." loc="17,2">Act. 17,2</ref>; <ref cRef="Act." loc="18,4">Act. 18,4</ref>; <ref cRef="Act." loc="18,6">Act. 18,6</ref>;
                     <foreign xml:lang="gez">አልቦ፡ ዘይትዋቀሰከ፡</foreign>
                  <ref cRef="Job" loc="11,3">Job 11,3</ref>; <ref cRef="Jer." loc="2,9">Jer.
                     2,9</ref>; <ref cRef="Jer." loc="2,35">Jer. 2,35</ref>; <foreign xml:lang="gez">ኢክህለት፡ ተዋቅሦተክሙ፡</foreign>
                  <ref cRef="Dan. apocr." loc="1,57">Dan. apocr. 1,57</ref>; <foreign xml:lang="gez">ተዋቀሥዋ፡ ለእምክሙ፡</foreign>
                  <ref cRef="Hos." loc="2,4">Hos. 2,4</ref>; <ref cRef="Mich." loc="6,2">Mich.
                     6,2</ref>; <ref cRef="Hez." loc="22,2">Hez. 22,2</ref>; <ref cRef="Luc." loc="21,15">Luc. 21,15</ref>; <ref cRef="Rom." loc="8,33">Rom. 8,33</ref>; <lbl expand="cum">c.</lbl>
                  <foreign xml:lang="gez">በ፡</foreign> rei <ref cRef="Luc." loc="2,34">Luc.
                     2,34</ref>; <lbl expand="cum">c.</lbl>
                  <foreign xml:lang="gez">በእንተ፡ </foreign> causae: <ref cRef="Joel" loc="4,2">Joel
                     4,2</ref>; <ref cRef="Hen." loc="1,9">Hen. 1,9</ref>. <sense n="a" xml:id="Da">
                     <cit type="translation" xml:lang="la">
                        <quote>causam suam defendere</quote>
                     </cit>, rationem <cit type="translation" xml:lang="la">
                        <quote>reddere</quote>
                     </cit>: <foreign xml:lang="gez">እትዋቀሥ፡</foreign> (v. <foreign xml:lang="gez">እወቅሥ፡</foreign>) <foreign xml:lang="gez">ቅድመ፡ እግዚአብሔር፡</foreign>
                     <ref cRef="Job" loc="13,3">Job 13,3</ref>; <foreign xml:lang="gez">አባእኮ፡ ቅድሜከ፡
                        ይትዋቀሥ፡</foreign>
                     <ref cRef="Job" loc="14,3">Job 14,3</ref>.</sense>
                  <sense n="b" xml:id="Db">
                     <lbl expand="cum">c.</lbl>
                     <foreign xml:lang="gez">ለ፡</foreign> pers.: <cit type="translation" xml:lang="la">
                        <quote>causam</quote>
                     </cit> alicujus <cit type="translation" xml:lang="la">
                        <quote>tueri</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>defendere</quote>
                     </cit> aliquem, <cit type="translation" xml:lang="la">
                        <quote>patrocinari</quote>
                     </cit> alicui: <foreign xml:lang="gez">ኵሉ፡ ይትዋቀሥ፡ ለእኁሁ፡</foreign>
                     <ref cRef="Jes." loc="41,6">Jes. 41,6</ref>; <ref cRef="Rom." loc="8,26">Rom.
                        8,26</ref>; <ref cRef="Rom." loc="8,27">Rom. 8,27</ref>; <ref cRef="Rom. " loc="8,34">Rom. 8,34</ref>; <ref cRef="Hebr." loc="7,25">Hebr.
                     7,25</ref>.</sense>
                  <sense n="c" xml:id="Dc">
                     <cit type="translation" xml:lang="la">
                        <quote>in jus ire</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>causam suam judicandam proponere</quote>
                     </cit> alicui, <lbl expand="cum">c.</lbl>
                     <foreign xml:lang="gez">በኀበ፡</foreign> judicis: <ref cRef="1 Cor." loc="6,1">1
                        Cor. 6,1</ref>; F.N. 43,9; <foreign xml:lang="gez">እለ፡ ይትዋቀሡ፡ በኀበ፡ አና፡
                        ቅጽ፡</foreign>
                     <ref cRef="Jes." loc="29,21">Jes. 29,21</ref>; vel <cit type="translation" xml:lang="la">
                        <quote>jus suum petere</quote>
                     </cit> ab aliquo: <foreign xml:lang="gez">ሶበ፡ ይትዋቀሡ፡ በኀቤየ፡</foreign>
                     <ref cRef="Job " loc="31,13">Job 31,13</ref>.</sense>
                  <sense n="d" xml:id="Dd">
                     <lbl expand="cum">c.</lbl>
                     <lbl expand="duplex">dupl.</lbl>
                     <gramGrp>
                        <case value="accusativus">Acc.</case>
                     </gramGrp>: <cit type="translation" xml:lang="la">
                        <quote>litigando exprobrare</quote>
                     </cit> alicui aliquid: Deus <foreign xml:lang="gez">ተዋቀሣ፡ ሞተ፡ ነቢያት፡</foreign>
                     <ref cRef="Sx. Masc." loc="8 ">Sx. Masc. 8 </ref>Enc.</sense>
               </sense>
               
            </entry>
