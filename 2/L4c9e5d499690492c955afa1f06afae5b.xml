         <div>
            <entry xml:id="L4c9e5d499690492c955afa1f06afae5b" n="9938">
               <form>
                  <foreign xml:lang="gez">ተፋቀረ</foreign>
               </form>
               <sense xml:lang="la" source="#dillmann" xml:id="Dmain"> <subc>III,3</subc> <cit type="translation" xml:lang="la">
                     <quote>sese</quote>
                  </cit>
                  <cit type="translation" xml:lang="la">
                     <quote>invicem</quote>
                  </cit>
                  <cit type="translation" xml:lang="la">
                     <quote>diligere</quote>
                  </cit> vel <cit type="translation" xml:lang="la">
                     <quote>amare</quote>
                  </cit>, <cit type="translation" xml:lang="la">
                     <quote>caritate</quote>
                  </cit>
                  <cit type="translation" xml:lang="la">
                     <quote>sese</quote>
                  </cit>
                  <cit type="translation" xml:lang="la">
                     <quote>complecti</quote>
                  </cit>, <cit type="translation" xml:lang="la">
                     <quote>amorem</quote>
                  </cit>
                  <cit type="translation" xml:lang="la">
                     <quote>habere</quote>
                  </cit>
                  <cit type="translation" xml:lang="la">
                     <quote>inter</quote>
                  </cit>
                  <cit type="translation" xml:lang="la">
                     <quote>se</quote>
                  </cit>: <ref cRef="Rom." loc="12,9">Rom. 12,9</ref>; <foreign xml:lang="gez">ተፋቀር፡
                     ምስለ፡ ብእሲትከ፡</foreign>
                  <ref cRef="Prov." loc="5,19">Prov. 5,19</ref>; Augustus Herodem jurare jussit;
                     <foreign xml:lang="gez">ከመ፡ ይትፋቀር፡ ምስለ፡ ውሉዱ፡ </foreign>
                  <ref cRef="Jsp." loc="331">Jsp. p. 331</ref>; <foreign xml:lang="gez">ትትፋቀሩ፡
                     በበይናቲክሙ፡</foreign>
                  <ref cRef="Kuf." loc="128">Kuf. p. 128</ref>; <ref cRef="Joh." loc="13,34">Joh.
                     13,34</ref>; <ref cRef="1 Joh." loc="3,11">1 Joh. 3,11</ref>; <ref cRef="1 Joh." loc="3,23">1 Joh. 3,23</ref>; <ref cRef="1 Joh." loc="4,7">1 Joh.
                     4,7</ref>; <foreign xml:lang="gez">ተፋቅሮ፡ </foreign>
                  <cit type="translation" xml:lang="la">
                     <quote>caritas</quote>
                  </cit>
                  <cit type="translation" xml:lang="la">
                     <quote>mutua</quote>
                  </cit>, creberrime, ut: <ref cRef="Rom." loc="13,10">Rom. 13,10</ref>; <ref cRef="1 Joh." loc="3,16">1 Joh. 3,16</ref>; <ref cRef="1 Joh." loc="4,7">1 Joh.
                     4,7</ref>; <ref cRef="1 Joh." loc="4,18">1 Joh. 4,18</ref>; <ref cRef="1 Petr. " loc="4,8">1 Petr. 4,8</ref>; <ref cRef="2 Petr." loc="1,7">2
                     Petr. 1,7</ref>; <ref cRef="Hebr." loc="13,1">Hebr. 13,1</ref>; <ref cRef="1 Cor." loc="13,1">1 Cor. 13,1</ref>
                  <lbl expand="sequens">seq.</lbl>; <ref cRef="Chrys. Ta." loc="3 ">Chrys. Ta. 3 </ref>
                  <lbl expand="sequens">seq.</lbl>; <foreign xml:lang="gez">ተፋቅሮትክሙ፡</foreign>
                  <ref cRef="1 Thess." loc="3,6">1 Thess. 3,6</ref>. <sense n="a" xml:id="Da"> de amoribus
                     inhonestis: <cit type="translation" xml:lang="la">
                        <quote>amores</quote>
                     </cit>
                     <cit type="translation" xml:lang="la">
                        <quote>habere</quote>
                     </cit> cum aliqua, <cit type="translation" xml:lang="la">
                        <quote>amoribus</quote>
                     </cit>
                     <cit type="translation" xml:lang="la">
                        <quote>mutuis</quote>
                     </cit>
                     <cit type="translation" xml:lang="la">
                        <quote>frui</quote>
                     </cit>, <foreign xml:lang="gez">ተፋቀረ፡ ምስሌሃ፡</foreign>
                     <ref cRef="Jsp." loc="327">Jsp. p. 327</ref>; <foreign xml:lang="gez">ፍቅር፡
                        ወተፋቅሮ፡</foreign>
                     <ref cRef="Jer." loc="2,33">Jer. 2,33</ref>; <foreign xml:lang="gez">ናስተሓውዝ፡
                        በስዒም፡ ወናንገርግር፡ በተፋቅሮ፡</foreign>
                     <ref cRef="Prov." loc="7,18">Prov. 7,18</ref>; <ref cRef="Did." loc="2.">Did.
                        2.</ref>
                  </sense>
                  <sense n="b" xml:id="Db">
                     <cit type="translation" xml:lang="la">
                        <quote>foedus</quote>
                     </cit>
                     <cit type="translation" xml:lang="la">
                        <quote>amicitiae</quote>
                     </cit>
                     <cit type="translation" xml:lang="la">
                        <quote>facere</quote>
                     </cit> cum aliquo: <cb n="1359" xml:id="c1359"/>
                     <foreign xml:lang="gez">ከመ፡ ይትፋቀሩ፡ ምስለ፡ ዮናናውያን፡ </foreign>
                     <ref cRef="Jsp." loc="277">Jsp. p. 277</ref>; <ref cRef="Jsp." loc="283.">Jsp.
                        p. 283.</ref>
                  </sense>
               </sense>
               
