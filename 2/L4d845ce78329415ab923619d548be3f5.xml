                  <foreign xml:lang="gez">ሐጸረ</foreign>
               </form>
               <sense xml:id="Dmain" xml:lang="la" source="#dillmann"> <subc>I,1</subc> [amh. <foreign xml:lang="gez">አጸረ፡</foreign>; i.q. <foreign xml:lang="he">חָצַר</foreign> (in <foreign xml:lang="he">חָצֵר</foreign> <cit type="translation" xml:lang="la"><quote> locus septus, area, atrium, villa </quote></cit>), <foreign xml:lang="ar">حَصَرَ</foreign> <cit type="translation" xml:lang="la"><quote> circumdedit, in angustiam redegit </quote></cit> (<foreign xml:lang="ar">حِصَارٌ</foreign> <cit type="translation" xml:lang="la"><quote> sepimentum, castellum </quote></cit>) <foreign xml:lang="ar">حَظَرَ</foreign> <cit type="translation" xml:lang="la"><quote> prohibuit </quote></cit> (<foreign xml:lang="ar">حِظَارٌ</foreign> <cit type="translation" xml:lang="la"><quote> paries, sepimentum </quote></cit>); <lbl expand="vicinus">vic.</lbl> est <foreign xml:lang="gez">ቀጸረ፡</foreign> q.v.] <gramGrp> <pos expand="Subjunctivus">Subj.</pos> </gramGrp> <foreign xml:lang="gez">ይሕጽር፡</foreign> <sense n="1" xml:id="D1"> <cit type="translation" xml:lang="la"><quote> circumdare </quote></cit> vel <cit type="translation" xml:lang="la"><quote> cingere </quote></cit> re, in sp. sepimento, muro, <cit type="translation" xml:lang="la"><quote> consepire </quote></cit>, <cit type="translation" xml:lang="la"><quote> circumsepire </quote></cit>, <cit type="translation" xml:lang="la"><quote> obstruere </quote></cit> vel <cit type="translation" xml:lang="la"><quote> concludere sepimento </quote></cit> vel <cit type="translation" xml:lang="la"><quote> muro </quote></cit>: <foreign xml:lang="gez">ሐጸሩኒ፡ በሐሰት፡</foreign> <foreign xml:lang="grc">ἐκύκλωσέ με ἐν ψεύει</foreign> <ref cRef="Hos." loc="11,12">Hos. 11,12</ref>; <foreign xml:lang="gez">ሰረገላተ፡ እሳት፡ ሐጸሮ፡</foreign> (v. <foreign xml:lang="gez">ሕጹር፡</foreign>) <foreign xml:lang="gez">ላዕለ፡ ኤልሳዕ፡</foreign> <foreign xml:lang="grc">περικύκλῳ Ἐλισαιέ</foreign> <ref cRef="4 Reg." loc="6,17">4 Reg. 6,17</ref>; <foreign xml:lang="gez">አሐጽር፡ ፍኖታ፡ በሦክ፡</foreign> <ref cRef="Hos." loc="2,8">Hos. 2,8</ref>; <foreign xml:lang="gez">እመኒ፡ ሐጸርከ፡ በሦክ፡ ላዕለ፡ ንዋይከ፡</foreign> <ref cRef="Sir." loc="28,24">Sir. 28,24</ref>; <foreign xml:lang="gez">አሐጽር፡ ሎሙ፡ ዐውደ፡ ደብርየ፡</foreign> <ref cRef="Hez." loc="34,26">Hez. 34,26</ref>; <foreign xml:lang="gez">ወሐጸሮ፡ ለማይ፡ በትእዛዙ፡</foreign> <foreign xml:lang="grc">πρόσταγμα ἐγύρωσεν ἐπὶ πρόσωπον ὕδατος</foreign> <ref cRef="Job" loc="26,10">Job 26,10</ref>; <foreign xml:lang="gez">ሐጸርክዋ፡ ለባሕር፡ በአናቅጽ፡</foreign> <ref cRef="Job" loc="38,8">Job 38,8</ref>; <foreign xml:lang="grc">συμφράξει</foreign> <ref cRef="Jes." loc="27,12">Jes. 27,12</ref>. Etiam <cit type="translation" xml:lang="la"><quote> obice intercludere </quote></cit> aquas: <foreign xml:lang="gez">ሐጸርኩ፡ ወአንጸፍኩ፡ ማየ፡</foreign> <foreign xml:lang="grc">ἔθηκα γέφυραν καὶ ἠρήμωσα ὕδατα</foreign> <ref cRef="Jes." loc="37,25">Jes. 37,25</ref>.
</sense> <sense n="2" xml:id="D2"> <cit type="translation" xml:lang="la"><quote> circumvallare </quote></cit> i.e. et <cit type="translation" xml:lang="la"><quote> munitionibus praemunire </quote></cit> locum et <cit type="translation" xml:lang="la"><quote> operibus cingere </quote></cit>, <cit type="translation" xml:lang="la"><quote> obsidere </quote></cit>. <sense n="a" xml:id="D2a"> <foreign xml:lang="gez">ሐጸሩ፡ አርእስተ፡ አድባር፡</foreign> <foreign xml:lang="grc">ἐτείχισαν</foreign> <ref cRef="Judith" loc="5,1">Judith 5,1</ref>; <lbl expand="translate">transl.</lbl>: <foreign xml:lang="gez">ይሕጽርክሙ፡ በሐጹረ፡ መስቀሉ፡</foreign> <ref cRef="Genz." loc="153.">Genz. f. 153.</ref> 
</sense> <sense n="b" xml:id="D2b"> <lbl expand="cum">c.</lbl> <gramGrp> <case value="accusativus">Acc.</case> </gramGrp>: <foreign xml:lang="gez">አሐጽረኪ፡ በማኅፈድ፡</foreign> <ref cRef="Jes." loc="29,3">Jes. 29,3</ref>; <ref cRef="Jer." loc="21,4">Jer. 21,4</ref> <lbl expand="versio altera">vers. alt.</lbl>; <ref cRef="Jer." loc="52,4">Jer. 52,4</ref>; <foreign xml:lang="gez">ተሐጽራ፡ በተዓይን፡</foreign> <ref cRef="Hez." loc="4,2">Hez. 4,2</ref>; <ref cRef="Hez." loc="21,22">Hez. 21,22</ref>; <ref cRef="Hez." loc="26,8">Hez. 26,8</ref>; <ref cRef="4 Reg." loc="25,1">4 Reg. 25,1</ref>; <cit type="translation" xml:lang="la"><quote> Vespasianus </quote></cit> <foreign xml:lang="gez">ሐጸራ፡ ለቤተ፡ መቅደስ፡</foreign> <ref cRef="Jsp." loc="342">Jsp. p. 342</ref>; <lbl expand="cum">c.</lbl> <foreign xml:lang="gez">ውስተ፡</foreign>, ut: <foreign xml:lang="gez">ሕጽሩ፡</foreign> (v. <foreign xml:lang="gez">ሕጸሩ፡</foreign>) <foreign xml:lang="gez">ሐጹረ፡ ውስተ፡ ሀገር፡</foreign> <ref cRef="3 Reg." loc="20,12">3 Reg. 20,12</ref>; <foreign xml:lang="gez">ላዕለ፡</foreign>, ut: <foreign xml:lang="gez">ይትዓየኑኪ፡</foreign> <cb n="0134" xml:id="c0134"/> <foreign xml:lang="gez">ወየሐጽሩ፡ ላዕሌኪ፡</foreign> <ref cRef="Luc." loc="19,43">Luc. 19,43</ref> <lbl expand="romanae editionis">rom.</lbl> – <foreign xml:lang="gez">ሐጸረ፡ ዘ፡ ደመቀ፡</foreign> (<lbl expand="videas">vid.</lbl> <foreign xml:lang="gez">ሐጸረ፡</foreign> <subc>III,3</subc>).
</sense> 
</sense> </sense>
               
            <sense xml:id="Tmain" xml:lang="en" source="#traces"> <cit type="transcription" xml:lang="gez"><quote>ḥaṣara</quote></cit>, <foreign xml:lang="gez">ኀፀረ</foreign> <cit type="transcription" xml:lang="gez"><quote>ḫaḍara</quote></cit> <sense n="G"> <foreign xml:lang="gez">ሐጸረ፡</foreign> <cit type="transcription" xml:lang="gez"><quote>ḥaṣara</quote></cit> <subc>I,1</subc> <foreign xml:lang="gez">የሐጽር፡</foreign>, <foreign xml:lang="gez">ይሕጽር፡</foreign> «<cit type="translation" xml:lang="fr"><quote>enceindre</quote></cit>, <cit type="translation" xml:lang="fr"><quote>entourer d’enceinte</quote></cit>» ― <foreign xml:lang="am">አጠረ፡</foreign> Ms. <ref type="BM" target="BNFabb217">BNFabb217</ref>, fol. 30v. <lb/>
 <bibl><ptr target="bm:Grebaut1952SuppLexicon"/><citedRange unit="page"> 67</citedRange></bibl> 
</sense> <sense n="L"> <foreign xml:lang="gez">ሐጸረ</foreign> <cit type="transcription" xml:lang="gez"><quote>ḥaṣara</quote></cit> (yǝḥṣǝr), (<bibl><ptr target="bm:AlaqaTayya1965MSawasew"/></bibl>) <foreign xml:lang="gez">ኀፀረ</foreign> <cit type="transcription" xml:lang="gez"><quote>ḫaḍara</quote></cit> <cit type="translation" xml:lang="en"><quote>fence off</quote></cit>, <cit type="translation" xml:lang="en"><quote>enclose with a fence</quote></cit>, <cit type="translation" xml:lang="en"><quote>make a rampart</quote></cit>, <cit type="translation" xml:lang="en"><quote>build a mound</quote></cit>, <cit type="translation" xml:lang="en"><quote>compass</quote></cit>, <cit type="translation" xml:lang="en"><quote>encompass</quote></cit>, <cit type="translation" xml:lang="en"><quote>besiege</quote></cit> <bibl><ptr target="bm:Leslau1987dic"/><citedRange unit="page"> 247a</citedRange></bibl> 
</sense> </sense></entry>
