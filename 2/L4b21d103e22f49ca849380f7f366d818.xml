            <entry xml:id="L4b21d103e22f49ca849380f7f366d818" n="1405">
               <form>
                  <foreign xml:lang="gez">መነየ</foreign>
               </form>
               <sense xml:lang="la" source="#dillmann" xml:id="Dmain"> <subc>I,2</subc> et <foreign xml:lang="gez">አመነየ፡</foreign> <subc>II,2</subc> nonnisi <ref cRef="1 Esr." loc="3,5">1 Esr. 3,5</ref>
                  <foreign xml:lang="gez">ዘይሜኒ፡</foreign>
                  <lbl expand="vel">v.</lbl>
                  <foreign xml:lang="gez">ዘያሜኒ፡ ወዘይኄይስ፡ ቃል፡ </foreign>
                  <foreign xml:lang="grc">λόγος ὃς ὑπερισχύνει</foreign> repertum, ubi significatio
                  non satis liquet; incertum est, num <cit type="translation" xml:lang="la">
                     <quote>optabiliorem esse</quote>
                  </cit> an <cit type="translation" xml:lang="la">
                     <quote>prosperius succedere</quote>
                  </cit> vel similis quaedam significatio statuenda sit. </sense>
               
            </entry>
