            <entry xml:id="L5a64ca065b0644ec8ed6f7664ae768f5" n="8359">
               <form>
                  <foreign xml:lang="gez">ተግሣጽ</foreign>
               </form>
               <sense xml:lang="la" source="#dillmann" xml:id="Dmain">
                  <gramGrp>
                     <pos expand="Substantivum">subst.</pos>
                  </gramGrp>, <gramGrp>
                     <gen value="masculinum">m.</gen>
                  </gramGrp> et <gramGrp>
                     <gen value="femininus">fem.</gen>
                  </gramGrp>, <lbl expand="pluralis">Pl.</lbl>
                  <foreign xml:lang="gez">ተግሣጻት፡ </foreign>
                  <sense n="1" xml:id="D1">
                     <cit type="translation" xml:lang="la">
                        <quote>increpatio</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>abjurgatio</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>reprehensio</quote>
                     </cit>: <foreign xml:lang="grc">πρόσταγμα</foreign>
                     <ref cRef="Job" loc="26,13">Job 26,13</ref>; <foreign xml:lang="grc">ἐπιτίμησις</foreign>
                     <ref cRef="Job " loc="26,11">Job 26,11</ref>; <ref cRef="Ps." loc="17,18">Ps.
                        17,18</ref>; <ref cRef="Ps." loc="75,6">Ps. 75,6</ref>; <ref cRef="Ps." loc="103,8">Ps. 103,8</ref>; <ref cRef="Koh." loc="7,6">Koh. 7,6</ref>;
                        <foreign xml:lang="grc">ἐλεγμός </foreign>
                     <ref cRef="Jes." loc="50,2">Jes. 50,2</ref>; <ref cRef="Sir." loc="21,6">Sir.
                        21,6</ref>; <foreign xml:lang="grc">ἔλεγχος</foreign>
                     <ref cRef="2 Tim. " loc="3,16">2 Tim. 3,16</ref>; <cit type="translation" xml:lang="la">
                        <quote>refutatio</quote>
                     </cit>
                     <ref cRef="Sx. Tachs." loc="22 ">Sx. Tachs. 22 </ref>Enc.</sense>
                  <sense n="2" xml:id="D2">
                     <cit type="translation" xml:lang="la">
                        <quote>castigatio</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>animadversio</quote>
                     </cit>: <foreign xml:lang="gez">ኢያፀርዕ፡ ተግሣጾ፡ </foreign>
                     <foreign xml:lang="grc">ἐνδελεχήσει μάστιγας αὐτῷ</foreign>
                     <ref cRef="Sir." loc="30,1">Sir. 30,1</ref>; <foreign xml:lang="gez">ተግሣጸ፡
                        እግዚአብሔር፡</foreign>
                     <foreign xml:lang="grc">ὀργὴ κυρίου</foreign>
                     <ref cRef="Mich." loc="7,9">Mich. 7,9</ref>. <sense n="a" xml:id="D2a">
                        <cit type="translation" xml:lang="la">
                           <quote>disciplina</quote>
                        </cit>, <cit type="translation" xml:lang="la">
                           <quote>morum conformatio</quote>
                        </cit>, <foreign xml:lang="grc">παιδεία</foreign>, <foreign xml:lang="gez">መቅሠፍት፡ ወተግሣጽ፡</foreign>
                        <foreign xml:lang="grc">μάστιγες καὶ παιδεία</foreign>
                        <ref cRef="Sir." loc="22,6">Sir. 22,6</ref>; <ref cRef="Prov." loc="1,2">Prov. 1,2</ref>; <ref cRef="Prov. " loc="3,11">Prov. 3,11</ref>; <ref cRef="Prov." loc="12,1">Prov. 12,1</ref>; <ref cRef="Ps." loc="49,18">Ps.
                           49,18</ref>; <ref cRef="Deut." loc="11,2">Deut. 11,2</ref>; <ref cRef="Jes." loc="50,5">Jes. 50,5</ref>; <ref cRef="Jer." loc="2,30">Jer.
                           2,30</ref>; <ref cRef="Jer." loc="17,23">Jer. 17,23</ref>; <ref cRef="Jer. " loc="39,33">Jer. 39,33</ref>; <ref cRef="Hebr." loc="12,5">Hebr. 12,5</ref>; <ref cRef="Hebr." loc="12,7">Hebr. 12,7</ref>;
                           <foreign xml:lang="gez">ተግሣጸ፡ ሰሎሞን፡ </foreign>
                        <foreign xml:lang="grc">αἱ παιδεῖαι Σαλωμῶντος</foreign> inscripta est
                        Proverbiorum pars posterior, <lbl expand="scilicet">sc.</lbl>
                        <ref cRef="Prov." loc="25">Prov. 25</ref>-29; <foreign xml:lang="gez">በእንተ፡
                           ተግሣጸ፡ ካህናት፡</foreign> F.M. 32; <foreign xml:lang="gez">በእንተ፡ ተግሣጸ፡
                           ሕዝባውያን፡ </foreign> F.M. 34.</sense>
                     <sense n="b" xml:id="D2b">
                        <cit type="translation" xml:lang="la">
                           <quote>institutio</quote>
                        </cit>, <cit type="translation" xml:lang="la">
                           <quote>eruditio</quote>
                        </cit>: <foreign xml:lang="gez">ተግሣጹ፡ </foreign>
                        <foreign xml:lang="grc">τὰ ἐντάλματα αὐτοῦ</foreign>
                        <ref cRef="Job" loc="23,11">Job 23,11</ref>; <foreign xml:lang="grc">δικαιώματα</foreign>
                        <ref cRef="Sir." loc="4,17">Sir. 4,17</ref>; <foreign xml:lang="gez">ዕቀብ፡
                           ተግሣጾሙ፡ ለአእሩግ፡</foreign>
                        <foreign xml:lang="grc">διήγημα </foreign>
                        <ref cRef="Sir." loc="8,9">Sir. 8,9</ref>; <foreign xml:lang="grc">συμβουλία</foreign>
                        <ref cRef="Sir." loc="6,23">Sir. 6,23</ref>; <foreign xml:lang="grc">ῥήματα</foreign>
                        <ref cRef="Sir." loc="12,12">Sir. 12,12</ref>; <foreign xml:lang="grc">διδασκαλία</foreign>
                        <ref cRef="Rom." loc="15,4">Rom. 15,4</ref>.</sense>
                     <sense n="c" xml:id="D2c">
                        <cit type="translation" xml:lang="la">
                           <quote>admonitio</quote>
                        </cit>, <cit type="translation" xml:lang="la">
                           <quote>adhortatio</quote>
                        </cit>, <cit type="translation" xml:lang="la">
                           <quote>paraenesis</quote>
                        </cit>: <foreign xml:lang="grc">νουθέτησις</foreign>, <foreign xml:lang="grc">νουθεσία</foreign>
                        <ref cRef="Prov." loc="2,2">Prov. 2,2</ref>; <ref cRef="Job" loc="5,17">Job
                           5,17</ref>; <ref cRef="1 Cor." loc="10,11">1 Cor. 10,11</ref>;<bibl>
                           <ptr target="bm:Ludolf1691Commentarius"/> Lud. Comm. hist.</bibl> p. 308;
                        Chrys., ubi singulis homiliis adjecta est pars paraenetica, <foreign xml:lang="gez">ተግሣጽ፡</foreign> inscripta; <cit type="translation" xml:lang="la">
                           <quote>tractatus paraeneticus</quote>
                        </cit>, ut: <foreign xml:lang="gez">አባ፡ ሲኖዳ፡ ደረሰ፡ ድርሳናተ፡ ቤዙኃተ፡ ወተግሣጻተ፡ ወቃላተ፡
                           በቍዔት፡</foreign>
                        <ref cRef="Sx. Haml." loc="7">Sx. Haml. 7</ref>; <ref cRef="Sx. Masc." loc="20">Sx. Masc. 20</ref>; Sx. <ref cRef="Ter" loc="15">Ter 15</ref>;
                           <ref cRef="Sx. Ter" loc="23 ">Sx. Ter 23 </ref>Enc.; Chrysostomus
                           <foreign xml:lang="gez">ደረሰ፡ ድርሳናተ፡ ወተግሣጻተ፡ ብዙኃተ፡</foreign>
                        <ref cRef="Chrys." loc="1.">Chrys. f. 1.</ref>
                     </sense>
                  </sense>
               </sense>
               
