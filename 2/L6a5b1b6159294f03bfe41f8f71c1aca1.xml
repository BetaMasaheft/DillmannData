         <div>
            <entry xml:id="L6a5b1b6159294f03bfe41f8f71c1aca1" n="4675">
               <form>
                  <foreign xml:lang="gez">ኅድገት</foreign>
               </form>
               <sense xml:lang="la" source="#dillmann" xml:id="Dmain">
                  <gramGrp>
                     <pos expand="nomen actionis">n. act.</pos>
                  </gramGrp>
                  <cit type="translation" xml:lang="la">
                     <quote>relictio</quote>
                  </cit>, <cit type="translation" xml:lang="la">
                     <quote>dimissio</quote>
                  </cit>, <foreign xml:lang="gez">ኅድገተ፡ ብዕል፡</foreign>
                  <ref cRef="Sx. Genb." loc="20 ">Sx. Genb. 20 </ref>Enc.; in specie <sense n="a" xml:id="Da">
                     <cit type="translation" xml:lang="la">
                        <quote>relictio haereditatis</quote>
                     </cit> F.N. 42.</sense>
                  <sense n="b" xml:id="Db">
                     <cit type="translation" xml:lang="la">
                        <quote>repudium</quote>
                     </cit>, ut: <foreign xml:lang="gez">በእንተ፡ ከሊአ፡ ኅድገተ፡ ብእሲት፡</foreign> F.N.
                     24,5.</sense>
                  <sense n="c" xml:id="Dc">
                     <cit type="translation" xml:lang="la">
                        <quote>manumissio</quote>
                     </cit>: <foreign xml:lang="gez">ከመ፡ ትጸውዕ፡ ኅድገተ፡</foreign>
                     <ref cRef="Jer." loc="41,8">Jer. 41,8</ref>; <ref cRef="Jer. " loc="41,15">Jer.
                        41,15</ref>; <ref cRef="Jer." loc="41,17">Jer. 41,17</ref> et in quibusdam
                     locis sub</sense>
                  <sense n="d" xml:id="Dd"> laudatis.</sense>
                  <sense n="e" xml:id="De">
                     <cit type="translation" xml:lang="la">
                        <quote>remissio</quote>
                     </cit> debiti, <cit type="translation" xml:lang="la">
                        <quote>immunitas</quote>
                     </cit>
                     <ref cRef="Lev." loc="27,17">Lev. 27,17</ref>; <ref cRef="Lev." loc="27,24">Lev. 27,24</ref>; <ref cRef="Deut." loc="15,1">Deut. 15,1</ref>; <ref cRef="Deut. " loc="15,9">Deut. 15,9</ref>; <ref cRef="Hez." loc="46,17">Hez.
                        46,17</ref>; <ref cRef="Kuf." loc="7">Kuf. 7</ref>; <ref cRef="2 Esr." loc="20,31">2 Esr. 20,31</ref> (<ref cRef="Esth." loc="2,18">Esth.
                        2,18</ref> libri <foreign xml:lang="gez">ኅድጋት፡</foreign> praebere
                     solent).</sense>
                  <sense n="f" xml:id="Df">
                     <cit type="translation" xml:lang="la">
                        <quote>venia</quote>
                     </cit> legis (dispensatio) <ref cRef="Judith" loc="11,14">Judith
                     11,14</ref>.</sense>
                  <sense n="g" xml:id="Dg">
                     <cit type="translation" xml:lang="la">
                        <quote>remissio</quote>
                     </cit> peccati <ref cRef="Luc." loc="24,47">Luc. 24,47</ref>; <ref cRef="Kuf." loc="15.">Kuf. 15.</ref>
                     <ref cRef="Kuf." loc="30">Kuf. 30</ref>; <ref cRef="Hen." loc="12,5">Hen.
                        12,5</ref>; <ref cRef="Hen." loc="13,4">Hen. 13,4</ref>; Cyr. ad <ref cRef="Theod." loc="22.">Theod. f. 22.</ref>
                  </sense>
               </sense>
               
