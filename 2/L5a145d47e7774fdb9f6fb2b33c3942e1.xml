               <sense xml:id="Dmain" xml:lang="la" source="#dillmann"> <gramGrp> <pos expand="Substantivum">subst.</pos> </gramGrp>, [<foreign xml:lang="he">אָב</foreign>, <foreign xml:lang="he">אַבָּא</foreign>, <foreign xml:lang="syr">ܐܰܒܳܐ</foreign>, <foreign xml:lang="ar">أَبٌ</foreign>] <gramGrp> <gen value="not found">m.</gen> </gramGrp>, st.c. et <gramGrp> <case value="accusativus">Acc.</case> </gramGrp> <foreign xml:lang="gez">አበ፡</foreign> (ut: <ref cRef="Gen. " loc="44,19">Gen.  44,19</ref>; <ref cRef="Gen." loc="44,20">Gen. 44,20</ref>; <foreign xml:lang="gez">አልብነ፡ አበ፡</foreign> <ref cRef="Thren." loc="5,3">Thren. 5,3</ref>; <foreign xml:lang="gez">ዘአልቦ፡ አበ፡</foreign> <foreign xml:lang="grc">ἀπάτωρ</foreign> <ref cRef="Hebr. " loc="7,3">Hebr.  7,3</ref>; <ref cRef="Matth." loc="3,9">Matth. 3,9</ref>; <ref cRef="Sir." loc="3,2">Sir. 3,2</ref>; quamquam <ref cRef="Matth." loc="19,29">Matth. 19,29</ref> Platt exhibet <foreign xml:lang="gez">አባ፡</foreign>), ante <gramGrp> <pos expand="pronomen suffixum">pron. suff.</pos> </gramGrp> <foreign xml:lang="gez">አቡ፡</foreign> in Nom. (<ref cRef="Gen." loc="24,23">Gen. 24,23</ref>; <ref cRef="Gen." loc="31,5">Gen. 31,5</ref>; <ref cRef="Gen." loc="31,6">Gen. 31,6</ref>; <ref cRef="Gen." loc="31,7">Gen. 31,7</ref>; <ref cRef="Gen." loc="38,11">Gen. 38,11</ref>; <ref cRef="Gen." loc="44,25">Gen. 44,25</ref>; <ref cRef="Gen." loc="44,31">Gen. 44,31</ref>; <ref cRef="Gen. " loc="44,32">Gen.  44,32</ref>; <ref cRef="Gen." loc="45,18">Gen. 45,18</ref>; <ref cRef="Gen." loc="45,19">Gen. 45,19</ref>; <ref cRef="Gen." loc="47,5">Gen. 47,5</ref>; <ref cRef="Gen." loc="47,6">Gen. 47,6</ref>; <ref cRef="Ex." loc="2,16">Ex. 2,16</ref>; <ref cRef="Ex." loc="2,18">Ex. 2,18</ref>; <ref cRef="Deut." loc="22,15">Deut. 22,15</ref>; <ref cRef="Deut." loc="22,16">Deut. 22,16</ref>). {<ref target="#c1432"/> Pro <foreign xml:lang="gez">አቡሃ፡</foreign> poëta <foreign xml:lang="gez">አባ፡</foreign> scripsit, vid. col. 1123 sub <foreign xml:lang="gez">ድክታም፡</foreign>}; <foreign xml:lang="gez">አባ፡</foreign> in <gramGrp> <case value="accusativus">Acc.</case> </gramGrp> (ut: <foreign xml:lang="gez">አባሁ፡</foreign> <ref cRef="Gen." loc="2,24">Gen. 2,24</ref>; <ref cRef="Lev." loc="19,3">Lev. 19,3</ref>; <ref cRef="Sir." loc="3,3">Sir. 3,3</ref> <lbl expand="sequens">seq.</lbl>; <ref cRef="Mal." loc="1,6">Mal. 1,6</ref>; <ref cRef="Mich." loc="7,6">Mich. 7,6</ref>; <ref cRef="Prov. " loc="10,1">Prov.  10,1</ref>; <ref cRef="Prov." loc="15,20">Prov. 15,20</ref>; <ref cRef="Matth." loc="19,5">Matth. 19,5</ref>; <foreign xml:lang="gez">አቦ፡</foreign> pro <foreign xml:lang="gez">አባሁ፡</foreign> praebent duo libri a me inspecti <ref cRef="Prov." loc="24,52">Prov. 24,52</ref>; <foreign xml:lang="gez">አባሃ፡</foreign> <ref cRef="Sir." loc="22,5">Sir. 22,5</ref>; <foreign xml:lang="gez">አባሆሙ፡</foreign> <ref cRef="Marc." loc="1,20">Marc. 1,20</ref>; <ref cRef="Rom." loc="4,17">Rom. 4,17</ref>; <foreign xml:lang="gez">አባከ፡</foreign> <ref cRef="Ex." loc="20,12">Ex. 20,12</ref>; <ref cRef="Deut. " loc="5,16">Deut.  5,16</ref>; <ref cRef="Eph." loc="6,2">Eph. 6,2</ref>; <foreign xml:lang="gez">አባኪ፡</foreign> <ref cRef="Jos." loc="2,18">Jos. 2,18</ref>; <ref cRef="Ruth" loc="2,11">Ruth 2,11</ref>; <foreign xml:lang="gez">አበኪ፡</foreign> <ref cRef="Tob." loc="11,16">Tob. 11,16</ref> in uno libro a me inspecto; <foreign xml:lang="gez">አባየ፡</foreign> <ref cRef="Joh." loc="8,49">Joh. 8,49</ref>), sed etiam <foreign xml:lang="gez">አቡ፡</foreign> (<lbl expand="videas">vid.</lbl> <bibl><ptr target="bm:Dillmann1857Gramm"/>gramm.</bibl> § 154, d; ut: <foreign xml:lang="gez">አቡሃ፡</foreign> <ref cRef="Deut." loc="21,13">Deut. 21,13</ref>; <ref cRef="Jud." loc="15,6">Jud. 15,6</ref>; <foreign xml:lang="gez">አቡሆሙ፡</foreign> <ref cRef="Gen." loc="4,21">Gen. 4,21</ref>; <foreign xml:lang="gez">አቡኪ፡</foreign> <ref cRef="Jos." loc="2,18">Jos. 2,18</ref> <lbl expand="annotatio">ann.</lbl>; <foreign xml:lang="gez">አቡየ፡</foreign> <ref cRef="Joh." loc="8,19">Joh. 8,19</ref>; <ref cRef="Joh." loc="15,24">Joh. 15,24</ref>; <foreign xml:lang="gez">አቡነ፡</foreign> <ref cRef="Jud." loc="18,19">Jud. 18,19</ref>), in Vocat. <foreign xml:lang="gez">አባ፡</foreign> (ut: <ref cRef="Kuf." loc="11">Kuf. 11</ref>; <ref cRef="Gen." loc="22,7">Gen. 22,7</ref>; Gen.27,18; <ref cRef="Gen." loc="44,32">Gen. 44,32</ref>; <ref cRef="4 Reg." loc="2,12">4 Reg. 2,12</ref>; <ref cRef="4 Reg." loc="13,14">4 Reg. 13,14</ref>; <ref cRef="Tob." loc="5,1">Tob. 5,1</ref>; <ref cRef="Tob." loc="11,10">Tob. 11,10</ref>; <ref cRef="Tob. " loc="12,2">Tob.  12,2</ref>; <ref cRef="Matth." loc="11,25">Matth. 11,25</ref>; vel <foreign xml:lang="gez">አቡየ፡</foreign> <ref cRef="Sir." loc="23,1">Sir. 23,1</ref>; <ref cRef="Sir." loc="23,4">Sir. 23,4</ref>; <foreign xml:lang="gez">አባ፡ ወአቡየ፡</foreign> <foreign xml:lang="grc">ἀββᾶ ὁ πατὴρ</foreign> Marc.14,36; <ref cRef="Gal." loc="4,6">Gal. 4,6</ref>; <ref cRef="Rom." loc="8,15">Rom. 8,15</ref>); <lbl expand="pluralis">Pl.</lbl> <foreign xml:lang="gez">አበው፡</foreign>, <cit type="translation" xml:lang="la"><quote> pater </quote></cit> <sense n="a" xml:id="Da"> <cit type="translation" xml:lang="la"><quote> parens </quote></cit>, <cit type="translation" xml:lang="la"><quote> genitor </quote></cit>, ut <ref cRef="1 Reg." loc="9,3">1 Reg. 9,3</ref>; <ref cRef="Ps." loc="103,13">Ps. 103,13</ref>; <ref cRef="Jer." loc="6,21">Jer. 6,21</ref>; <ref cRef="Matth." loc="2,22">Matth. 2,22</ref>; <ref cRef="Matth." loc="4,21">Matth. 4,21</ref>; <ref cRef="Luc." loc="1,17">Luc. 1,17</ref>; <ref cRef="Eph." loc="6,4">Eph. 6,4</ref>, aliis in locis innumeris; <foreign xml:lang="gez">አበወ፡ ሥጋ፡</foreign> et <foreign xml:lang="gez">አበው፡ ሥጋውያን፡</foreign> (<lbl expand="oppositum, -o, -nitur">opp.</lbl> spiritualibus) <ref cRef="Chrys. ho." loc="29.">Chrys. ho. 29.</ref> 
</sense> <sense n="b" xml:id="Db"> <lbl expand="pluralis">Pl.</lbl> <foreign xml:lang="gez">አበው፡</foreign> (non tantum <cit type="translation" xml:lang="la"><quote> patres </quote></cit> sed etiam) <cit type="translation" xml:lang="la"><quote> parentes </quote></cit> i.e. <cit type="translation" xml:lang="la"><quote> pater </quote></cit> et <cit type="translation" xml:lang="la"><quote> mater </quote></cit>: <ref cRef="Hebr." loc="11,23">Hebr. 11,23</ref>; Reg. Pach.; <ref cRef="Sx. Mag." loc="27">Sx. Mag. 27</ref>; <ref cRef="Sx. Nah." loc="15">Sx. Nah. 15</ref>; <ref cRef="Const. Ap." loc="34">Const. Ap. 34</ref>; 
</sense> <sense n="c" xml:id="Dc"> <cit type="translation" xml:lang="la"><quote> progenitor </quote></cit>, <cit type="translation" xml:lang="la"><quote> auctor </quote></cit> <cit type="translation" xml:lang="la"><quote> generis </quote></cit>, <cit type="translation" xml:lang="la"><quote> majores </quote></cit>, ut: <foreign xml:lang="gez">አበ፡ ብዙኅ፡ አሕዛብ፡</foreign> <ref cRef="Gen." loc="17,4">Gen. 17,4</ref>; <ref cRef="Gen." loc="19,37">Gen. 19,37</ref>; <ref cRef="Gen." loc="22,21">Gen. 22,21</ref>; <ref cRef="Matth." loc="3,9">Matth. 3,9</ref>; <foreign xml:lang="gez">አበ፡ ኵልነ፡</foreign> <ref cRef="Rom." loc="4,16">Rom. 4,16</ref>; <foreign xml:lang="gez">አበዊነ፡</foreign> <ref cRef="Sir." loc="44,1">Sir. 44,1</ref>; <foreign xml:lang="gez">አበዊክሙ፡</foreign> <ref cRef="Zach." loc="1,5">Zach. 1,5</ref>; <ref cRef="Ps." loc="38,16">Ps. 38,16</ref>; <ref cRef="Ps." loc="44,18">Ps. 44,18</ref>; <ref cRef="Matth. " loc="23,30">Matth.  23,30</ref>; <ref cRef="Matth." loc="23,32">Matth. 23,32</ref>; <foreign xml:lang="gez">ርእሰ፡ አበው፡</foreign> <ref cRef="Act." loc="2,29">Act. 2,29</ref>; <foreign xml:lang="gez">አበ፡ አበወ፡</foreign> <cit type="translation" xml:lang="la"><quote> patriarcha </quote></cit> <ref cRef="Chrys. ho." loc="25">Chrys. ho. 25</ref>; <foreign xml:lang="gez">አበው፡ ቀደምት፡</foreign> <ref cRef="Act." loc="7,8">Act. 7,8</ref>. 
</sense> <sense n="d" xml:id="Dd"> <cit type="translation" xml:lang="la"><quote> altor </quote></cit> vel <cit type="translation" xml:lang="la"><quote> tutor </quote></cit>, qui patris instar beneficiis de aliis bene meretur: <ref cRef="Ps." loc="67,5">Ps. 67,5</ref>; <ref cRef="Jes." loc="9,6">Jes. 9,6</ref> <lbl expand="versio altera">vers. alt.</lbl>; <ref cRef="Jes." loc="22,21">Jes. 22,21</ref>; <foreign xml:lang="gez">አነ፡ ውእቱ፡ አበ፡ ምሰኪናን፡</foreign> <ref cRef="Job" loc="29,16">Job 29,16</ref>. 
</sense> <sense n="e" xml:id="De"> de Deo, patre hominum, in specie Christi et Christianorum: <ref cRef="Jes." loc="63,16">Jes. 63,16</ref>; <ref cRef="Jes." loc="64,8">Jes. 64,8</ref>; <ref cRef="Tob." loc="13,4">Tob. 13,4</ref>; <ref cRef="Deut." loc="32,6">Deut. 32,6</ref>; <ref cRef="2 Reg." loc="7,14">2 Reg. 7,14</ref>; <ref cRef="Matth." loc="6,9">Matth. 6,9</ref>; <ref cRef="Matth." loc="11,27">Matth. 11,27</ref>; <ref cRef="Matth. " loc="24,36">Matth.  24,36</ref>; <ref cRef="Hebr." loc="12,9">Hebr. 12,9</ref>; <ref cRef="Eph." loc="4,6">Eph. 4,6</ref>; <ref cRef="Rom." loc="1,7">Rom. 1,7</ref>; <ref cRef="1 Cor." loc="1,3">1 Cor. 1,3</ref>, al.; <foreign xml:lang="gez">አቡነ፡ ዘበሰማያት፡</foreign> „<cit type="translation" xml:lang="la"><quote> pater </quote></cit> <cit type="translation" xml:lang="la"><quote> noster </quote></cit>“, ut: <foreign xml:lang="gez">ወትብል፡ ኪርያላይሶን፡ ወአቡነ፡ ዘበሰማያት፡</foreign> <ref cRef="Genz." loc="52.">Genz. f. 52.</ref> 
</sense> <sense n="f" xml:id="Df"> <cit type="translation" xml:lang="la"><quote> patris </quote></cit> nomine honorifice compellantur. <sense n="α" xml:id="Dfα"> magistri, prophetae, sacerdotes <ref cRef="Jud." loc="17,10">Jud. 17,10</ref>; <ref cRef="Jud." loc="18,19">Jud. 18,19</ref>; <ref cRef="4 Reg." loc="2,12">4 Reg. 2,12</ref>; <ref cRef="4 Reg." loc="6,21">4 Reg. 6,21</ref>; <ref cRef="4 Reg." loc="13,14">4 Reg. 13,14</ref>; <ref cRef="Matth." loc="23,9">Matth. 23,9</ref>; et in libris Christianis sacerdotes episcopi al. patres spirituales appellantur, ut: <foreign xml:lang="gez">አበ፡ መንፈስ፡ የዐቢ፡ እምአበ፡ ሥጋ፡</foreign> <ref cRef="Genz." loc="148">Genz. f. 148</ref>; <ref cRef="Did." loc="8.">Did. 8.</ref> 
</sense> <sense n="β" xml:id="Dfβ"> <cit type="translation" xml:lang="la"><quote> abbas </quote></cit> monachorum, sive <foreign xml:lang="gez">አብ፡</foreign> ut <ref cRef="Phlx." loc="">Phlx. </ref>, sive <foreign xml:lang="gez">አበ፡ ምኔት፡</foreign> <ref cRef="Kedr f." loc="79,">Kedr f. 79,</ref> al. 
</sense> <sense n="γ" xml:id="Dfγ"> annis provectiores, seniores, ut <ref cRef="1 Joh." loc="2,13">1 Joh. 2,13</ref>; <ref cRef="Joh." loc="2,14">Joh. 2,14</ref>; magistratus, senatores, ut <ref cRef="Act." loc="7,2">Act. 7,2</ref>; <ref cRef="Act." loc="22,1">Act. 22,1</ref>; seniores gentium vel familiarum; ut: <foreign xml:lang="gez">እሉ፡ መላእክተ፡ ቤተ፡ አበዊሆሙ፡</foreign> <ref cRef="1 Par." loc="7,2">1 Par. 7,2</ref>; <foreign xml:lang="gez">መላእክተ፡ አበዊሆሙ፡</foreign> <ref cRef="2 Esr." loc="18,13">2 Esr. 18,13</ref>, et rex ipse, in formula <foreign xml:lang="gez">ሕያው፡</foreign> vel <foreign xml:lang="gez">ሕየው፡ አበ፡ ነጋሢ፡</foreign> (<lbl expand="videas">vid.</lbl> supra sub <foreign xml:lang="gez">ነጋሢ፡</foreign> col. 688.; <lbl expand="secundum">sec.</lbl> Lud. in <lbl expand="lexicon">lex.</lbl> <lbl expand="cum">c.</lbl> 350: <foreign xml:lang="gez">ሕየው፡ አባ፡ ነጋሢ፡</foreign>). 
</sense> 
</sense> <sense n="g" xml:id="Dg"> <foreign xml:lang="gez">አበ፡ ደም፡</foreign> consanguineorum maximus, cujus vindicta sanguinis est i.e. <cit type="translation" xml:lang="la"><quote> sanguinis </quote></cit> <cit type="translation" xml:lang="la"><quote> ultor </quote></cit> vel <cit type="translation" xml:lang="la"><quote> vindex </quote></cit>, <ref cRef="Num." loc="35,12">Num. 35,12</ref>; <ref cRef="Num." loc="35,19">Num. 35,19</ref>; <ref cRef="Num." loc="35,21">Num. 35,21</ref>; <ref cRef="Num." loc="35,25">Num. 35,25</ref>; <ref cRef="Num." loc="35,27">Num. 35,27</ref>; <ref cRef="Deut." loc="19,6">Deut. 19,6</ref>; <ref cRef="Deut." loc="19,12">Deut. 19,12</ref>; <ref cRef="Jos." loc="20,5">Jos. 20,5</ref>; <ref cRef="2 Reg." loc="14,11">2 Reg. 14,11</ref>. 
</sense> <sense n="h" xml:id="Dh"> <cit type="translation" xml:lang="la"><quote> possessor </quote></cit> <cit type="translation" xml:lang="la"><quote> dominus </quote></cit>: <foreign xml:lang="gez">አበ፡ ላህም፡</foreign> <ref cRef="Ex." loc="21,35">Ex. 21,35</ref>; (coll. <foreign xml:lang="gez">አበጉንባሕ፡</foreign> sub <foreign xml:lang="gez">ጉንባሕ፡</foreign>). 
</sense> <sense n="i" xml:id="Di"> <cit type="translation" xml:lang="la"><quote> auctor </quote></cit>, ut: <foreign xml:lang="gez">አቡሃ፡ ለሐሰት፡</foreign> <ref cRef="Joh." loc="8,44">Joh. 8,44</ref>; ubi de libris agitur: <cit type="translation" xml:lang="la"><quote> prototypum </quote></cit>, <cb n="0755" xml:id="c0755"/> <cit type="translation" xml:lang="la"><quote> archetypum </quote></cit> (exemplar) libri: <lbl expand="Novum Testamentum">N.T.</lbl> <lbl expand="romanae editionis">rom.</lbl> f. 157 (sub finem Actorum). 
</sense> </sense>
               
            <sense xml:id="Tmain" xml:lang="en" source="#traces"><cit type="transcription" xml:lang="gez"><quote>ʾab</quote></cit> <ptr target="#Dg"/> <cit type="transcription" xml:lang="gez"><quote>ʾaba dam</quote></cit> <foreign xml:lang="gez">እመቦ፡ አበ፡ ደም፡ ለውእቱ፡ ብእሲ፡ ወይፈቅድ፡ ይቅትሎ፡ ይትቤዘዎ፡ እምኔሁ፡ በቃለ፡ ድኩም፡</foreign> ‘<cit type="translation" xml:lang="en"><quote>If there was an avanger of the blood and he wanted to kill this man (culprit), he redeemed him from him with humble word and paid the proper redemption</quote></cit>’ <bibl><ptr target="bm:Solomon2019GalawdewosI"/><citedRange unit="page"> 62 § 117 (ed.)</citedRange></bibl>, <bibl><ptr target="bm:Solomon2019GalawdewosII"/><citedRange unit="page"> 36 § 117 (tr.)</citedRange></bibl> <sense n="L" xml:id="TL"> <foreign xml:lang="gez">አበ፡ ምኔት</foreign> <cit type="transcription" xml:lang="gez"><quote>ʾabba mǝnet</quote></cit> <cit type="translation" xml:lang="en"><quote>abbot of a monastery</quote></cit>, <lbl expand="videas">vid.</lbl> <foreign xml:lang="gez">ምኔት</foreign> <bibl><ptr target="bm:Leslau1987dic"/><citedRange unit="page"> 351b</citedRange></bibl> 
