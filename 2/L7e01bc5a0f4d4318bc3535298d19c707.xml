            <entry xml:id="L7e01bc5a0f4d4318bc3535298d19c707" n="5651">
               <form>
                  <foreign xml:lang="gez">አእበደ</foreign>
               </form>
               <sense xml:lang="la" source="#dillmann" xml:id="Dmain"> <subc>II,1</subc> <sense n="1" xml:id="D1"> <cit type="translation" xml:lang="la">
                                <quote> amentem </quote>
                            </cit> vel <cit type="translation" xml:lang="la">
                                <quote> stultum reddere </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> infatuare </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> decipere </quote>
                            </cit>: <foreign xml:lang="gez">አኮኑ፡ አእበዳ፡ እግዚአብሔር፡ ለጥበበ፡ ዝዓለም፡</foreign> <ref cRef="1 Cor." loc="1,20">1 Cor. 1,20</ref>; <foreign xml:lang="gez">ያአብድዎሙ፡</foreign> (<foreign xml:lang="gez">አጋንንት፡ ለሰብእ፡</foreign>) <ref cRef="Kuf." loc="10">Kuf. 10</ref>; <ref cRef="Act." loc="8,11">Act. 8,11</ref>; <foreign xml:lang="gez">ኢታእብደኒ፡</foreign> (v. <foreign xml:lang="gez">ታስተአብደኒ፡</foreign>) <foreign xml:lang="grc">μὴ πλάνα με</foreign> <ref cRef="Tob." loc="10,7">Tob. 10,7</ref>; <foreign xml:lang="gez">አእብዶት፡</foreign> <ref cRef="Ssal. Req." loc="">Ssal. Req. </ref>
                        </sense> <sense n="2" xml:id="D2"> <cit type="translation" xml:lang="la">
                                <quote> ad insaniam adigere </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> in furorem </quote>
                            </cit> vel <cit type="translation" xml:lang="la">
                                <quote> rabiem agere </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> furiosum reddere </quote>
                            </cit>: <ref cRef="Luc." loc="8,29">Luc. 8,29</ref>; <foreign xml:lang="gez">ለምንት፡ ታአብድ፡</foreign> (<foreign xml:lang="gez">ሰይጣን፡</foreign>) <foreign xml:lang="gez">ሰብአ፡</foreign> <ref cRef="Gad. T.H." loc="">Gad. T.H. </ref> </sense> </sense>
               
