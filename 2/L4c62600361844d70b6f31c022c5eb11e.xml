               <sense xml:id="Dmain" xml:lang="la" source="#dillmann"> <subc>I,1</subc> [<foreign xml:lang="ar">رَقَبَ</foreign>] <gramGrp> <pos expand="Subjunctivus">Subj.</pos> </gramGrp> <foreign xml:lang="gez">ይዕቀብ፡</foreign>, Imperat. <foreign xml:lang="gez">ዕቀብ፡</foreign> (forma <foreign xml:lang="gez">ዕቅብ፡</foreign> <ref cRef="4 Esr." loc="14,6">4 Esr. 14,6</ref> in editione Laur. lapsu calami scripta videtur), <sense n="1" xml:id="D1"> <cit type="translation" xml:lang="la"><quote> custodire </quote></cit>, <lbl expand="cum">c.</lbl> <gramGrp> <case value="accusativus">Acc.</case> </gramGrp>, <ref cRef="4 Reg." loc="9,14">4 Reg. 9,14</ref>; <ref cRef="Jes." loc="21,12">Jes. 21,12</ref>; <ref cRef="1 Reg." loc="25,21">1 Reg. 25,21</ref>; <ref cRef="Luc." loc="11,21">Luc. 11,21</ref>; <ref cRef="Act. " loc="22,20">Act.  22,20</ref>; <ref cRef="Matth." loc="27,36">Matth. 27,36</ref>; gregem <ref cRef="Luc." loc="2,8">Luc. 2,8</ref>; <ref cRef="Gen." loc="30,31">Gen. 30,31</ref>; <foreign xml:lang="gez">ኮነ፡ የዐቅብ፡ አባግዐ፡ አቡሁ፡</foreign> <ref cRef="Sx. Sen." loc="4">Sx. Sen. 4</ref>; urbem <ref cRef="Ps." loc="126,2">Ps. 126,2</ref>; portam <ref cRef="Judith" loc="13,11">Judith 13,11</ref>; <ref cRef="3 Reg. " loc="14,27">3 Reg.  14,27</ref>; <ref cRef="Asc. Jes." loc="10,24">Asc. Jes. 10,24</ref>; hortum <ref cRef="Gen." loc="2,15">Gen. 2,15</ref>; <lbl expand="cum">c.</lbl> <foreign xml:lang="gez">እምነ፡</foreign> <cit type="translation" xml:lang="la"><quote> ab </quote></cit> aliquo: <foreign xml:lang="gez">የዐቅብ፡ ገነተ፡ እምአዕዋፍ፡</foreign> <ref cRef="Kuf." loc="13">Kuf. p. 13</ref>; <foreign xml:lang="gez">ከመ፡ ይዕቀቡ፡ ዘርኦሙ፡ እምገጸ፡ ቋዓት፡</foreign> <ref cRef="Kuf." loc="47">Kuf. p. 47</ref>; <ref cRef="Kuf." loc="157">Kuf. p. 157</ref>; homines: <foreign xml:lang="gez">እለ፡ የዐቅቡኪ፡</foreign> <ref cRef="Jes." loc="52,8">Jes. 52,8</ref>; <foreign xml:lang="gez">እመ፡ ብከ፡ አዋልደ፡ ዕቀቦን፡</foreign> <ref cRef="Sir. " loc="7,24">Sir.  7,24</ref>; <foreign xml:lang="gez">ለወለትከ፡ አጽንዓ፡ ዐቂቦታ፡</foreign> <ref cRef="Sir." loc="26,10">Sir. 26,10</ref>; <ref cRef="1 Reg." loc="26,15">1 Reg. 26,15</ref>. <sense n="a" xml:id="D1a"> <gramGrp> <pos expand="absolute">absol.</pos> </gramGrp>: <cit type="translation" xml:lang="la"><quote> vigilare </quote></cit>, <cit type="translation" xml:lang="la"><quote> excubias agere </quote></cit>, <ref cRef="Judith" loc="7,5">Judith 7,5</ref>; <ref cRef="Jes. " loc="21,12">Jes.  21,12</ref>; <ref cRef="Jes." loc="21,13">Jes. 21,13</ref> <lbl expand="varia lectio">var.</lbl>; <ref cRef="Koh." loc="5,7">Koh. 5,7</ref>; <ref cRef="Matth." loc="28,4">Matth. 28,4</ref>; <foreign xml:lang="gez">ሠገራት፡ የዐቅቡ፡ እንተ፡ ኵለሄ፡</foreign> Org. 
</sense> <sense n="b" xml:id="D1b"> <cit type="translation" xml:lang="la"><quote> tueri </quote></cit>, <cit type="translation" xml:lang="la"><quote> incolumem servare </quote></cit>, <cit type="translation" xml:lang="la"><quote> defendere </quote></cit>: <ref cRef="Ps." loc="11,8">Ps. 11,8</ref>; <ref cRef="Ps." loc="120,5">Ps. 120,5</ref>; <ref cRef="Ps." loc="120,8">Ps. 120,8</ref>; <ref cRef="Prov." loc="2,11">Prov. 2,11</ref>; <cb n="0978" xml:id="c0978"/> <ref cRef="Hen." loc="1,8">Hen. 1,8</ref>; <ref cRef="4 Esr." loc="6,68">4 Esr. 6,68</ref>; <foreign xml:lang="gez">ዐቀበ፡ መንፈስየ፡</foreign> <ref cRef="Job" loc="10,12">Job 10,12</ref>; <ref cRef="Gen." loc="28,15">Gen. 28,15</ref>; <ref cRef="Gen." loc="28,20">Gen. 28,20</ref>; <ref cRef="Ex." loc="23,20">Ex. 23,20</ref>; <ref cRef="1 Reg. " loc="30,23">1 Reg.  30,23</ref>; <ref cRef="Matth." loc="4,6">Matth. 4,6</ref>; <ref cRef="Joh." loc="17,12">Joh. 17,12</ref>; <lbl expand="cum">c.</lbl> <foreign xml:lang="gez">እምነ፡</foreign> <cit type="translation" xml:lang="la"><quote> ab </quote></cit>: <ref cRef="2 Thess." loc="3,3">2 Thess. 3,3</ref>; <ref cRef="Ps." loc="120,7">Ps. 120,7</ref>; <ref cRef="Ps." loc="139,4">Ps. 139,4</ref>; <ref cRef="Sir." loc="31,19">Sir. 31,19</ref>. 
</sense> <sense n="c" xml:id="D1c"> custodire aliquid i.e. <cit type="translation" xml:lang="la"><quote> caute uti </quote></cit>, <cit type="translation" xml:lang="la"><quote> cautionem adhibere </quote></cit>(<cit type="translation" xml:lang="de"><quote> in Acht nehmen </quote></cit>): <foreign xml:lang="gez">ዕቀብ፡ እገሪከ፡</foreign> <ref cRef="Koh." loc="4,17">Koh. 4,17</ref>; <foreign xml:lang="gez">ዐቂበ፡ ልሳን፡</foreign> Fal.; <foreign xml:lang="gez">ዘየዐቅብ፡ አፉሁ፡</foreign> <ref cRef="Prov." loc="13,3">Prov. 13,3</ref>; <ref cRef="Prov." loc="21,23">Prov. 21,23</ref>; <lbl expand="sequens">seq.</lbl> <foreign xml:lang="gez">ርእሶ፡</foreign> vel <foreign xml:lang="gez">ነፍሶ፡</foreign> i.q. <cit type="translation" xml:lang="la"><quote> cavere </quote></cit>: <foreign xml:lang="gez">ዕቀቡ፡ ነፍሰክሙ፡</foreign> <ref cRef="Jer." loc="17,21">Jer. 17,21</ref>; <ref cRef="Deut." loc="4,9">Deut. 4,9</ref>; <ref cRef="Deut." loc="4,15">Deut. 4,15</ref>; <foreign xml:lang="gez">ዐቀብኩ፡ ነፍስየ፡ ከመ፡ ኢይብላዕ፡</foreign> Tob.1,11; <foreign xml:lang="gez">ዕቀቡ፡ ርእሰክሙ፡ እምነ፡ አማልክት፡</foreign> 1 <ref cRef="Joh." loc="5,21">Joh. 5,21</ref>; <foreign xml:lang="gez">ከመ፡ ይዕቀቡ፡ ነፍሶሙ፡ እምዝሙት፡</foreign> <ref cRef="Kuf. " loc="7.">Kuf.  7.</ref> 
</sense> <sense n="d" xml:id="D1d"> <cit type="translation" xml:lang="la"><quote> rationem </quote></cit> vel <cit type="translation" xml:lang="la"><quote> curam habere </quote></cit> alicujus, <cit type="translation" xml:lang="la"><quote> respicere </quote></cit>, <cit type="translation" xml:lang="la"><quote> colere </quote></cit>: <foreign xml:lang="gez">ዕቀብ፡ እመከ፡</foreign> <foreign xml:lang="grc">μὴ ὑπερίδῃς τὴν μητέρα σου ἐπιμελεῖσθαι</foreign> <ref cRef="Tob." loc="4,3">Tob. 4,3</ref>; <foreign xml:lang="gez">ዕቀቢ፡ ያዕቆብሃ፡</foreign> <ref cRef="Kuf." loc="19">Kuf. 19</ref>; <foreign xml:lang="grc">ἐπισκέπτεσθαι</foreign> <ref cRef="Jac." loc="1,27">Jac. 1,27</ref>; <foreign xml:lang="grc">ἐπιμελεῖσθαι</foreign> <ref cRef="Gen." loc="44,21">Gen. 44,21</ref>; <foreign xml:lang="gez">ዕቀብ፡ ዐርከከ፡</foreign> <foreign xml:lang="grc">στέρξον</foreign> <ref cRef="Sir." loc="27,17">Sir. 27,17</ref>. 
</sense> 
</sense> <sense n="2" xml:id="D2"> <cit type="translation" xml:lang="la"><quote> observare </quote></cit>, <cit type="translation" xml:lang="la"><quote> attendere </quote></cit>, <gramGrp> <pos expand="absolute">abs.</pos> </gramGrp> <ref cRef="Jes." loc="42,20">Jes. 42,20</ref>; <lbl expand="cum">c.</lbl> <gramGrp> <case value="accusativus">Acc.</case> </gramGrp> rei: <foreign xml:lang="gez">ዘየዐቅብ፡ ንፋሰ፡</foreign> <ref cRef="Koh." loc="11,4">Koh. 11,4</ref>; <foreign xml:lang="gez">ዕቀብ፡ ፍናዊሃ፡</foreign> <foreign xml:lang="grc">ἐνεδρεύειν</foreign> <ref cRef="Sir." loc="14,22">Sir. 14,22</ref>; <foreign xml:lang="gez">ዕቀብ፡ አፈ፡ ንጉሥ፡</foreign> <ref cRef="Koh." loc="8,2">Koh. 8,2</ref>; <foreign xml:lang="gez">ዘየዐቅብ፡ ተግሣጸ፡</foreign> <ref cRef="Prov." loc="13,18">Prov. 13,18</ref>; <foreign xml:lang="gez">ዐቀበ፡ ኵሎ፡ ፍናውየ፡</foreign> <ref cRef="Job" loc="33,11">Job 33,11</ref>; <ref cRef="Ps." loc="106,43">Ps. 106,43</ref>; in malam partem: <cit type="translation" xml:lang="la"><quote> callide attendere </quote></cit>, <cit type="translation" xml:lang="la"><quote> insidiari </quote></cit>, <lbl expand="cum">c.</lbl> <gramGrp> <case value="accusativus">Acc.</case> </gramGrp> <ref cRef="Gen." loc="3,15">Gen. 3,15</ref>; <foreign xml:lang="gez">ሶበ፡ አበስኩ፡ እምዐቀብከኒ፡</foreign> <ref cRef="Job" loc="10,14">Job 10,14</ref>. 
</sense> <sense n="3" xml:id="D3"> <cit type="translation" xml:lang="la"><quote> servare </quote></cit>, <cit type="translation" xml:lang="la"><quote> observare </quote></cit> praecepta, legem al.: <ref cRef="Gen." loc="17,9">Gen. 17,9</ref>; <ref cRef="Ex." loc="12,24">Ex. 12,24</ref>; <ref cRef="Ex. " loc="12,25">Ex.  12,25</ref>; <ref cRef="Lev." loc="19,37">Lev. 19,37</ref>; <ref cRef="Num." loc="18,5">Num. 18,5</ref>; <ref cRef="Ps." loc="102,17">Ps. 102,17</ref>; <ref cRef="Ps." loc="118,5">Ps. 118,5</ref>; <ref cRef="Ps." loc="118,8">Ps. 118,8</ref>; <ref cRef="Jer." loc="16,11">Jer. 16,11</ref>; <foreign xml:lang="gez">ዕቀቡ፡ ጽድቀ፡</foreign> <ref cRef="Jes." loc="56,1">Jes. 56,1</ref>; <ref cRef="Jes." loc="56,6">Jes. 56,6</ref>; <ref cRef="4 Esr. " loc="6,67">4 Esr.  6,67</ref>; <ref cRef="4 Esr." loc="9,31">4 Esr. 9,31</ref>; <ref cRef="Sir." loc="21,11">Sir. 21,11</ref>; <ref cRef="Sir." loc="44,20">Sir. 44,20</ref>; <foreign xml:lang="gez">አልቦ፡ ዘይጥዕም፡ እምነ፡ ዐቂበ፡ ትእዛዙ፡</foreign> <ref cRef="Sir." loc="23,27">Sir. 23,27</ref>; <ref cRef="Sir." loc="28,6">Sir. 28,6</ref>; <ref cRef="Joh." loc="14,21">Joh. 14,21</ref>; <ref cRef="1 Joh." loc="2,3">1 Joh. 2,3</ref>; <ref cRef="Apoc." loc="1,3">Apoc. 1,3</ref>; <ref cRef="Apoc." loc="22,7">Apoc. 22,7</ref>; <ref cRef="Matth." loc="19,17">Matth. 19,17</ref>; sabbathum, festum <ref cRef="Kuf." loc="2">Kuf. 2</ref>; <ref cRef="Ex." loc="23,15">Ex. 23,15</ref>; <ref cRef="Ex. " loc="31,14">Ex.  31,14</ref>; <ref cRef="Lev." loc="19,30">Lev. 19,30</ref>; <ref cRef="Joh." loc="9,16">Joh. 9,16</ref>; foedus <ref cRef="Gen." loc="17,10">Gen. 17,10</ref>; <ref cRef="Deut." loc="7,9">Deut. 7,9</ref>; <ref cRef="Deut." loc="7,12">Deut. 7,12</ref>. 
</sense> <sense n="4" xml:id="D4"> <cit type="translation" xml:lang="la"><quote> servare </quote></cit>, <cit type="translation" xml:lang="la"><quote> reservare </quote></cit>, <cit type="translation" xml:lang="la"><quote> asservare </quote></cit>: <foreign xml:lang="gez">ዐቂብ፡</foreign> (<lbl expand="oppositum, -o, -nitur">opp.</lbl> <foreign xml:lang="gez">አውጽኦ፡</foreign>) <ref cRef="Koh." loc="3,6">Koh. 3,6</ref>; <lbl expand="cum">c.</lbl> <gramGrp> <case value="accusativus">Acc.</case> </gramGrp>: <ref cRef="Jer. " loc="43,20">Jer.  43,20</ref> <lbl expand="varia lectio">var.</lbl>; <ref cRef="Gen." loc="41,36">Gen. 41,36</ref>; <ref cRef="4 Esr." loc="4,60">4 Esr. 4,60</ref>; <ref cRef="4 Esr." loc="12,37">4 Esr. 12,37</ref>; <ref cRef="4 Esr." loc="13,33">4 Esr. 13,33</ref>; <ref cRef="4 Esr." loc="14,51">4 Esr. 14,51</ref>; <foreign xml:lang="gez">ማሕፀንተ፡</foreign> F.N. 29; <ref cRef="1 Tim." loc="6,20">1 Tim. 6,20</ref>; <ref cRef="Joh." loc="2,10">Joh. 2,10</ref>; <ref cRef="Joh." loc="12,7">Joh. 12,7</ref>; <ref cRef="2 Petr." loc="2,17">2 Petr. 2,17</ref>. <sense n="a" xml:id="D4a"> in animo servare, memoria <cit type="translation" xml:lang="la"><quote> tenere </quote></cit>: <foreign xml:lang="gez">ዐቀብኩ፡ ነገሮ፡ በልብየ፡</foreign> <ref cRef="Dan." loc="7,28">Dan. 7,28</ref>; <ref cRef="4 Esr." loc="14,6">4 Esr. 14,6</ref>; <ref cRef="4 Esr." loc="14,43">4 Esr. 14,43</ref>; <foreign xml:lang="gez">ዐቀቦ፡</foreign> (<foreign xml:lang="gez">ለሕልም፡</foreign>) <foreign xml:lang="gez">ውስተ፡ ልቡ፡</foreign> <ref cRef="Esth." loc="1 ">Esth. 1 </ref>apocr.; <ref cRef="Gen." loc="37,11">Gen. 37,11</ref>; 1 <ref cRef="Reg." loc="21,12">Reg. 21,12</ref>; <ref cRef="Sir." loc="21,14">Sir. 21,14</ref>; <ref cRef="Prov." loc="3,1">Prov. 3,1</ref>. <sense n="α" xml:id="D4aα"> peccata (<lbl expand="oppositum, -o, -nitur">opp.</lbl> remissione): <foreign xml:lang="gez">ዐቀብከ፡ ሊተ፡ ኀጢአተ፡ ንእስየ፡</foreign> <ref cRef="Job" loc="13,26">Job 13,26</ref>; M.M. f. 96. 
</sense> <sense n="β" xml:id="D4aβ"> <cit type="translation" xml:lang="la"><quote> reticere </quote></cit>: <foreign xml:lang="gez">አብድ፡ ኢይክል፡ ዐቂበ፡ ነገር፡ ዘሰምዐ፡</foreign> <ref cRef="Sir." loc="8,17">Sir. 8,17</ref>. 
</sense> 
</sense> <sense n="b" xml:id="D4b"> <cit type="translation" xml:lang="la"><quote> conservare </quote></cit>: <foreign xml:lang="gez">ዐቀብከ፡ ሎቱ፡ ዘንተ፡ ምሕረተ፡</foreign> 3 <ref cRef="Reg." loc="3,6">Reg. 3,6</ref> <foreign xml:lang="gez">ዐቀበ፡ መራደ፡</foreign> <ref cRef="Am." loc="1,11">Am. 1,11</ref>; <foreign xml:lang="gez">ዘዐቀበ፡ ፈሪሀ፡ እግዚአብሔር፡</foreign> (<foreign xml:lang="grc">κρατῶν</foreign>) <ref cRef="Sir." loc="25,11">Sir. 25,11</ref>; <ref cRef="Judae" loc="6">Judae 6</ref>; <ref cRef="Apoc." loc="16,15">Apoc. 16,15</ref>; <ref cRef="Jac." loc="1,27">Jac. 1,27</ref>; omisso <gramGrp> <case value="accusativus">Acc.</case> </gramGrp>, <ref cRef="Eph." loc="4,3">Eph. 4,3</ref>. – <lbl expand="Vocabularium Aethiopicum, -a -a">Voc. Ae.</lbl>: <foreign xml:lang="gez">ዐቀበ፡ ዘ፡ ጠበቀ።</foreign> 
