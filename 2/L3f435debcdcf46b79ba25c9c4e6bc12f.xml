               <sense xml:id="Dmain" xml:lang="la" source="#dillmann"> <gramGrp> <pos expand="Substantivum">subst.</pos> </gramGrp>, <gramGrp> <gen value="not found">m.</gen> </gramGrp> et <gramGrp> <gen value="femininus">fem.</gen> </gramGrp>; in libris serioris aetatis occurrit <lbl expand="pluralis">Pl.</lbl> <foreign xml:lang="gez">ምክራት፡</foreign> <sense n="1" xml:id="D1"> <cit type="translation" xml:lang="la"><quote> consilium </quote></cit> et <cit type="translation" xml:lang="la"><quote> consilia </quote></cit>, <foreign xml:lang="grc">βουλή</foreign>, <foreign xml:lang="grc">βουλαί</foreign>, <foreign xml:lang="grc">βούλημα</foreign> al. <sense n="a" xml:id="D1a"> quod quis vel dat vel petit ab aliquo: <foreign xml:lang="gez">ያረምሙ፡ በምክረ፡ ዚአየ፡</foreign> <ref cRef="Job" loc="29,21">Job 29,21</ref>; <foreign xml:lang="gez">ዘዚአየ፡ ምክር፡</foreign> <ref cRef="Prov." loc="1,30">Prov. 1,30</ref>; <ref cRef="2 Reg." loc="15,31">2 Reg. 15,31</ref>; <ref cRef="2 Reg." loc="15,34">2 Reg. 15,34</ref>; <ref cRef="3 Reg." loc="1,12">3 Reg. 1,12</ref>; <ref cRef="Sir." loc="37,7">Sir. 37,7</ref>; <ref cRef="Jes." loc="19,11">Jes. 19,11</ref>; <ref cRef="Ps." loc="1,1">Ps. 1,1</ref>; <ref cRef="Ps." loc="118,24">Ps. 118,24</ref>. 
</sense> <sense n="b" xml:id="D1b"> quod quis capit: <foreign xml:lang="gez">ምክሩ፡ ዘመከረ፡ ላዕለ፡</foreign> <foreign xml:lang="grc">ἐπιβουλή</foreign> <ref cRef="Esth." loc="2,22">Esth. 2,22</ref>; <ref cRef="Judith" loc="2,2">Judith 2,2</ref>; <ref cRef="Judith" loc="2,4">Judith 2,4</ref>; <ref cRef="Job" loc="5,12">Job 5,12</ref>; <ref cRef="Job" loc="10,3">Job 10,3</ref>; <ref cRef="Job" loc="12,13">Job 12,13</ref>; <ref cRef="Job" loc="18,7">Job 18,7</ref>; <ref cRef="Jes." loc="7,7">Jes. 7,7</ref>; <ref cRef="Jes." loc="14,26">Jes. 14,26</ref>; <ref cRef="Jer. " loc="29,8">Jer.  29,8</ref>; <ref cRef="Jer." loc="27,45">Jer. 27,45</ref>; <ref cRef="Ps." loc="32,10">Ps. 32,10</ref>; <ref cRef="Ps." loc="32,11">Ps. 32,11</ref>; <ref cRef="Prov." loc="15,22">Prov. 15,22</ref>; <foreign xml:lang="gez">ኢኮነ፡ ምክርየ፡ ከመ፡ ምክርክሙ፡</foreign> <ref cRef="Jes." loc="55,8">Jes. 55,8</ref>; <foreign xml:lang="gez">ምክረ፡ ሰላም፡</foreign> <ref cRef="Zach." loc="6,13">Zach. 6,13</ref>; <foreign xml:lang="gez">በምክረ፡ ሕሊናሆሙ፡</foreign> <foreign xml:lang="grc">ἐν τοῖς διαβουλίοις αὐτῶν</foreign> <ref cRef="Sap. " loc="1,9">Sap.  1,9</ref>; <foreign xml:lang="gez">ምክረ፡ ፈቃዱ፡</foreign> <ref cRef="Eph." loc="1,11">Eph. 1,11</ref>; <ref cRef="Hebr." loc="6,17">Hebr. 6,17</ref>; de arcano alicujus consilio (<foreign xml:lang="grc">μυστήριον</foreign>) <ref cRef="Sir. " loc="27,16">Sir.  27,16</ref>; <ref cRef="Sir." loc="27,21">Sir. 27,21</ref>; <foreign xml:lang="gez">ዘያወጽእ፡ ምክረ፡ ቢጹ፡</foreign> <ref cRef="Sir." loc="13,12">Sir. 13,12</ref>; <ref cRef="Rom." loc="11,25">Rom. 11,25</ref>. 
</sense> 
</sense> <sense n="2" xml:id="D2"> <cit type="translation" xml:lang="la"><quote> deliberatio </quote></cit>, <cit type="translation" xml:lang="la"><quote> consideratio </quote></cit>, <cit type="translation" xml:lang="la"><quote> intelligentia </quote></cit>, <cit type="translation" xml:lang="la"><quote> prudentia </quote></cit>, <cit type="translation" xml:lang="la"><quote> sapientia </quote></cit>: <foreign xml:lang="gez">ኵሉ፡ ምክሩ፡ ዘከመ፡ ያጠነቅቅ፡ ኀሪወ፡ ማኅተም፡</foreign> <ref cRef="Sir." loc="38,27">Sir. 38,27</ref>; <foreign xml:lang="gez">አልቦ፡ ዘትገብር፡ እንበለ፡ ምክር፡</foreign> <foreign xml:lang="grc">ἄνευ κρίσεως</foreign> <ref cRef="Sir." loc="30,38">Sir. 30,38</ref>; <foreign xml:lang="grc">ἄνευ βουλῆς</foreign> <ref cRef="Sir." loc="35,18">Sir. 35,18</ref>; <ref cRef="Sir." loc="35,19">Sir. 35,19</ref>; <foreign xml:lang="grc">μετὰ ἀβουλίας</foreign> <ref cRef="Prov." loc="14,17">Prov. 14,17</ref>; <foreign xml:lang="gez">በምክር፡ ፍጹም፡</foreign> (re rite perpensa) F.N. 24,2; <foreign xml:lang="gez">ጠቢበ፡ ምክር፡ ውእቱ፡</foreign> <foreign xml:lang="grc">σοφὸς διανοίᾳ</foreign> <ref cRef="Job" loc="9,4">Job 9,4</ref>; <foreign xml:lang="grc">σύνεσις</foreign> <foreign xml:lang="gez">ምክር፡ ወጥበብ፡</foreign> <ref cRef="Job" loc="22,2">Job 22,2</ref>; <foreign xml:lang="gez">ምክሮሙ፡ ለሊቃውንት፡</foreign> <ref cRef="Job" loc="12,20">Job 12,20</ref>; <ref cRef="Job" loc="28,20">Job 28,20</ref>; <ref cRef="Dan." loc="2,20">Dan. 2,20</ref>; <ref cRef="Ps. " loc="48,3">Ps.  48,3</ref>; <ref cRef="Jes." loc="10,13">Jes. 10,13</ref>; <foreign xml:lang="grc">φρόνησις</foreign> <ref cRef="Jer." loc="10,12">Jer. 10,12</ref>; <foreign xml:lang="grc">γνῶσις</foreign> <ref cRef="Sir." loc="21,18">Sir. 21,18</ref>; <foreign xml:lang="gez">ለስሐ፡ ኵሉ፡ ምክረ፡ እጓለ፡ እመሕያው፡</foreign> <ref cRef="Jer." loc="10,14">Jer. 10,14</ref>; <ref cRef="Mal." loc="2,7">Mal. 2,7</ref>; <foreign xml:lang="grc">ἐπιστήμη</foreign> <ref cRef="Dan." loc="1,20">Dan. 1,20</ref>; <foreign xml:lang="grc">βουλή</foreign> <foreign xml:lang="gez">መንፈሰ፡ ምክር፡</foreign> <ref cRef="Jes." loc="11,2">Jes. 11,2</ref>. 
</sense> <sense n="3" xml:id="D3"> <cit type="translation" xml:lang="la"><quote> sententia </quote></cit>, <cit type="translation" xml:lang="la"><quote> judicium </quote></cit>, <cit type="translation" xml:lang="la"><quote> sentiendi et cogitandi ratio </quote></cit>: <foreign xml:lang="grc">γνώμη</foreign> <ref cRef="Sir." loc="6,23">Sir. 6,23</ref>; <ref cRef="1 Cor." loc="1,10">1 Cor. 1,10</ref>; <foreign xml:lang="gez">በምክርየ፡</foreign> <ref cRef="1 Cor." loc="7,25">1 Cor. 7,25</ref>; <foreign xml:lang="grc">ἐννοιαί</foreign> hebr. 4,12; <foreign xml:lang="grc">βουλή</foreign> <ref cRef="Luc." loc="23,51">Luc. 23,51</ref>; <ref cRef="Kuf. " loc="10">Kuf.  10</ref>; <cit type="translation" xml:lang="la"><quote> de hac doctrina Christiani discedunt </quote></cit> <foreign xml:lang="gez">ኀበ፡ ፪ምክራት፡</foreign> <cit type="translation" xml:lang="la"><quote> in duas sententias </quote></cit> (opiniones) F.N. 24,2. 
</sense> <sense n="4" xml:id="D4"> <cit type="translation" xml:lang="la"><quote> voluntas </quote></cit>: <foreign xml:lang="grc">βούλημα θεοῦ</foreign> <ref cRef="Rom." loc="9,19">Rom. 9,19</ref>; <cit type="translation" xml:lang="la"><quote> Leonis papae </quote></cit> <foreign xml:lang="gez">ሃይማኖት፡ እኪት፡ ዘረሰዮ፡ ለክርስቶስ፡ ፪ጠባይዓት፡</foreign> (<cit type="translation" xml:lang="la"><quote> naturas </quote></cit>) <foreign xml:lang="gez">ወ፪ምክረ፡</foreign> (<cit type="translation" xml:lang="la"><quote> voluntates </quote></cit>) <foreign xml:lang="gez">እምድኅረ፡ ተዋሕዶ፡</foreign> <ref cRef="Sx. Masc." loc="7.">Sx. Masc. 7.</ref> 
</sense> </sense>
               
