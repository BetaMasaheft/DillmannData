               <sense xml:lang="la" source="#dillmann" xml:id="Dmain"> <gramGrp> <pos expand="praepositio">praep.</pos> </gramGrp>; in libris Mss. antiquioribus et impressis nonnumquam <foreign xml:lang="gez">ኀቤ፡</foreign> (ut: <foreign xml:lang="gez">በኀቤ፡ አንቀጽ፡</foreign> <ref cRef="Am. " loc="5,15">Am.  5,15</ref> <lbl expand="">Cod. Laur.</lbl>; <foreign xml:lang="gez">ኀቤአ፡</foreign> <ref cRef="Jos." loc="10,4">Jos. 10,4</ref> <lbl expand="annotatio">ann.</lbl>; <ref cRef="Joh." loc="4,6">Joh. 4,6</ref> <lbl expand="romanae editionis">rom.</lbl>; <lbl expand="videas">vid.</lbl> <bibl>
                            <ptr target="bm:Dillmann1857Gramm"/>gramm.</bibl> § 167,3); <lbl expand="cum">c.</lbl> suff.: <foreign xml:lang="gez">ኀቤየ፡</foreign>, <foreign xml:lang="gez">ኀቤከ፡ ኀቤሁ፡ ኀቤነ፡</foreign> al. (<bibl>
                            <ptr target="bm:Dillmann1857Gramm"/>gramm.</bibl> § 167,3); significat <sense n="A" xml:id="DA"> propinquitatem et contiguitatem: <cit type="translation" xml:lang="la">
                                <quote> ad </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> apud </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> juxta </quote>
                            </cit>; conjungitur cum nominibus loci, rei, personae: <foreign xml:lang="gez">ኀበ፡ ተከዚ፡</foreign> <foreign xml:lang="grc">παρά τὸν ποταμόν</foreign> <ref cRef="Ex." loc="2,3">Ex. 2,3</ref>; <ref cRef="Ps." loc="1,3">Ps. 1,3</ref>; <ref cRef="Hez." loc="19,10">Hez. 19,10</ref>; <foreign xml:lang="gez">ቆመት፡ ድኅሬሁ፡ ኀበ፡ እገሪሁ፡</foreign> <ref cRef="Luc." loc="7,38">Luc. 7,38</ref>; <ref cRef="Luc. " loc="8,35">Luc.  8,35</ref>; <ref cRef="Luc." loc="5,2">Luc. 5,2</ref>; <foreign xml:lang="gez">ኀበ፡ ዐዘቅት፡</foreign> <foreign xml:lang="grc">ἐπί τῇ πηγῇ</foreign> <ref cRef="Joh." loc="4,6">Joh. 4,6</ref>; <foreign xml:lang="gez">ሀሎ፡ ኀበ፡ ኆኅት፡</foreign> <ref cRef="Matth." loc="24,33">Matth. 24,33</ref>; <foreign xml:lang="gez">ሀለዉ፡ ኀቤሁ፡ ሰብእ፡ ብዙኃን፡</foreign> <ref cRef="Luc. " loc="5,1">Luc.  5,1</ref>; <foreign xml:lang="gez">ቆመ፡ አፍአ፡ ኀበ፡ ኆኅት፡</foreign> <foreign xml:lang="grc">πρὸς τῇ θύρᾳ</foreign> <ref cRef="Joh." loc="18,16">Joh. 18,16</ref>; <foreign xml:lang="grc">πρὸς τὸ μνημεῖον</foreign> <ref cRef="Joh." loc="20,11">Joh. 20,11</ref>; <foreign xml:lang="gez">እነብር፡ ኀቤክሙ፡</foreign> <ref cRef="1 Cor." loc="16,6">1 Cor. 16,6</ref>; 2 <ref cRef="Cor." loc="1,12">Cor. 1,12</ref>; <foreign xml:lang="gez">ቃል፡ ሀለወ፡ ኀበ፡ እግዚአብሔር፡</foreign> <ref cRef="Joh." loc="1,1">Joh. 1,1</ref>; <ref cRef="Joh. " loc="1,2">Joh.  1,2</ref>; <foreign xml:lang="gez">እንተ፡ ኀቤየ፡</foreign> <foreign xml:lang="grc">ἡ πλησίον μου</foreign> <ref cRef="Cant." loc="1,9">Cant. 1,9</ref>; <ref cRef="Cant." loc="1,15">Cant. 1,15</ref>; <foreign xml:lang="gez">ነፋኄ፡ ቀርን፡ ኀቤየ፡ ሀሎ፡</foreign> <foreign xml:lang="grc">ἐχόμενά μου</foreign> <ref cRef="2 Esr." loc="14,18">2 Esr. 14,18</ref>; <ref cRef="Dan." loc="10,4">Dan. 10,4</ref>. <sense n="a" xml:id="DAa"> <cit type="translation" xml:lang="la">
                                    <quote> apud </quote>
                                </cit> aliquem i.e. <cit type="translation" xml:lang="la">
                                    <quote> in domo </quote>
                                </cit> ejus: <foreign xml:lang="gez">ቦቱ፡ ኀቤነ፡ ሣዕረ፡</foreign> <foreign xml:lang="grc">παρ᾽ ἡμῖν</foreign> <ref cRef="Gen." loc="24,25">Gen. 24,25</ref>; <foreign xml:lang="gez">ንበር፡ ኀቤሁ፡</foreign> <foreign xml:lang="grc">οἴχησον μετ᾽ αὐτοῦ</foreign> <ref cRef="Gen." loc="27,44">Gen. 27,44</ref>; <foreign xml:lang="gez">አኀድር፡ ኀቤክሙ፡</foreign> <foreign xml:lang="grc">κατοικιῶ ὑμᾶς</foreign> <ref cRef="Jer." loc="7,3">Jer. 7,3</ref>.</sense> <sense n="b" xml:id="DAb"> in potestate alicujus i.e. <cit type="translation" xml:lang="la">
                                    <quote> penes </quote>
                                </cit>: <foreign xml:lang="gez">ኵሎ፡ ዘሀሎ፡ ኀቤሆሙ፡</foreign> <foreign xml:lang="grc">τὰ ἐν ταῖς χερσίν αὐτῶν</foreign> <ref cRef="Gen." loc="35,4">Gen. 35,4</ref>; <foreign xml:lang="gez">ብየ፡ ኀቤየ፡ ራብዕተ፡ እዴሃ፡ ለሰቅል፡</foreign> <ref cRef="1 Reg." loc="9,8">1 Reg. 9,8</ref>; <ref cRef="1 Reg." loc="21,3">1 Reg. 21,3</ref>; <foreign xml:lang="gez">ዝንቱ፡ እምከመ፡ ሀለወ፡ ኀቤክሙ፡</foreign> <foreign xml:lang="grc">ταῦτα ὑμῖν ὑπάρχοντα</foreign> <ref cRef="2 Petr. " loc="1,8">2 Petr.  1,8</ref>; <foreign xml:lang="gez">ኀቤየ፡ ሀሎ፡ መራኁት፡</foreign> <foreign xml:lang="grc">ἔχω</foreign> <ref cRef="Apoc." loc="1,18">Apoc. 1,18</ref>; <foreign xml:lang="gez">ኀቤከ፡ ውእቱ፡</foreign> Kuf. p. 136.</sense> <sense n="c" xml:id="DAc"> in animo alicujus i.e. <cit type="translation" xml:lang="la">
                                    <quote> in </quote>
                                </cit>, <cit type="translation" xml:lang="la">
                                    <quote> penes </quote>
                                </cit>: <foreign xml:lang="gez">ኢሀለወት፡ ጽድቅ፡ ኀቤነ፡</foreign> <foreign xml:lang="grc">ἐν ἡμῖν</foreign> <ref cRef="1 Joh." loc="1,8">1 Joh. 1,8</ref>; <ref cRef="1 Joh." loc="1,10">1 Joh. 1,10</ref>; <ref cRef="1 Joh." loc="2,4">1 Joh. 2,4</ref>; <foreign xml:lang="gez">ትእዛዘ፡ ዚአየ፡ ኅባእ፡ኀቤከ፡</foreign> <foreign xml:lang="grc">παρά σεαυτῷ</foreign> <ref cRef="Prov. " loc="7,1">Prov.  7,1</ref>; <foreign xml:lang="gez">ፈቂደ፡ ገቢረ፡ ሠናይ፡ ሀሎ፡ ኀቤየ፡</foreign> <foreign xml:lang="grc">παράχειταί μοι</foreign> <ref cRef="Rom." loc="7,18">Rom. 7,18</ref>.</sense> </sense> <sense n="B" xml:id="DB"> motionem vel directionem ad locum, rem, personam: <cit type="translation" xml:lang="la">
                                <quote> ad </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> versus </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> in </quote>
                            </cit>, <foreign xml:lang="grc">ἐπὶ</foreign>, <foreign xml:lang="grc">πρός</foreign> al.: <foreign xml:lang="gez">አዕይንቲሁ፡ ለእግዚአብሔር፡ ኀበ፡ እለ፡ ይፈርህዎ፡</foreign> <ref cRef="Sir." loc="31,19">Sir. 31,19</ref>; <ref cRef="Sir." loc="15,19">Sir. 15,19</ref>; <ref cRef="Ps." loc="33,16">Ps. 33,16</ref>; <ref cRef="Ps." loc="33,17">Ps. 33,17</ref>; 1 <ref cRef="Petr." loc="3,12">Petr. 3,12</ref>; <foreign xml:lang="gez">ወእምድኅሬሁ፡ ኀበ፡ ምውታን፡</foreign> <ref cRef="Koh." loc="9,3">Koh. 9,3</ref>; <foreign xml:lang="gez">ወፅአ፡ ኀበ፡ ኆኅት፡</foreign> <ref cRef="Tob." loc="11,9">Tob. 11,9</ref>; <foreign xml:lang="gez">ኣሌዕል፡ እዴየ፡ ኀበ፡ እግዚአብሔር፡</foreign> <ref cRef="Gen." loc="14,22">Gen. 14,22</ref>; <foreign xml:lang="gez">ኣቀውም፡ ሥርዐትየ፡ ኀቤሁ፡</foreign> <ref cRef="Gen." loc="17,19">Gen. 17,19</ref>; frequentissime usurpatur post verba eundi: <foreign xml:lang="gez">መኑ፡ የሐውር፡ ኀበ፡ ዝንቱ፡ ሕዝብ፡</foreign> <ref cRef="Jes." loc="6,8">Jes. 6,8</ref>; exeundi: <foreign xml:lang="gez">ይወጽእ፡ ኀበ፡ ማይ፡</foreign> <ref cRef="Ex. " loc="7,15">Ex.  7,15</ref>; <ref cRef="Joh." loc="6,68">Joh. 6,68</ref>; <ref cRef="Gen." loc="29,1">Gen. 29,1</ref>; intrandi <ref cRef="Ex." loc="5,1">Ex. 5,1</ref>, ascendendi <ref cRef="Ruth" loc="4,1">Ruth 4,1</ref>; veniendi <foreign xml:lang="gez">እመጽእ፡ ኀቤከ፡</foreign> <ref cRef="1 Reg." loc="20,9">1 Reg. 20,9</ref>; <ref cRef="Hez." loc="20,1">Hez. 20,1</ref>; <ref cRef="Marc. " loc="4,1">Marc.  4,1</ref>; <ref cRef="Marc." loc="5,21">Marc. 5,21</ref>; <ref cRef="Luc." loc="8,4">Luc. 8,4</ref>; <ref cRef="Luc." loc="8,35">Luc. 8,35</ref>; <ref cRef="Luc." loc="19,37">Luc. 19,37</ref>; mittendi <ref cRef="Luc." loc="11,49">Luc. 11,49</ref>; <ref cRef="Gen. " loc="37,13">Gen.  37,13</ref>; currendi <ref cRef="Jes." loc="28,4">Jes. 28,4</ref>; <ref cRef="Job" loc="39,30">Job 39,30</ref>; appropinquandi <ref cRef="Luc." loc="10,9">Luc. 10,9</ref>; <ref cRef="Luc." loc="7,6">Luc. 7,6</ref>; tradendi et vendendi (alicui) <ref cRef="Matth." loc="20,18">Matth. 20,18</ref>; <ref cRef="Lev." loc="25,14">Lev. 25,14</ref>; <ref cRef="Lev." loc="25,39">Lev. 25,39</ref>; ferendi <ref cRef="Luc. " loc="12,11">Luc.  12,11</ref>; deponendi et jaciendi <ref cRef="Matth." loc="15,30">Matth. 15,30</ref>; <ref cRef="Act." loc="7,58">Act. 7,58</ref>; <ref cRef="1 Petr." loc="4,19">1 Petr. 4,19</ref>; fugiendi, se recipiendi <ref cRef="Ps." loc="142,11">Ps. 142,11</ref>; <ref cRef="Kuf." loc="27 ">Kuf. 27 </ref>(<foreign xml:lang="gez">ጕየይ፡ ኀበ፡ ላባ፡</foreign>); revertendi et reducendi <ref cRef="1 Petr." loc="2,3">1 Petr. 2,3</ref>; <ref cRef="Ex." loc="24,14">Ex. 24,14</ref>; <ref cRef="4 Esr." loc="9,40">4 Esr. 9,40</ref>; attingendi <ref cRef="2 Par. " loc="3,11">2 Par.  3,11</ref>; inclinandi <ref cRef="Ps." loc="16,7">Ps. 16,7</ref>; <ref cRef="Ps." loc="30,2">Ps. 30,2</ref>; monstrandi (<lbl expand="videas">vid.</lbl> <foreign xml:lang="gez">አመረ፡</foreign>, <foreign xml:lang="gez">መርሐ፡</foreign>); aspiciendi <foreign xml:lang="gez">ይኔጽሩ፡ ኀበ፡</foreign> <ref cRef="Hos." loc="3,1">Hos. 3,1</ref>; <ref cRef="Matth." loc="14,19">Matth. 14,19</ref>; clamandi, rogandi, precandi: <foreign xml:lang="gez">ጽራኅ፡ ኀቤየ፡</foreign> <ref cRef="Jer. " loc="40,3">Jer.  40,3</ref>; <foreign xml:lang="gez">ይስእሉ፡ ኀበ፡ እግዚአብሔር፡</foreign> <ref cRef="Sir." loc="38,14">Sir. 38,14</ref>; <foreign xml:lang="gez">ጸሊ፡ ለነ፡ ኀበ፡ እግዚአብሔር፡</foreign> <ref cRef="Jer." loc="49,2">Jer. 49,2</ref>; querendi, accusandi <ref cRef="Hen." loc="89,15">Hen. 89,15</ref>; <ref cRef="Ps." loc="108,19">Ps. 108,19</ref>; transmutandi: <foreign xml:lang="gez">ተወለጠ፡ ፍሥሓሆሙ፡ ኀበ፡ ሐዘን፡</foreign> <ref cRef="Sx. Teq." loc="14">Sx. Teq. 14</ref>; <foreign xml:lang="gez">ወለጡ፡ ግዕዞሙ፡ ኀበ፡ ሥርዐተ፡ መላእክት፡</foreign> Org. — De usu vocis <foreign xml:lang="gez">ኀበ፡</foreign> post <foreign xml:lang="gez">ከፈለ፡</foreign> vid. <foreign xml:lang="gez">ክፈለ፡</foreign> — Pronomini relativo postponere licet praepositionem <foreign xml:lang="gez">ኀበ፡</foreign>, ut: <foreign xml:lang="gez">ምድር፡ ዘኀበ፡ ነበርክሙ፡</foreign> <ref cRef="Lev." loc="18,3">Lev. 18,3</ref>; <ref cRef="Lev." loc="14,40">Lev. 14,40</ref>; <ref cRef="Gen." loc="47,4">Gen. 47,4</ref>; <cb n="0593" xml:id="c0593"/> <foreign xml:lang="gez">አንቀጽ፡ ዘኀበ፡ ይክዕዉ፡ እዳወ፡</foreign> <ref cRef="2 Esr." loc="12,13">2 Esr. 12,13</ref>; <ref cRef="Sir." loc="36,31">Sir. 36,31</ref> (<bibl>
                                <ptr target="bm:Dillmann1857Gramm"/>gramm.</bibl> § 202,2). Componitur cum aliis praepositionibus: <foreign xml:lang="gez">ለኀበ፡</foreign>, quod accuratius, quam <foreign xml:lang="gez">ኀበ፡</foreign>, directionem in locum, rem, personam notat <sense n="1" xml:id="DB1"> <cit type="translation" xml:lang="la">
                                    <quote> ad </quote>
                                </cit>, <cit type="translation" xml:lang="la">
                                    <quote> versus </quote>
                                </cit>, <cit type="translation" xml:lang="la">
                                    <quote> in </quote>
                                </cit> <ref cRef="Hen." loc="56,5">Hen. 56,5</ref>; <foreign xml:lang="gez">ባሕታውያን፡ ተመያጥያን፡ ለኀበ፡ ዓለም፡</foreign> <cit type="translation" xml:lang="la">
                                    <quote> anachoretae in mundum revertentes </quote>
                                </cit>, <ref cRef="Phlx. " loc="5">Phlx.  5</ref>; <foreign xml:lang="gez">መርሕ፡ ለኀበ፡ ክርስቶስ፡</foreign> <foreign xml:lang="grc">παιδαγωγὸς εἰς Χριστόν</foreign> <ref cRef="Gal." loc="3,24">Gal. 3,24</ref>; <ref cRef="2 Reg." loc="24,11">2 Reg. 24,11</ref>; <foreign xml:lang="gez">ዝንቱ፡ ጾታ፡ ይትከፈል፡ ለኀበ፡ ብዙኅ፡ ክፍል፡</foreign> F.M. 23,2.</sense> <sense n="2" xml:id="DB2"> adhibetur designandis rationibus, quae inter res vel personas intercedunt: <sense n="a" xml:id="DB2a"> <cit type="translation" xml:lang="la">
                                        <quote> cum </quote>
                                    </cit>, <cit type="translation" xml:lang="la">
                                        <quote> inter </quote>
                                    </cit>, <cit type="translation" xml:lang="la">
                                        <quote> erga </quote>
                                    </cit>: <foreign xml:lang="gez">ከመ፡ ትሑሩ፡ በኂሩት፡ ለኀበ፡ እለ፡ አፍአ፡</foreign> <foreign xml:lang="grc">εὐσχημόνως πρὸς τοὺς ἔξω</foreign> <ref cRef="1 Thess." loc="4,12">1 Thess. 4,12</ref>; <ref cRef="2 Cor." loc="5,12">2 Cor. 5,12</ref>.</sense> <sense n="b" xml:id="DB2b"> <cit type="translation" xml:lang="la">
                                        <quote> in iis quae pertinent ad </quote>
                                    </cit>: <foreign xml:lang="gez">ለኀበ፡ እግዚአብሔር፡</foreign> <foreign xml:lang="grc">τὰ πρὸς τὸν θεόν</foreign> <ref cRef="Hebr." loc="5,1">Hebr. 5,1</ref> (at <ref cRef="Hebr." loc="2,17">Hebr. 2,17</ref> <foreign xml:lang="gez">ለመንገለ፡</foreign>).</sense> <sense n="c" xml:id="DB2c"> <cit type="translation" xml:lang="la">
                                        <quote> in modum </quote>
                                    </cit>, <cit type="translation" xml:lang="la">
                                        <quote> tamquam </quote>
                                    </cit>, <cit type="translation" xml:lang="la">
                                        <quote> instar </quote>
