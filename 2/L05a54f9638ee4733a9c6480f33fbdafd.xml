               <form>
                  <foreign xml:lang="gez">ተዘለፈ</foreign>
               </form>
               <sense xml:lang="la" source="#dillmann" xml:id="Dmain"> <subc>III,1</subc> et <subc>III,2</subc> reflex. et <gramGrp>
                     <pos expand="passivum">pass.</pos>
                  </gramGrp>
                  <sense n="1" xml:id="D1">
                     <cit type="translation" xml:lang="la">
                        <quote>se ipsum reprehendere</quote>
                     </cit> vel <cit type="translation" xml:lang="la">
                        <quote>arguere</quote>
                     </cit>: <foreign xml:lang="gez">ተዘለፍኩ፡ </foreign>
                     <foreign xml:lang="grc">ἐφαύλισα ἐμαυτόν</foreign>
                     <ref cRef="Job" loc="42,6">Job 42,6</ref>.</sense>
                  <sense n="2" xml:id="D2">
                     <cit type="translation" xml:lang="la">
                        <quote>redargui</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>convinci</quote>
                     </cit>: <foreign xml:lang="gez">ቦእለ፡ ይዜለፉ፡ ወይትመውኡ፡ </foreign>
                     <ref cRef="Cyr." loc="104.">Cyr. f. 104.</ref> – ( De loco <ref cRef="Job" loc="21,6">Job 21,6</ref>
                     <lbl expand="videas">vid.</lbl> II <foreign xml:lang="gez">ዘለፈ፡ </foreign>
                     <subc>III,1</subc>).</sense>
                  <sense n="3" xml:id="D3"> i.q. <foreign xml:lang="gez">ተዛለፈ፡</foreign> n. 2. <lbl expand="quod videas">q.v.</lbl>
                  </sense>
               </sense>
               
