            <entry xml:id="L4eba86ba7d724f7e9fa812dcbe47a959" n="8870">
               <form>
                  <foreign xml:lang="gez">ተጠብለለ</foreign>
               </form>
               <sense xml:lang="la" source="#dillmann" xml:id="Dmain"> <subc>III,1</subc> <sense n="1" xml:id="D1">
                     <cit type="translation" xml:lang="la">
                        <quote>involvi</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>obvolvi</quote>
                     </cit>, fasciis (de Jesulo) <ref cRef="Kid." loc="33">Kid. f. 33</ref>; <ref cRef="Lit." loc="173,4">Lit. 173,4</ref>; <foreign xml:lang="gez">በአፅርቅት፡
                        ዘተጠብለልከ፡</foreign> Cod. Mus. Brit. LX, <ref cRef="Nr." loc="1">Nr. 1</ref>;
                     linteis (de corpore exanimi) Ssal. Req.</sense>
                  <sense n="2" xml:id="D2">
                     <cit type="translation" xml:lang="la">
                        <quote>convolvi</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>complicari</quote>
                     </cit>: <ref cRef="Hen." loc="90,29">Hen. 90,29</ref>; <foreign xml:lang="gez">ሰማያት፡ ይጠበለሉ፡ </foreign>
                     <ref cRef="Kid." loc="32">Kid. f. 32</ref>; de velis navis <ref cRef="Chrys. Ta." loc="34 ">Chrys. Ta. 34 </ref>(<lbl expand="videas">vid.
                        sub</lbl>
                     <foreign xml:lang="gez">ላኅልኀ፡</foreign>).</sense>
                  <sense n="3" xml:id="D3">
                     <cit type="translation" xml:lang="la">
                        <quote>circumvolvi</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>circumplicari</quote>
                     </cit>: <foreign xml:lang="gez">ከይሲ፡ ተጠብለለ፡ ውስተ፡ ክሳዱ፡</foreign>
                     <ref cRef="Sx. Haml." loc="24.">Sx. Haml. 24.</ref>
                  </sense>
               </sense>
               
