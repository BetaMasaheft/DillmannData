         <div>
            <entry xml:id="L3e104b8ba15a4e61845d65e5c3c59522" n="7402">
               <form>
                  <foreign xml:lang="gez">ተዐጽወ</foreign>
               </form>
               <sense xml:lang="la" source="#dillmann" xml:id="Dmain"> <subc>III,1</subc> (raro <foreign xml:lang="gez">ተዐጸወ፡</foreign> ut <ref cRef="Kuf." loc="42 ">Kuf. p. 42 </ref>
                  <lbl expand="annotatio">ann.</lbl> 11) <sense n="1" xml:id="D1"> reflex. <cit type="translation" xml:lang="la">
                        <quote>se includere</quote>
                     </cit>: <foreign xml:lang="gez">ተዐጸው፡ ማእከለ፡ ቤትከ፡ </foreign>
                     <ref cRef="Hez." loc="3,24">Hez. 3,24</ref>.</sense>
                  <sense n="2" xml:id="D2">
                     <gramGrp>
                        <pos expand="passivum">pass.</pos>
                     </gramGrp>
                     <cit type="translation" xml:lang="la">
                        <quote>claudi</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>occludi</quote>
                     </cit>, <foreign xml:lang="gez">ይትዐጸው፡ ኆኅት፡ </foreign>
                     <ref cRef="Koh." loc="12,4">Koh. 12,4</ref>; <ref cRef="Jes." loc="60,11">Jes.
                        60,11</ref>; <ref cRef="Mal." loc="1,10">Mal. 1,10</ref>; <ref cRef="Matth." loc="25,10">Matth. 25,10</ref>; <ref cRef="Apoc." loc="21,25">Apoc.
                        21,25</ref>; <foreign xml:lang="gez">ተዐጽወ፡ አናቅጸ፡ ግብጽ፡</foreign>
                     <ref cRef="Kuf." loc="46">Kuf. 46</ref>; <foreign xml:lang="gez">ተዐጽወ፡
                        አብያት፡</foreign>
                     <ref cRef="Jes." loc="24,10">Jes. 24,10</ref>; <foreign xml:lang="gez">ተዐጽዉ፡
                        አህጉር፡</foreign>
                     <ref cRef="Jer. " loc="13,19">Jer. 13,19</ref>. <sense n="a" xml:id="D2a">
                        <cit type="translation" xml:lang="la">
                           <quote>concludi</quote>
                        </cit>, <cit type="translation" xml:lang="la">
                           <quote>includi</quote>
                        </cit>: <ref cRef="1 Reg." loc="23,7">1 Reg. 23,7</ref>; <foreign xml:lang="gez">ሶበ፡ ተዐጽዉ፡ ክልኤሆሙ፡</foreign>
                        <ref cRef="Tob." loc="8,4">Tob. 8,4</ref>; <foreign xml:lang="gez">ኢይትዐጸው፡
                           አሠርከ፡ </foreign>
                        <ref cRef="Prov." loc="4,12">Prov. 4,12</ref>; <ref cRef="Ps." loc="67,33">Ps. 67,33</ref>; <ref cRef="Hen." loc="10,13">Hen. 10,13</ref>; <ref cRef="Hen." loc="69,28">Hen. 69,28</ref>; <ref cRef="Kuf." loc="10">Kuf.
                           10</ref>; <ref cRef="Jes. " loc="24,22">Jes. 24,22</ref>.</sense>
                     <sense n="b" xml:id="D2b">
                        <cit type="translation" xml:lang="la">
                           <quote>excludi</quote>
                        </cit>: virgines stultae flebant <foreign xml:lang="gez">እስመ፡ ተዐጽዋ፡ </foreign>
                        <ref cRef="Kid." loc="41.">Kid. f. 41.</ref>
                     </sense>
                     <sense n="c" xml:id="D2c">
                        <cit type="translation" xml:lang="la">
                           <quote>praecludi</quote>
                        </cit>, <lbl expand="cum">c.</lbl>
                        <gramGrp>
                           <pos expand="pronomen suffixum">pron. suff.</pos>
                        </gramGrp> (pro <gramGrp>
                           <case value="dativus">Dat.</case>
                        </gramGrp>): <foreign xml:lang="gez">ኢተዐጽወተኒ፡ ዛቲ፡ ፍሥሓ፡</foreign>
                        <ref cRef="2 Cor." loc="11,10">2 Cor. 11,10</ref>; <lbl expand="cum">c.</lbl>
                        <foreign xml:lang="gez">ላዕለ፡ </foreign> pers.: <ref cRef="Hen." loc="80,7">Hen. 80,7</ref>. – <lbl expand="Vocabularium Aethiopicum, -a -a.">Voc. Ae.</lbl>: <foreign xml:lang="gez">ተዐጽወ፡ ዘ፡ ተዘጋ።</foreign>
                     </sense>
                  </sense>
               </sense>
               
