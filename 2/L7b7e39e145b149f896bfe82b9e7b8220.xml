         <div>
            <entry xml:id="L7b7e39e145b149f896bfe82b9e7b8220" n="5199">
               <form>
                  <foreign xml:lang="gez">አስተናገረ</foreign>
               </form>
               <sense xml:lang="la" source="#dillmann" xml:id="Dmain"> <subc>IV,3</subc> <sense n="1" xml:id="D1">
                     <cit type="translation" xml:lang="la">
                        <quote>loqui facere</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>loquendi facultatem praebere</quote>
                     </cit>: <foreign xml:lang="gez">መንፈሰ፡ እግዚአብሔር፡ ዘያስተናግረኒ፡</foreign>
                     <ref cRef="Job" loc="27,3">Job 27,3</ref>; Christus <foreign xml:lang="gez">አስተናገረ፡ በሃማነ፡</foreign>
                     <ref cRef="Syn." loc="311,">Syn. f. 311,</ref> vel <foreign xml:lang="gez">አፈ፡
                        በሃማን፡ </foreign> Lud. e Ssal. Req.; Sancti <foreign xml:lang="gez">አስተናገሩ፡
                        ምዉተ፡ </foreign>
                     <ref cRef="M.M." loc="f. 48">M.M. f. 48</ref>; <foreign xml:lang="gez">እለ፡
                        ያስተናግሩ፡ በመቃብር፡ </foreign> (omisso <gramGrp>
                        <case value="accusativus">Acc.</case>
                     </gramGrp>) <ref cRef="2 Par." loc="35,19">2 Par. 35,19</ref>
                     <lbl expand="varia lectio">var.</lbl>
                     <sense n="a" xml:id="D1a">
                        <cit type="translation" xml:lang="la">
                           <quote>disertum reddere</quote>
                        </cit>: <foreign xml:lang="gez">አኮ፡ ዓመታት፡ ዘያስተናግራ፡ </foreign>
                        <ref cRef="Job" loc="32,8">Job 32,8</ref>.</sense>
                     <sense n="b" xml:id="D1b">
                        <cit type="translation" xml:lang="la">
                           <quote>loquacem</quote>
                        </cit>, <cit type="translation" xml:lang="la">
                           <quote>lingua promtum facere</quote>
                        </cit>: <foreign xml:lang="gez">ለኵሉ፡ በከብድ፡ ያስተናግሮሙ፡</foreign> (<foreign xml:lang="gez">ወይን፡</foreign>) <ref cRef="1 Esr. " loc="3,20">1 Esr.
                           3,20</ref>.</sense>
                     <sense n="2" xml:id="D12"> „<cit type="translation" xml:lang="la">
                           <quote>loqui pro</quote>
                        </cit> aliquo, ut videtur“, Lud. e Ssal. Req.</sense>
                  </sense>
               </sense>
               
