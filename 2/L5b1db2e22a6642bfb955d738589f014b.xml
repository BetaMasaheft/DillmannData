               <sense xml:id="Dmain" xml:lang="la" source="#dillmann"> <gramGrp> <pos expand="adjectivum">adj.</pos> </gramGrp> et <gramGrp> <pos expand="Substantivum">subst.</pos> </gramGrp>, <gramGrp> <gen value="femininus">fem.</gen> </gramGrp> <foreign xml:lang="gez">ልህቅት፡</foreign> (<lbl expand="videas">vid.</lbl> <bibl><ptr target="bm:Dillmann1857Gramm"/>gramm.</bibl> § 129,a) et <foreign xml:lang="gez">ለሀቅት፡</foreign> (<ref cRef="Herm." loc="6">Herm. p. 6</ref>; <ref cRef="Herm." loc="17">Herm. p. 17</ref>; per archaismum <foreign xml:lang="gez">ልሀቅት፡</foreign> <ref cRef="Herm." loc="4">Herm. p. 4</ref>; <foreign xml:lang="gez">ልሀቅ፡</foreign> <ref cRef="Herm." loc="2 ">Herm. p. 2 </ref>sine dubio pro <foreign xml:lang="gez">ልህቅት፡</foreign> errore librarii), <lbl expand="pluralis">Pl.</lbl> <gramGrp> <gen value="not found">m.</gen> </gramGrp> <foreign xml:lang="gez">ልሂቃን፡</foreign>, <gramGrp> <gen value="femininus">fem.</gen> </gramGrp> <foreign xml:lang="gez">ልሂቃት፡</foreign> (de <foreign xml:lang="gez">ሊቃውንት፡</foreign> vid. sub <foreign xml:lang="gez">ሊቅ፡</foreign>) <sense n="1" xml:id="D1"> <cit type="translation" xml:lang="la"><quote> aetate provectus </quote></cit>, <cit type="translation" xml:lang="la"><quote> senior </quote></cit> <ref cRef="1 Reg." loc="2,32">1 Reg. 2,32</ref>; <ref cRef="2 Reg." loc="19,32">2 Reg. 19,32</ref>; <ref cRef="Koh." loc="4,13">Koh. 4,13</ref> (<foreign xml:lang="grc">πρεσβύτερος</foreign>); <ref cRef="Jes." loc="9,14">Jes. 9,14</ref> (<foreign xml:lang="grc">πρεσβύτης</foreign>); <foreign xml:lang="gez">ልሂቅኒ፡ ወአእሩግኒ፡ በኀቤነ፡ ሀለወ፡</foreign> <ref cRef="Job" loc="15,10">Job 15,10</ref>; <foreign xml:lang="gez">ንኡስ፡ አነ፡ በመዋዕልየ፡ ወአንትሙ፡ ልሂቃን፡ ወሊቃውንት፡</foreign> <ref cRef="Job" loc="32,7">Job 32,7</ref>; <ref cRef="Sap." loc="2,10">Sap. 2,10</ref>; <foreign xml:lang="gez">ልሂቅ፡ ምስለ፡ አረጋዊ፡</foreign> <ref cRef="Jer." loc="6,11">Jer. 6,11</ref> (<foreign xml:lang="grc">πρεσβύτερος μετὰ πλήρους ἡμερῶν</foreign>); <foreign xml:lang="gez">ክልኤሆሙ፡ ልሂቃን፡ እሙንቱ፡</foreign> <ref cRef="Luc. " loc="1,7">Luc.  1,7</ref>; <ref cRef="Luc." loc="1,18">Luc. 1,18</ref>; <foreign xml:lang="gez">ልሂቃት፡</foreign> <foreign xml:lang="grc">πρεσβύτεραι</foreign> <ref cRef="Zach." loc="8,4">Zach. 8,4</ref>; <foreign xml:lang="gez">ልሂቃት፡ መበለታት፡</foreign> (<lbl expand="oppositum, -o, -nitur">opp.</lbl> <foreign xml:lang="gez">ንኡሳት፡ መበለታት፡</foreign>) <ref cRef="Did." loc="14.">Did. 14.</ref> 
</sense> <sense n="2" xml:id="D2"> <cit type="translation" xml:lang="la"><quote> senior dignitate </quote></cit>, <cit type="translation" xml:lang="la"><quote> princeps </quote></cit>, <cit type="translation" xml:lang="la"><quote> presbyter </quote></cit>, <cit type="translation" xml:lang="la"><quote> praefectus </quote></cit>, quamquam hoc sensu forma contracta <foreign xml:lang="gez">ሊቅ፡</foreign> crebrius adhibetur: <foreign xml:lang="gez">እምውሉደ፡ ልሂቃነ፡ ሀገረ፡ ሮሜ፡</foreign> <cit type="translation" xml:lang="la"><quote> e filiis patriciorum </quote></cit> <ref cRef="Sx. Masc." loc="17">Sx. Masc. 17</ref>; <ref cRef="Sx. Teq." loc="3 ">Sx. Teq. 3 </ref>al.; <foreign xml:lang="gez">ዘይፈቅድ፡ ልሂቅ፡ ይኩን፡</foreign> (<foreign xml:lang="grc">πρῶτος</foreign>) <ref cRef="Marc." loc="9,35">Marc. 9,35</ref>; <foreign xml:lang="grc">πρεσβύτερος</foreign> <cb n="0027" xml:id="c0027"/> (ecclesiae) <ref cRef="1 Petr. " loc="5,1">1 Petr.  5,1</ref>; <foreign xml:lang="grc">πρσβύτιδες</foreign> <foreign xml:lang="gez">ልሂቃት፡</foreign> <ref cRef="Tit." loc="2,3">Tit. 2,3</ref> et <ref cRef="1 Tim." loc="5,2">1 Tim. 5,2</ref> (Platt); <foreign xml:lang="gez">ልሂቃነ፡ መንግሥቱ፡</foreign> <cit type="translation" xml:lang="la"><quote> primores regni </quote></cit>, <ref cRef="Sx. Masc." loc="2.">Sx. Masc. 2.</ref> 
</sense> </sense>
               
