                  <foreign xml:lang="gez">ተናገረ</foreign>
               </form>
               <sense xml:lang="la" source="#dillmann" xml:id="Dmain"> <subc>III,3</subc> <sense n="1" xml:id="D1"> <cit type="translation" xml:lang="la">
                                <quote> colloqui </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> confabulari </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> sermocinari </quote>
                            </cit>: <ref cRef="Ruth" loc="4,1">Ruth 4,1</ref>; <foreign xml:lang="gez">ኢይኩን፡ ተናግሮ፡ እንዘ፡ ይበልዑ፡</foreign> Reg. Pach.; <cb n="0690" xml:id="c0690"/> <lbl expand="cum">c.</lbl> <foreign xml:lang="gez">ምስለ፡</foreign> <ref cRef="Gen." loc="18,27">Gen. 18,27</ref>; <ref cRef="Joh." loc="4,27">Joh. 4,27</ref>; <ref cRef="Prov." loc="23,31">Prov. 23,31</ref> (<foreign xml:lang="grc">ὁμιλεῖν τινί</foreign>). <sense n="a" xml:id="D1a"> <cit type="translation" xml:lang="la">
                                    <quote> sermonem conferre </quote>
                                </cit>, vel <cit type="translation" xml:lang="la">
                                    <quote> communicare </quote>
                                </cit> de re cum aliquo, <lbl expand="cum">c.</lbl> <gramGrp> <case value="accusativus">Acc.</case> </gramGrp> rei: <foreign xml:lang="gez">ተናገረ፡ ምስሌሆሙ፡ ምስጢረ፡ ምክሩ፡</foreign> <ref cRef="Judith" loc="2,2">Judith 2,2</ref>; <ref cRef="4 Esr. " loc="1,3">4 Esr.  1,3</ref>; <foreign xml:lang="gez">ምስለ፡ ብእሲ፡ ጻድቅ፡ ዘልፈ፡ ተናገር፡ ምክረከ፡</foreign> <ref cRef="Sir. " loc="37,12">Sir.  37,12</ref>; <ref cRef="Luc." loc="24,17">Luc. 24,17</ref> (c. <foreign xml:lang="gez">በበይናት፡</foreign>).</sense> <sense n="b" xml:id="D1b"> <cit type="translation" xml:lang="la">
                                    <quote> sermone celebrare </quote>
                                </cit> aliquid, <lbl expand="cum">c.</lbl> <foreign xml:lang="gez">በ፡</foreign> rei: <foreign xml:lang="gez">ከመ፡ ይትናገሩ፡ በዕበየ፡ ግብሩ፡</foreign> <ref cRef="Sir." loc="17,9">Sir. 17,9</ref>; <ref cRef="Sir. " loc="44,8">Sir.  44,8</ref>; <ref cRef="Sir." loc="44,15">Sir. 44,15</ref>; <lbl expand="cum">c.</lbl> <gramGrp> <case value="accusativus">Acc.</case> </gramGrp> rei: <ref cRef="Luc." loc="1,65">Luc. 1,65</ref>.</sense> <sense n="c" xml:id="D1c"> <cit type="translation" xml:lang="la">
                                    <quote> sibi invicem </quote>
                                </cit> <cit type="translation" xml:lang="la">
                                    <quote> indicare </quote>
                                </cit> vel <cit type="translation" xml:lang="la">
                                    <quote> confiteri </quote>
                                </cit> aliquid: <foreign xml:lang="gez">ኢተናገሩ፡ በበይናቲሆሙ፡ ዘውስተ፡ ልቦሙ፡</foreign> <ref cRef="Dan. apocr." loc="1,10">Dan. apocr. 1,10</ref>; <ref cRef="Dan. apocr." loc="1,14">Dan. apocr. 1,14</ref>.</sense> </sense> <sense n="2" xml:id="D2"> <cit type="translation" xml:lang="la">
                                <quote> loqui cum </quote>
                            </cit> aliquo vel alicui, <lbl expand="cum">c.</lbl> <foreign xml:lang="gez">ምስለ፡</foreign> pers.: <ref cRef="Ex." loc="20,19">Ex. 20,19</ref>; <ref cRef="Kuf." loc="3">Kuf. 3</ref>; <ref cRef="Kuf." loc="12">Kuf. 12</ref>; <ref cRef="Kuf." loc="43">Kuf. 43</ref>; <ref cRef="Dan." loc="9,22">Dan. 9,22</ref>; <ref cRef="Apoc." loc="10,8">Apoc. 10,8</ref>; <ref cRef="Apoc." loc="4,1">Apoc. 4,1</ref>; <lbl expand="cum">c.</lbl> <foreign xml:lang="gez">ኀበ፡</foreign> pers., ut: <foreign xml:lang="gez">ጸሎት፡ ይእቲ፡ ተናግሮተ፡ ሰብእ፡ ኀበ፡ እግዚአብሔር፡</foreign> <ref cRef="F.N." loc="14">F.N. 14</ref>; <ref cRef="Ex." loc="4,16">Ex. 4,16</ref>; <lbl expand="cum">c.</lbl> <gramGrp> <case value="accusativus">Acc.</case> </gramGrp> <lbl expand="persona">pers.</lbl>: <ref cRef="Gen." loc="45,12">Gen. 45,12</ref>; <ref cRef="Ex. " loc="6,29">Ex.  6,29</ref>; <ref cRef="Ex." loc="40,1">Ex. 40,1</ref>; <ref cRef="Dan." loc="1,19">Dan. 1,19</ref>; <ref cRef="Asc. Jes." loc="7,6">Asc. Jes. 7,6</ref>; <ref cRef="Asc. Jes." loc="7,7">Asc. Jes. 7,7</ref>; <foreign xml:lang="gez">ኀፍረት፡ ተናግሮ፡ ብእሲተ፡ ባዕድ፡</foreign> <ref cRef="Sir." loc="41,21">Sir. 41,21</ref>; <ref cRef="Matth." loc="12,46">Matth. 12,46</ref>; <ref cRef="Matth." loc="12,47">Matth. 12,47</ref>; <ref cRef="Marc." loc="5,36">Marc. 5,36</ref>; <ref cRef="Luc." loc="1,22">Luc. 1,22</ref>; <foreign xml:lang="gez">ደገምኩ፡ ተናግሮታ፡</foreign> <ref cRef="4 Esr. " loc="10,26">4 Esr.  10,26</ref>; <lbl expand="cum">c.</lbl> <gramGrp> <case value="accusativus">Acc.</case> </gramGrp> rei: <foreign xml:lang="gez">ሠናየ፡ ይትናገረከ፡</foreign> <foreign xml:lang="grc">λαλήσει σοι χαλά</foreign> <ref cRef="Sir." loc="13,6">Sir. 13,6</ref>; <ref cRef="Hen." loc="91,3">Hen. 91,3</ref>; <ref cRef="1 Reg. " loc="18,23">1 Reg.  18,23</ref>; <ref cRef="Deut." loc="23,7">Deut. 23,7</ref>; <ref cRef="Gen." loc="50,21">Gen. 50,21</ref>; <ref cRef="Gen." loc="37,4">Gen. 37,4</ref>; <ref cRef="Joh." loc="8,20">Joh. 8,20</ref>; <lbl expand="cum">c.</lbl> <foreign xml:lang="gez">በ፡</foreign>, ut: <foreign xml:lang="gez">ኢትትናገር፡ ምስለ፡ ብእሲት፡ በዘ፡ ታቀንኣ፡</foreign> <ref cRef="Sir." loc="37,11">Sir. 37,11</ref>; <lbl expand="cum">c.</lbl> <foreign xml:lang="gez">በእንተ፡</foreign> <ref cRef="Jud." loc="14,7">Jud. 14,7</ref>; <ref cRef="Gen. " loc="34,3">Gen.  34,3</ref>. <sense n="a" xml:id="D2a"> <cit type="translation" xml:lang="la">
                                    <quote> alloqui </quote>
                                </cit>, ut: <foreign xml:lang="gez">ተናገራ፡ ለምድር፡</foreign> <ref cRef="Job " loc="12,8">Job  12,8</ref>.</sense> <sense n="b" xml:id="D2b"> <cit type="translation" xml:lang="la">
                                    <quote> addicere </quote>
                                </cit>, <cit type="translation" xml:lang="la">
                                    <quote> promittere </quote>
                                </cit> <ref cRef="Marc." loc="14,11">Marc. 14,11</ref>; <ref cRef="Luc." loc="22,5">Luc. 22,5</ref>.</sense> <sense n="c" xml:id="D2c"> <cit type="translation" xml:lang="la">
                                    <quote> sententiam dicere </quote>
                                </cit> de aliquo: <foreign xml:lang="gez">ተናገሮ፡ ቅስተ፡</foreign> <ref cRef="Jer." loc="46,5">Jer. 46,5</ref> <lbl expand="versio nova">vers. nov.</lbl> </sense> <sense n="d" xml:id="D2d"> <cit type="translation" xml:lang="la">
                                    <quote> loqui </quote>
                                </cit> cum aliquo de femina vel <cit type="translation" xml:lang="la">
                                    <quote> alloqui </quote>
                                </cit> eam i.e. <cit type="translation" xml:lang="la">
                                    <quote> uxorem petere </quote>
                                </cit> eam: <lbl expand="cum">c.</lbl> <foreign xml:lang="gez">በ፡</foreign> feminae <ref cRef="Cant." loc="8,8">Cant. 8,8</ref>; <lbl expand="cum">c.</lbl> <foreign xml:lang="gez">በእንተ፡</foreign> <ref cRef="Jud." loc="14,7">Jud. 14,7</ref>; <lbl expand="cum">c.</lbl> <gramGrp> <case value="accusativus">Acc.</case> </gramGrp> <ref cRef="1 Reg. " loc="25,39">1 Reg.  25,39</ref>.</sense> </sense> <sense n="3" xml:id="D3"> in genere: <cit type="translation" xml:lang="la">
                                <quote> loqui </quote>
                            </cit>: <foreign xml:lang="gez">እለ፡ ይትናገሩ፡ በታሕተ፡ ምድር፡</foreign> <ref cRef="Jes." loc="19,3">Jes. 19,3</ref>; <foreign xml:lang="gez">ተናግሮ፡</foreign> <ref cRef="Koh." loc="3,7">Koh. 3,7</ref> (<lbl expand="oppositum, -o, -nitur">opp.</lbl> tacere); <ref cRef="Ps." loc="37,15">Ps. 37,15</ref>; <ref cRef="Hen." loc="10,1">Hen. 10,1</ref>; <ref cRef="Hen." loc="13,5">Hen. 13,5</ref>; <ref cRef="Hen." loc="14,2">Hen. 14,2</ref>; <ref cRef="Hen." loc="84,1">Hen. 84,1</ref>; <ref cRef="Joh." loc="7,46">Joh. 7,46</ref>; <lbl expand="cum">c.</lbl> <gramGrp> <case value="accusativus">Acc.</case> </gramGrp> rei: <foreign xml:lang="gez">ኢተናገርክሙ፡ ጽድቀ፡</foreign> <ref cRef="Job" loc="42,7">Job 42,7</ref>; <ref cRef="Jer." loc="9,5">Jer. 9,5</ref>; <foreign xml:lang="gez">ኀጢአተ፡ ተናገረት፡ ልሳንክሙ፡</foreign> (<foreign xml:lang="grc">μελετᾶν</foreign>) <ref cRef="Jes." loc="59,3">Jes. 59,3</ref>; <ref cRef="Hen." loc="13,10">Hen. 13,10</ref>; <ref cRef="Hen." loc="14,7">Hen. 14,7</ref>; <ref cRef="Hen. " loc="27,2">Hen.  27,2</ref>; <ref cRef="Matth." loc="10,19">Matth. 10,19</ref>; <ref cRef="Matth." loc="10,20">Matth. 10,20</ref>; <lbl expand="cum">c.</lbl> <foreign xml:lang="gez">በ፡</foreign> rei <ref cRef="Deut." loc="11,19">Deut. 11,19</ref> vel verbi <ref cRef="Hen." loc="67,9">Hen. 67,9</ref>; <lbl expand="cum">c.</lbl> <foreign xml:lang="gez">በ፡</foreign> linguae: <foreign xml:lang="gez">እትናገር፡ በዕብራይስጥ፡</foreign> <ref cRef="Kuf." loc="12">Kuf. 12</ref>; <ref cRef="Kuf." loc="43">Kuf. 43</ref>; vel <lbl expand="cum">c.</lbl> <gramGrp> <case value="accusativus">Acc.</case> </gramGrp> linguae: <foreign xml:lang="gez">ይትናገሩ፡ ከንፈረ፡ አሐደ፡ ወልሳነ፡ ፩</foreign> <ref cRef="Kuf." loc="3">Kuf. 3</ref>; <ref cRef="2 Esr." loc="23,24">2 Esr. 23,24</ref>; de inanimatis usurpatur: <ref cRef="Hebr." loc="12,24">Hebr. 12,24</ref>; <ref cRef="Apoc. " loc="1,12">Apoc.  1,12</ref>. <sense n="a" xml:id="D3a"> <cit type="translation" xml:lang="la">
                                    <quote> diserte loqui </quote>
                                </cit>, <cit type="translation" xml:lang="la">
                                    <quote> disserere </quote>
                                </cit>, <cit type="translation" xml:lang="la">
                                    <quote> orare </quote>
                                </cit>: <foreign xml:lang="gez">ኢይክል፡ ተናግሮ፡</foreign> <ref cRef="Jer." loc="1,6">Jer. 1,6</ref>. <ref cRef="Act." loc="18,25">Act. 18,25</ref>; <lbl expand="cum">c.</lbl> <foreign xml:lang="gez">ኀበ፡</foreign> <ref cRef="Ex." loc="4,16">Ex. 4,16</ref>.</sense> <sense n="b" xml:id="D3b"> <foreign xml:lang="gez">ተናገረ፡ ውስተ፡ ልቦን፡</foreign> <cit type="translation" xml:lang="la">
                                    <quote> benigne eas allocutus est </quote>
                                </cit> (consolandi causa) <ref cRef="Kuf." loc="29.">Kuf. 29.</ref> </sense> </sense> </sense>
               
            </entry>
