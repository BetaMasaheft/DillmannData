                  <foreign xml:lang="gez">ክሕደ</foreign>
               <rs type="root"/>
                    </form>
               <sense xml:lang="la" source="#dillmann" xml:id="Dmain"> <subc>I,1</subc> [amh. <foreign xml:lang="gez">ካደ፡</foreign>; <foreign xml:lang="ar">جَحَدَ</foreign>, <foreign xml:lang="he">כָּחַשׁ</foreign>, <lbl expand="vicinus">vic.</lbl> <foreign xml:lang="he">כָּחַד</foreign> <lbl expand="videas">vid.</lbl> Ges. thes. p. 675] <gramGrp> <pos expand="Subjunctivus">Subj.</pos> </gramGrp> <foreign xml:lang="gez">ይክሐድ፡</foreign>, <sense n="1" xml:id="D1"> <cit type="translation" xml:lang="la">
                                <quote> negare </quote>
                            </cit> id quod verum est, <cit type="translation" xml:lang="la">
                                <quote> infitiari </quote>
                            </cit>; <gramGrp> <pos expand="absolute">abs.</pos> </gramGrp>: <ref cRef="Gen." loc="18,15">Gen. 18,15</ref>; <ref cRef="Matth." loc="26,70">Matth. 26,70</ref>; <ref cRef="Matth." loc="26,72">Matth. 26,72</ref>; <ref cRef="Luc." loc="8,45">Luc. 8,45</ref>; <ref cRef="Joh." loc="1,20">Joh. 1,20</ref>; <lbl expand="cum">c.</lbl> <gramGrp> <case value="accusativus">Acc.</case> </gramGrp> rei, ut: rem inventam F.N. 45. <sense n="a" xml:id="D1a"> <cit type="translation" xml:lang="la">
                                    <quote> recusare </quote>
                                </cit>, <cit type="translation" xml:lang="la">
                                    <quote> ne </quote>
                                </cit>, <lbl expand="sequens">seq.</lbl> <foreign xml:lang="gez">ኢ፡</foreign> <lbl expand="cum">c.</lbl> <gramGrp> <pos expand="Subjunctivus">Subj.</pos> </gramGrp> verbi <ref cRef="Hebr." loc="11,24">Hebr. 11,24</ref>.</sense> <sense n="b" xml:id="D1b"> <cit type="translation" xml:lang="la">
                                    <quote> abnegare </quote>
                                </cit>, <cit type="translation" xml:lang="la">
                                    <quote> denegare </quote>
                                </cit> aliquem (i.e. negavit quis, se novisse aliquem), <lbl expand="cum">c.</lbl> <gramGrp> <case value="accusativus">Acc.</case> </gramGrp>: <foreign xml:lang="gez">ይክሕዱከ፡</foreign> <ref cRef="Sap." loc="16,16">Sap. 16,16</ref>; <ref cRef="Matth. " loc="10,33">Matth.  10,33</ref>; <ref cRef="Matth." loc="26,34">Matth. 26,34</ref>; <ref cRef="Marc." loc="14,27">Marc. 14,27</ref>.</sense>.</sense> <sense n="2" xml:id="D2"> in specie: negare eum vel id, cui quis obedientiam vel fidem debet, i.e <sense n="a" xml:id="D2a"> <cit type="translation" xml:lang="la">
                                    <quote> infidelem esse </quote>
                                </cit>, <cit type="translation" xml:lang="la">
                                    <quote> non </quote>
                                </cit> <cit type="translation" xml:lang="la">
                                    <quote> credere </quote>
                                </cit> <ref cRef="Act." loc="19,9">Act. 19,9</ref>.</sense> <sense n="b" xml:id="D2b"> <cit type="translation" xml:lang="la">
                                    <quote> inobedientem </quote>
                                </cit> vel <cit type="translation" xml:lang="la">
                                    <quote> contumacem </quote>
                                </cit> <cit type="translation" xml:lang="la">
                                    <quote> esse </quote>
                                </cit>, <cit type="translation" xml:lang="la">
                                    <quote> rebellare </quote>
                                </cit>: <foreign xml:lang="gez">ኢይክሕድ፡</foreign> <foreign xml:lang="grc">οὐκ ἀπειθῶ</foreign> <ref cRef="Jes." loc="50,5">Jes. 50,5</ref>; <lbl expand="cum">c.</lbl> <gramGrp> <case value="accusativus">Acc.</case> </gramGrp>: <foreign xml:lang="gez">ዝንቱ፡ ወልድ፡ ይክሕደነ፡</foreign> <ref cRef="Deut." loc="21,20">Deut. 21,20</ref>; <foreign xml:lang="gez">እለ፡ ኢይክሕድዎ፡</foreign> <ref cRef="Sap." loc="1,2">Sap. 1,2</ref>; <ref cRef="Jes." loc="59,13">Jes. 59,13</ref> (<foreign xml:lang="grc">ἀπειθεῖν</foreign>); <lbl expand="cum">c.</lbl> <foreign xml:lang="gez">በ፡</foreign> rei: <foreign xml:lang="gez">እስመ፡ ክሕድክሙ፡ በዝንቱ፡ ነገር፡</foreign> <ref cRef="Jes. " loc="30,12">Jes.  30,12</ref>.</sense> <sense n="c" xml:id="D2c"> <cit type="translation" xml:lang="la">
                                    <quote> fidem </quote>
                                </cit> alicui datam <cit type="translation" xml:lang="la">
                                    <quote> fallere </quote>
                                </cit>, <cit type="translation" xml:lang="la">
                                    <quote> perfide </quote>
                                </cit> <cit type="translation" xml:lang="la">
                                    <quote> agere </quote>
                                </cit>, <cit type="translation" xml:lang="la">
                                    <quote> perfide </quote>
                                </cit> <cit type="translation" xml:lang="la">
                                    <quote> violare </quote>
                                </cit> vel <cit type="translation" xml:lang="la">
                                    <quote> deserere </quote>
                                </cit>, <cit type="translation" xml:lang="la">
                                    <quote> deficere </quote>
                                </cit>, <cit type="translation" xml:lang="la">
                                    <quote> desciscere </quote>
                                </cit>, <gramGrp> <pos expand="absolute">abs.</pos> </gramGrp>: <foreign xml:lang="gez">ክሕድነ፡</foreign> <foreign xml:lang="grc">ἀπέστημεν</foreign> <ref cRef="Dan." loc="9,9">Dan. 9,9</ref>; <lbl expand="cum">c.</lbl> <gramGrp> <case value="accusativus">Acc.</case> </gramGrp> <lbl expand="persona">pers.</lbl> vel rei: <foreign xml:lang="gez">ክሕዱኒ፡</foreign> <foreign xml:lang="grc">κατεφρόνησέ μου</foreign> <ref cRef="Hos." loc="6,7">Hos. 6,7</ref>; <foreign xml:lang="gez">ክሕዱኒ፡ ቤተ፡ እስራኤል፡</foreign> <foreign xml:lang="grc">ἠθέτησεν</foreign> <ref cRef="Jer." loc="3,20">Jer. 3,20</ref>; <foreign xml:lang="gez">ክሒደ፡ ትክሕደኒ፡</foreign> <ref cRef="Jes." loc="48,8">Jes. 48,8</ref>; <foreign xml:lang="gez">እምትጥበብ፡ ወትክሐድ፡ ሕጎ፡ ለእግዚአብሔር፡</foreign> <foreign xml:lang="grc">παραβαίνειν</foreign> <ref cRef="Sir." loc="19,24">Sir. 19,24</ref>; <foreign xml:lang="gez">ክሕዱ፡ ኪዳነ፡</foreign> <foreign xml:lang="grc">ἤλλαξαν</foreign> <ref cRef="Jes." loc="24,5">Jes. 24,5</ref>; <ref cRef="4 Esr." loc="5,11">4 Esr. 5,11</ref>; <foreign xml:lang="grc">ἀρνεῖσθαι</foreign> <ref cRef="Tit." loc="1,16">Tit. 1,16</ref>; <foreign xml:lang="gez">ኢይክል፡ ክሒደ፡ ርእሶ፡</foreign> <cit type="translation" xml:lang="la">
                                    <quote> non </quote>
                                </cit> <cit type="translation" xml:lang="la">
                                    <quote> potest </quote>
                                </cit> (se ipsum negare i.e.) <cit type="translation" xml:lang="la">
                                    <quote> a </quote>
                                </cit> <cit type="translation" xml:lang="la">
                                    <quote> se </quote>
                                </cit> <cit type="translation" xml:lang="la">
                                    <quote> ipso </quote>
                                </cit> <cit type="translation" xml:lang="la">
                                    <quote> desciscere </quote>
                                </cit> <ref cRef="2 Tim." loc="2,13">2 Tim. 2,13</ref>; <foreign xml:lang="gez">ክሕደ፡ መሐላ፡</foreign> F.M. 22,9; <foreign xml:lang="gez">አምጽኡ፡ ላዕሌነ፡ መሐላ፡ ዘኢንክል፡ ክሒደ፡</foreign> <ref cRef="Judith " loc="8,30">Judith  8,30</ref>; <lbl expand="cum">c.</lbl> <foreign xml:lang="gez">ለ፡</foreign> <ref cRef="1 Thess." loc="4,8">1 Thess. 4,8</ref>; <lbl expand="cum">c.</lbl> <foreign xml:lang="gez">በ፡</foreign> pers. vel rei: <foreign xml:lang="gez">ክሕደት፡ በእግዚአብሔር፡</foreign> <ref cRef="Sir." loc="23,23">Sir. 23,23</ref>; Jos.7,11; <ref cRef="1 Joh." loc="2,22">1 Joh. 2,22</ref>; <ref cRef="1 Joh." loc="2,23">1 Joh. 2,23</ref>; <lbl expand="cum">c.</lbl> <foreign xml:lang="gez">እምነ፡</foreign> <ref cRef="Tob." loc="1,4">Tob. 1,4</ref> (<foreign xml:lang="grc">ἀπέστη</foreign>); <lbl expand="cum">c.</lbl> <foreign xml:lang="gez">እምውስተ፡</foreign> <ref cRef="Deut." loc="9,12">Deut. 9,12</ref> (<foreign xml:lang="grc">παρέβησαν</foreign>).</sense> <sense n="d" xml:id="D2d"> in specie: <cit type="translation" xml:lang="la">
                                    <quote> abnegare </quote>
                                </cit> Deum Christum, <cit type="translation" xml:lang="la">
                                    <quote> abjurare </quote>
                                </cit> fidem Christianam, <cit type="translation" xml:lang="la">
                                    <quote> apostatam </quote>
                                </cit> <cit type="translation" xml:lang="la">
                                    <quote> fieri </quote>
                                </cit>: <foreign xml:lang="gez">ክሕደ፡ ሃይማኖቶ፡</foreign> <ref cRef="1 Tim." loc="5,8">1 Tim. 5,8</ref>; <ref cRef="1 Tim. " loc="5,12">1 Tim.  5,12</ref>; <foreign xml:lang="gez">ክሒድ፡ ስመ፡ እግዚእነ፡ ኢየሱስ፡</foreign> F.M. 16; Sx. ut <ref cRef="Sx. Masc." loc="13">Sx. Masc. 13</ref>; Syn., crebro.</sense> </sense> </sense>
               
