         <div>
            <entry xml:id="L72a7103aa7e7410aa6625c565dce6b45" n="9585">
               <form>
                  <foreign xml:lang="gez">ጽጋብ</foreign>
               </form>
               <sense xml:lang="la" source="#dillmann" xml:id="Dmain">
                  <gramGrp>
                     <pos expand="Substantivum">subst.</pos>
                  </gramGrp> (semel reperi <foreign xml:lang="gez">ጽገብ፡</foreign>) <sense n="1" xml:id="D1">
                     <cit type="translation" xml:lang="la">
                        <quote>saturatio</quote>
                     </cit> (i.e. epulatio ad satietatem), <foreign xml:lang="grc">εὐφροσύνη </foreign>
                     <ref cRef="Esth." loc="9,18">Esth. 9,18</ref>; <ref cRef="Esth." loc="9,22">Esth. 9,22</ref>.</sense>
                  <sense n="2" xml:id="D2">
                     <cit type="translation" xml:lang="la">
                        <quote>satietas</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>saturitas</quote>
                     </cit>: <foreign xml:lang="gez">ጽጋበ፡ ከርሥ፡</foreign>
                     <ref cRef="Prov. " loc="24,15">Prov. 24,15</ref>; <foreign xml:lang="gez">እለ፡
                        ኢያአምሩ፡ ጽጋበ፡</foreign>
                     <ref cRef="Jes." loc="56,11">Jes. 56,11</ref>; <foreign xml:lang="gez">ኢይከውነክሙ፡
                        ለጽጋብ፡</foreign> (v. <foreign xml:lang="gez">ለጽገብ፡</foreign>) <ref cRef="Jes." loc="55,2">Jes. 55,2</ref>; <ref cRef="Lev." loc="26,5">Lev.
                        26,5</ref>; <ref cRef="Ps." loc="105,16">Ps. 105,16</ref>; <ref cRef="Hez." loc="16,49">Hez. 16,49</ref>; <foreign xml:lang="gez">ይከሪ፡ ጽጋብ፡</foreign>
                     <ref cRef="Sir." loc="11,27">Sir. 11,27</ref>; <foreign xml:lang="gez">ጽጋብ፡
                        ኢይምጽአኒ፡</foreign>
                     <foreign xml:lang="grc">κοιλίας ὄρεχις</foreign>
                     <ref cRef="Sir. " loc="23,6">Sir. 23,6</ref>.</sense>
                  <sense n="3" xml:id="D3">
                     <cit type="translation" xml:lang="la">
                        <quote>abundantia</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>ubertas</quote>
                     </cit> frugum: <foreign xml:lang="gez">ዓመተ፡ ጽጋብ፡ </foreign> (<lbl expand="oppositum, -o, -nitur">opp.</lbl>
                     <foreign xml:lang="gez">ዐባር፡</foreign>) <ref cRef="Kuf." loc="40">Kuf.
                     40</ref>; <ref cRef="Gen." loc="41,29">Gen. 41,29</ref>; <foreign xml:lang="gez">መዋዕለ፡ ጽጋብ፡</foreign> (<lbl expand="oppositum, -o, -nitur">opp.</lbl>
                     <foreign xml:lang="gez">ረኃብ፡</foreign>) <ref cRef="Sir." loc="18,25">Sir.
                        18,25</ref>; <ref cRef="Sx. Sen." loc="26 ">Sx. Sen. 26 </ref>Enc.</sense>
               </sense>
               
            </entry>
