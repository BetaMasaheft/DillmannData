               <sense xml:lang="la" source="#dillmann" xml:id="Dmain"> <subc>II,1</subc> et <subc>II,2</subc> <sense n="1" xml:id="D1"> insidere facere i.e. <sense n="a" xml:id="D1a"> <cit type="translation" xml:lang="la">
                                    <quote> equitare facere </quote>
                                </cit> vel <cit type="translation" xml:lang="la">
                                    <quote> jubere </quote>
                                </cit>: <ref cRef="Marc." loc="11,7">Marc. 11,7</ref>; <foreign xml:lang="gez">ውስተ፡ ፈረስ፡</foreign> <ref cRef="Gen." loc="41,43">Gen. 41,43</ref>; <foreign xml:lang="gez">ዲበ፡ በቅል፡</foreign> <ref cRef="3 Reg." loc="1,33">3 Reg. 1,33</ref>; <foreign xml:lang="gez">ያጽዕንዎ፡</foreign> (<gramGrp> <pos expand="Subjunctivus">Subj.</pos> </gramGrp>) <foreign xml:lang="gez">ዲበ፡ ፈረሱ፡</foreign> <ref cRef="Esth." loc="6,9">Esth. 6,9</ref>; <ref cRef="Esth." loc="6,11">Esth. 6,11</ref>.</sense> <sense n="b" xml:id="D1b"> <cit type="translation" xml:lang="la">
                                    <quote> invehi facere </quote>
                                </cit> vel <cit type="translation" xml:lang="la">
