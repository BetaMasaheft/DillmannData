         <div>
            <entry xml:id="L62b34f3096e545c686ad0a75a1b8532b" n="442">
               <form>
                  <foreign xml:lang="gez">መሌሊት</foreign>
               </form>
               <sense xml:lang="la" source="#dillmann" xml:id="Dmain">
                  <gramGrp>
                     <pos expand="Substantivum">subst.</pos>
                  </gramGrp>, <lbl expand="pluralis">Pl.</lbl>
                  <foreign xml:lang="gez">መለያልይ፡</foreign>, <lbl expand="pluralis">Pl.</lbl>
                  <lbl expand="pluralis">Pl.</lbl>
                  <foreign xml:lang="gez">መለያልያት፡</foreign> pars corporis separabilis i.e. <cit type="translation" xml:lang="la">
                     <quote>membrum</quote>
                  </cit>, <cit type="translation" xml:lang="la">
                     <quote>artus</quote>
                  </cit>, nec non locus, quo et junguntur et separantur singula membra i.e. <cit type="translation" xml:lang="la">
                     <quote>articulus</quote>
                  </cit>, <cit type="translation" xml:lang="la">
                     <quote>junctura membrorum</quote>
                  </cit>: <foreign xml:lang="gez">ገመዱ፡ መሌሊቶሙ፡ ከመ፡ ሥጋ፡</foreign>
                  <ref cRef="Mich." loc="3,3">Mich. 3,3</ref>; <foreign xml:lang="gez">መተረ፡</foreign> (<foreign xml:lang="gez">አልህምተ፡</foreign>) <foreign xml:lang="gez">በበመለያልይሆሙ፡</foreign>
                  <ref cRef="1 Reg. " loc="11,7">1 Reg. 11,7</ref>; <ref cRef="Lev." loc="1,6">Lev.
                     1,6</ref>; <foreign xml:lang="gez">ትጠብሖ፡ በበመሌሊቱ፡</foreign>
                  <ref cRef="Ex." loc="29,17">Ex. 29,17</ref>; <foreign xml:lang="gez">አስተጋብኦሙ፡
                     ለአዕጽምት፡ ውስተ፡ መለያልዩ፡ ዘዘዚአሆሙ፡</foreign>
                  <ref cRef="Hez." loc="37,7">Hez. 37,7</ref>; <ref cRef="Job" loc="9,28">Job
                     9,28</ref>; <ref cRef="Job" loc="31,12">Job 31,12</ref>; <ref cRef="4 Esr." loc="8,12">4 Esr. 8,12</ref>; <ref cRef="4 Esr." loc="8,14">4 Esr. 8,14</ref>;
                     <ref cRef="Rom." loc="12,4">Rom. 12,4</ref>; <ref cRef="Hebr." loc="4,12">Hebr.
                     4,12</ref>; <ref cRef="1 Cor." loc="12,12">1 Cor. 12,12</ref> rom; <ref cRef="1 Cor. " loc="12,14">1 Cor. 12,14</ref>
                  <lbl expand="romanae editionis">rom.</lbl>; <ref cRef="1 Cor." loc="12,18">1 Cor.
                     12,18</ref>
                  <lbl expand="romanae editionis">rom.</lbl>, <foreign xml:lang="gez">መለያልየ፡
                     አዕጽምቲሃ፡</foreign>
                  <ref cRef="Jud. " loc="19,29">Jud. 19,29</ref>; <foreign xml:lang="gez">መሌሊተ፡
                     ሐቌሁ፡</foreign>
                  <foreign xml:lang="grc">οἱ σύνδεσμοι τῆς ὀσφύος αὐτοῦ</foreign>
                  <ref cRef="Dan." loc="5,6">Dan. 5,6</ref>; <foreign xml:lang="grc">μηροί</foreign>
                  (?) <ref cRef="Hez." loc="21,7">Hez. 21,7</ref>; <foreign xml:lang="gez">መለያልያት፡
                     ኅቡረ፡</foreign>
                  <cit type="translation" xml:lang="la">
                     <quote>omnia membra simul</quote>
                  </cit>, <ref cRef="Chrys. Ta." loc="5 ">Chrys. Ta. 5 </ref>[Amh.: <foreign xml:lang="gez">ብልት፡</foreign>]. </sense>
               
