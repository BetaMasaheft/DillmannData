            <entry xml:id="L76cd032062f14792a8b51a9cc28e39dc" n="903">
               <form>
                  <foreign xml:lang="gez">ሐዘበ</foreign>
               <rs type="root"/>
                    </form>
               <sense xml:lang="la" source="#dillmann" xml:id="Dmain"> radix et significatione et etymo intime conjuncta cum <foreign xml:lang="gez">ሐሰበ፡</foreign> (<lbl expand="quod videas">q.v.</lbl>). Notio prima <sense n="1" xml:id="D1"> est <cit type="translation" xml:lang="la">
                                <quote> colligere </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> congregare </quote>
                            </cit> (<lbl expand="videas">vid.</lbl> <foreign xml:lang="ar">حَزَبَ</foreign> II III V; <lbl expand="vicinus">vic.</lbl> <foreign xml:lang="ar">حَشَبَ</foreign> VIII), unde <foreign xml:lang="gez">ሕዝብ፡</foreign> <cit type="translation" xml:lang="la">
                                <quote> populus </quote>
                            </cit>;</sense> <sense n="2" xml:id="D2"> <cit type="translation" xml:lang="la">
                                <quote> colligere </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> conferre </quote>
                            </cit> i.e. <cit type="translation" xml:lang="la">
                                <quote> calculum subducere </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> putare </quote>
                            </cit>, <lbl expand="videas">vid.</lbl> <foreign xml:lang="gez">ሐሰበ።</foreign> </sense> </sense>
               
