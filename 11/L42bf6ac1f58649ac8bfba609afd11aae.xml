            <entry xml:id="L42bf6ac1f58649ac8bfba609afd11aae" n="5449">
               <form>
                  <foreign xml:lang="gez">ተአመረ</foreign>
               </form>
               <sense xml:lang="la" source="#dillmann" xml:id="Dmain"> <subc>III,2</subc> <sense n="1" xml:id="D1"> i.q. <foreign xml:lang="gez">ተአምረ፡</foreign> Nr. 1: <cit type="translation" xml:lang="la">
                                <quote> se cognoscendum praebere </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> apparere </quote>
                            </cit>, <ref cRef="Ex." loc="29,42">Ex. 29,42</ref>; <ref cRef="Ex." loc="30,6">Ex. 30,6</ref>; <ref cRef="Ex. " loc="30,36">Ex.  30,36</ref>.</sense> <sense n="2" xml:id="D2"> <cit type="translation" xml:lang="la">
                                <quote> promulgari </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> indici </quote>
                            </cit>: <foreign xml:lang="gez">እንበለ፡ ይትአመር፡ ለኵሉ፡ ሥጋ፡ ለአሰንብቶ፡</foreign> <ref cRef="Kuf." loc="10.">Kuf. p. 10.</ref> </sense> <sense n="3" xml:id="D3"> <gramGrp> <pos expand="denominates">denom.</pos> </gramGrp> a <foreign xml:lang="gez">ትእምርት፡</foreign> <sense n="a" xml:id="D3a"> <cit type="translation" xml:lang="la">
                                    <quote> signum dare </quote>
                                </cit>: <foreign xml:lang="gez">ትትኤመሩ፡ በመጥቅዕ፡</foreign> <ref cRef="Num." loc="10,9">Num. 10,9</ref>; <foreign xml:lang="gez">ተአመሩ፡ በቃለ፡ ቀርን፡</foreign> <ref cRef="Jer." loc="4,5">Jer. 4,5</ref>; <lbl expand="cum">c.</lbl> <gramGrp> <pos expand="pronomen suffixum">pron. suff.</pos> </gramGrp> (sensu Dativi): <foreign xml:lang="gez">እትኤመሮሙ፡</foreign> <foreign xml:lang="grc">σημανῶ αὐτοῖς</foreign> <ref cRef="Zach." loc="10,8">Zach. 10,8</ref>; <foreign xml:lang="gez">አንብሮ፡ ኀበ፡ ይትኤመሩ፡</foreign> <foreign xml:lang="grc">θὲς αὐτὸν ἐπὶ σημείου</foreign> (<foreign xml:lang="he">נֵס</foreign>) <ref cRef="Num." loc="21,8">Num. 21,8</ref>; <ref cRef="Num. " loc="21,9">Num.  21,9</ref>.</sense> <sense n="b" xml:id="D3b"> <cit type="translation" xml:lang="la">
                                    <quote> signum </quote>
                                </cit>, <cit type="translation" xml:lang="la">
                                    <quote> omen captare </quote>
                                </cit>: <foreign xml:lang="gez">ዘይትኤመር፡ በአጻብዕ፡ አው፡ በአሕጻ፡</foreign> <ref cRef="Kedr f." loc="65">Kedr f. 65</ref>; <ref cRef="Did. ed. Platt f." loc="48.">Did. ed. Platt f. 48.</ref> </sense> </sense> <sense n="4" xml:id="D4"> i.q. <foreign xml:lang="gez">ተአምረ፡</foreign> Nr. 3, <lbl expand="quod videas">q.v.</lbl> </sense> </sense>
               
