               <sense xml:lang="la" source="#dillmann" xml:id="Dmain"> <subc>I,1</subc> [<foreign xml:lang="he">סָחָה סוּהַ</foreign> <foreign xml:lang="ar">سَحَا</foreign> <cit type="translation" xml:lang="la">
                            <quote> verrendo removit </quote>
                        </cit>; an etiam <foreign xml:lang="ar">صَاعٌ صَاعَةٌ</foreign> <cit type="translation" xml:lang="la">
                            <quote> terra plana </quote>
                        </cit>?] <gramGrp> <pos expand="Subjunctivus">Subj.</pos> </gramGrp> <foreign xml:lang="gez">ይጺሕ፡</foreign>, (purgare), <cit type="translation" xml:lang="la">
                            <quote> aequare </quote>
                        </cit>, <cit type="translation" xml:lang="la">
                            <quote> complanare </quote>
                        </cit>, <sense n="a" xml:id="Da"> viam, <cit type="translation" xml:lang="la">
                                <quote> sternere </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> munire </quote>
                            </cit>: <foreign xml:lang="gez">ጺሑ፡ ፍኖተ፡ ቅድመ፡ ገጹ፡</foreign> (<foreign xml:lang="grc">καθαρίσατε</foreign>) <ref cRef="Jes." loc="57,14">Jes. 57,14</ref>; <foreign xml:lang="gez">ጺሑ፡ ፍኖተ፡ ለሕዝብ፡</foreign> <ref cRef="Jes." loc="62,10">Jes. 62,10</ref>; <ref cRef="Jes." loc="40,3">Jes. 40,3</ref>; <ref cRef="Ps." loc="77,55">Ps. 77,55</ref>; <foreign xml:lang="gez">ጼሕከ፡ ፍኖተ፡</foreign> <ref cRef="Ps." loc="79,10">Ps. 79,10</ref>; <ref cRef="Matth." loc="11,10">Matth. 11,10</ref> <foreign xml:lang="gez">ኢጼሐ፡ መንገደ፡</foreign> <ref cRef="Jer." loc="10,23">Jer. 10,23</ref>; <ref cRef="Jsp." loc="358 ">Jsp. p. 358 </ref>(<lbl expand="videas">vid. sub</lbl> <foreign xml:lang="gez">ኰስተረ፡</foreign>); <foreign xml:lang="gez">ንጸይሕ፡ ፍኖተ፡ ለሐመርነ፡</foreign> <ref cRef="Clem." loc="129">Clem. f. 129</ref>; omisso <foreign xml:lang="gez">ፍኖተ፡</foreign>, <ref cRef="Sx. Hed." loc="17 ">Sx. Hed. 17 </ref>Enc.</sense> <sense n="b" xml:id="Db"> alia, ut colles, tumulos, salebrosa: <foreign xml:lang="gez">እጸይሕ፡ ሎሙ፡ አድባረ፡ ወኵሎ፡ ፍኖተ፡</foreign> <ref cRef="Jes." loc="49,11">Jes. 49,11</ref>; <foreign xml:lang="gez">መብእሰ፡</foreign> <ref cRef="Jes." loc="42,16">Jes. 42,16</ref>; cineres busti: <foreign xml:lang="gez">ዘጼሑ፡ ሐመደ፡</foreign> Sx. <ref cRef="Hed." loc="7 ">Hed. 7 </ref>Enc. – <lbl expand="Vocabularium Aethiopicum, -a -a">Voc. Ae.</lbl>: <foreign xml:lang="gez">ጼሐ፡ ኰስተረ፡ ቀጽዐ፡ ዘ፡ ጸረገ፡</foreign>; <cit type="translation" xml:lang="la">
                                <quote> aliud </quote>
                            </cit>: <foreign xml:lang="gez">ጼሐ፡ ዘ፡ ጸረገ፡ መንጸረሂ፡ ይ፡ ቈለዘሂ፡ ይ፡</foreign> </sense> </sense>
               
