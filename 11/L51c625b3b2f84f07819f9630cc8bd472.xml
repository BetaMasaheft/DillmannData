         <div>
            <entry xml:id="L51c625b3b2f84f07819f9630cc8bd472" n="5969">
               <form>
                  <foreign xml:lang="gez">ተክህለ</foreign>
               </form>
               <sense xml:lang="la" source="#dillmann" xml:id="Dmain"> <subc>III,1</subc> <sense n="1" xml:id="D1"> <cit type="translation" xml:lang="la">
                                <quote> possibile esse, fieri posse, posse </quote>
                            </cit> <lbl expand="sequens">seq.</lbl> verbo passivo vel intransitivo: <foreign xml:lang="gez">ኢይትከሀልዝ፡</foreign>, <foreign xml:lang="gez">ኵሉ፡ ይትከሀል፡</foreign> <ref cRef="Matth." loc="19,26">Matth. 19,26</ref>; <lbl expand="construitur cum">c.c.</lbl> Nom. vel <gramGrp> <case value="accusativus">Acc.</case> </gramGrp> Infin.: <foreign xml:lang="gez">እምእደ፡ ዚአከ፡ ጐይይ፡ ኢይትከሀል፡ ወኢይደሉ፡</foreign> <ref cRef="Sap." loc="16,15">Sap. 16,15</ref>; <foreign xml:lang="gez">ኢይትከሀል፡ ረቲዐ፡</foreign> <ref cRef="Koh." loc="1,15">Koh. 1,15</ref>; <ref cRef="Dan." loc="5,16">Dan. 5,16</ref> <lbl expand="varia lectio">var.</lbl>; <ref cRef="Marc." loc="6,5">Marc. 6,5</ref>; <ref cRef="Hebr." loc="6,18">Hebr. 6,18</ref> <lbl expand="romanae editionis">rom.</lbl>; <lbl expand="sequens">seq.</lbl> <foreign xml:lang="gez">ለ፡</foreign> <lbl expand="cum">c.</lbl> Infin.: <foreign xml:lang="gez">ኢይትከሀል፡ ለኈልቆ፡</foreign> (<cit type="translation" xml:lang="la">
                                <quote> non </quote>
                            </cit> <cit type="translation" xml:lang="la">
                                <quote> potest </quote>
                            </cit> <cit type="translation" xml:lang="la">
                                <quote> numerari </quote>
                            </cit>) <ref cRef="Koh." loc="3,14">Koh. 3,14</ref>; <lbl expand="cum">c.</lbl> <gramGrp> <pos expand="Subjunctivus">Subj.</pos> </gramGrp> verbi: <foreign xml:lang="gez">ኢይትከሀል፡ ከመዝ፡ ይኩን፡</foreign> <ref cRef="Ex." loc="8,22">Ex. 8,22</ref>; <ref cRef="4 Esr." loc="10,68">4 Esr. 10,68</ref>; <ref cRef="Hen." loc="9,5">Hen. 9,5</ref>; <ref cRef="Jac." loc="3,11">Jac. 3,11</ref>; <ref cRef="Hebr." loc="5,11">Hebr. 5,11</ref>; <ref cRef="Hebr. " loc="6,4">Hebr.  6,4</ref>; <ref cRef="Hebr." loc="6,5">Hebr. 6,5</ref>; <ref cRef="Hebr." loc="6,6">Hebr. 6,6</ref>; <lbl expand="sequens">seq.</lbl> <foreign xml:lang="gez">ከመ፡</foreign> <lbl expand="cum">c.</lbl> <gramGrp> <pos expand="Subjunctivus">Subj.</pos> </gramGrp> <ref cRef="4 Esr." loc="9,33">4 Esr. 9,33</ref>; <lbl expand="cum">c.</lbl> <foreign xml:lang="gez">በኀበ፡</foreign> pers. <ref cRef="Matth." loc="19,26">Matth. 19,26</ref>; <lbl expand="cum">c.</lbl> <foreign xml:lang="gez">ለ፡</foreign> pers. <ref cRef="Hebr." loc="6,18">Hebr. 6,18</ref> <lbl expand="romanae editionis">rom.</lbl>; <lbl expand="cum">c.</lbl> <gramGrp> <case value="accusativus">Acc.</case> </gramGrp> <lbl expand="persona">pers.</lbl> (<cit type="translation" xml:lang="la">
                                <quote> possibile est alicui i.e. potest aliquis</quote>
                            </cit>): <foreign xml:lang="gez">ኵሉ፡ ይትከሀለከ፡</foreign> <ref cRef="Job" loc="42,2">Job 42,2</ref>; <foreign xml:lang="gez">በከመ፡ ይትከሀለከ፡</foreign> <ref cRef="Sir." loc="38,11">Sir. 38,11</ref>; <foreign xml:lang="gez">ኢተክህተሙ፡ በዊአ፡ ውስተ፡ ሀገር፡</foreign> <ref cRef="2 Reg." loc="17,17">2 Reg. 17,17</ref>; <ref cRef="Ps. " loc="140,8">Ps.  140,8</ref>; <ref cRef="Rom." loc="12,18">Rom. 12,18</ref>; <ref cRef="Marc." loc="6,5">Marc. 6,5</ref>.</sense> <sense n="2" xml:id="D2"> <sense n="a" xml:id="D2a"> <cit type="translation" xml:lang="la">
                                    <quote> potentiâ </quote>
                                </cit> <cit type="translation" xml:lang="la">
                                    <quote> superare </quote>
                                </cit> aliquem: <foreign xml:lang="gez">ኢትሕትት፡ ዘይትከሀለከ፡</foreign> <foreign xml:lang="grc">μὴ ἐξέταζε τὰ ἰσχυρότερά σου</foreign> <ref cRef="Sir." loc="3,21">Sir. 3,21</ref>.</sense> <sense n="b" xml:id="D2b"> impersonaliter: <foreign xml:lang="gez">ኢተክህሎ፡ ላዕሌሃ፡</foreign> <cit type="translation" xml:lang="la">
                                    <quote> ei non obtigit potentia super eam (mulierem) i.e. eam (resistentem) superare non potuit </quote>
                                </cit>, <ref cRef="Sx. Masc." loc="21 ">Sx. Masc. 21 </ref>(bis).</sense> </sense> </sense>
               
