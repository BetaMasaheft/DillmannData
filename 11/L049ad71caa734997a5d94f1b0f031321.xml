               <sense xml:id="Dmain" xml:lang="la" source="#dillmann"> vel <foreign xml:lang="gez">ሕጉር፡</foreign>, proprie <gramGrp> <pos expand="participium">part.</pos> </gramGrp> <cit type="translation" xml:lang="la"><quote> compactus </quote></cit>, <cit type="translation" xml:lang="la"><quote> solidus </quote></cit> [a <lbl expand="radix">rad.</lbl> <lbl expand="inusitatus">inus.</lbl> <foreign xml:lang="gez">ሐገረ፡</foreign>, quae primitus <cit type="translation" xml:lang="la"><quote> cingere </quote></cit>, <cit type="translation" xml:lang="la"><quote> ligare </quote></cit>, deinde <cit type="translation" xml:lang="la"><quote> firmare </quote></cit>, <cit type="translation" xml:lang="la"><quote> solidare </quote></cit> significasse videtur, <lbl expand="conferas">cfr.</lbl> <foreign xml:lang="ar">حَجَرٌ</foreign> <cit type="translation" xml:lang="la"><quote> lapis </quote></cit>] usurpatur, cum <foreign xml:lang="gez">ዘቢብ፡</foreign> conjunctum, de <cit type="translation" xml:lang="la"><quote> uva passa </quote></cit> vel <cit type="translation" xml:lang="la"><quote> siccata </quote></cit> <foreign xml:lang="gez">ዘቢበ፡ ሕጕር፡</foreign> (C: <foreign xml:lang="gez">ዘቢብ፡ ሕጉር፡</foreign>) <ref cRef="Num." loc="6,4">Num. 6,4</ref>. Formam <foreign xml:lang="gez">ሐጕር፡</foreign> (<foreign xml:lang="gez">ዘቢብ፡ ሐጕር፡</foreign>) exhibet cod. G, eandemque formam Ludolfus e suo <cit type="translation" xml:lang="la"><quote> Synodi </quote></cit> exemplari allegavit. – <lbl expand="Vocabularium Aethiopicum, -a -a">Voc. Ae.</lbl>: <foreign xml:lang="gez">ሐጉር፡ ዘ፡ ዘቢብ።</foreign> </sense>
               
            <sense xml:id="Tmain" xml:lang="en" source="#traces"><cit type="transcription" xml:lang="gez"><quote>ḥǝgʷǝr</quote></cit>, <cit type="transcription" xml:lang="gez"><quote>ḥǝgur</quote></cit>, <foreign xml:lang="gez">ሐጕር፡</foreign> <cit type="transcription" xml:lang="gez"><quote>ḥagʷǝr</quote></cit> <sense n="G" xml:id="TG"> <foreign xml:lang="gez">ሐጕር፡</foreign> <cit type="transcription" xml:lang="gez"><quote>ḥagʷǝr</quote></cit> «<cit type="translation" xml:lang="fr"><quote>raisin sec</quote></cit> (zibibbo)» ― <foreign xml:lang="am">ዘቢብ፡</foreign> Ms. <ref type="BM" target="BNFabb217">BNFabb217</ref>, fol. 29v. <lb/>
 <bibl><ptr target="bm:Grebaut1952SuppLexicon"/><citedRange unit="page"> 65</citedRange></bibl> 
</sense> <sense n="L" xml:id="TL"> <foreign xml:lang="gez">ሕጕር፡</foreign> <cit type="transcription" xml:lang="gez"><quote>ḥǝgʷǝr</quote></cit>, <foreign xml:lang="gez">ሕጉር</foreign> <cit type="transcription" xml:lang="gez"><quote>ḥǝgur</quote></cit>, <foreign xml:lang="gez">ሐጕር</foreign> <cit type="transcription" xml:lang="gez"><quote>ḥagʷǝr</quote></cit> in <foreign xml:lang="gez">ዘቢበ፡ ሕጕር</foreign> <cit type="transcription" xml:lang="gez"><quote>zabiba ḥǝgʷǝr</quote></cit> <cit type="translation" xml:lang="en"><quote>grape seeds</quote></cit> <lb/>
