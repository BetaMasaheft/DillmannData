            <entry xml:id="L72d8f0500512431d98b21372b01c4a55" n="6730">
               <form>
                  <foreign xml:lang="gez">ውዕየት</foreign>
               </form>
               <sense xml:lang="la" source="#dillmann" xml:id="Dmain">
                  <gramGrp>
                     <pos expand="nomen actionis">n. act.</pos>
                  </gramGrp> et <gramGrp>
                     <pos expand="Substantivum">subst.</pos>
                  </gramGrp>
                  <sense n="1" xml:id="D1">
                     <cit type="translation" xml:lang="la">
                        <quote>ustio</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>combustio</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>incendium</quote>
                     </cit>: <foreign xml:lang="gez">ውዕየተ፡ ሀገር፡ </foreign>
                     <ref cRef="Apoc." loc="18,18">Apoc. 18,18</ref>; <ref cRef="4 Esr." loc="12,51">4 Esr. 12,51</ref>; <foreign xml:lang="gez">ውዕየተ፡ ቤተ፡ እግዚአብሔር፡ </foreign>
                     <ref cRef="Abush." loc="20">Abush. 20</ref>; <foreign xml:lang="gez">ይከውን፡
                        ለውዕየት፡</foreign>
                     <ref cRef="Jes." loc="5,5">Jes. 5,5</ref>
                     <lbl expand="varia lectio">var.</lbl>; <ref cRef="Jes." loc="9,5">Jes.
                        9,5</ref>
                     <lbl expand="varia lectio">var.</lbl>; <foreign xml:lang="gez">በውዕየተ፡
                        እሳት፡</foreign>
                     <ref cRef="2 Petr." loc="3,10">2 Petr. 3,10</ref>; <foreign xml:lang="grc">ἐμπυρισμός</foreign>
                     <ref cRef="Num." loc="11,3">Num. 11,3</ref>; <ref cRef="Deut." loc="9,22">Deut.
                        9,22</ref>.</sense>
                  <sense n="2" xml:id="D2">
                     <cit type="translation" xml:lang="la">
                        <quote>adustio</quote>
                     </cit> corporis, <cit type="translation" xml:lang="la">
                        <quote>locus corporis adustus</quote>
                     </cit>: <ref cRef="Jes." loc="14,11">Jes. 14,11</ref> (v. <foreign xml:lang="gez">ዕየት፡</foreign>); <ref cRef="Ex." loc="21,25">Ex. 21,25</ref>
                     (v. <foreign xml:lang="gez">ዕየት፡</foreign>); <ref cRef="Lev." loc="13,24">Lev.
                        13,24</ref>; <ref cRef="Lev." loc="13,25">Lev. 13,25</ref>; <ref cRef="Lev." loc="13,28">Lev. 13,28</ref>.</sense>
                  <sense n="3" xml:id="D3">
                     <cit type="translation" xml:lang="la">
                        <quote>ardor</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>fervor</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>aestus</quote>
                     </cit> animi, cupiditatum: <foreign xml:lang="gez">ውዕየት፡ ዘፍትወት፡</foreign> F.N.
                     24,1; <foreign xml:lang="gez">በውዕየተ፡ ልብ፡</foreign> (<note>mit Inbrunst</note>)
                        <ref cRef="Sx. Haml." loc="6 ">Sx. Haml. 6 </ref>Enc.; <ref cRef="Jsp." loc="279">Jsp. p. 279</ref>; legas hos psalmos <foreign xml:lang="gez">በልባዌ፡
                        ወበውዕየተ፡ ነፍስ፡</foreign>
                     <ref cRef="Phlx." loc="163">Phlx. 163</ref>; etiam <foreign xml:lang="gez">ውዕየት፡</foreign>
                     <gramGrp>
                        <pos expand="absolute">absol.</pos>
                     </gramGrp> (<note>Inbrunst</note>) <ref cRef="Kedr f." loc="75.">Kedr f.
                        75.</ref>
                  </sense>
