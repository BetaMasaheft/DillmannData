            <entry xml:id="L080dc1a3a4c4455ea8ba67e664c42f11" n="6946">
               <form>
                  <foreign xml:lang="gez">ዕመቅ</foreign>
               </form>
               <sense xml:lang="la" source="#dillmann" xml:id="Dmain">
                  <gramGrp>
                     <pos expand="Substantivum">subst.</pos>
                  </gramGrp> (<gramGrp>
                     <gen value="masculinum">m.</gen>
                  </gramGrp>), <lbl expand="pluralis">Pl.</lbl>
                  <foreign xml:lang="gez">ዕመቃት፡ </foreign>
                  <cit type="translation" xml:lang="la">
                     <quote>profunditas</quote>
                  </cit>,<cit type="translation" xml:lang="la">
                     <quote> profundum</quote>
                  </cit>, (<cit type="translation" xml:lang="la">
                     <quote>altitudo</quote>
                  </cit>,<cit type="translation" xml:lang="la">
                     <quote> altum</quote>
                  </cit>): <ref cRef="Eph." loc="3,18">Eph. 3,18</ref>; <ref cRef="Matth." loc="13,5">Matth. 13,5</ref>; <foreign xml:lang="gez">ዕመቀ፡ ቀላይ፡</foreign>
                  <ref cRef="Job" loc="11,8">Job 11,8</ref>; <ref cRef="Hen." loc="17,3">Hen.
                     17,3</ref>; <ref cRef="Hen." loc="18,11">Hen. 18,11</ref>; <ref cRef="Hen." loc="61,5">Hen. 61,5</ref>; <ref cRef="Hen." loc="90,26">Hen. 90,26</ref>;
                     <foreign xml:lang="gez">ዕመቂሆሙ፡</foreign> (Mss.: <foreign xml:lang="gez">ዕመቃቲሆሙ፡</foreign>) <foreign xml:lang="gez">ለአብሕርት፡ </foreign>
                  <ref cRef="Lit." loc="172,1">Lit. 172,1</ref>; <foreign xml:lang="gez">ዕመቀ፡
                     ዕመቅ፡</foreign>
                  <foreign xml:lang="grc">βαθὺ βάθος</foreign>
                  <ref cRef="Koh." loc="7,24">Koh. 7,24</ref>; <lbl expand="translate">transl.</lbl>: <foreign xml:lang="gez">ዘዕመቀ፡ ልቡ፡ ኢታአምሩ፡</foreign>
                  <ref cRef="Judith" loc="8,14">Judith 8,14</ref>; <foreign xml:lang="gez">ይመጽእ፡
                     ሕርትምና፡ ላዕሌኪ፡ ወኢታአምሪ፡ ዕመቂሁ፡</foreign>
                  <ref cRef="Jes." loc="47,11">Jes. 47,11</ref>; <foreign xml:lang="gez">ዕመቀ፡ እከያት፡ </foreign>
                  <ref cRef="Prov." loc="18,3">Prov. 18,3</ref>; <foreign xml:lang="gez">ዕመቀ፡
                     ንዴቶሙ፡</foreign>
                  <ref cRef="2 Cor." loc="8,2">2 Cor. 8,2</ref>; <foreign xml:lang="gez">ኣዕመቂሁ፡
                     ለብዕለ፡ ጥበቢሁ፡</foreign>
                  <ref cRef="Rom." loc="11,33">Rom. 11,33</ref>; <foreign xml:lang="gez">ዕመቀ፡
                     ምስጢር፡</foreign> Cyr. ad <ref cRef="Reg." loc="26.">Reg. f. 26.</ref>
               </sense>
               
