                    </form>
               <sense xml:lang="la" source="#dillmann" xml:id="Dmain"> <gramGrp> <pos expand="nomen">nom.</pos> </gramGrp>, ubique munitum (sicut <foreign xml:lang="gez">ኵሉ፡</foreign>, <bibl>
                            <ptr target="bm:Dillmann1857Gramm"/>gramm.</bibl> § 157,2) suffixo <cit type="translation" xml:lang="la">
                            <quote> û </quote>
                        </cit>, [etymon incertum; nescio an referendum sit nomen ad <foreign xml:lang="ar">خان خَونٌ</foreign>]; in <gramGrp> <case value="accusativus">Acc.</case> </gramGrp> <foreign xml:lang="gez">ከንቶ፡</foreign>; in st.c. c. <gramGrp> <pos expand="pronomen suffixum">pron. suff.</pos> </gramGrp> <foreign xml:lang="gez">ከንቶ፡</foreign> <sense n="1" xml:id="D1"> Substantive: <cit type="translation" xml:lang="la">
                                <quote> inane </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> vanum </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> vanitas </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> inanitas </quote>
                            </cit>, <foreign xml:lang="gez">ፈጠረት፡ ዓለመ፡ እምነ፡ ከንቱ፡ ዘአልቦቱ፡ ገጸ፡</foreign> <foreign xml:lang="grc">ἐξ ἀμόρφου ὕλης</foreign> <ref cRef="Sap." loc="11,18">Sap. 11,18</ref>; <foreign xml:lang="gez">ከንቱ፡ ይረክቦ፡</foreign> <foreign xml:lang="grc">κενά</foreign> <ref cRef="Job " loc="34,19">Job  34,19</ref>; <ref cRef="Job" loc="7,16">Job 7,16</ref>; <foreign xml:lang="gez">ከመ፡ ይኅሥሥ፡ ከንቶ፡</foreign> <foreign xml:lang="grc">μάταια</foreign> <ref cRef="Jes." loc="45,19">Jes. 45,19</ref>; <foreign xml:lang="gez">ሕልም፡ ወሰገል፡ ወመቅስም፡ ከንቱ፡ ኵሎሙ፡</foreign> <ref cRef="Sir." loc="31,5">Sir. 31,5</ref>; <foreign xml:lang="grc">εἶδος</foreign> <ref cRef="Sir." loc="25,2">Sir. 25,2</ref>; <foreign xml:lang="grc">ματαιότης</foreign> <ref cRef="Koh." loc="1,1">Koh. 1,1</ref>; <ref cRef="Koh. " loc="2,11">Koh.  2,11</ref>; <ref cRef="Koh." loc="4,7">Koh. 4,7</ref>; <ref cRef="Koh." loc="6,11">Koh. 6,11</ref>; <foreign xml:lang="gez">ከንቱ፡ ከንቱ፡</foreign> <ref cRef="Sir." loc="12,8">Sir. 12,8</ref>; <ref cRef="Ps." loc="61,9">Ps. 61,9</ref>; <ref cRef="Rom." loc="8,20">Rom. 8,20</ref>; <foreign xml:lang="gez">እለ፡ ይትዌከሉ፡ በከንቱ፡ ወበሐሰት፡</foreign> <ref cRef="Jes. " loc="28,17">Jes.  28,17</ref>; <ref cRef="Jes." loc="59,4">Jes. 59,4</ref>; <foreign xml:lang="gez">ነገረ፡ ከንቱ፡</foreign> <foreign xml:lang="grc">ῥήματα κενά</foreign> <ref cRef="Job" loc="6,6">Job 6,6</ref>; <foreign xml:lang="gez">አውራኀ፡ ከንቱ፡</foreign> <ref cRef="Job" loc="7,3">Job 7,3</ref>; <ref cRef="Eph." loc="5,6">Eph. 5,6</ref>; <foreign xml:lang="gez">ልቦሙ፡ ከንቱ፡</foreign> <ref cRef="Ps." loc="5,10">Ps. 5,10</ref>; <ref cRef="Jer." loc="10,15">Jer. 10,15</ref>; <ref cRef="Jes." loc="1,13">Jes. 1,13</ref>; <foreign xml:lang="gez">ነቢያት፡ ርእዩ፡ ከንቶ፡</foreign> <ref cRef="Thren. " loc="2,14">Thren.  2,14</ref>; <ref cRef="4 Esr." loc="2,28">4 Esr. 2,28</ref>; <ref cRef="Jer." loc="2,5">Jer. 2,5</ref>; <foreign xml:lang="gez">እለ፡ ይትናገሩ፡ ከንቶ፡</foreign> <foreign xml:lang="grc">κενολόγοι</foreign> <ref cRef="Jes." loc="8,19">Jes. 8,19</ref>; <foreign xml:lang="gez">ንግድፍ፡ ዘንተ፡ ኵሎ፡ ከንቶ፡</foreign> <ref cRef="Clem." loc="260">Clem. f. 260</ref>; <foreign xml:lang="gez">ይከውን፡ ከንቶ፡ ኀይለ፡ መንፈስክሙ፡</foreign> <ref cRef="Jes." loc="33,11">Jes. 33,11</ref>; <foreign xml:lang="gez">ከንቶ፡ ኮነ፡</foreign> <foreign xml:lang="grc">ματαιωθήσεται</foreign> <ref cRef="Judith" loc="6,4">Judith 6,4</ref>; <ref cRef="4 Reg." loc="17,15">4 Reg. 17,15</ref>. <sense n="a" xml:id="D1a"> <sense n="α" xml:id="D1aα"> <cit type="translation" xml:lang="la">
                                        <quote> inutile </quote>
                                    </cit>, <cit type="translation" xml:lang="la">
                                        <quote> nequam </quote>
                                    </cit>: <foreign xml:lang="gez">ነገረ፡ ከንቱ፡</foreign> <foreign xml:lang="grc">ῥματα ἃ οὐ δεῖ</foreign> <ref cRef="Job" loc="15,3">Job 15,3</ref>; <cb n="0854" xml:id="c0854"/> <cit type="translation" xml:lang="la">
                                        <quote> nocens </quote>
                                    </cit>: <foreign xml:lang="gez">ከመ፡ ቆዕ፡ ከንቱ፡ ለስነን፡</foreign> <foreign xml:lang="grc">βλαβερός</foreign> <ref cRef="Prov. " loc="10,26">Prov.  10,26</ref>.</sense> <sense n="β" xml:id="D1aβ"> <cit type="translation" xml:lang="la">
                                        <quote> irritum </quote>
                                    </cit>, <cit type="translation" xml:lang="la">
                                        <quote> cassum </quote>
                                    </cit>: <foreign xml:lang="gez">ከንቱ፡ ላሕይኪ፡</foreign> <ref cRef="Jer." loc="4,30">Jer. 4,30</ref>.</sense> <sense n="γ" xml:id="D1aγ"> de <cit type="translation" xml:lang="la">
                                        <quote> idolis </quote>
                                    </cit> utpote vanis et nihil valentibus <ref cRef="Lev." loc="17,7">Lev. 17,7</ref>; 3 <ref cRef="Reg." loc="16,26">Reg. 16,26</ref>; <ref cRef="4 Reg." loc="17,15">4 Reg. 17,15</ref>; <ref cRef="Am." loc="2,4">Am. 2,4</ref>.</sense> </sense>
                            <sense n="b" xml:id="D1b"> Quamquam <foreign xml:lang="gez">ከንቱ፡</foreign> in st.c. alias non occurrit, tamen passim <gramGrp> <pos expand="pronomen suffixum">pron. suff.</pos> </gramGrp> annectuntur ad formam <foreign xml:lang="gez">ከንቶ፡</foreign> (sive Nom. sit sive <gramGrp> <case value="accusativus">Acc.</case> </gramGrp>), ut: <foreign xml:lang="gez">ከንቶሙኬ፡ ግብጽ፡</foreign> (v. <foreign xml:lang="gez">ከንቶኬ፡</foreign>) <ref cRef="Jes." loc="30,7">Jes. 30,7</ref> <lbl expand="">cod. Laur.</lbl>; <foreign xml:lang="gez">ግብጽ፡ እለ፡ ከንቶሙ፡</foreign> <cit type="translation" xml:lang="la">
                                    <quote> (Aegyptii qui vani sunt) </quote>
                                </cit> <ref cRef="Jes." loc="31,2">Jes. 31,2</ref> <lbl expand="">cod. Laur.</lbl>, <foreign xml:lang="gez">በከንቶሙ፡</foreign> <foreign xml:lang="grc">ἐν τοῖς ματαίος αὐτῶν</foreign> <ref cRef="3 Reg." loc="16,26">3 Reg. 16,26</ref>; <foreign xml:lang="gez">አስሐቶሙ፡ ከንቶሆሙ፡ ዘገብሩ፡ አበዊሆሙ፡</foreign> <ref cRef="Am." loc="2,4">Am. 2,4</ref>.</sense> </sense>
                        <sense n="2" xml:id="D2"> Adjectivi instar substantivo apponitur: <cit type="translation" xml:lang="la">
                                <quote> vanus </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> inanis </quote>
                            </cit>, <foreign xml:lang="gez">ከንቶ፡ ነገረ፡</foreign> <foreign xml:lang="grc">μάταιον λόγον</foreign> <ref cRef="Prov." loc="24,31">Prov. 24,31</ref>; <foreign xml:lang="gez">ከንቶ፡ ምግባሪሁ፡ ይሴልጥ፡</foreign> <ref cRef="Prov." loc="22,8">Prov. 22,8</ref>; <foreign xml:lang="gez">ሰገለ፡ ከንቶ፡ ነገርክሙ፡</foreign> <ref cRef="Hez." loc="13,7">Hez. 13,7</ref>; <foreign xml:lang="gez">እለ፡ ያመጽኡ፡ ነገረ፡ ከንቶ፡</foreign> <ref cRef="Hez." loc="13,19">Hez. 13,19</ref>; <ref cRef="1 Tim. " loc="6,20">1 Tim.  6,20</ref>.</sense> <sense n="3" xml:id="D3"> Adv., sive <foreign xml:lang="gez">ከንቱ፡</foreign> et <foreign xml:lang="gez">ከንቶ፡</foreign> sive <foreign xml:lang="gez">በከንቱ፡</foreign> et <foreign xml:lang="gez">ለከንቱ፡</foreign> <sense n="a" xml:id="D3a"> <cit type="translation" xml:lang="la">
                                    <quote> incassum </quote>
                                </cit>, <cit type="translation" xml:lang="la">
                                    <quote> frustra </quote>
                                </cit>; <foreign xml:lang="gez">ከንቱ፡</foreign> <lbl expand="vel">v.</lbl> <foreign xml:lang="gez">ከንቶ፡ ያመልኩኒ፡</foreign> <foreign xml:lang="grc">μάτην</foreign> <ref cRef="Jes." loc="29,13">Jes. 29,13</ref>; <foreign xml:lang="gez">ከንቱ፡</foreign> <lbl expand="vel">v.</lbl> <foreign xml:lang="gez">ከንቶ፡ ተሰፈወ፡</foreign> <ref cRef="Jes." loc="29,8">Jes. 29,8</ref>; <ref cRef="4 Esr." loc="9,25">4 Esr. 9,25</ref>; <ref cRef="Ps. " loc="72,13">Ps.  72,13</ref> (<foreign xml:lang="grc">ματαίως</foreign>); <ref cRef="Matth." loc="15,9">Matth. 15,9</ref>; <foreign xml:lang="gez">በከንቱ፡</foreign> <ref cRef="Mal." loc="1,10">Mal. 1,10</ref>; <foreign xml:lang="gez">ለከንቱ፡</foreign> ut: <foreign xml:lang="gez">ጻመውኩ፡ ለከንቱ፡</foreign> <ref cRef="Job" loc="2,9">Job 2,9</ref>; <ref cRef="Job " loc="39,16">Job  39,16</ref>; <ref cRef="Jes." loc="22,1">Jes. 22,1</ref>; <ref cRef="Jes." loc="45,18">Jes. 45,18</ref>; <ref cRef="Jer." loc="18,15">Jer. 18,15</ref>; <ref cRef="Jer." loc="26,11">Jer. 26,11</ref>; <ref cRef="Ps." loc="88,46">Ps. 88,46</ref>; <ref cRef="Gal." loc="2,2">Gal. 2,2</ref>; <ref cRef="Gal." loc="2,21">Gal. 2,21</ref>; <ref cRef="Phil." loc="2,16">Phil. 2,16</ref>; <ref cRef="1 Thess." loc="3,5">1 Thess. 3,5</ref>; <ref cRef="2 Cor." loc="6,1">2 Cor. 6,1</ref>.</sense> <sense n="b" xml:id="D3b"> <cit type="translation" xml:lang="la">
                                    <quote> inaniter </quote>
                                </cit>, <cit type="translation" xml:lang="la">
                                    <quote> sine causa </quote>
                                </cit>, <cit type="translation" xml:lang="la">
                                    <quote> temere </quote>
                                </cit>, <foreign xml:lang="gez">በከንቱ፡</foreign> <ref cRef="Ps." loc="3,7">Ps. 3,7</ref>; <ref cRef="Ps." loc="68,5">Ps. 68,5</ref>; <ref cRef="Thren." loc="3,51">Thren. 3,51</ref>; <ref cRef="Joh." loc="15,25">Joh. 15,25</ref>; <foreign xml:lang="gez">ለከንቱ፡</foreign>, ut: <foreign xml:lang="gez">ጸላኤ፡ ይከውኖ፡ ለከንቱ፡</foreign> <ref cRef="Sir." loc="20,23">Sir. 20,23</ref>.</sense> <sense n="c" xml:id="D3c"> <cit type="translation" xml:lang="la">
                                    <quote> gratis </quote>
                                </cit> (<foreign xml:lang="he">חִנָּם</foreign>), <foreign xml:lang="grc">δωρεάν</foreign>, <foreign xml:lang="gez">በከንቱ፡</foreign> <ref cRef="Job" loc="1,9">Job 1,9</ref>; <ref cRef="Jes." loc="52,3">Jes. 52,3</ref>; <ref cRef="Jes." loc="52,5">Jes. 52,5</ref>; <ref cRef="Jer." loc="22,13">Jer. 22,13</ref> <lbl expand="versio altera">vers. alt.</lbl>; <ref cRef="Ex." loc="21,2">Ex. 21,2</ref>; <ref cRef="Rom." loc="3,24">Rom. 3,24</ref>; <ref cRef="2 Cor." loc="11,7">2 Cor. 11,7</ref>; <ref cRef="Apoc." loc="21,6">Apoc. 21,6</ref>; <ref cRef="Apoc." loc="22,17">Apoc. 22,17</ref>. <cit type="translation" xml:lang="la">
                                    <quote> – Voc. Ae.: </quote>
                                </cit> <foreign xml:lang="gez">ከንቱ፡ ብ፡ ቢላሽ።</foreign> </sense> </sense> </sense>
               
            </entry>
