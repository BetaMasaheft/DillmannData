            <entry xml:id="L45ede620d8904f35a81a54e8bcb34210" n="9888">
               <form>
                  <foreign xml:lang="gez">ፍሩህ</foreign>
               </form>
               <sense xml:lang="la" source="#dillmann" xml:id="Dmain">
                  <gramGrp>
                     <pos expand="participium">part.</pos>
                  </gramGrp>
                  <cit type="translation" xml:lang="la">
                     <quote>timore</quote>
                  </cit>
                  <cit type="translation" xml:lang="la">
                     <quote>correptus</quote>
                  </cit>, <cit type="translation" xml:lang="la">
                     <quote>timens</quote>
                  </cit>: abierunt <foreign xml:lang="gez">ፍሩሃን፡</foreign> Jsp. p. 275; <foreign xml:lang="gez">ፍሩሀከ፡ ንበር፡ እንከ፡</foreign>
                  <ref cRef="Rom." loc="11,20">Rom. 11,20</ref>. <sense n="a" xml:id="Da">
                     <cit type="translation" xml:lang="la">
                        <quote>timidus</quote>
                     </cit>, <foreign xml:lang="grc">δειλός</foreign>
                     <ref cRef="Sap." loc="4,20">Sap. 4,20</ref>.</sense>
                  <sense n="b" xml:id="Db">
                     <cit type="translation" xml:lang="la">
                        <quote>cautus</quote>
                     </cit>: Satanas numquam non adest, <foreign xml:lang="gez">በእንተዝ፡ ይደልወነ፡ ከመ፡
                        ንኩን፡ ፍሩሃነ፡ በኵሉ፡ ጊዜ፡</foreign>
                     <ref cRef="Phlx." loc="203.">Phlx. 203.</ref>
                  </sense>
               </sense>
               
