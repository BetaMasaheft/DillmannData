               <form>
                  <foreign xml:lang="gez">ጸንሐ</foreign>
               <rs type="root"/>
                    </form>
               <sense xml:lang="la" source="#dillmann" xml:id="Dmain"> <subc>I,1</subc> [quod equidem non cum Ludolfo ad <foreign xml:lang="syr">ܨܢܰܥ</foreign> i.e. <foreign xml:lang="ar">صنع</foreign> (<foreign xml:lang="gez">ጸንዐ፡</foreign>) <cit type="translation" xml:lang="la">
                            <quote> artificiose conficere </quote>
                        </cit>, sed ad <foreign xml:lang="he">צָנַע צָנַח</foreign> retulerim (at <foreign xml:lang="ar">دنق</foreign> <lbl expand="videas">vid. sub</lbl> <foreign xml:lang="gez">ጠንቀቀ፡</foreign>); a <cit type="translation" xml:lang="la">
                            <quote> subsidendo </quote>
                        </cit> enim et <cit type="translation" xml:lang="la">
                            <quote> se demittendo </quote>
                        </cit> facile derivatur <cit type="translation" xml:lang="la">
                            <quote> insidiari </quote>
                        </cit> et <cit type="translation" xml:lang="la">
                            <quote> exspectare </quote>
                        </cit>] <gramGrp> <pos expand="Subjunctivus">Subj.</pos> </gramGrp> <foreign xml:lang="gez">ይጽናሕ፡</foreign>, <sense n="1" xml:id="D1"> <cit type="translation" xml:lang="la">
                                <quote> insidere </quote>
                            </cit> viam, <cit type="translation" xml:lang="la">
                                <quote> insidiari </quote>
                            </cit>, <lbl expand="cum">c.</lbl> <gramGrp> <case value="accusativus">Acc.</case> </gramGrp> viae: <foreign xml:lang="gez">ጽንሕ፡</foreign> (sic) <foreign xml:lang="gez">ፍኖተ፡ ወቅትል፡ ነፍሰ፡ ሰብእ፡</foreign> <ref cRef="Clem." loc="226">Clem. f. 226</ref>; <foreign xml:lang="gez">ጸኒሐ፡ ፍኖት፡</foreign> <ref cRef="Clem." loc="211">Clem. f. 211</ref>; <foreign xml:lang="grc">ἐνεδρεύειν</foreign> <ref cRef="Prov." loc="7,12">Prov. 7,12</ref>; <ref cRef="Ps." loc="9,30">Ps. 9,30</ref>; <ref cRef="Ps." loc="9,31">Ps. 9,31</ref>; <ref cRef="Ps." loc="9,32">Ps. 9,32</ref>; <ref cRef="Act." loc="23,21">Act. 23,21</ref>; <lbl expand="cum">c.</lbl> <gramGrp> <case value="accusativus">Acc.</case> </gramGrp> <lbl expand="persona">pers.</lbl>: <foreign xml:lang="gez">በገዳም፡ ጸንሑኒ፡</foreign> <ref cRef="Thren. " loc="4,19">Thren.  4,19</ref>; <ref cRef="Num." loc="35,20">Num. 35,20</ref>; <foreign xml:lang="gez">እለ፡ ይጸንሑኒ፡</foreign> <foreign xml:lang="grc">ἐγκάθετοι</foreign> <ref cRef="Job" loc="19,12">Job 19,12</ref>. <sense n="a" xml:id="D1a"> <cit type="translation" xml:lang="la">
                                    <quote> observare </quote>
                                </cit>, <cit type="translation" xml:lang="la">
                                    <quote> speculari </quote>
                                </cit>; <lbl expand="cum">c.</lbl> <gramGrp> <case value="accusativus">Acc.</case> </gramGrp>: <foreign xml:lang="gez">ዐይነ፡ ዘማዊ፡ ይጸንሕ፡ ጽልመተ፡</foreign> <ref cRef="Job" loc="24,15">Job 24,15</ref>; <foreign xml:lang="gez">ይጸንሕዋ፡ ይርከብዋ፡</foreign> <foreign xml:lang="grc">παρετηροῦσαν</foreign> <ref cRef="Dan. apocr." loc="1,12">Dan. apocr. 1,12</ref>; <foreign xml:lang="grc">ὑποβλέπεσθαι</foreign> <ref cRef="1 Reg." loc="18,9">1 Reg. 18,9</ref>.</sense> <sense n="b" xml:id="D1b"> <cit type="translation" xml:lang="la">
                                    <quote> occasionem captare </quote>
                                </cit>: <foreign xml:lang="gez">ይጸንሕ፡</foreign> <foreign xml:lang="grc">ἐτήρει καιρόν</foreign> <ref cRef="Judith" loc="12,16">Judith 12,16</ref>.</sense> </sense> <sense n="2" xml:id="D2"> <cit type="translation" xml:lang="la">
                                <quote> exspectare </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> manere </quote>
                            </cit>, <ref cRef="Job" loc="2,9">Job 2,9</ref>; <ref cRef="Thren." loc="3,21">Thren. 3,21</ref>; <ref cRef="Thren." loc="3,24">Thren. 3,24</ref>; <foreign xml:lang="gez">ጸንሐ፡ ህየ፡ እስከ፡ የኀልፍ፡ ኤርምያስ፡</foreign> Rel. Bar.; <ref cRef="Ex." loc="19,11">Ex. 19,11</ref>; <ref cRef="Hen." loc="3">Hen. 3</ref>; <foreign xml:lang="gez">ጽናሕ፡ ንስቲተ፡</foreign> <ref cRef="Hen." loc="52,5">Hen. 52,5</ref>; <ref cRef="Hen." loc="108,2">Hen. 108,2</ref>; <ref cRef="Hen. " loc="108,3">Hen.  108,3</ref>; <ref cRef="Hebr." loc="10,13">Hebr. 10,13</ref>; <foreign xml:lang="gez">ይጸንሑ፡ ኀቤየ፡</foreign> <ref cRef="Matth." loc="15,32">Matth. 15,32</ref>; <lbl expand="cum">c.</lbl> <gramGrp> <case value="accusativus">Acc.</case> </gramGrp> <lbl expand="persona">pers.</lbl>: <foreign xml:lang="gez">ጸናሕኩክሙ፡</foreign> <ref cRef="Job" loc="32,16">Job 32,16</ref>; <ref cRef="Ruth" loc="1,13">Ruth 1,13</ref>; <foreign xml:lang="gez">እጸንሖ፡ ለእግዚአብሔር፡</foreign> <ref cRef="Jes." loc="8,17">Jes. 8,17</ref>; <ref cRef="Ps." loc="39,1">Ps. 39,1</ref>; <foreign xml:lang="gez">ጽንሐኒ፡</foreign> <ref cRef="Tob." loc="5,7">Tob. 5,7</ref>; <foreign xml:lang="gez">ጽንሖሙ፡ ምስለ፡ ተዓይኒከ፡</foreign> <ref cRef="Judith" loc="7,12">Judith 7,12</ref>; <ref cRef="Act." loc="20,5">Act. 20,5</ref>; vel rei: <foreign xml:lang="gez">ጽንሕዋ፡ ለምሕረቱ፡</foreign> <ref cRef="Sir." loc="2,7">Sir. 2,7</ref>; <foreign xml:lang="gez">ጽናሕ፡ ፍሬሃ፡ ቡሩከ፡</foreign> <ref cRef="Sir." loc="6,19">Sir. 6,19</ref>; <foreign xml:lang="gez">ጸናሕነ፡ ፍትሐ፡</foreign> <ref cRef="Jes." loc="59,11">Jes. 59,11</ref>; <foreign xml:lang="gez">ጽንሖ፡</foreign> <ref cRef="Hab." loc="2,3">Hab. 2,3</ref>; <ref cRef="Rom. " loc="8,19">Rom.  8,19</ref>; <lbl expand="sequens">seq.</lbl> <gramGrp> <pos expand="Subjunctivus">Subj.</pos> </gramGrp> verbi: <foreign xml:lang="gez">ጸንሖሙ፡ ኤልዩስ፡ ያውሥእዎ፡ ለኢዮብ፡</foreign> <ref cRef="Job" loc="32,4">Job 32,4</ref>; <foreign xml:lang="gez">ጸናሕክዎ፡ ይፍረይ፡</foreign> <ref cRef="Jes." loc="5,2">Jes. 5,2</ref>; <ref cRef="Jes." loc="5,4">Jes. 5,4</ref>; <ref cRef="Jes." loc="5,7">Jes. 5,7</ref>; (<ref cRef="Dan. apocr." loc="1,12">Dan. apocr. 1,12</ref>); <lbl expand="cum">c.</lbl> <foreign xml:lang="gez">ለ፡</foreign> rei: <foreign xml:lang="gez">ለምንት፡</foreign> (v. <foreign xml:lang="gez">ምንተ፡</foreign>) <foreign xml:lang="gez">ይጸንሕ፡ ኃጥእ፡</foreign> <ref cRef="Job" loc="27,8">Job 27,8</ref>; (<ref cRef="Prov." loc="19,29">Prov. 19,29</ref>); <lbl expand="cum">c.</lbl> <foreign xml:lang="gez">እምነ፡</foreign> <cit type="translation" xml:lang="la">
                                <quote> ab </quote>
                            </cit> aliquo: <foreign xml:lang="gez">እምትጽናሕ፡ አንተ፡ እምእዴሆሙ፡ ለደቂቅከ፡</foreign> (v. <foreign xml:lang="gez">ውስተ፡ እዴሆሙ፡</foreign>, <lbl expand="vel">v.</lbl> <foreign xml:lang="gez">እዴሆሙ፡</foreign>) <foreign xml:lang="grc">ἐμβλέπειν</foreign> <ref cRef="Sir." loc="30,30">Sir. 30,30</ref>. <sense n="a" xml:id="D2a"> de rebus, quae aliquem manent, exspectant, ei <cit type="translation" xml:lang="la">
                                    <quote> imminent </quote>
                                </cit>, <cit type="translation" xml:lang="la">
                                    <quote> instant </quote>
                                </cit>, <cit type="translation" xml:lang="la">
                                    <quote> parata sunt </quote>
                                </cit>: <foreign xml:lang="gez">እሉ፡ ክልኤቱ፡ ይጸንሑኪ፡</foreign> <ref cRef="Jes." loc="51,19">Jes. 51,19</ref>; <ref cRef="Job" loc="15,23">Job 15,23</ref>; <ref cRef="Job" loc="21,30">Job 21,30</ref>; <ref cRef="1 Thess." loc="3,3">1 Thess. 3,3</ref>; <ref cRef="Act." loc="20,23">Act. 20,23</ref>; <lbl expand="cum">c.</lbl> <foreign xml:lang="gez">ለ፡</foreign> pers.: <cb n="1285" xml:id="c1285"/> <foreign xml:lang="gez">ይጸንሕ፡ መቅሠፍት፡ ለእኩያን፡</foreign> <foreign xml:lang="grc">ἑτοιμάζονται</foreign> <ref cRef="Prov." loc="19,29">Prov. 19,29</ref>.</sense> <sense n="b" xml:id="D2b"> <cit type="translation" xml:lang="la">
                                    <quote> vacare </quote>
                                </cit> alicui, <cit type="translation" xml:lang="la">
                                    <quote> curandi </quote>
                                </cit> et <cit type="translation" xml:lang="la">
                                    <quote> protegendi </quote>
                                </cit> causa (<note>abwarten</note>): <foreign xml:lang="gez">ትጸንሖ፡ ከመ፡ እሙ፡</foreign> <foreign xml:lang="grc">ὑπαντήσεται αὐτῷ</foreign> <ref cRef="Sir." loc="15,2">Sir. 15,2</ref>; <foreign xml:lang="gez">ጽንሕኒ፡ ውስተ፡ ሠናይ፡ ወሰውርኒ፡ እሞት፡</foreign> <ref cRef="III Sal. Mar." loc="16.">III Sal. Mar. 16.</ref> </sense> <sense n="c" xml:id="D2c"> <cit type="translation" xml:lang="la">
                                    <quote> manere </quote>
                                </cit> i.e. <cit type="translation" xml:lang="la">
                                    <quote> durare </quote>
                                </cit>, <ref cRef="Ps." loc="71,5">Ps. 71,5</ref>; <lbl expand="cum">c.</lbl> <gramGrp> <pos expand="pronomen suffixum">pron. suff.</pos> </gramGrp> : <cit type="translation" xml:lang="la">
                                    <quote> alicui </quote>
                                </cit>, <foreign xml:lang="gez">መንግሥትከ፡ ይጸንሐከ፡</foreign> <foreign xml:lang="grc">μένει σοι</foreign> <ref cRef="Dan." loc="4,23">Dan. 4,23</ref>.</sense> </sense> </sense>
               
            <sense xml:lang="en" source="#traces" xml:id="Tmain"> <foreign xml:lang="gez">ጽናሕ፡</foreign> <cit type="transcription" xml:lang="gez">
                            <quote> ṣǝnāḥ </quote>
                        </cit> part of <gramGrp> <pos expand="nomen proprium">n. pr.</pos> </gramGrp> (personal name) <lbl expand="videas">vid.</lbl> e.g. <foreign xml:lang="gez">አምላክ፡ ጽናሕ፡</foreign> </sense>
                </entry>
