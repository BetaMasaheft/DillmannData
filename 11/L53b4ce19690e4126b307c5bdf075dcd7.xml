               <sense xml:lang="la" source="#dillmann" xml:id="Dmain"> <subc>III,1</subc> (<gramGrp>
                     <pos expand="denominates">denom.</pos>
                  </gramGrp>) <sense n="1" xml:id="D1">
                     <cit type="translation" xml:lang="la">
                        <quote>arte conficere</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>artificiose fabricari</quote>
                     </cit>: <foreign xml:lang="gez">ተኬነዉ፡ ኵልክሙ፡ እለ፡ ትስሕቱ፡</foreign>
                     <foreign xml:lang="grc">τεχνάσαστε</foreign>
                     <ref cRef="Jes. " loc="46,5">Jes. 46,5</ref>; <cb n="0872" xml:id="c0872"/>
                     <ref cRef="Hez." loc="28,12">Hez. 28,12</ref>
                     <lbl expand="varia lectio">var.</lbl>; <lbl expand="cum">c.</lbl>
                     <gramGrp>
                        <case value="accusativus">Acc.</case>
                     </gramGrp> rei, <ref cRef="Sap." loc="14,2">Sap. 14,2</ref>; <foreign xml:lang="gez">ተኬነዎ፡ ሠናየ፡ ወገብሮ፡</foreign>
                     <ref cRef="Sap." loc="13,11">Sap. 13,11</ref>; <foreign xml:lang="gez">በተኬንዎ፡
                        ምግባር፡ </foreign>
                     <ref cRef="Sap." loc="13,10">Sap. 13,10</ref>; Deus <foreign xml:lang="gez">ዘዐቢይ፡ በተኬንዎቱ፡ ወኀያል፡ በምግባሩ፡ </foreign>
                     <ref cRef="Lit." loc="161,1">Lit. 161,1</ref>; <foreign xml:lang="gez">አልባስ፡
                        ብሩህ፡ ዘኢተኬነወ፡ እደ፡ እጓለ፡ እመሕያው፡ </foreign>
                     <ref cRef="Lit." loc="173,3">Lit. 173,3</ref>; <foreign xml:lang="gez">ተኬንዎ፡</foreign>
                     <cit type="translation" xml:lang="la">
                        <quote>opus artificiosum</quote>
                     </cit>, <ref cRef="Org." loc="1.">Org. 1.</ref>
                  </sense>
                  <sense n="2" xml:id="D2">
                     <cit type="translation" xml:lang="la">
                        <quote>artem adhibere</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>machinari</quote>
                     </cit>, <ref cRef="Ex." loc="22,7">Ex. 22,7</ref>; <ref cRef="Ex." loc="22,10">Ex. 22,10</ref>; <foreign xml:lang="gez">ዘይትኬነው፡</foreign> (paulo antea:
                        <foreign xml:lang="gez">ዘይገብር፡ ፈውሰ፡</foreign>) <foreign xml:lang="gez">ለመቲረ፡
                        ዘርእ፡</foreign> (sc. ne femina concipiat vel pariat) F.M. 11,3.</sense>
               </sense>
               
