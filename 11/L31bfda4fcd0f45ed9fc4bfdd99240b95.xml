               <sense xml:id="Dmain" xml:lang="la" source="#dillmann"> <subc>I,1</subc> [<foreign xml:lang="ar">مَلَكَ</foreign> <cit type="translation" xml:lang="la"><quote> possedit, regnavit </quote></cit>; <foreign xml:lang="he">מָלַךְ</foreign> <cit type="translation" xml:lang="la"><quote> regnavit </quote></cit>, <foreign xml:lang="syr">ܡܠܰܟ</foreign> <cit type="translation" xml:lang="la"><quote> consuluit </quote></cit>] <gramGrp> <pos expand="Subjunctivus">Subj.</pos> </gramGrp> <foreign xml:lang="gez">ይምልክ፡</foreign> <sense n="1" xml:id="D1"> <cit type="translation" xml:lang="la"><quote> possidere </quote></cit>, <cit type="translation" xml:lang="la"><quote> dominio tenere </quote></cit>, <cit type="translation" xml:lang="la"><quote> occupare </quote></cit>, <cit type="translation" xml:lang="la"><quote> potiri </quote></cit>, <lbl expand="cum">c.</lbl> <gramGrp> <case value="accusativus">Acc.</case> </gramGrp>: <foreign xml:lang="grc">κατέχειν</foreign> <ref cRef="Ex." loc="32,13">Ex. 32,13</ref>; <ref cRef="Gen." loc="48,4">Gen. 48,4</ref>; <foreign xml:lang="gez">ዘይመልክ፡ ሀገረ፡</foreign> <foreign xml:lang="grc">καταλαμβανόμενος πόλιν</foreign> <ref cRef="Prov." loc="16,32">Prov. 16,32</ref>; <foreign xml:lang="gez">ምልካ፡ ወተዋረሳ፡</foreign> <foreign xml:lang="grc">ἐνάρχου κληρονομεῖν</foreign> <ref cRef="Deut." loc="2,24">Deut. 2,24</ref>; <foreign xml:lang="gez">ዮሐናን፡ ወእሊአሁ፡ ተጸወኑ፡ ኀበ፡ ቍድስ፡ ወመለክዋ፡</foreign> <cit type="translation" xml:lang="la"><quote> operibus praemunierunt templum et occuparunt </quote></cit> <ref cRef="Jsp." loc="353">Jsp. p. 353</ref>; <foreign xml:lang="gez">ይሁቡ፡ ኵሎ፡ ዘይመልኩ፡ ለነዳያን፡</foreign> (<cit type="translation" xml:lang="la"><quote> omnia, quae possident </quote></cit>) <ref cRef="Chrys. Ta." loc="13.">Chrys. Ta. 13.</ref> <sense n="a" xml:id="D1a"> ex usu fori <foreign xml:lang="gez">መለከ፡</foreign> non tantum <cit type="translation" xml:lang="la"><quote> possidere </quote></cit>, <cit type="translation" xml:lang="la"><quote> jure mancipii tenere </quote></cit> significat, sed etiam <cit type="translation" xml:lang="la"><quote> usu capere </quote></cit>, ut <ref cRef="F.N." loc=" 37">F.N.  37</ref>. 
</sense> <sense n="b" xml:id="D1b"> transl.: animis potiri i. e. <cit type="translation" xml:lang="la"><quote> animos capere </quote></cit> vel <cit type="translation" xml:lang="la"><quote> sibi conciliare </quote></cit> (<cit type="translation" xml:lang="de"><quote> gewinnen </quote></cit>), de oratore: <foreign xml:lang="gez">እምድኅረ፡ መለከ፡ አልባቢሆሙ፡ ለሰማውያን፡ በብዙኅ፡ ተግሣጽ፡</foreign> <ref cRef="Chrys. ho. " loc="3.">Chrys. ho.  3.</ref> 
</sense> 
</sense> <sense n="2" xml:id="D2"> <cit type="translation" xml:lang="la"><quote> dominari </quote></cit>, <cit type="translation" xml:lang="la"><quote> sub imperio tenere </quote></cit>, <cit type="translation" xml:lang="la"><quote> regnare </quote></cit>, <lbl expand="construitur cum">c.c.</lbl> <gramGrp> <case value="accusativus">Acc.</case> </gramGrp>: <foreign xml:lang="grc">κρατεῖν</foreign>: <foreign xml:lang="gez">ኢያአምር፡ ከመ፡ መሓሪ፡ ይመልኮ፡</foreign> <ref cRef="Prov." loc="28,22">Prov. 28,22</ref>; <foreign xml:lang="gez">እለ፡ ይመልክዋ፡ ለየብስ፡</foreign> <ref cRef="Hen." loc="62,9">Hen. 62,9</ref> (aliis in locis <foreign xml:lang="gez">ይእኅዝዋ፡</foreign>); saepissime de Deo: <foreign xml:lang="gez">ዘኵሎ፡ ይመልክ፡</foreign> <foreign xml:lang="grc">ὁ παντοκράτωρ</foreign>, ut <ref cRef="Job" loc="15,25">Job 15,25</ref>; <ref cRef="Job" loc="22,17">Job 22,17</ref>; <ref cRef="Job" loc="27,2">Job 27,2</ref>; <ref cRef="Sir." loc="42,17">Sir. 42,17</ref>; <ref cRef="Jer." loc="3,9">Jer. 3,9</ref>; <ref cRef="Jer." loc="5,14">Jer. 5,14</ref>; <ref cRef="Hagg." loc="1,5">Hagg. 1,5</ref>; <ref cRef="Mal." loc="1,4">Mal. 1,4</ref>; <ref cRef="Mal." loc="1,8">Mal. 1,8</ref>; <ref cRef="Zach." loc="1,3">Zach. 1,3</ref>; 2 <ref cRef="Reg." loc="5,10">Reg. 5,10</ref>; <ref cRef="Judith" loc="4,13">Judith 4,13</ref>; <ref cRef="Judith" loc="15,10">Judith 15,10</ref>; <ref cRef="Apoc." loc="15,3">Apoc. 15,3</ref>; <ref cRef="2 Cor." loc="6,18">2 Cor. 6,18</ref> (alias: <foreign xml:lang="gez">አኃዜ፡ ኵሉ፡</foreign>); <foreign xml:lang="grc">ἄρχειν</foreign> <ref cRef="Gen." loc="1,16">Gen. 1,16</ref>; <foreign xml:lang="gez">ይመልኮ፡ እበድ፡</foreign> <foreign xml:lang="grc">ἄγεται ἀφροσύνη</foreign> <ref cRef="Prov." loc="18,2">Prov. 18,2</ref>; <foreign xml:lang="gez">ከመ፡ ይምልክ፡ ቤተ፡ ክርስቲያን፡</foreign> Syn.; <lbl expand="cum">c.</lbl> <foreign xml:lang="gez">ለ፡</foreign>, ut: <foreign xml:lang="gez">ኢይመልክ፡</foreign> (v. <foreign xml:lang="gez">ኢይትመልአክ፡</foreign>) <foreign xml:lang="gez">አንሰ፡ ለክሙ፡</foreign> <ref cRef="Jud." loc="8,23">Jud. 8,23</ref>; <lbl expand="cum">c.</lbl> <foreign xml:lang="gez">ላዕለ፡</foreign>, ut: <foreign xml:lang="gez">መለከ፡ ላዕሌነ፡ ሀኬት፡</foreign> <ref cRef="Chrys. ho." loc="24">Chrys. ho. 24</ref>; vel <gramGrp> <pos expand="absolute">absol.</pos> </gramGrp>, ut: <foreign xml:lang="gez">መለከ፡ እምታሕቴሁ፡ ለንጉሥ፡</foreign> <cit type="translation" xml:lang="la"><quote> provex fuit </quote></cit> <ref cRef="Lud." loc="">Lud. </ref> (sine <lbl expand="auctore, auctoritate">auct.</lbl>); <ref cRef="Hen." loc="75,3">Hen. 75,3</ref>.
</sense> </sense>
               
            <sense xml:id="Tmain" xml:lang="en" source="#traces"> <cit type="transcription" xml:lang="gez"><quote>malaka</quote></cit> <sense n="G"> <foreign xml:lang="gez">መለከ፡</foreign> <cit type="transcription" xml:lang="gez"><quote>malaka</quote></cit> <subc>I,1</subc> <foreign xml:lang="gez">ይመልክ፡</foreign>, <foreign xml:lang="gez">ይምልክ፡</foreign> «<cit type="translation" xml:lang="fr"><quote>dominer</quote></cit>, <cit type="translation" xml:lang="fr"><quote>mâitriser</quote></cit>» ― <foreign xml:lang="am">ገዛ፡</foreign> Ms. <ref type="BM" target="BNFabb217">BNFabb217</ref>, fol. 33v. <lb/>
 <bibl><ptr target="bm:Grebaut1952SuppLexicon"/><citedRange unit="page"> 76</citedRange></bibl> 
</sense> <sense n="L"> <foreign xml:lang="gez">መለከ፡</foreign> <cit type="transcription" xml:lang="gez"><quote>malaka</quote></cit> <cit type="translation" xml:lang="en"><quote>own</quote></cit> (property), <cit type="translation" xml:lang="en"><quote>possess</quote></cit>, <cit type="translation" xml:lang="en"><quote>have dominion</quote></cit>, <cit type="translation" xml:lang="en"><quote>dominate</quote></cit>, <cit type="translation" xml:lang="en"><quote>become master</quote></cit>, <cit type="translation" xml:lang="en"><quote>take possession of</quote></cit>, <cit type="translation" xml:lang="en"><quote>occupy</quote></cit>, <cit type="translation" xml:lang="en"><quote>rule</quote></cit>, <cit type="translation" xml:lang="en"><quote>reign</quote></cit>, <cit type="translation" xml:lang="en"><quote>have authority</quote></cit> <bibl><ptr target="bm:Leslau1987dic"/><citedRange unit="page"> 343b</citedRange></bibl> 
</sense> </sense></entry>
