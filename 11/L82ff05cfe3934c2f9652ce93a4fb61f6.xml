               <sense xml:lang="la" source="#dillmann" xml:id="Dmain"> <subc>I,1</subc> et <subc>I,2</subc>, raro <foreign xml:lang="gez">ወጥነ፡</foreign> <subc>I,1</subc> (<ref cRef="Sx. Mag. " loc="13,">Sx. Mag.  13,</ref>si quidem lectio sana est) [<foreign xml:lang="ar">بَدَأَ بَدَا</foreign>], <gramGrp> <pos expand="Subjunctivus">Subj.</pos> </gramGrp> <subc>I,1</subc> <foreign xml:lang="gez">ይውጥን፡</foreign> (ut F.M. 2,11; <ref cRef="Chrys. ho." loc="4">Chrys. ho. 4</ref>; <ref cRef="Jsp." loc="363,">Jsp. p. 363,</ref> al.), raro <foreign xml:lang="gez">ይጠን፡</foreign> (M.M. f. 211), <cit type="translation" xml:lang="la">
