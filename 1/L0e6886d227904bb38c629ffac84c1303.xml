               <sense xml:lang="la" source="#dillmann" xml:id="Dmain"> <subc>I,1</subc> [<foreign xml:lang="he">נָזָה</foreign> (<foreign xml:lang="ar">نَزَا</foreign>) <cit type="translation" xml:lang="la">
                            <quote> saliit </quote>
                        </cit>, Hiph. <cit type="translation" xml:lang="la">
                            <quote> sparsit </quote>
                        </cit>; <foreign xml:lang="ar">نَضَح</foreign> et <foreign xml:lang="ar">نَضَخَ</foreign> <cit type="translation" xml:lang="la">
                            <quote> conspersit </quote>
                        </cit>; <lbl expand="videas">vid.</lbl> <foreign xml:lang="gez">ነስነሰ፡</foreign>] <cit type="translation" xml:lang="la">
                            <quote> spargere </quote>
                        </cit>, <cit type="translation" xml:lang="la">
                            <quote> respargere </quote>
                        </cit>, <cit type="translation" xml:lang="la">
                            <quote> conspargere </quote>
                        </cit>, <cit type="translation" xml:lang="la">
                            <quote> aspergere </quote>
                        </cit>, potissimum de ritu aspersionis sacro (<cit type="translation" xml:lang="la">
                            <quote> libare </quote>
                        </cit>): <lbl expand="construitur cum">c.c.</lbl> <gramGrp> <case value="accusativus">Acc.</case> </gramGrp> liquidi quod spargitur, (vel <lbl expand="cum">c.</lbl> <foreign xml:lang="gez">እምነ፡</foreign>, <foreign xml:lang="gez">እምውስተ፡</foreign> partitivo): <foreign xml:lang="gez">ነዝኀ፡ ወይነ፡</foreign>, <foreign xml:lang="gez">ነዝኀ፡ ደመ፡</foreign> <ref cRef="Kuf." loc="6">Kuf. 6</ref>; <ref cRef="2 Par." loc="29,22">2 Par. 29,22</ref>; <foreign xml:lang="gez">ወነዝኆ፡</foreign> (<foreign xml:lang="gez">ለማይ፡</foreign>) <foreign xml:lang="gez">ለእግዚአብሔር፡</foreign> <ref cRef="1 Par. " loc="11,18">1 Par.  11,18</ref> (<foreign xml:lang="grc">ἔσπεισε</foreign>); et <lbl expand="cum">c.</lbl> <foreign xml:lang="gez">ውስተ፡</foreign> vel <foreign xml:lang="gez">ላዕለ፡</foreign> vel <foreign xml:lang="gez">ቅድመ፡</foreign> rei vel loci, quae asperguntur: <foreign xml:lang="gez">ነዝኀ፡ ወይነ፡ ውስተ፡ እሳት፡</foreign> <ref cRef="Kuf." loc="7">Kuf. 7</ref>; <ref cRef="Lev." loc="4,6">Lev. 4,6</ref>; <ref cRef="Lev." loc="4,17">Lev. 4,17</ref>; <ref cRef="Lev." loc="5,9">Lev. 5,9</ref>; <ref cRef="Lev." loc="8,11">Lev. 8,11</ref>; <ref cRef="Lev." loc="8,30">Lev. 8,30</ref>; <ref cRef="Lev." loc="16,14">Lev. 16,14</ref> <lbl expand="sequens">seq.</lbl>; <ref cRef="Num." loc="19,18">Num. 19,18</ref>; <lbl expand="cum">c.</lbl> <gramGrp> <case value="accusativus">Acc.</case> </gramGrp> rei vel <lbl expand="persona">pers.</lbl>, quae aspergitur: <ref cRef="Ex." loc="24,8">Ex. 24,8</ref>; <ref cRef="Lev." loc="14,7">Lev. 14,7</ref>; <ref cRef="Num." loc="19,19">Num. 19,19</ref>; <foreign xml:lang="gez">እነዝኀክሙ፡ በማይ፡ ንጹሕ፡</foreign> <ref cRef="Hez." loc="36,25">Hez. 36,25</ref>; <ref cRef="2 Par." loc="29,24">2 Par. 29,24</ref>; <ref cRef="Ps. " loc="50,8">Ps.  50,8</ref>; <ref cRef="Hebr." loc="9,19">Hebr. 9,19</ref>; <lbl expand="cum">c.</lbl> <lbl expand="duplex">dupl.</lbl> <gramGrp> <case value="accusativus">Acc.</case> </gramGrp> <ref cRef="Num." loc="8,7">Num. 8,7</ref>. </sense>
               
