            <entry xml:id="L1d97704346f74d8cbf1838c7868ac6a9" n="6315">
               <form>
                  <foreign xml:lang="gez">ምክዕቢት</foreign>
               </form>
               <sense xml:lang="la" source="#dillmann" xml:id="Dmain">, rarius <foreign xml:lang="gez">መክዕቢት፡</foreign> vel <foreign xml:lang="gez">መክዕብት፡</foreign> (<ref cRef="Jer." loc="17,18">Jer. 17,18</ref> <lbl expand="varia lectio">var.</lbl>; <ref cRef="Sap." loc="11,13">Sap. 11,13</ref> <lbl expand="varia lectio">var.</lbl>), <foreign xml:lang="gez">ምክዒቢት፡</foreign> (<ref cRef="Sir." loc="2,12">Sir. 2,12</ref>, <lbl expand="varia lectio">var.</lbl>), <foreign xml:lang="gez">መክዐቢት፡</foreign> (Did. <lbl expand="edidit">ed.</lbl> <ref cRef="Platt p." loc="117">Platt p. 117</ref>), <gramGrp> <pos expand="not found">subst.</pos> </gramGrp>, <lbl expand="pluralis">Pl.</lbl> <foreign xml:lang="gez">ምክዕቢታት፡</foreign> (<ref cRef="Hen." loc="93,10">Hen. 93,10</ref>), <sense n="1" xml:id="D1"> <cit type="translation" xml:lang="la">
                                <quote> duplum </quote>
                            </cit>: <foreign xml:lang="gez">በምክዕቢተ፡ ቅጥቃጤ፡ ቀጥቅጦሙ፡</foreign> <ref cRef="Jer." loc="17,18">Jer. 17,18</ref>; 1 <ref cRef="Tim." loc="5,17">Tim. 5,17</ref>; Did. <lbl expand="edidit">ed.</lbl> <ref cRef="Platt p." loc="117">Platt p. 117</ref>; <ref cRef="Deut." loc="21,17">Deut. 21,17</ref>. <sense n="a" xml:id="D1a"> pro adjectivo: <cit type="translation" xml:lang="la">
                                    <quote> duplus </quote>
                                </cit>, <foreign xml:lang="gez">ምክዕቢት፡ ውእቱ፡ ኀይሎን፡</foreign> <ref cRef="Herm. " loc="32">Herm.  p. 32</ref>; <ref cRef="Herm." loc="35">Herm. p. 35</ref>; <ref cRef="Herm." loc="94.">Herm. p. 94.</ref> </sense> <sense n="b" xml:id="D1b"> <gramGrp> <pos expand="adverbium, adverbialiter">adv.</pos> </gramGrp> <foreign xml:lang="gez">ምክዕቢተ፡</foreign> <sense n="α" xml:id="D1bα"> <cit type="translation" xml:lang="la">
                                        <quote> dupliciter </quote>
                                    </cit>, <cit type="translation" xml:lang="la">
                                        <quote> bifariam </quote>
                                    </cit>: <ref cRef="Sir." loc="22,5">Sir. 22,5</ref>; <foreign xml:lang="gez">ኵሉ፡ መክዕቢተ፡ ፩፩ ቅድመ፡ ካልኡ፡</foreign> <ref cRef="Sir." loc="42,24">Sir. 42,24</ref>; <ref cRef="Sap." loc="11,13">Sap. 11,13</ref>; <ref cRef="Clem." loc="203">Clem. f. 203</ref>; <foreign xml:lang="gez">ትሠርዕዎ፡ ምክዕቢተ፡</foreign> <ref cRef="Lev." loc="24,6">Lev. 24,6</ref>; <ref cRef="4 Reg." loc="10,8">4 Reg. 10,8</ref>; <foreign xml:lang="gez">ኃጥእ፡ ዘክልኤቱ፡ ግዕዙ፡ ወምክዕቢተ፡ የሐውር፡</foreign> <ref cRef="Sir. " loc="2,12">Sir.  2,12</ref>; <ref cRef="2 Reg." loc="10,9">2 Reg. 10,9</ref>.</sense> <sense n="β" xml:id="D1bβ"> <cit type="translation" xml:lang="la">
                                        <quote> secundo </quote>
                                    </cit>, <cit type="translation" xml:lang="la">
                                        <quote> iterum </quote>
                                    </cit>, <ref cRef="Jes." loc="61,7">Jes. 61,7</ref> <lbl expand="varia lectio">var.</lbl> </sense> <sense n="γ" xml:id="D1bγ"> <cit type="translation" xml:lang="la">
                                        <quote> bis </quote>
                                    </cit>, <ref cRef="Hez." loc="45,13">Hez. 45,13</ref>; <ref cRef="Apoc." loc="9,16">Apoc. 9,16</ref>.</sense> </sense> </sense> <sense n="2" xml:id="D2"> Aliis nominibus numeralibus additum, significat <cit type="translation" xml:lang="la">
                                <quote> -plum </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> -plex </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> -pliciter </quote>
                            </cit>: <foreign xml:lang="gez">መኮኖ፡ ምእተ፡ ምክዕቢተ፡</foreign> <cit type="translation" xml:lang="la">
                                <quote> et factum est ei centuplum </quote>
                            </cit> <ref cRef="Gen." loc="26,12">Gen. 26,12</ref>; <foreign xml:lang="gez">፯ ምክዕቢታተ፡ ትምህርት፡</foreign> <cit type="translation" xml:lang="la">
                                <quote> septplus </quote>
                            </cit> <ref cRef="Hen." loc="93,10">Hen. 93,10</ref>; <ref cRef="Matth." loc="19,29">Matth. 19,29</ref>; <ref cRef="Marc." loc="10,30">Marc. 10,30</ref>; <ref cRef="Luc." loc="8,8">Luc. 8,8</ref>; <ref cRef="2 Reg." loc="24,3">2 Reg. 24,3</ref>; <ref cRef="Hen." loc="91,16">Hen. 91,16</ref>; <foreign xml:lang="gez">ካዕበተ፡ ምክዕቢተ፡</foreign> <ref cRef="Apoc." loc="18,6">Apoc. 18,6</ref>; <foreign xml:lang="gez">ብዙኀ፡ ምክዕቢተ፡</foreign> <cit type="translation" xml:lang="la">
                                <quote> multipliciter </quote>
                            </cit> <ref cRef="Luc. " loc="18,30">Luc.  18,30</ref>.</sense> </sense>
               
            </entry>
