               <sense xml:id="Dmain" xml:lang="la" source="#dillmann"> <gramGrp> <pos expand="adjectivum">adj.</pos> <pos expand="numerale">num.</pos> </gramGrp> (ordinale), <gramGrp> <gen value="femininus">fem.</gen> </gramGrp> <foreign xml:lang="gez">ሣልስት፡</foreign> <cit type="translation" xml:lang="la"><t:quote xmlns:t="http://www.tei-c.org/ns/1.0"> tertius </t:quote></cit> <ref cRef="Gen." loc="1,13">Gen. 1,13</ref>; <ref cRef="Ex." loc="19,1">Ex. 19,1</ref>; <ref cRef="Ex." loc="19,11">Ex. 19,11</ref>; <ref cRef="Ex." loc="19,16">Ex. 19,16</ref>; <ref cRef="Luc." loc="20,12">Luc. 20,12</ref>; <ref cRef="Apoc." loc="6,5">Apoc. 6,5</ref>; <ref cRef="Asc. Jes." loc="7,24">Asc. Jes. 7,24</ref>; <sense n="a" xml:id="Da"> <lbl expand="cum">c.</lbl> <gramGrp> <pos expand="pronomen suffixum">pron. suff.</pos> </gramGrp> <foreign xml:lang="gez">ሣልሱ፡</foreign> <cit type="translation" xml:lang="la"><t:quote xmlns:t="http://www.tei-c.org/ns/1.0"> tertius </t:quote></cit> <ref cRef="1 Reg." loc="17,13">1 Reg. 17,13</ref>; <ref cRef="Marc." loc="12,21">Marc. 12,21</ref>; <foreign xml:lang="gez">ሣልስታ፡</foreign> <cit type="translation" xml:lang="la"><t:quote xmlns:t="http://www.tei-c.org/ns/1.0"> tertia </t:quote></cit> (earum) <ref cRef="Job" loc="42,14">Job 42,14</ref>; <foreign xml:lang="gez">ሣልሶሙ፡</foreign> <cit type="translation" xml:lang="la"><t:quote xmlns:t="http://www.tei-c.org/ns/1.0"> tertium </t:quote></cit> (eorum) <ref cRef="Sir." loc="23,16">Sir. 23,16</ref>; <ref cRef="Sir." loc="50,25">Sir. 50,25</ref>; <foreign xml:lang="gez">ይከውኖሙ፡ እስራኤል፡ ሣልሶሙ፡ ለግብጽ፡ ወለፋርስ፡</foreign> <ref cRef="Jes." loc="19,24">Jes. 19,24</ref>; <foreign xml:lang="gez">ናሁ፡ ሣልሶሙ፡ ዮም፡ እንዘ፡ ይጸንሑኒ፡</foreign> <ref cRef="Marc." loc="8,2">Marc. 8,2</ref>. Etiam ubi plures actiones ejusdem personae enumerantur, <foreign xml:lang="gez">ሣልስ፡</foreign> <lbl expand="cum">c.</lbl> <gramGrp> <pos expand="pronomen suffixum">pron. suff.</pos> </gramGrp> ad personam relato usurpatur, ut: <foreign xml:lang="gez">ሣልስታ፡ ሰረቀት፡ በዝሙታ፡</foreign> <foreign xml:lang="grc">τὸ τρίτον</foreign> <cit type="translation" xml:lang="la"><t:quote xmlns:t="http://www.tei-c.org/ns/1.0"> tertio </t:quote></cit>, <ref cRef="Sir. " loc="23,23">Sir.  23,23</ref>. 
</sense> <sense n="b" xml:id="Db"> nota locutionem: <foreign xml:lang="gez">ሣልስከ፡ ዝንቱ፡</foreign> tertium tuum hoc est i.e. <cit type="translation" xml:lang="la"><t:quote xmlns:t="http://www.tei-c.org/ns/1.0"> jam tertiâ vice </t:quote></cit> (facis id) <ref cRef="Num." loc="22,28">Num. 22,28</ref>; <ref cRef="Num." loc="22,32">Num. 22,32</ref>; <ref cRef="Joh." loc="21,14">Joh. 21,14</ref>, et sine <gramGrp> <pos expand="pronomen suffixum">pron. suff.</pos> </gramGrp> <foreign xml:lang="gez">ሣልስ፡ ዝንቱ፡</foreign> <ref cRef="Jud." loc="16,15">Jud. 16,15</ref>. 
</sense> <sense n="c" xml:id="Dc"> <foreign xml:lang="gez">በሣልስ፡</foreign> <cit type="translation" xml:lang="la"><t:quote xmlns:t="http://www.tei-c.org/ns/1.0"> tertium </t:quote></cit>, <cit type="translation" xml:lang="la"><t:quote xmlns:t="http://www.tei-c.org/ns/1.0"> tertia vice </t:quote></cit> <ref cRef="1 Reg. " loc="3,8">1 Reg.  3,8</ref>; <ref cRef="Matth." loc="26,44">Matth. 26,44</ref>; <ref cRef="Marc." loc="14,41">Marc. 14,41</ref>; <ref cRef="Luc." loc="23,22">Luc. 23,22</ref> (sed. <ref cRef="Num." loc="2,24">Num. 2,24</ref> <cit type="translation" xml:lang="la"><t:quote xmlns:t="http://www.tei-c.org/ns/1.0"> tertio loco </t:quote></cit>). 
</sense> <sense n="d" xml:id="Dd"> <gramGrp> <gen value="femininus">fem.</gen> </gramGrp> <foreign xml:lang="gez">ሣልስት፡</foreign> nonnumquam est <sense n="α" xml:id="Ddα"> <cit type="translation" xml:lang="la"><t:quote xmlns:t="http://www.tei-c.org/ns/1.0"> tertia pars </t:quote></cit> <foreign xml:lang="grc">τὸ τρίτον</foreign> <ref cRef="2 Par." loc="23,4">2 Par. 23,4</ref>; <ref cRef="2 Par." loc="23,5">2 Par. 23,5</ref> (v. <foreign xml:lang="gez">ሣልስተ፡ እድ፡</foreign>) 
</sense> <sense n="β" xml:id="Ddβ"> <cit type="translation" xml:lang="la"><t:quote xmlns:t="http://www.tei-c.org/ns/1.0"> tertia dies </t:quote></cit> (subintellecto <foreign xml:lang="gez">ዕለት፡</foreign>) Asc. <ref cRef="Jes." loc="9,16">Jes. 9,16</ref>; <foreign xml:lang="gez">በሣልስታ፡</foreign> <ref cRef="Luc." loc="2,46">Luc. 2,46</ref>; <foreign xml:lang="gez">ሣልስተ፡</foreign> <cit type="translation" xml:lang="la"><t:quote xmlns:t="http://www.tei-c.org/ns/1.0"> tertia </t:quote></cit> i.e. <cit type="translation" xml:lang="la"><t:quote xmlns:t="http://www.tei-c.org/ns/1.0"> perendie </t:quote></cit> <ref cRef="Luc. " loc="13,32">Luc.  13,32</ref>; et <cit type="translation" xml:lang="la"><t:quote xmlns:t="http://www.tei-c.org/ns/1.0"> nudius tertius </t:quote></cit>: <foreign xml:lang="gez">ትማልመኒ፡ ወሣልስተኒ፡</foreign> <ref cRef="1 Par." loc="11,2">1 Par. 11,2</ref>; <ref cRef="Ex." loc="21,29">Ex. 21,29</ref>. 
</sense> <sense n="γ" xml:id="Ddγ"> <cit type="translation" xml:lang="la"><t:quote xmlns:t="http://www.tei-c.org/ns/1.0"> tertia vicis </t:quote></cit>: <foreign xml:lang="gez">ሣልስተ፡</foreign> <cit type="translation" xml:lang="la"><t:quote xmlns:t="http://www.tei-c.org/ns/1.0"> tertium </t:quote></cit> <ref cRef="Num. " loc="10,6">Num.  10,6</ref>. 
</sense> 
</sense> </sense>
               
