                        <quote>molestia confectus</quote>
                     </cit> (<cit type="translation" xml:lang="la">
                        <quote>pressus</quote>
                     </cit>), <cit type="translation" xml:lang="la">
                        <quote>fatigatus</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>vexatus</quote>
