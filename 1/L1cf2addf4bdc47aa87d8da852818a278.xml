         <div>
            <entry xml:id="L1cf2addf4bdc47aa87d8da852818a278" n="6445">
               <form>
                  <foreign xml:lang="gez">ሙላድ</foreign>
               </form>
               <sense xml:lang="la" source="#dillmann" xml:id="Dmain">
                  <gramGrp>
                     <pos expand="Substantivum">subst.</pos>
                  </gramGrp>, <lbl expand="pluralis">Pl.</lbl>
                  <foreign xml:lang="gez">ሙላዳት፡</foreign>
                  <sense n="1" xml:id="D1">
                     <cit type="translation" xml:lang="la">
                        <quote>locus natalis</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>patria</quote>
                     </cit>: <foreign xml:lang="gez">ብሔረ፡ ሙላድኪ፡ </foreign>
                     <ref cRef="Ruth" loc="2,11">Ruth 2,11</ref>; <foreign xml:lang="gez">ምድረ፡
                        ሙላዱ፡</foreign>
                     <ref cRef="Jer." loc="22,10">Jer. 22,10</ref> vers. alt.; <cit type="translation" xml:lang="la">
                        <quote>locus</quote>
                     </cit> vel <cit type="translation" xml:lang="la">
                        <quote>tempus ortus</quote>
                     </cit> sideris, <foreign xml:lang="gez">ሙላዳት፡ </foreign>
                     <ref cRef="Hen." loc="72,1">Hen. 72,1</ref>.</sense>
                  <sense n="2" xml:id="D2">
                     <cit type="translation" xml:lang="la">
                        <quote>generatio</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>genitura</quote>
                     </cit>: <foreign xml:lang="gez">ማሕፀነ፡ ሙላድ፡</foreign> M.M. f. 28; <foreign xml:lang="gez">ዘርአ፡ ሙላድ፡</foreign>
                     <cit type="translation" xml:lang="la">
                        <quote>semen genitale</quote>
                     </cit> M.M. f. 36.</sense>
                  <sense n="3" xml:id="D3">
                     <cit type="translation" xml:lang="la">
                        <quote>progenies</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>gens</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>cognati</quote>
                     </cit>, <foreign xml:lang="grc">γένεσις </foreign>
                     <ref cRef="Gen." loc="5,1">Gen. 5,1</ref>; <ref cRef="Lev." loc="20,18">Lev.
                        20,18</ref>; <ref cRef="Num." loc="9,10">Num. 9,10</ref>; <ref cRef="Num." loc="10,30">Num. 10,30</ref>; <ref cRef="Num." loc="13,28">Num.
                     13,28</ref>.</sense>
               </sense>
               
