         <div>
            <entry xml:id="L03c623aa3c2a43b791de530e9011156c" n="6245">
               <form>
                  <foreign xml:lang="gez">አኰነነ</foreign>
               </form>
               <sense xml:lang="la" source="#dillmann" xml:id="Dmain"> <subc>II,2</subc> <sense n="1" xml:id="D1">
                     <sense n="a" xml:id="D1a">
                        <cit type="translation" xml:lang="la">
                           <quote>judicem constituere</quote>
                        </cit> aliquem: <foreign xml:lang="gez">ኢያኴንንዎሙ፡ </foreign>
                        <ref cRef="Sir." loc="38,33">Sir. 38,33</ref>.</sense>
                     <sense n="b" xml:id="D1b">
                        <cit type="translation" xml:lang="la">
                           <quote>condemnationem adsciscere</quote>
                        </cit> alicui, <ref cRef="2 Cor." loc="3,9">2 Cor. 3,9</ref> Platt; <lbl expand="cum">c.</lbl>
                        <gramGrp>
                           <case value="accusativus">Acc.</case>
                        </gramGrp>
                        <ref cRef="Herm. " loc="108.">Herm. p. 108.</ref>
                     </sense>
                     <sense n="2" xml:id="D12">
                        <cit type="translation" xml:lang="la">
                           <quote>principatum deferre</quote>
                        </cit>, <cit type="translation" xml:lang="la">
                           <quote>potestatem dare</quote>
                        </cit> alicui, <cit type="translation" xml:lang="la">
                           <quote>dominum</quote>
                        </cit>, <cit type="translation" xml:lang="la">
                           <quote>principem constituere</quote>
                        </cit>, <cit type="translation" xml:lang="la">
                           <quote>praeficere</quote>
                        </cit> aliquem: <foreign xml:lang="gez">ምኵናንየ፡ ኀበ፡ አኰነነኒ፡</foreign>
                        <ref cRef="Sir." loc="24,11">Sir. 24,11</ref>; <lbl expand="cum">c.</lbl>
                        <lbl expand="duplex">dupl.</lbl>
                        <gramGrp>
                           <case value="accusativus">Acc.</case>
                        </gramGrp>: <foreign xml:lang="gez">አኮ፡ ለከንቱ፡ አኰነንዎ፡ መጥባሕተ፡</foreign>
                        <ref cRef="Rom." loc="13,4">Rom. 13,4</ref>; <foreign xml:lang="gez">ኣኴንኖ፡
                           ምኵናኒከ፡</foreign>
                        <ref cRef="Jes." loc="22,21">Jes. 22,21</ref>; <foreign xml:lang="gez">ለወልድከ፡ ወለብእሲትከ፡ ኢታኰንኖሙ፡ ንዋየከ፡</foreign>
                        <ref cRef="Sir." loc="30,28">Sir. 30,28</ref>; <ref cRef="Sir. " loc="17,2">Sir. 17,2</ref>; <foreign xml:lang="gez">አኰነኖሙ፡ ለኵሎሙ፡ አይሁድ፡</foreign>
                        <ref cRef="Dan." loc="3,30">Dan. 3,30</ref>; <ref cRef="Ps." loc="135,8">Ps.
                           135,8</ref>; <ref cRef="Ps." loc="135,9">Ps. 135,9</ref>; <lbl expand="cum">c.</lbl>
                        <gramGrp>
                           <case value="accusativus">Acc.</case>
                        </gramGrp> et <foreign xml:lang="gez">ላዕለ፡</foreign>
                        <ref cRef="Ps." loc="104,19">Ps. 104,19</ref>; <ref cRef="Joh. " loc="17,2">Joh. 17,2</ref>; vel <lbl expand="cum">c.</lbl>
                        <foreign xml:lang="gez">ውስተ፡</foreign>
                        <ref cRef="Kuf." loc="40">Kuf. 40</ref>; <lbl expand="sequens">seq.</lbl>
                        <foreign xml:lang="gez">ለ፡</foreign>
                        <lbl expand="cum">c.</lbl> Infin.: <ref cRef="2 Cor." loc="13,11">2 Cor.
                           13,11</ref>.</sense>
                  </sense>
               </sense>
               
