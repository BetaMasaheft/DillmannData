            <entry xml:id="L0ab1774ce4a9482dbe2922ce14c01e60" n="5696">
               <form>
                  <foreign xml:lang="gez">ተኣኀዘ</foreign>
               </form>
               <sense xml:id="Dmain" xml:lang="la" source="#dillmann"> <subc>III,3</subc> sese invicem prehendere i.e. <sense n="1" xml:id="D1"> <sense n="a" xml:id="D1a"> <cit type="translation" xml:lang="la"><quote> confinem esse </quote></cit>, <cit type="translation" xml:lang="la"><quote> contingere </quote></cit>, <cit type="translation" xml:lang="la"><quote> attingere </quote></cit>, <foreign xml:lang="grc">συνάπτειν</foreign>, <lbl expand="cum">c.</lbl> <foreign xml:lang="gez">ምስለ፡</foreign> <ref cRef="Jos." loc="19,11">Jos. 19,11</ref>; <lbl expand="cum">c.</lbl> <foreign xml:lang="gez">ላዕለ፡</foreign> <ref cRef="Jos." loc="19,22">Jos. 19,22</ref>; <lbl expand="cum">c.</lbl> <foreign xml:lang="gez">በ፡</foreign> <ref cRef="Jos." loc="19,26">Jos. 19,26</ref>. 
</sense> <sense n="b" xml:id="D1b"> <cit type="translation" xml:lang="la"><quote> coagmentatum </quote></cit>, <cit type="translation" xml:lang="la"><quote> commissum esse </quote></cit> <ref cRef="3 Reg." loc="7,15">3 Reg. 7,15</ref>. 
</sense> 
</sense> <sense n="2" xml:id="D2"> <sense n="a" xml:id="D2a"> <cit type="translation" xml:lang="la"><quote> pugna congredi </quote></cit>, <cit type="translation" xml:lang="la"><quote> adoriri </quote></cit>, <cit type="translation" xml:lang="la"><quote> aggredi </quote></cit> (angreifen), <cit type="translation" xml:lang="la"><quote> aciem instruere </quote></cit>, <foreign xml:lang="grc">συνάπτειν</foreign>, <foreign xml:lang="grc">παρατάσσεσθαι</foreign>, <foreign xml:lang="gez">ተኣኀዙ፡ ይትቃተሉ፡</foreign> <ref cRef="2 Reg." loc="10,8">2 Reg. 10,8</ref>; <foreign xml:lang="gez">ምስለ፡</foreign> <ref cRef="Gen." loc="14,8">Gen. 14,8</ref>; <lbl expand="cum">c.</lbl> <gramGrp> <case value="accusativus">Acc.</case> </gramGrp> <ref cRef="Jos." loc="11,5">Jos. 11,5</ref>; <ref cRef="Jud." loc="20,20">Jud. 20,20</ref>; <ref cRef="Ex." loc="17,9">Ex. 17,9</ref>; <ref cRef="1 Reg." loc="4,2">1 Reg. 4,2</ref>; <ref cRef="1 Reg." loc="7,10">1 Reg. 7,10</ref>; <ref cRef="Ps." loc="151,6">Ps. 151,6</ref>; <cit type="translation" xml:lang="la"><quote> pugnam </quote></cit> vel <cit type="translation" xml:lang="la"><quote> bellum </quote></cit> <cit type="translation" xml:lang="la"><quote> capessere </quote></cit> contra aliquem: <foreign xml:lang="gez">ኢትትኣኀዙ፡ ቀትለ፡ ምስሌሆሙ፡</foreign> <ref cRef="Deut." loc="2,19">Deut. 2,19</ref>. 
</sense> <sense n="b" xml:id="D2b"> <cit type="translation" xml:lang="la"><quote> verbis aggredi </quote></cit> aliquem, <cit type="translation" xml:lang="la"><quote> concertare </quote></cit>, <cit type="translation" xml:lang="la"><quote> disputare </quote></cit> cum aliquo, <lbl expand="cum">c.</lbl> <gramGrp> <case value="accusativus">Acc.</case> </gramGrp>, <ref cRef="Act." loc="17,18">Act. 17,18</ref> (<foreign xml:lang="grc">συνέβαλον</foreign>). 
</sense> 
</sense> </sense>
               
