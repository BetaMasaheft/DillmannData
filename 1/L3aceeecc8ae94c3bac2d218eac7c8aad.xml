            <entry xml:id="L3aceeecc8ae94c3bac2d218eac7c8aad" n="6117">
               <form>
                  <foreign xml:lang="gez">ከርካዕ</foreign>
               <rs type="root"/>
                    </form>
               <sense xml:lang="la" source="#dillmann" xml:id="Dmain"> <gramGrp> <pos expand="Substantivum">subst.</pos> </gramGrp>, <lbl expand="">sing.</lbl> et <lbl expand="collatio, -is vel collectivum,&#xA;                            -e">coll.</lbl>, <cit type="translation" xml:lang="la">
                            <quote> nux </quote>
                        </cit> [videtur a rotunditate dicta, <lbl expand="videas">vid.</lbl> <foreign xml:lang="gez">ከረረ፡</foreign>; <foreign xml:lang="gez">ኰርኰረ፡</foreign>) <sense n="a" xml:id="Da"> fructus: <cit type="translation" xml:lang="la">
                                <quote> nux juglans </quote>
                            </cit>, sine dubio etiam <cit type="translation" xml:lang="la">
                                <quote> amygdala </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> amygdalum </quote>
                            </cit>, <foreign xml:lang="grc">τὰ κάρυα</foreign> <ref cRef="Gen." loc="43,11">Gen. 43,11</ref>; <ref cRef="Kuf. " loc="42">Kuf.  42</ref>; <ref cRef="Num." loc="17,23">Num. 17,23</ref>; <ref cRef="Hen." loc="31,2">Hen. 31,2</ref>; <ref cRef="Const. Ap." loc="39.">Const. Ap. 39.</ref> </sense> <sense n="b" xml:id="Db"> arbor, <cit type="translation" xml:lang="la">
                                <quote> nux </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> amygdalus </quote>
                            </cit>: <foreign xml:lang="gez">ገነተ፡ ከርካዕ፡</foreign> <foreign xml:lang="grc">κῆπος καρύας</foreign> <ref cRef="Cant." loc="6,11">Cant. 6,11</ref>; <foreign xml:lang="gez">ዘከርካዕ፡</foreign> <foreign xml:lang="grc">καρύϊνος</foreign> <cit type="translation" xml:lang="la">
                                <quote> nuceus </quote>
                            </cit> <ref cRef="Gen." loc="30,37">Gen. 30,37</ref>; <ref cRef="Jer." loc="1,11">Jer. 1,11</ref>; <foreign xml:lang="gez">ይጸጊ፡ ከርካዕ፡</foreign> <foreign xml:lang="grc">ἀνθήσῃ τὸ ἀμύγδαλον</foreign> <ref cRef="Koh." loc="12,5">Koh. 12,5</ref>; <foreign xml:lang="gez">ዕፀ፡ ከርካዕ፡</foreign> <ref cRef="Org." loc="2.">Org. 2.</ref> </sense> <sense n="c" xml:id="Dc"> Obscurus est locus <ref cRef="Jer." loc="52,21">Jer. 52,21</ref> <lbl expand="versio nova">vers. nov.</lbl>, ubi legitur <foreign xml:lang="gez">ወግዘፉ፡ ፬አጻብዕ፡ ዘከርካዕ፡</foreign> (hebr. <foreign xml:lang="he">נָביּב</foreign> <cit type="translation" xml:lang="la">
                                <quote> cavus </quote>
                            </cit>); an <cit type="translation" xml:lang="la">
                                <quote> nucalis </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> nuci similis </quote>
                            </cit> ob rotunditatem et concavitem? (<lbl expand="videas">Vid.</lbl>, quomodo <foreign xml:lang="grc">καρυωτά</foreign> interpretati sint <ref cRef="Ex. " loc="38,16">Ex.  38,16</ref>) <cit type="translation" xml:lang="la">
                                <quote> – Voc. Ae.: </quote>
                            </cit> <foreign xml:lang="gez">ከርካዕ፡ ዘ፡ ሎሚ፡</foreign> (sic!).</sense> </sense>
               
