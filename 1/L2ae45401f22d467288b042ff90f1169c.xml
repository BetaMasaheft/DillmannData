         <div>
            <entry xml:id="L2ae45401f22d467288b042ff90f1169c" n="8338">
               <form>
                  <foreign xml:lang="gez">ግምድ</foreign>
               </form>
               <sense xml:lang="la" source="#dillmann" xml:id="Dmain">
                  <gramGrp>
                     <pos expand="Substantivum">subst.</pos>
                  </gramGrp>
                  <sense n="1" xml:id="D1">
                     <cit type="translation" xml:lang="la">
                        <quote>segmentum</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>pars abscisa</quote>
                     </cit>: <foreign xml:lang="gez">ግምደ፡ ድርግሓ፡ </foreign>
                     <ref cRef="Marc." loc="2,21">Marc. 2,21</ref>; <ref cRef="Luc." loc="5,36">Luc.
                        5,36</ref>.</sense>
                  <sense n="2" xml:id="D2">
                     <lbl expand="pluralis">Pl.</lbl>
                     <foreign xml:lang="gez">ግምዳት፡</foreign> legitur <ref cRef="Prov." loc="16,11">Prov. 16,11</ref>, <lbl expand="scilicet">sc.</lbl>
                     <foreign xml:lang="gez">ከመ፡ ግምዳተ፡ ለጽሊጸ፡ መዳልው፡</foreign>
                     <foreign xml:lang="grc">ῥοπή ζυγοῦ</foreign>, quod vertendum esset <cit type="translation" xml:lang="la">
                        <quote>frustula</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>particulae</quote>
                     </cit>, sed emendandum videtur <foreign xml:lang="gez">ግምጸተ፡</foreign> – <lbl expand="Vocabularium Aethiopicum, -a -a.">Voc. Ae.</lbl>: <foreign xml:lang="gez">ግምድ፡ ዘ፡ ቍራጭ።</foreign>
                  </sense>
               </sense>
               
