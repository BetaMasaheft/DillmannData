               <sense xml:lang="la" source="#dillmann" xml:id="Dmain"> <subc>I,1</subc> [<foreign xml:lang="he">בָּשַׁל</foreign> <foreign xml:lang="syr">ܒܫܶܠ</foreign>] <gramGrp> <pos expand="Subjunctivus">Subj.</pos> </gramGrp> <foreign xml:lang="gez">ይብስል፡</foreign>, <cit type="translation" xml:lang="la">
                            <quote> coqui </quote>
                        </cit> <sense n="1" xml:id="D1"> igne: <foreign xml:lang="gez">ኢበልዐት፡ ምንተኒ፡ በእሳት፡ ዘበሰለ፡</foreign> <ref cRef="Sx. Teq. " loc="1">Sx. Teq.  1</ref>; <foreign xml:lang="gez">ይብስል፡ አዕፅምቲሁኒ፡ በማእከላ፡</foreign> <ref cRef="Hez." loc="24,5">Hez. 24,5</ref>; de pane <ref cRef="Lit." loc="171,1">Lit. 171,1</ref>.</sense> <sense n="2" xml:id="D2"> solis ardore i.e. <cit type="translation" xml:lang="la">
                                <quote> maturescere </quote>
                            </cit>, de frugibus <ref cRef="Jes." loc="17,5">Jes. 17,5</ref> <lbl expand="varia lectio">var.</lbl>; pomis: <foreign xml:lang="gez">ዘእንበለ፡ ይብስል፡</foreign> <ref cRef="Jer." loc="31,32">Jer. 31,32</ref>.</sense> </sense>
               
