                  <foreign xml:lang="gez">ኄራን፡</foreign>
                  <gramGrp>
                     <gen value="femininus">fem.</gen>
                  </gramGrp>
                  <foreign xml:lang="gez">ኄራት፡</foreign>
                  <cit type="translation" xml:lang="la">
