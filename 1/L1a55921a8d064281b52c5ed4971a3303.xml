               <sense xml:id="Dmain" xml:lang="la" source="#dillmann"> <gramGrp> <pos expand="Substantivum">subst.</pos> </gramGrp>, plerumque <gramGrp> <gen value="not found">m.</gen> </gramGrp>, Plur. <foreign xml:lang="gez">አሕጻ፡</foreign> et <foreign xml:lang="gez">አሕጻት፡</foreign> <cit type="translation" xml:lang="la"><quote> sagitta </quote></cit>, <cit type="translation" xml:lang="la"><quote> telum </quote></cit> [quamquam enim hebr. <foreign xml:lang="he">חֵץ</foreign> a <lbl expand="radix">rad.</lbl> <foreign xml:lang="he">חָצַץ</foreign> provenit, tamen ob pluralis formas, quae cum pluralibus vocum <foreign xml:lang="gez">ስም፡</foreign> et <foreign xml:lang="gez">ጥብ፡</foreign> magis quam cum pluralibus vocum <foreign xml:lang="gez">ፀር፡</foreign>, <foreign xml:lang="gez">ልብ፡</foreign> al. conveniunt, nunc (aliter atque in <bibl><ptr target="bm:Dillmann1857Gramm"/>gramm.</bibl> § 136,2,c) hujus vocis radicem <foreign xml:lang="gez">ሐጸየ፡</foreign> statuere maluerim, <lbl expand="collatio, -is vel collectivum,&#xA;                            -e">coll.</lbl> arab. <foreign xml:lang="ar">حَظْوَةٌ</foreign> praeter <foreign xml:lang="ar">حَنْطٌ</foreign>] <foreign xml:lang="grc">σίδηρος</foreign> <ref cRef="Job" loc="39,22">Job 39,22</ref>; <foreign xml:lang="grc">ἰός</foreign> <ref cRef="Thren." loc="3,13">Thren. 3,13</ref>; <foreign xml:lang="grc">τοξεύματα</foreign> <ref cRef="Hez." loc="39,3">Hez. 39,3</ref>; <ref cRef="Jer. " loc="28,11">Jer.  28,11</ref>; <foreign xml:lang="grc">βολίς</foreign> <ref cRef="Jer." loc="27,9">Jer. 27,9</ref>; plerumque <foreign xml:lang="grc">βέλος</foreign> <ref cRef="Job" loc="20,25">Job 20,25</ref>; <ref cRef="Sap." loc="5,12">Sap. 5,12</ref>; <ref cRef="Sir." loc="19,12">Sir. 19,12</ref>; <ref cRef="Ps." loc="56,6">Ps. 56,6</ref>; <foreign xml:lang="gez">ይበውእዋ፡ በሐጽ፡ ወበቀስት፡</foreign> <ref cRef="Jes." loc="7,24">Jes. 7,24</ref>; <foreign xml:lang="gez">ረሰየኒ፡ ከመ፡ ሐጽ፡ ኅሩይ፡</foreign> <ref cRef="Jes." loc="49,2">Jes. 49,2</ref>; <foreign xml:lang="gez">ፈነወ፡ ሐጾ፡</foreign> <ref cRef="Kuf. " loc="38">Kuf.  38</ref>; <foreign xml:lang="gez">ሰብአ፡ ሐጽ፡</foreign> <ref cRef="Jes." loc="21,15">Jes. 21,15</ref>; <foreign xml:lang="gez">ያወጽእ፡ አሕጻሁ፡ እምጕንጳሁ፡</foreign> <ref cRef="Sir." loc="26,12">Sir. 26,12</ref>; <foreign xml:lang="gez">ይደረብዩ፡ በአሕጻት፡</foreign> <ref cRef="Phlx." loc="100">Phlx. 100</ref>; <foreign xml:lang="gez">አሕጻ፡</foreign> <ref cRef="2 Reg." loc="18,14">2 Reg. 18,14</ref>; <ref cRef="2 Par." loc="26,15">2 Par. 26,15</ref>; <ref cRef="Job" loc="6,4">Job 6,4</ref>; <ref cRef="Job" loc="16,9">Job 16,9</ref>; <ref cRef="Job" loc="30,14">Job 30,14</ref>; <ref cRef="Hez." loc="4,2">Hez. 4,2</ref> (<foreign xml:lang="grc">βελοστάσεις</foreign>); <foreign xml:lang="gez">አሕጻት፡</foreign> <ref cRef="Ps." loc="119,4">Ps. 119,4</ref>), – <lbl expand="Vocabularium Aethiopicum, -a -a">Voc. Ae.</lbl>: <foreign xml:lang="gez">ሐጽ፡ ዘ፡ ፍላጻ።</foreign> <cb n="0135" xml:id="c0135"/> </sense>
               
