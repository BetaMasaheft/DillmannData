            <entry xml:id="L2d888571de8f44f4be1bcbf281c2b25a" n="346">
               <form>
                  <foreign xml:lang="gez">ለአከ</foreign>
               <rs type="root"/>
                    </form>
               <sense xml:lang="la" source="#dillmann" xml:id="Dmain"> <subc>I,1</subc> [radix satis nota, cujus derivata vocabula in caeteris quoque linguis Semitarum usitata sunt; amh. <foreign xml:lang="gez">ላከ፡</foreign>] <gramGrp> <pos expand="Subjunctivus">Subj.</pos> </gramGrp> <foreign xml:lang="gez">ይልአክ፡</foreign> Infin. <foreign xml:lang="gez">ልኢክ፡</foreign>, <cit type="translation" xml:lang="la">
                            <quote> legare </quote>
                        </cit>, <cit type="translation" xml:lang="la">
                            <quote> nuntium mittere </quote>
                        </cit>; <lbl expand="construitur cum">c.c.</lbl> <gramGrp> <case value="accusativus">Acc.</case> </gramGrp> <lbl expand="persona">pers.</lbl> et <foreign xml:lang="gez">ኀበ፡</foreign> vel <foreign xml:lang="gez">ለ</foreign> pers. et <foreign xml:lang="gez">ኀበ፡</foreign> vel <gramGrp> <case value="accusativus">Acc.</case> </gramGrp> loci, nec non omisso <gramGrp> <case value="accusativus">Acc.</case> </gramGrp> <lbl expand="persona">pers.</lbl>, <lbl expand="sequens">seq.</lbl> <gramGrp> <pos expand="Subjunctivus">Subj.</pos> </gramGrp> vel <foreign xml:lang="gez">ከመ፡</foreign> <lbl expand="cum">c.</lbl> <gramGrp> <pos expand="Subjunctivus">Subj.</pos> </gramGrp>: <foreign xml:lang="gez">እልአከ፡ ኀቤሆሙ፡</foreign> <ref cRef="Gen." loc="37,13">Gen. 37,13</ref>; <ref cRef="Hagg." loc="1,12">Hagg. 1,12</ref>; <ref cRef="Sir." loc="48,18">Sir. 48,18</ref>; <foreign xml:lang="gez">ኢለአከከ፡ እግዚአብሔር፡</foreign> <ref cRef="Jer." loc="35,15">Jer. 35,15</ref>; <ref cRef="Jer." loc="21,1">Jer. 21,1</ref>; <foreign xml:lang="gez">ለአክ፡ ለነ፡</foreign> <ref cRef="2 Esr." loc="5,17">2 Esr. 5,17</ref>; <foreign xml:lang="gez">ለአክ፡ ሎሙ፡</foreign> <cit type="translation" xml:lang="la">
                            <quote> ad eos </quote>
                        </cit> <ref cRef="3 Reg. " loc="18,19">3 Reg.  18,19</ref>; <foreign xml:lang="gez">ለአኩ፡ ኀበ፡ ጠቢባት፡ ይምጽኣ፡</foreign> <ref cRef="Jer." loc="9,17">Jer. 9,17</ref>; <ref cRef="Jer." loc="36,31">Jer. 36,31</ref>; <ref cRef="Jer." loc="49,6">Jer. 49,6</ref>; <foreign xml:lang="gez">ለአከ፡ ኀበ፡ ኢዮርብዓም፡ እንዘ፡ ይብል፡</foreign> <ref cRef="Am." loc="7,10">Am. 7,10</ref>; <ref cRef="Matth. " loc="27,19">Matth.  27,19</ref>; <ref cRef="Jos." loc="10,3">Jos. 10,3</ref>; <ref cRef="Jes." loc="11,1">Jes. 11,1</ref>; <foreign xml:lang="gez">ደገመ፡ ልኢከ፡ ኀቤሁ፡</foreign> <ref cRef="3 Reg. " loc="20,5">3 Reg.  20,5</ref>; <foreign xml:lang="gez">ለአከ፡ ኢየሩሳሌም፡ ኀበ፡ ካህናት፡</foreign> Rel. Bar.; <foreign xml:lang="gez">ለአከ፡ ኀበ፡ አቡሁ፡ ከመ፡ ይምጻእ፡</foreign> <ref cRef="Kuf." loc="31">Kuf. 31</ref>; <ref cRef="Jes." loc="37,4">Jes. 37,4</ref>; <foreign xml:lang="gez">ለአከቶ፡ ትስማዕ፡ ጥዩቀ፡ ነገረ፡</foreign> <ref cRef="Esth." loc="4,5">Esth. 4,5</ref>; <ref cRef="Esth." loc="8,5">Esth. 8,5</ref>; <foreign xml:lang="gez">ለአከ፡ ያስተጋብኡ፡</foreign> <ref cRef="Dan." loc="3,2">Dan. 3,2</ref>; <foreign xml:lang="gez">ለአከ፡ ወነሥኣ፡</foreign> <ref cRef="Gen." loc="20,2">Gen. 20,2</ref>; <lbl expand="cum">c.</lbl> <lbl expand="duplex">dupl.</lbl> <gramGrp> <case value="accusativus">Acc.</case> </gramGrp>: <foreign xml:lang="gez">ነገር፡ ዘአነ፡ እልእከከ፡</foreign> <foreign xml:lang="grc">τὸ ῥῆμα περὶ οὗ ἐγὼ ἀποστέλλω σε</foreign> <ref cRef="1 Reg. " loc="21,2">1 Reg.  21,2</ref> (cfr. hebr. <ref cRef="Jes." loc="55,11">Jes. 55,11</ref>). Construitur etiam <lbl expand="cum">c.</lbl> <gramGrp> <case value="accusativus">Acc.</case> </gramGrp> literarum vel rei, quae per aliquem mittuntur: <foreign xml:lang="gez">ለአከ፡ መጻሕፍተ፡ ወተናብልተ፡ ወአምኃ፡ ኀበ፡</foreign> <ref cRef="Jes. " loc="39,1">Jes.  39,1</ref>, et <lbl expand="cum">c.</lbl> <foreign xml:lang="gez">ምስለ፡</foreign> pers., <cit type="translation" xml:lang="la">
                            <quote> per </quote>
                        </cit> quam mittitur: <foreign xml:lang="gez">ኵሉ፡ ዘለአኩ፡ ሎሙ፡ ምስለ፡ አግብርት፡</foreign> <ref cRef="4 Reg." loc="17,13">4 Reg. 17,13</ref> (<lbl expand="videas">vid.</lbl> <foreign xml:lang="gez">ምስለ፡</foreign>). </sense>
               
