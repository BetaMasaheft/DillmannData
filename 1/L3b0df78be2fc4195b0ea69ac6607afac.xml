            <entry xml:id="L3b0df78be2fc4195b0ea69ac6607afac" n="9403">
               <form>
                  <foreign xml:lang="gez">ምጽናዕ</foreign>
               </form>
               <sense xml:lang="la" source="#dillmann" xml:id="Dmain">
                  <gramGrp>
                     <pos expand="Substantivum">subst.</pos>
                  </gramGrp> (<gramGrp>
                     <gen value="masculinum">m.</gen>
                  </gramGrp>), <lbl expand="pluralis">Pl.</lbl>
                  <foreign xml:lang="gez">ምጽንዓት፡</foreign> et <foreign xml:lang="gez">ምጽናዓት፡</foreign>
                  <cit type="translation" xml:lang="la">
                     <quote>firmamen</quote>
                  </cit>, <cit type="translation" xml:lang="la">
                     <quote>firmamentum</quote>
                  </cit>
                  <sense n="a" xml:id="Da">
                     <cit type="translation" xml:lang="la">
                        <quote>locus in quo</quote>
                     </cit> quid destinatum <cit type="translation" xml:lang="la">
                        <quote>firmatur</quote>
                     </cit>, a quo quid pendens firmatur: <foreign xml:lang="grc">στηριγμός</foreign>
                     <ref cRef="2 Petr." loc="3,17">2 Petr. 3,17</ref>; <foreign xml:lang="gez">ተበትከ፡ ሐብል፡ እምጽናዑ፡</foreign> <ref cRef="Gad. T.H." loc="">Gad. T.H.</ref>
                        </sense>
                  <sense n="b" xml:id="Db">
                     <cit type="translation" xml:lang="la">
                        <quote>firmamentum coeli</quote>
                     </cit>: <ref cRef="Asc. Jes." loc="4,2">Asc. Jes. 4,2</ref>; <ref cRef="Asc. Jes." loc="6,13">Asc. Jes. 6,13</ref>; <ref cRef="Asc. Jes." loc="7,9">Asc. Jes. 7,9</ref>
                     <lbl expand="sequens">seq.</lbl>; Asc. <ref cRef="Jes." loc="10,8">Jes.
                        10,8</ref>; <ref cRef="Asc. Jes." loc="10,29">Asc. Jes. 10,29</ref>; <ref cRef="Asc. Jes." loc="11,23">Asc. Jes. 11,23</ref>; <foreign xml:lang="gez">ምጽንዓት፡ </foreign>
                     <ref cRef="Kuf." loc="2,19">Kuf. 2,19</ref>; <foreign xml:lang="gez">እፎኬ፡
                        ዘኢንኅሉ፡ ምጽንዓተ፡ ሰማይ፡</foreign> M.M. f. 31; <foreign xml:lang="gez">ምጽናዓት፡</foreign> Lud. ex <ref cRef="Org." loc="2">Org. 2</ref>; Lud. ex
                        <ref cRef="Org." loc="4">Org. 4</ref>; etiam pro <lbl expand="singularis">Sing.</lbl>, ut: <foreign xml:lang="gez">እስከ፡ ምጽንዓት፡</foreign> (Lud.:
                        <foreign xml:lang="gez">ምጽናዓት፡</foreign>) <foreign xml:lang="gez">ኃምስ፡ ተለውኮ፡ </foreign>
                     <ref cRef="Sx. Tachs." loc="12 ">Sx. Tachs. 12 </ref>Enc.</sense>
               </sense>
               
