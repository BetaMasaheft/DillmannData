               <sense xml:lang="la" source="#dillmann" xml:id="Dmain"> <gramGrp> <pos expand="Substantivum">subst.</pos> </gramGrp> <sense n="1" xml:id="D1"> <cit type="translation" xml:lang="la">
                                <quote> demonstratio </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> argumentatio </quote>
                            </cit>: <foreign xml:lang="gez">ብያኔ፡ ልላዊ፡</foreign> <cit type="translation" xml:lang="la">
                                <quote> dem. </quote>
                            </cit> <cit type="translation" xml:lang="la">
                                <quote> rationalis </quote>
                            </cit> (<lbl expand="oppositum, -o, -nitur">opp.</lbl> exegetica vel biblica) F.N. 9.</sense> <sense n="2" xml:id="D2"> <cit type="translation" xml:lang="la">
