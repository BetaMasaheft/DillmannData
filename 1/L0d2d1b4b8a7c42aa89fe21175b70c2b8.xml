               <sense xml:id="Dmain" xml:lang="la" source="#dillmann"> <gramGrp> <pos expand="Substantivum">subst.</pos> </gramGrp>, plerumque <gramGrp> <gen value="not found">m.</gen> </gramGrp>, <lbl expand="pluralis">Pl.</lbl> <foreign xml:lang="gez">ምግባራት፡</foreign> <sense n="1" xml:id="D1"> <cit type="translation" xml:lang="la"><quote> actio </quote></cit>, <cit type="translation" xml:lang="la"><quote> agendi ratio </quote></cit>, <cit type="translation" xml:lang="la"><quote> institutum vitae </quote></cit>, <cit type="translation" xml:lang="la"><quote> facinora </quote></cit>, <cit type="translation" xml:lang="la"><quote> facta </quote></cit> vel <cit type="translation" xml:lang="la"><quote> opera quae quis facere solet </quote></cit>, <foreign xml:lang="grc">πρᾶγμα</foreign> <ref cRef="Koh." loc="3,1">Koh. 3,1</ref>; <ref cRef="Koh." loc="3,17">Koh. 3,17</ref>; <foreign xml:lang="gez">ለኵሉ፡ ሰብእ፡ ይፈድዮ፡ በከመ፡ ምግባሩ፡</foreign> <foreign xml:lang="grc">κατὰ τὰς πράξεις</foreign> <ref cRef="Sir. " loc="32,24">Sir.  32,24</ref>; <ref cRef="Ps." loc="27,5">Ps. 27,5</ref>; <ref cRef="Matth." loc="16,27">Matth. 16,27</ref>; <ref cRef="Luc." loc="23,51">Luc. 23,51</ref>; <ref cRef="Rom." loc="8,13">Rom. 8,13</ref>; <ref cRef="Col." loc="3,9">Col. 3,9</ref>; <foreign xml:lang="grc">ποιήματα</foreign> <ref cRef="Ps." loc="63,10">Ps. 63,10</ref>; <ref cRef="Koh." loc="9,7">Koh. 9,7</ref>; <foreign xml:lang="grc">ἐργασίαι</foreign> <ref cRef="Koh." loc="9,1">Koh. 9,1</ref>; <ref cRef="Jes." loc="1,31">Jes. 1,31</ref>; <foreign xml:lang="grc">ὁδούς</foreign> <ref cRef="Sir." loc="23,19">Sir. 23,19</ref>; <foreign xml:lang="gez">ምግባርኪ፡</foreign> <foreign xml:lang="grc">τὰ ἐπιτηδεύματά σου</foreign> <ref cRef="Jer." loc="4,18">Jer. 4,18</ref>; <ref cRef="Jer." loc="7,3">Jer. 7,3</ref>; <ref cRef="Jer." loc="23,2">Jer. 23,2</ref>; <foreign xml:lang="gez">ኵሉ፡ ምግባሮሙ፡ ርኵስ፡</foreign> <ref cRef="Kuf." loc="78">Kuf. p. 78</ref>; <ref cRef="4 Esr." loc="8,34">4 Esr. 8,34</ref>; <ref cRef="4 Esr." loc="9,8">4 Esr. 9,8</ref>; <ref cRef="4 Esr." loc="9,16">4 Esr. 9,16</ref>; <foreign xml:lang="grc">ἀναστροφή</foreign> <ref cRef="1 Petr." loc="1,18">1 Petr. 1,18</ref>; <foreign xml:lang="grc">τὰ ἔργα</foreign>, <ref cRef="Ps." loc="76,12">Ps. 76,12</ref>; <ref cRef="Ps." loc="85,7">Ps. 85,7</ref>; <ref cRef="Ps." loc="104,1">Ps. 104,1</ref>; <ref cRef="Hebr. " loc="3,9">Hebr.  3,9</ref>; <foreign xml:lang="gez">ንጹሕ፡ አነ፡ በምግባርየ፡</foreign> <ref cRef="Job" loc="11,4">Job 11,4</ref>; <ref cRef="Job" loc="21,16">Job 21,16</ref>; <ref cRef="Jes. " loc="19,14">Jes.  19,14</ref>; <ref cRef="Ex." loc="23,24">Ex. 23,24</ref>; <foreign xml:lang="gez">በደኃሪቱ፡ ለሰብእ፡ ይትዐወቅ፡ ምግባሩ፡</foreign> <ref cRef="Sir. " loc="11,27">Sir.  11,27</ref>; <ref cRef="Matth." loc="11,2">Matth. 11,2</ref>; <ref cRef="Matth." loc="23,5">Matth. 23,5</ref>; <ref cRef="Jac." loc="2,22">Jac. 2,22</ref>; <ref cRef="Jac." loc="2,24">Jac. 2,24</ref>; <ref cRef="Jac." loc="3,13">Jac. 3,13</ref>; <ref cRef="Rom." loc="2,9">Rom. 2,9</ref>; <ref cRef="Rom. " loc="2,10">Rom.  2,10</ref>; <foreign xml:lang="gez">ሶበ፡ አብርሃም፡ በምግባሩ፡ ጸድቀ፡</foreign> <ref cRef="Rom." loc="4,2">Rom. 4,2</ref>; <lbl expand="oppositum, -o, -nitur">opp.</lbl> <foreign xml:lang="gez">ጸጋ፡</foreign> <ref cRef="Rom." loc="11,6">Rom. 11,6</ref>; <foreign xml:lang="gez">ኵሉ፡ እከየ፡ ምግባር፡</foreign> <foreign xml:lang="grc">πᾶν φαῦλον πρᾶγμα</foreign> <ref cRef="Jac. " loc="3,16">Jac.  3,16</ref>; <ref cRef="Deut." loc="28,20">Deut. 28,20</ref>; <foreign xml:lang="gez">ምግባረ፡ ሠናይ፡</foreign> <ref cRef="Jac." loc="2,14">Jac. 2,14</ref>; <ref cRef="Jac. " loc="2,26">Jac.  2,26</ref>; <ref cRef="4 Esr." loc="8,38">4 Esr. 8,38</ref>; <ref cRef="4 Esr." loc="8,42">4 Esr. 8,42</ref>; <foreign xml:lang="gez">ምግባር፡ ሠናይ፡</foreign> <ref cRef="Sx. Mag." loc="20">Sx. Mag. 20</ref>; <foreign xml:lang="gez">ምግባረ፡ እኩይ፡</foreign> <ref cRef="Job" loc="1,1">Job 1,1</ref>; <foreign xml:lang="gez">ምግባር፡ እኩይ፡</foreign> <ref cRef="Job" loc="1,18">Job 1,18</ref>; <foreign xml:lang="gez">ዘሠናይ፡ ምግባሩ፡</foreign> <foreign xml:lang="grc">φιλάγαθος</foreign> <ref cRef="Tit." loc="1,8">Tit. 1,8</ref>; <foreign xml:lang="grc">ἡ ποίησις</foreign> <ref cRef="Jac." loc="1,25">Jac. 1,25</ref>; <foreign xml:lang="gez">በምግባር፡</foreign> <foreign xml:lang="grc">ἐν ἔργῳ</foreign> (<lbl expand="oppositum, -o, -nitur">opp.</lbl> <foreign xml:lang="gez">በቃል፡</foreign>) <ref cRef="1 Joh." loc="3,18">1 Joh. 3,18</ref>; <ref cRef="4 Esr." loc="9,7">4 Esr. 9,7</ref>; <foreign xml:lang="gez">በቃልከኒ፡ ወበምግባርከኒ፡ አክብሮ፡ ለአቡከ፡</foreign> <ref cRef="Sir." loc="3,8">Sir. 3,8</ref>; <foreign xml:lang="gez">ምግባራት፡</foreign> <ref cRef="Asc. Jes." loc="9,22">Asc. Jes. 9,22</ref>; <ref cRef="Lit." loc="170,3">Lit. 170,3</ref>. 
</sense> <sense n="2" xml:id="D2"> <cit type="translation" xml:lang="la"><quote> effectus </quote></cit>, <cit type="translation" xml:lang="la"><quote> efficacitas </quote></cit>, <foreign xml:lang="grc">ἐνέργεια</foreign> <ref cRef="Sap. " loc="7,17">Sap.  7,17</ref>; <foreign xml:lang="grc">ἐνεργήματα</foreign> <ref cRef="1 Cor." loc="12,6">1 Cor. 12,6</ref>; <foreign xml:lang="gez">ኀይለ፡ ምግባረ፡ አሥራው፡</foreign> <foreign xml:lang="grc">δυνάμεις ῥιζῶν</foreign> <ref cRef="Sap." loc="7,20">Sap. 7,20</ref>. 
</sense> <sense n="3" xml:id="D3"> <cit type="translation" xml:lang="la"><quote> negotium </quote></cit>, <cit type="translation" xml:lang="la"><quote> officium </quote></cit>, <cit type="translation" xml:lang="la"><quote> munus </quote></cit>, <cit type="translation" xml:lang="la"><quote> res agenda </quote></cit>, <foreign xml:lang="gez">ምግባረ፡ ወሀብናሁ፡</foreign> <ref cRef="Kuf." loc="13">Kuf. p. 13</ref>; <foreign xml:lang="grc">πρᾶξις</foreign> <ref cRef="Rom." loc="12,4">Rom. 12,4</ref>; <foreign xml:lang="gez">ምግባራት፡</foreign> <ref cRef="Num." loc="3,7">Num. 3,7</ref>; <ref cRef="Num." loc="3,8">Num. 3,8</ref>; <foreign xml:lang="gez">ውእቱ፡ ይበቍዕ፡ ለምግበ፡ ምግባራቲሁ፡ ወንዋዩ፡</foreign> <ref cRef="F.N." loc=" 32">F.N.  32</ref>. <sense n="a" xml:id="D3a"> <cit type="translation" xml:lang="la"><quote> quaestus </quote></cit> <ref cRef="Kid." loc="24.">Kid. f. 24.</ref> 
</sense> <sense n="b" xml:id="D3b"> <cit type="translation" xml:lang="la"><quote> opus </quote></cit> vel <cit type="translation" xml:lang="la"><quote> ministerium sacrum </quote></cit>: <foreign xml:lang="gez">ለምግባረ፡ ደብተራ፡ ዘመርጡር፡</foreign> <ref cRef="Ex." loc="30,16">Ex. 30,16</ref>. 
</sense> <sense n="c" xml:id="D3c"> <cit type="translation" xml:lang="la"><quote> res </quote></cit>: <foreign xml:lang="gez">በኵሉ፡ ምግባር፡</foreign> <foreign xml:lang="grc">κατὰ πάντα τρόπον</foreign> <ref cRef="Rom." loc="3,2">Rom. 3,2</ref>; cura <foreign xml:lang="gez">ምግባራ፡ ለነፍስ፡</foreign> Lud. ex <ref cRef="Sx. Hed." loc="29 Enc.">Sx. Hed. 29 Enc.</ref> 
</sense> 
</sense> <sense n="4" xml:id="D4"> <cit type="translation" xml:lang="la"><quote> opera </quote></cit> quae quis produxit, <sense n="a" xml:id="D4a"> a Deo <cit type="translation" xml:lang="la"><quote> creata </quote></cit>: <ref cRef="Hen." loc="5,2">Hen. 5,2</ref>. 
</sense> <sense n="b" xml:id="D4b"> ad hominibus <cit type="translation" xml:lang="la"><quote> confecta </quote></cit>: <ref cRef="Hen." loc="8,1">Hen. 8,1</ref>; <foreign xml:lang="gez">ኵሉ፡ ምግባረ፡ ማእስ፡</foreign> <ref cRef="Lev." loc="13,48">Lev. 13,48</ref>. 
</sense> <sense n="c" xml:id="D4c"> <cit type="translation" xml:lang="la"><quote> merces </quote></cit> labore parta, <ref cRef="Jes." loc="40,10">Jes. 40,10</ref>; <ref cRef="Jes." loc="62,11">Jes. 62,11</ref> (<lbl expand="videas">vid.</lbl> <foreign xml:lang="gez">ግብር፡</foreign> n. 3,c et <foreign xml:lang="gez">ተግባር፡</foreign> <gramGrp> <pos expand="nomen">n.</pos> </gramGrp> 3). 
</sense> 
</sense> </sense>
               
            <sense xml:id="Tmain" xml:lang="en" source="#traces"> <cit type="transcription" xml:lang="gez"><quote>mǝgbār</quote></cit>, <cit type="transcription" xml:lang="gez"><quote>mǝgbārāt</quote></cit> <sense n="G" xml:id="TG"> <foreign xml:lang="gez">ምግባረ፡ ሥራይ፡</foreign> <cit type="transcription" xml:lang="gez"><quote>mǝgbāra śǝrāy</quote></cit> L’expression <foreign xml:lang="gez">ምግባረ፡ ሥራይ፡</foreign> se substitue parfois au pluriel <foreign xml:lang="gez">ሥራያት፡</foreign> pour désigner <cit type="translation" xml:lang="fr"><quote>les procédés magiques</quote></cit>» <foreign xml:lang="gez">በውእቱ፡ መዋዕል፡ አስተርአየት፡ አሐቲ፡ ብእሲት፡ ሐነፋዊት፡ ፍልስፍት፡ በሀገረ፡ እስክንድርያ፡ ዘስማ፡ አንባድያ፡ ወይእቲሰ፡ ተፀምደት፡ ለምግባረ፡ ሥራይ።</foreign> <bibl><ptr target="bm:Zotenberg1883JohnNikiu"/></bibl>, fol. 235. <lb/>
