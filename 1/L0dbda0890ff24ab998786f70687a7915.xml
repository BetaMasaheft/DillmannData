            <entry xml:id="L0dbda0890ff24ab998786f70687a7915" n="9940">
               <form>
                  <foreign xml:lang="gez">ፍቁር</foreign>
               </form>
               <sense xml:lang="la" source="#dillmann" xml:id="Dmain">
                  <gramGrp>
                     <pos expand="participium">part.</pos>
                  </gramGrp>
                  <cit type="translation" xml:lang="la">
                     <quote>carus</quote>
                  </cit>, <cit type="translation" xml:lang="la">
                     <quote>dilectus</quote>
                  </cit>, <cit type="translation" xml:lang="la">
                     <quote>amatus</quote>
                  </cit>, (<cit type="translation" xml:lang="la">
                     <quote>amicus</quote>
                  </cit>), de rebus <ref cRef="Ps." loc="83,1">Ps. 83,1</ref>; de personis: <foreign xml:lang="gez">እንተ፡ ኢኮነት፡ ፍቅርተ፡</foreign>
                  <ref cRef="Hos. " loc="2,25">Hos. 2,25</ref>; <ref cRef="Jer." loc="11,15">Jer.
                     11,15</ref>; <ref cRef="Eph." loc="5,1">Eph. 5,1</ref>; <ref cRef="Phil." loc="4,1">Phil. 4,1</ref>; <ref cRef="Jac." loc="1,16">Jac. 1,16</ref>; <lbl expand="construitur cum">c.c.</lbl>
                  <foreign xml:lang="gez">በኀበ፡ </foreign>
                  <ref cRef="Judith" loc="9,4">Judith 9,4</ref>; <foreign xml:lang="gez">ጽሉእ፡ በኀበ፡
                     እግዚአብሔር፡ ወፍቁር፡ በኀበ፡ አጋንንት፡ </foreign>
                  <ref cRef="Did." loc="4">Did. 4</ref>; <foreign xml:lang="gez">ኮነ፡ ፍቁረ፡
                     በኀቤሆሙ፡</foreign>
                  <ref cRef="Sx. Ter" loc="27">Sx. Ter 27</ref>; per st. constr.: <foreign xml:lang="gez">ፍቁሩ፡ ለዝ፡ ዓለም፡</foreign>
                  <ref cRef="Jac." loc="4,4">Jac. 4,4</ref>; <ref cRef="Deut." loc="33,12">Deut.
                     33,12</ref>; <ref cRef="Cant. " loc="5,16">Cant. 5,16</ref>; <ref cRef="Ps." loc="59,5">Ps. 59,5</ref>; <ref cRef="Ps." loc="107,5">Ps. 107,5</ref>; <ref cRef="Ps." loc="126,3">Ps. 126,3</ref>; <ref cRef="Jes." loc="5,1">Jes.
                     5,1</ref>; <ref cRef="Jes." loc="44,2">Jes. 44,2</ref>; <ref cRef="Matth." loc="12,18">Matth. 12,18</ref>; <ref cRef="Col." loc="4,14">Col. 4,14</ref>; 1
                     <ref cRef="Cor." loc="4,14">Cor. 4,14</ref>; <ref cRef="Rom." loc="1,7">Rom.
                     1,7</ref>; <foreign xml:lang="gez">ፍቁራኒሃ፡</foreign>
                  <foreign xml:lang="grc">οἱ φιλοῦντες αὐτήν</foreign>
                  <ref cRef="Thren." loc="1,2">Thren. 1,2</ref>; <foreign xml:lang="gez">ኦፍቁረ፡
                     ክርስቶስ፡ ንጉሥ፡ </foreign>
                  <foreign xml:lang="grc">ὦ φιλόχριστε βασιλεῦ</foreign> Cyr. ad <ref cRef="Theod. " loc="23">Theod. f. 23</ref>; <foreign xml:lang="gez">ኦፍቁራንየ፡</foreign>
                  compellantur auditores in concionibus sacris, <ref cRef="Genz." loc="120 ">Genz.
                     f. 120 </ref>al.; <foreign xml:lang="gez">በአሐዱ፡ ፍቁር፡ ይሰሐብ፡ ካልእ፡</foreign> Prov.
                     <ref cRef="Th. Petr." loc="17.">Th. Petr. p. 17.</ref>
                  <sense n="a" xml:id="Da"> epitheton Messiae, <ref cRef="Asc. Jes." loc="1,5">Asc. Jes.
                        1,5</ref>; <ref cRef="Asc. Jes." loc="3,13">Asc. Jes. 3,13</ref>; Asc. <ref cRef="Jes." loc="3,17">Jes. 3,17</ref>; <ref cRef="Asc. Jes." loc="4,18">Asc. Jes. 4,18</ref>;</sense>
                  <sense n="b" xml:id="Db">
                     <cit type="translation" xml:lang="la">
                        <quote>amasius</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>amator</quote>
                     </cit>, <bibl>
                        <ptr target="bm:Ludolf1691Commentarius"/>Lud. Comm. </bibl>p. 496 n.
                     12.</sense>
               </sense>
               
