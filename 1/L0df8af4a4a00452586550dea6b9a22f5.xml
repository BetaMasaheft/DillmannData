               <sense xml:id="Dmain" xml:lang="la" source="#dillmann"> <gramGrp> <pos expand="Substantivum">subst.</pos> </gramGrp> <sense n="1" xml:id="D1"> <cit type="translation" xml:lang="la"><quote> durities </quote></cit>: <foreign xml:lang="gez">ጽንዐ፡ ልብ፡</foreign> <ref cRef="Hen." loc="16,3">Hen. 16,3</ref>; <ref cRef="Marc. " loc="16,14">Marc.  16,14</ref>.
</sense> <sense n="2" xml:id="D2"> <cit type="translation" xml:lang="la"><quote> severitas </quote></cit>, <cit type="translation" xml:lang="la"><quote> rigor </quote></cit>, <ref cRef="Sx. Haml." loc="1 ">Sx. Haml. 1 </ref>Enc.
</sense> <sense n="3" xml:id="D3"> <cit type="translation" xml:lang="la"><quote> firmitas </quote></cit>, aedificii <ref cRef="4 Esr." loc="10,69">4 Esr. 10,69</ref>. <sense n="a" xml:id="D3a"> <cit type="translation" xml:lang="la"><quote> constantia </quote></cit>: <ref cRef="Sx. Nah." loc="26 ">Sx. Nah. 26 </ref>Enc.; <ref cRef="Sx. Masc." loc="7 ">Sx. Masc. 7 </ref>Enc.
</sense> <sense n="b" xml:id="D3b"> <cit type="translation" xml:lang="la"><quote> firmamentum </quote></cit> coeli: <foreign xml:lang="gez">ጽንዐ፡ ሰማይ፡</foreign> <ref cRef="Ex." loc="24,10">Ex. 24,10</ref>; <ref cRef="Hen." loc="18,2">Hen. 18,2</ref>; <ref cRef="Hen." loc="18,5">Hen. 18,5</ref>; <ref cRef="Sir." loc="43,1">Sir. 43,1</ref>.
</sense> <sense n="c" xml:id="D3c"> <cit type="translation" xml:lang="la"><quote> munimentum </quote></cit>, <foreign xml:lang="grc">ὀχύρωμα</foreign> <ref cRef="Prov." loc="10,29">Prov. 10,29</ref>; <ref cRef="Prov. " loc="21,22">Prov.  21,22</ref>.</sense>
</sense> <sense n="4" xml:id="D4"> <cit type="translation" xml:lang="la"><quote> robur </quote></cit> corporis <ref cRef="Job" loc="40,11">Job 40,11</ref>; <ref cRef="Deut." loc="3,24">Deut. 3,24</ref>; <ref cRef="Ps." loc="32,17">Ps. 32,17</ref>.
</sense> <sense n="5" xml:id="D5"> <cit type="translation" xml:lang="la"><quote> fortitudo </quote></cit>, <cit type="translation" xml:lang="la"><quote> virtus </quote></cit>, <cit type="translation" xml:lang="la"><quote> robur </quote></cit>, <cit type="translation" xml:lang="la"><quote> vis </quote></cit>, <cit type="translation" xml:lang="la"><quote> potentia </quote></cit>: <foreign xml:lang="grc">ἀνδρεία</foreign> <ref cRef="Koh." loc="2,21">Koh. 2,21</ref>; <ref cRef="Koh." loc="5,10">Koh. 5,10</ref>; <ref cRef="Sap." loc="8,7">Sap. 8,7</ref>; <foreign xml:lang="grc">ἰσχύς</foreign> <ref cRef="Ex." loc="9,16">Ex. 9,16</ref>; <ref cRef="2 Par." loc="3,17">2 Par. 3,17</ref>; <ref cRef="Apoc." loc="5,12">Apoc. 5,12</ref>; <ref cRef="Apoc." loc="7,12">Apoc. 7,12</ref>; <foreign xml:lang="grc">κράτος</foreign> <ref cRef="Sap." loc="15,2">Sap. 15,2</ref>; <foreign xml:lang="gez">ጽንዐ፡ ኀይሉ፡</foreign> <ref cRef="Sir." loc="18,5">Sir. 18,5</ref>; <ref cRef="Eph." loc="1,19">Eph. 1,19</ref>; <ref cRef="Eph." loc="6,10">Eph. 6,10</ref>; <foreign xml:lang="gez">ጽንዐ፡ ስብሐቲሁ፡</foreign> <ref cRef="Col." loc="1,11">Col. 1,11</ref>; <foreign xml:lang="grc">δυναστεῖαι</foreign> <ref cRef="Ps." loc="20,13">Ps. 20,13</ref>; <foreign xml:lang="gez">ጽንዐ፡ ምግባር፡</foreign> <ref cRef="2 Petr." loc="1,10">2 Petr. 1,10</ref>; <foreign xml:lang="gez">ጽንዐ፡ ልቦሙ፡</foreign> <ref cRef="Sx. Hed. " loc="4.">Sx. Hed.  4.</ref> <sense n="a" xml:id="D5a"> pro concreto: <foreign xml:lang="gez">ጽንዖሙ፡</foreign> <foreign xml:lang="grc">ὁ δυνατὸς αὐτῶν</foreign> <ref cRef="1 Reg." loc="17,51">1 Reg. 17,51</ref>.
</sense> <sense n="b" xml:id="D5b"> <cit type="translation" xml:lang="la"><quote> vehementia </quote></cit>: <foreign xml:lang="gez">ጽንዐ፡ ንዴት፡</foreign> <ref cRef="Tab. Tab." loc="23 ">Tab. Tab. 23 </ref> (<lbl expand="videas">vid.</lbl> <foreign xml:lang="gez">መጽዕቅ፡</foreign>).
</sense> 
