            <entry xml:id="L0b14b683ce9240bbad879d33e816dfd2" n="536">
               <form>
                  <foreign xml:lang="gez">ሐመልማል</foreign>
               </form>
               <sense xml:lang="la" source="#dillmann" xml:id="Dmain">
                  <gramGrp>
                     <pos expand="Substantivum">subst.</pos>
                  </gramGrp>, plerumque <gramGrp>
                     <gen value="masculinum">m.</gen>
                  </gramGrp>, <lbl expand="pluralis">Pl.</lbl>
                  <foreign xml:lang="gez">ሐመልማላት፡ </foreign> (<ref cRef="Jsp." loc="285">Jsp. p.
                     285</ref>) et <foreign xml:lang="gez">አሕመልማላት፡</foreign> (Lit. Diosc.) <cit type="translation" xml:lang="la">
                     <quote>viror</quote>
                  </cit>, <cit type="translation" xml:lang="la">
                     <quote>viride</quote>
                  </cit>, <cit type="translation" xml:lang="la">
                     <quote>viriditas</quote>
                  </cit> (<cit type="translation" xml:lang="la">
                     <quote>flavum</quote>
                  </cit>, <cit type="translation" xml:lang="la">
                     <quote>fulvum</quote>
                  </cit>): <foreign xml:lang="gez">በውስተ፡ ሐመልማል፡</foreign>
                  <ref cRef="Job" loc="6,5">Job 6,5</ref>; <foreign xml:lang="grc">βούτομον</foreign>
                  <ref cRef="Job" loc="8,11">Job 8,11</ref>; <foreign xml:lang="gez">ኀበ፡ ሐመልማል፡
                     ይዴግን፡ </foreign> (<foreign xml:lang="grc">παντὸς χλωροῦ</foreign>) <ref cRef="Job" loc="39,8">Job 39,8</ref>; <ref cRef="Jes." loc="27,11">Jes.
                     27,11</ref>; <ref cRef="Dan." loc="4,12">Dan. 4,12</ref> (<foreign xml:lang="grc">χλόη</foreign>); <ref cRef="Prov." loc="27,24">Prov. 27,24</ref>
                     (<foreign xml:lang="grc">τὰ χλωρά</foreign>); <ref cRef="Apoc." loc="9,4">Apoc.
                     9,4</ref>; <ref cRef="Gen." loc="2,5">Gen. 2,5</ref>; <ref cRef="Ex." loc="10,15">Ex. 10,15</ref>; <ref cRef="Deut." loc="29,22">Deut. 29,22</ref>;
                     <ref cRef="2 Reg." loc="23,4">2 Reg. 23,4</ref>; <ref cRef="Ps. " loc="103,15">Ps. 103,15</ref>; <ref cRef="Ps." loc="146,9">Ps. 146,9</ref>; <foreign xml:lang="gez">ሐመልማለ፡ ሐምል፡</foreign> vide sub <foreign xml:lang="gez">ሐምል፡</foreign>; <foreign xml:lang="gez">ሐመልማለ፡ ዘርእ፡</foreign>
                  <ref cRef="Sir. " loc="40,22">Sir. 40,22</ref>; <foreign xml:lang="gez">ሐመልማለ፡
                     ሣዕር፡</foreign>
                  <ref cRef="Marc." loc="6,39">Marc. 6,39</ref>; <ref cRef="Gen." loc="1,11">Gen.
                     1,11</ref>; <ref cRef="Gen." loc="1,12">Gen. 1,12</ref>; <ref cRef="Gen." loc="9,3">Gen. 9,3</ref>; <ref cRef="Jes." loc="19,7">Jes. 19,7</ref>; <ref cRef="Jes." loc="15,16">Jes. 15,16</ref>; <foreign xml:lang="gez">ሐመልማለ፡
                     አቍጽል፡</foreign>
                  <ref cRef="Hen. " loc="5,1">Hen. 5,1</ref>; <foreign xml:lang="gez">ሐመልማለ፡
                     ወርቅ፡</foreign>
                  <ref cRef="Ps." loc="67,14">Ps. 67,14</ref>; <foreign xml:lang="gez">እምቅድመ፡ ይብቍሉ፡
                     አሕመልማላት፡ ሀሎ፡ እግዚአብሔር፡</foreign> Lit. Diosc. – <lbl expand="Vocabularium Aethiopicum, -a -a.">Voc. Ae.</lbl>: <foreign xml:lang="gez">ሐመልማል፡ ዘ፡ ሐረንጋዋ፡ የሚመስል፡ ለምለም።</foreign>
               </sense>
               
