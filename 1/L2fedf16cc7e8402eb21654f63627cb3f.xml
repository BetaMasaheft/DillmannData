         <div>
            <entry xml:id="L2fedf16cc7e8402eb21654f63627cb3f" n="4886">
               <form>
                  <foreign xml:lang="gez">ንቃው</foreign>
               </form>
               <sense xml:lang="la" source="#dillmann" xml:id="Dmain">
                  <gramGrp>
                     <pos expand="Substantivum">subst.</pos>
                  </gramGrp>
                  <sense n="1" xml:id="D1">
                     <cit type="translation" xml:lang="la">
                        <quote>sonus</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>sonitus</quote>
                     </cit>, ut citharae <ref cRef="Org." loc="3.">Org. 3.</ref>
                  </sense>
                  <sense n="2" xml:id="D2">
                     <cit type="translation" xml:lang="la">
                        <quote>vox</quote>
                     </cit> omnium animalium: <foreign xml:lang="gez">ንቃወ፡ አራዊት፡</foreign>
                     <ref cRef="Clem. " loc="82">Clem. f. 82</ref>; <ref cRef="Clem." loc="178">Clem. f. 178</ref>; <foreign xml:lang="gez">ንቃወ፡ አዕዋፍ፡ ወአራዊት፡</foreign>
                     <ref cRef="Macc." loc="22">Macc. f. 22</ref>; <ref cRef="Clem." loc="179">Clem.
                        f. 179</ref>; <lbl expand="exempli gratia">ex. gr.</lbl>
                     <cit type="translation" xml:lang="la">
                        <quote>rugitus </quote>
                     </cit>leonum <ref cRef="Job" loc="4,10">Job 4,10</ref>; <ref cRef="Org." loc="2">Org. 2</ref>; <cit type="translation" xml:lang="la">
                        <quote>hinnitus</quote>
                     </cit> equorum, <foreign xml:lang="gez">ንቃወ፡ አፍራስ፡</foreign>
                     <ref cRef="Clem." loc="17">Clem. f. 17</ref>; <cit type="translation" xml:lang="la">
                        <quote>latratus</quote>
                     </cit> canum <ref cRef="Org." loc="1">Org. 1</ref>; <cit type="translation" xml:lang="la">
                        <quote>bombus</quote>
                     </cit> vel <cit type="translation" xml:lang="la">
                        <quote>murmur</quote>
                     </cit>
                     <cb n="0647" xml:id="c0647"/> apium, <foreign xml:lang="gez">ንቃወ፡ ዜማ፡
                        ንህብ፡</foreign> Lud. sine <lbl expand="auctore, auctoritate">auct.</lbl>;
                        <cit type="translation" xml:lang="la">
                        <quote>cantus </quote>
                     </cit>avium Sx. <ref cRef="Mij." loc="17 ">Mij. 17 </ref>Enc.; <cit type="translation" xml:lang="la">
                        <quote>clangor </quote>
                     </cit>aquilae, anseris, Lud. <lbl expand="auctore, auctoritate">auct.</lbl>
                     Greg.</sense>
               </sense>
               
