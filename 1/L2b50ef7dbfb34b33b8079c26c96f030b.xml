            <entry xml:id="L2b50ef7dbfb34b33b8079c26c96f030b" n="2881">
               <form>
                  <foreign xml:lang="gez">ተሰከመ</foreign>
               </form>
               <sense xml:lang="la" source="#dillmann" xml:id="Dmain"> et <foreign xml:lang="gez">ተሰክመ፡</foreign>
                     (<ref cRef="Jud." loc="16,3">Jud. 16,3</ref>) <subc>III,1</subc> et <gramGrp>
                     <pos expand="reflexivum">refl.</pos>
                  </gramGrp>
                  <sense n="1" xml:id="D1">
                     <cit type="translation" xml:lang="la">
                        <quote>humeris suis imponere</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>in humeros suscipere</quote>
                     </cit>, <lbl expand="cum">c.</lbl>
                     <gramGrp>
                        <case value="accusativus">Acc.</case>
                     </gramGrp> rei: <foreign xml:lang="gez">ተሰከማ፡ ለይእቲ፡ ቀሡት፡</foreign> (Perf.) <ref cRef="Sx. Tachs." loc="11">Sx. Tachs. 11</ref>; <ref cRef="Jud." loc="16,3">Jud. 16,3</ref>; <foreign xml:lang="gez">ይሰክምዎ፡ ዲበ፡ መትከፍቶሙ፡</foreign>
                     <ref cRef="Jes." loc="46,7">Jes. 46,7</ref>; Imperat. <foreign xml:lang="gez">አትሕት፡ መታክፍቲከ፡</foreign>
                     <cb n="0379" xml:id="c0379"/>
                     <foreign xml:lang="gez">ወተሰከማ፡</foreign>
                     <ref cRef="Sir." loc="6,25">Sir. 6,25</ref>; <foreign xml:lang="gez">ክቡደ፡
                        ኢታንሥእ፡ ወኢትሰከም፡</foreign> (v. <foreign xml:lang="gez">ትስከም፡</foreign>)
                        <foreign xml:lang="gez">ላዕሌከ፡</foreign>
                     <ref cRef="Sir." loc="13,2">Sir. 13,2</ref>; <ref cRef="Sir." loc="51,26">Sir.
                        51,26</ref>.</sense>
                  <sense n="2" xml:id="D2">
                     <cit type="translation" xml:lang="la">
                        <quote>se invicem sustinere</quote>
                     </cit>: <foreign xml:lang="gez">እንዘ፡ ይሰከም፡ ዐምድ፡ ለዐምድ፡ </foreign>
                     <ref cRef="Ex." loc="26,17">Ex. 26,17</ref> (var. <foreign xml:lang="gez">ይሳመክ፡</foreign> CG).</sense>
               </sense>
               
