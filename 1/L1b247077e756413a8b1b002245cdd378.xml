                     <ref cRef="Jes." loc="55,5">Jes. 55,5</ref>; <ref cRef="Rom." loc="8,30">Rom.
                        8,30</ref>; <foreign xml:lang="grc">ὕψωσε</foreign>
                     <ref cRef="Jes." loc="63,9">Jes. 63,9</ref>; <foreign xml:lang="grc">ἐμεγάλυνε</foreign>
