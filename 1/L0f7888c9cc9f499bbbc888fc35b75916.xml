         <div>
            <entry xml:id="L0f7888c9cc9f499bbbc888fc35b75916" n="8532">
               <form>
                  <foreign xml:lang="gez">ተጋአዘ</foreign>
               </form>
               <sense xml:lang="la" source="#dillmann" xml:id="Dmain"> <subc>III,3</subc> (crebro <foreign xml:lang="gez">ተገዓዘ፡</foreign> scriptum) <sense n="1" xml:id="D1">
                     <cit type="translation" xml:lang="la">
                        <quote>stomachari</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>irasci</quote>
                     </cit> (sibi invicem): <foreign xml:lang="gez">አልቦ፡ ዘትትጋአዙ፡ በፍኖት፡</foreign>
                     <foreign xml:lang="grc">ὀργίζεσθε</foreign>
                     <ref cRef="Gen." loc="45,24">Gen. 45,24</ref>; <foreign xml:lang="gez">ደናግለ፡
                        ቀተለ፡ ዮልያኖስ፡ በተጋእዞ፡</foreign>
                     <ref cRef="Sx. Hed." loc="10 ">Sx. Hed. 10 </ref>Enc.; <lbl expand="cum">c.</lbl>
                     <gramGrp>
                        <case value="accusativus">Acc.</case>
                     </gramGrp>
                     <lbl expand="persona">pers.</lbl> (alicui), <foreign xml:lang="grc">πικραίνεσθαι</foreign>
                     <ref cRef="Col." loc="3,19">Col. 3,19</ref>
                     <lbl expand="romanae editionis">rom.</lbl>
                  </sense>
                  <sense n="2" xml:id="D2">
                     <cit type="translation" xml:lang="la">
                        <quote>altercari</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>rixari</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>jurgare</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>litigare</quote>
                     </cit>: <foreign xml:lang="gez">እለ፡ ይትጋአዙ፡</foreign>
                     <foreign xml:lang="grc">οἱ φιλονεικοῦντες </foreign>
                     <ref cRef="Prov." loc="10,12">Prov. 10,12</ref>; <foreign xml:lang="grc">μάχεσθαι</foreign>
                     <ref cRef="Tit." loc="3,2">Tit. 3,2</ref>; <ref cRef="2 Tim. " loc="2,24">2
                        Tim. 2,24</ref>; <ref cRef="Jes." loc="58,4">Jes. 58,4</ref>; <foreign xml:lang="grc">ἀντιλογία</foreign>
                     <ref cRef="Ex." loc="18,16">Ex. 18,16</ref>; <foreign xml:lang="gez">በበይናቲሆሙ፡</foreign>
                     <ref cRef="Joh." loc="6,52">Joh. 6,52</ref>; <lbl expand="sequens">seq.</lbl>
                     <foreign xml:lang="gez">ምስለ፡ </foreign>
                     <ref cRef="Kuf." loc="24">Kuf. 24</ref>; <ref cRef="Kuf." loc="37">Kuf.
                        37</ref>; <foreign xml:lang="gez">ተጋአዝኩ፡ ምስሌሃ፡</foreign>
                     <foreign xml:lang="grc">ἠρυθρίων πρὸς αὐτήν</foreign>
                     <ref cRef="Tob." loc="2,14">Tob. 2,14</ref>; <foreign xml:lang="grc">διαμάχεσθαι</foreign>
                     <ref cRef="Sir." loc="8,1">Sir. 8,1</ref>; <ref cRef="Sir." loc="8,16">Sir.
                        8,16</ref>; <foreign xml:lang="gez">ኢይክል፡ ተጋእዞ፡ ምስለ፡ ዘይጸንዕ፡</foreign>
                     <ref cRef="Koh." loc="6,10">Koh. 6,10</ref>; <foreign xml:lang="grc">διεκρίνοντο πρός </foreign>
                     <ref cRef="Act." loc="11,2">Act. 11,2</ref>
                     <lbl expand="romanae editionis">rom.</lbl>
                  </sense>
                  <sense n="3" xml:id="D3">
                     <cit type="translation" xml:lang="la">
                        <quote>objurgare</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>conviciis consectari</quote>
                     </cit> aliquem, <lbl expand="cum">c.</lbl>
                     <gramGrp>
                        <case value="accusativus">Acc.</case>
                     </gramGrp>, <ref cRef="Kuf." loc="92">Kuf. p. 92</ref>; <ref cRef="Kuf. " loc="147.">Kuf. p. 147.</ref>
                  </sense>
               </sense>
               
