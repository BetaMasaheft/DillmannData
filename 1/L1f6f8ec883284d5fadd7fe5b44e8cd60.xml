               <sense xml:lang="la" source="#dillmann" xml:id="Dmain"> <subc>I,1</subc>; raro <foreign xml:lang="gez">ኀፍረ፡</foreign> [i.q. <foreign xml:lang="he">חָפֵר</foreign> <foreign xml:lang="syr">ܚܦܰܪ</foreign> <foreign xml:lang="ar">خَفِرَ</foreign>, amh. <foreign xml:lang="gez">አፈረ፡</foreign>; <lbl expand="vicinus">vic.</lbl> <foreign xml:lang="he">חמר</foreign> <foreign xml:lang="ar">خمر</foreign> (<lbl expand="videas">vid.</lbl> <foreign xml:lang="gez">ሐመረ፡</foreign>)] <gramGrp> <pos expand="Subjunctivus">Subj.</pos> </gramGrp> <foreign xml:lang="gez">ይኅፈር፡</foreign> <cb n="0626" xml:id="c0626"/> <sense n="1" xml:id="D1"> <cit type="translation" xml:lang="la">
                                <quote> erubescere </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> rubore suffundi </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> pudere </quote>
                            </cit>, <gramGrp> <pos expand="absolute">abs.</pos> </gramGrp>, ut <ref cRef="Gen." loc="2,25">Gen. 2,25</ref>; <foreign xml:lang="gez">ዘኢየኀፍር፡</foreign> <foreign xml:lang="grc">ὁ ἀπολωλεκώς αἰσχύνην</foreign> <ref cRef="Sir." loc="29,14">Sir. 29,14</ref>; <foreign xml:lang="gez">ዘየኀፍር፡</foreign> <foreign xml:lang="grc">αἰσχυντηρός</foreign> <cit type="translation" xml:lang="la">
                                <quote> pudibundus </quote>
                            </cit> <ref cRef="Sir." loc="35,10">Sir. 35,10</ref>; <ref cRef="Kuf." loc="3">Kuf. 3</ref>; <foreign xml:lang="gez">ተኀፍሩ፡ ወኀፊረ፡ ኢኀፈሩ፡</foreign> (var. <foreign xml:lang="gez">ኢኀፍሩ፡</foreign>) <ref cRef="Jer." loc="6,15">Jer. 6,15</ref>; <foreign xml:lang="gez">ወኀፍረተ፡ ኢኀፈሩ፡</foreign> <ref cRef="Jer." loc="8,11">Jer. 8,11</ref> (vers. alt.); <foreign xml:lang="gez">ኅፈሩኬ፡</foreign> <ref cRef="Chrys. Ta. " loc="4">Chrys. Ta.  4</ref>; <lbl expand="construitur cum">c.c.</lbl> <gramGrp> <case value="accusativus">Acc.</case> </gramGrp> <lbl expand="persona">pers.</lbl> <ref cRef="Marc." loc="8,38">Marc. 8,38</ref>; <ref cRef="1 Cor." loc="4,14">1 Cor. 4,14</ref> <lbl expand="romanae editionis">rom.</lbl>; <foreign xml:lang="gez">ዘኢየኀፍር፡ ገጸ፡ ጻድቃን፡</foreign> <ref cRef="Prov." loc="28,21">Prov. 28,21</ref>; <foreign xml:lang="gez">ኢየኀፍር፡ ገጸ፡</foreign> <foreign xml:lang="grc">ἀναιδὴς προςώπῳ</foreign> <ref cRef="Dan." loc="8,23">Dan. 8,23</ref>; <lbl expand="cum">c.</lbl> <foreign xml:lang="gez">እምነ፡</foreign> pers.: <foreign xml:lang="gez">እመ፡ ኀፈርኩ፡ እምብዙኅ፡ ሰብእ፡ ተናህየ፡ በኀቤሆሙ፡</foreign> <ref cRef="Job" loc="31,34">Job 31,34</ref>; <foreign xml:lang="gez">ኢየኀፍር፡ እምእጓለ፡ እመሕያው፡</foreign> <ref cRef="Job" loc="32,21">Job 32,21</ref>; <ref cRef="1 Reg." loc="27,12">1 Reg. 27,12</ref>; <lbl expand="cum">c.</lbl> <gramGrp> <case value="accusativus">Acc.</case> </gramGrp> Infin.: <foreign xml:lang="gez">ኀፈሩ፡ ነጊረ፡</foreign> <ref cRef="Dan. apocr." loc="1,11">Dan. apocr. 1,11</ref>; <ref cRef="Rom." loc="1,16">Rom. 1,16</ref>; <ref cRef="Hebr." loc="2,11">Hebr. 2,11</ref>; <ref cRef="Herm." loc="86">Herm. p. 86</ref>; <foreign xml:lang="gez">ስኢለ፡ አኀፍር፡</foreign> <ref cRef="Luc." loc="16,3">Luc. 16,3</ref>; <ref cRef="Sir." loc="4,26">Sir. 4,26</ref>; <ref cRef="Sir. " loc="22,25">Sir.  22,25</ref>, vel nominis verbalis, ut: <foreign xml:lang="gez">ኀፈርኩ፡ ስእለቶ፡ ለንጉሥ፡ ኀይለ፡</foreign> <ref cRef="2 Esr." loc="8,22">2 Esr. 8,22</ref>; vel <lbl expand="sequens">seq.</lbl> <gramGrp> <pos expand="Subjunctivus">Subj.</pos> </gramGrp> verbi: <foreign xml:lang="gez">አኀፍር፡ ኣንሥእ፡ ገጽየ፡</foreign> <ref cRef="2 Esr." loc="9,6">2 Esr. 9,6</ref>; <ref cRef="Job" loc="19,3">Job 19,3</ref>, vel <lbl expand="sequens">seq.</lbl> <foreign xml:lang="gez">እስመ፡</foreign>, ut: <foreign xml:lang="gez">ኢትፍርሂ፡ እስመ፡ ተኀፈርኪ፡ ወኢትኅፈሪ፡ እስመ፡ ጸአሉኪ፡</foreign> <ref cRef="Jes." loc="54,4">Jes. 54,4</ref>, vel <foreign xml:lang="gez">እንዘ፡</foreign>, ut: <foreign xml:lang="gez">ኢየኀፍር፡ ምስለ፡ ዘአልቦቱ፡ ነፍስ፡ እንዘ፡ ይትናገር፡</foreign> <ref cRef="Sap. " loc="13,17">Sap.  13,17</ref>; nec non <lbl expand="cum">c.</lbl> <foreign xml:lang="gez">በእንተ፡</foreign>, vel <foreign xml:lang="gez">እንበይነ፡</foreign> rei: <ref cRef="2 Tim." loc="1,8">2 Tim. 1,8</ref>; <ref cRef="2 Tim." loc="1,16">2 Tim. 1,16</ref>; <foreign xml:lang="gez">እንበይነ፡ ዝንቱሰ፡ ባሕቱ፡ ኢትኅፈር፡</foreign> <ref cRef="Sir." loc="42,1">Sir. 42,1</ref> <lbl expand="sequens">seq.</lbl> </sense> <sense n="2" xml:id="D2"> <cit type="translation" xml:lang="la">
                                <quote> revereri </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> vereri </quote>
                            </cit> (<foreign xml:lang="grc">ἐπαισχύνεσθαι</foreign>, <foreign xml:lang="grc">ἐντρέπεσθαι</foreign>, <foreign xml:lang="grc">εὐλαβεῖσθαι</foreign>, nonnumquam <foreign xml:lang="grc">φοβεῖσθαι</foreign>), <gramGrp> <pos expand="absolute">abs.</pos> </gramGrp>, ut: <foreign xml:lang="gez">ኢኀፈረት፡</foreign> <foreign xml:lang="grc">οὐκ ἐφοβήθη</foreign> <ref cRef="Jer." loc="3,8">Jer. 3,8</ref>; <ref cRef="Sir." loc="4,22">Sir. 4,22</ref>; <lbl expand="cum">c.</lbl> <gramGrp> <case value="accusativus">Acc.</case> </gramGrp> <lbl expand="persona">pers.</lbl> vel rei: <ref cRef="Matth." loc="21,37">Matth. 21,37</ref>; <ref cRef="Marc." loc="12,6">Marc. 12,6</ref>; <ref cRef="Hebr." loc="12,9">Hebr. 12,9</ref>; <foreign xml:lang="gez">ተአቢ፡ ኀፊሮትየ፡</foreign> <ref cRef="Ex." loc="10,3">Ex. 10,3</ref>; <foreign xml:lang="gez">ኅፈሩ፡ እንከ፡ ቃልየ፡</foreign> <ref cRef="Sir." loc="41,16">Sir. 41,16</ref>; <ref cRef="Job" loc="13,25">Job 13,25</ref> (var.); <foreign xml:lang="gez">ዘኢየኀፍር፡ ገጸ፡ ክቡር፡</foreign> <ref cRef="Job" loc="34,19">Job 34,19</ref>; vel <lbl expand="cum">c.</lbl> <foreign xml:lang="gez">እምነ፡</foreign>, ut: <foreign xml:lang="gez">ኢንኅፈር፡ እምሢበተ፡ ልሂቅ፡</foreign> <ref cRef="Sap." loc="2,10">Sap. 2,10</ref>; F.N. 43,8.</sense> <sense n="3" xml:id="D3"> <cit type="translation" xml:lang="la">
                                <quote> pudefieri </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> pudore </quote>
                            </cit> <cit type="translation" xml:lang="la">
                                <quote> affici </quote>
                            </cit> (quamquam hic significatus in stirpe <subc>III,1</subc> frequentius occurrit): <ref cRef="Jer. " loc="2,26">Jer.  2,26</ref>; <foreign xml:lang="gez">ኢኀፈረ፡ በኵሉ፡ ተኣምር፡</foreign> <ref cRef="Kuf." loc="48">Kuf. 48</ref>; <ref cRef="Sir." loc="30,13">Sir. 30,13</ref>; (1 <ref cRef="Reg." loc="27,12">Reg. 27,12</ref>).</sense> </sense>
               
