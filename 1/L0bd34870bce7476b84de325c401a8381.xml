            <entry xml:id="L0bd34870bce7476b84de325c401a8381" n="6110">
               <form>
                  <foreign xml:lang="gez">አንኰርኰረ</foreign>
               </form>
               <sense xml:lang="la" source="#dillmann" xml:id="Dmain"> V <sense n="1" xml:id="D1"> intransit. <cit type="translation" xml:lang="la">
                        <quote>volvi</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>volutari</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>rotari</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>involvi</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>advolvi</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>provolvi</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>devolvi</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>vol</quote>
                     </cit>
                     <cit type="translation" xml:lang="la">
                        <quote>v</quote>
                     </cit>
                     <cit type="translation" xml:lang="la">
                        <quote>endo amoveri</quote>
                     </cit>, <ref cRef="Hen." loc="18,15">Hen. 18,15</ref>; <ref cRef="Zach." loc="9,16">Zach. 9,16</ref>; <foreign xml:lang="gez">ልሙጽ፡ ዘያንኰረኵር፡</foreign>
                     <ref cRef="Hen." loc="22,2">Hen. 22,2</ref>; <ref cRef="Jud." loc="7,13">Jud.
                        7,13</ref>; <foreign xml:lang="gez">ያንኰረኵር፡ መንኰራኵር፡ ውስተ፡ ዐዘቅት፡</foreign>
                     <ref cRef="Koh." loc="12,6">Koh. 12,6</ref>; Infin. <foreign xml:lang="gez">አንኰርኵሮ፡</foreign>
                     <ref cRef="Koh." loc="12,6">Koh. 12,6</ref> (var.); <foreign xml:lang="gez">እኪት፡ ፍትወት፡ እምአይቴ፡ አንኰርኰርኪ፡</foreign>
                     <foreign xml:lang="grc">ἐνεκυλίσθης </foreign>
                     <ref cRef="Sir." loc="37,3">Sir. 37,3</ref>; <foreign xml:lang="gez">ድምፀ፡
                        ጸዋልዕት፡ እለ፡ ያንኰረኵሩ፡ በግርማ፡ </foreign>
                     <ref cRef="Sap." loc="17,17">Sap. 17,17</ref>; <foreign xml:lang="grc">ἀποκεκύλισται</foreign>
                     <ref cRef="Marc." loc="16,4">Marc. 16,4</ref>; <ref cRef="Luc." loc="24,2">Luc.
                        24,2</ref>.</sense>
                  <sense n="2" xml:id="D2"> transit. <cit type="translation" xml:lang="la">
                        <quote>volvere</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>volutare</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>advolvere</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>provolvere</quote>
                     </cit>
                     <cit type="translation" xml:lang="la">
                        <quote>devolvere</quote>
                     </cit>, <foreign xml:lang="gez">ዘያኰረኵር፡ እብነ፡ ላዕሌሁ፡ ያንኰረኵር፡</foreign>
                     <ref cRef="Prov. " loc="26,27">Prov. 26,27</ref>; <ref cRef="Jos." loc="10,18">Jos. 10,18</ref>; <ref cRef="1 Reg." loc="14,33">1 Reg. 14,33</ref>; <ref cRef="Matth." loc="27,60">Matth. 27,60</ref>. <sense n="a" xml:id="D2a">
                        <cit type="translation" xml:lang="la">
                           <quote>volvendo amoliri</quote>
                        </cit>
                        <ref cRef="Matth." loc="28,2">Matth. 28,2</ref>.</sense>
                     <sense n="b" xml:id="D2b">
                        <cit type="translation" xml:lang="la">
                           <quote>supervolvendo</quote>
                        </cit> vel <cit type="translation" xml:lang="la">
                           <quote>rotando deprimere</quote>
                        </cit> aliquid: <foreign xml:lang="gez">አነ፡ ኣንኰረኵረክሙ፡ በታሕቴክሙ፡ ከመ፡ ያንኰረኵር፡
                           ሰረገላ፡ ብርዐ፡</foreign>
                        <ref cRef="Am. " loc="2,13">Am. 2,13</ref>. <cb n="0839" xml:id="c0839"/>
                     </sense>
                  </sense>
               </sense>
               
