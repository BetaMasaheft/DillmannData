               <sense xml:id="Dmain" xml:lang="la" source="#dillmann"> <subc>I,1</subc> [Ges. in thes. p.1177 comparavit <foreign xml:lang="he">צָעָה</foreign> <cit type="translation" xml:lang="la"><quote> inclinavit </quote></cit> vas effundendum, at potestas radicis non in <cit type="translation" xml:lang="la"><quote> evacuando </quote></cit> sed in <cit type="translation" xml:lang="la"><quote> fundendo </quote></cit> est, sicut in <foreign xml:lang="ar">سَكَبَ</foreign> et <foreign xml:lang="ar">صَبَّ</foreign>] Imperf. <foreign xml:lang="gez">ይክዑ፡</foreign>, <gramGrp> <pos expand="Subjunctivus">Subj.</pos> </gramGrp> <foreign xml:lang="gez">ይክዐው፡</foreign> Imperat. <foreign xml:lang="gez">ከዐው፡</foreign> <cit type="translation" xml:lang="la"><quote> fundere </quote></cit>, <cit type="translation" xml:lang="la"><quote> effundere </quote></cit>, <cit type="translation" xml:lang="la"><quote> profundere </quote></cit>, <cit type="translation" xml:lang="la"><quote> superfundere </quote></cit>, <lbl expand="cum">c.</lbl> <gramGrp> <case value="accusativus">Acc.</case> </gramGrp> liquidi: <foreign xml:lang="gez">ደመና፡ ዝናመ፡ ውስተ፡ ምድር፡ ይክዑ፡</foreign> <ref cRef="Koh." loc="11,3">Koh. 11,3</ref>; <foreign xml:lang="gez">ፈለግ፡ ውስተ፡ ባሕር፡ ይክዑ፡ ማዮ፡</foreign> <ref cRef="Hen." loc="77,5">Hen. 77,5</ref>; <ref cRef="Hen." loc="77,6">Hen. 77,6</ref>; <ref cRef="Kuf. " loc="8">Kuf.  8</ref>; aquam <ref cRef="Ex." loc="4,9">Ex. 4,9</ref>; <ref cRef="1 Reg." loc="7,6">1 Reg. 7,6</ref>; oleum (c. <foreign xml:lang="gez">ላዕለ፡</foreign> vel <foreign xml:lang="gez">ዲበ፡</foreign> <cit type="translation" xml:lang="la"><quote> super </quote></cit>) <ref cRef="Ex." loc="29,7">Ex. 29,7</ref>; <ref cRef="Lev." loc="2,1">Lev. 2,1</ref>; <ref cRef="Lev." loc="2,6">Lev. 2,6</ref>; <ref cRef="Gen." loc="35,14">Gen. 35,14</ref>; jusculum <ref cRef="Jud." loc="6,20">Jud. 6,20</ref>; lacrymas <ref cRef="Hen." loc="95,1">Hen. 95,1</ref>; sanguinem <ref cRef="Deut." loc="12,16">Deut. 12,16</ref>; <ref cRef="Deut." loc="12,24">Deut. 12,24</ref>; <ref cRef="Ex." loc="29,12">Ex. 29,12</ref>; <ref cRef="Lev." loc="1,5">Lev. 1,5</ref>; <ref cRef="Lev." loc="1,11">Lev. 1,11</ref>; <ref cRef="Lev." loc="4,7">Lev. 4,7</ref>; <ref cRef="Lev." loc="4,18">Lev. 4,18</ref>; crebro: sanguinem hominum (i.e. <cit type="translation" xml:lang="la"><quote> caedem committere </quote></cit>) <ref cRef="Gen." loc="9,6">Gen. 9,6</ref>; <ref cRef="Kuf." loc="6">Kuf. 6</ref>; <ref cRef="Kuf." loc="7">Kuf. 7</ref>; <foreign xml:lang="gez">ለክዒወ፡ ደመ፡ ሰብእ፡</foreign> <ref cRef="Kuf." loc="11">Kuf. 11</ref>; <ref cRef="Sir." loc="8,16">Sir. 8,16</ref>; <ref cRef="Jes." loc="59,7">Jes. 59,7</ref>; <ref cRef="Jer." loc="22,17">Jer. 22,17</ref>; <ref cRef="Rom." loc="3,15">Rom. 3,15</ref>; <foreign xml:lang="gez">እመ፡ ኢከዐውክሙ፡ ደመ፡ ንጹሐ፡</foreign> <ref cRef="Jer." loc="7,6">Jer. 7,6</ref>; <foreign xml:lang="gez">ኢትክዐዉ፡ ደመ፡ ንጹሐ፡</foreign> <ref cRef="Jer." loc="22,3">Jer. 22,3</ref>; <foreign xml:lang="gez">ደም፡ ዘከዐዉ፡</foreign> <ref cRef="Hez." loc="36,18">Hez. 36,18</ref>; <ref cRef="Ps." loc="78,3">Ps. 78,3</ref>; semen (sperma): <foreign xml:lang="gez">ከዐወ፡ ዘርአ፡ ውስተ፡ ምድር፡</foreign> <ref cRef="Kuf." loc="41">Kuf. 41</ref>; <ref cRef="Gen." loc="38,9">Gen. 38,9</ref>; Arius <foreign xml:lang="gez">አማዑተ፡ ከርሡ፡ ከዐወ፡ ከመ፡ ፀፍዕ፡ ዘላህም፡</foreign> <ref cRef="Sx. Nah." loc="18 ">Sx. Nah. 18 </ref>Enc.; <lbl expand="translate">transl.</lbl>: <foreign xml:lang="gez">ንክዐው፡ ነገሮሙ፡</foreign> <ref cRef="Gen." loc="11,7">Gen. 11,7</ref>; <foreign xml:lang="gez">ንክዐው፡ ልሳናቲሆሙ፡</foreign> <ref cRef="Kuf." loc="10">Kuf. 10</ref>; <foreign xml:lang="gez">እክዑ፡ እምነ፡ መንፈስየ፡ ላዕለ፡ ኵሉ፡ ዘሥጋ፡</foreign> <ref cRef="Joel" loc="3,1">Joel 3,1</ref>; <foreign xml:lang="gez">ከዐወ፡ ላዕሌሁ፡ ምሕረቶ፡</foreign> <ref cRef="Gen." loc="39,21">Gen. 39,21</ref>; <foreign xml:lang="gez">ከዐው፡ መዐተከ፡ ላዕለ፡ አሕዛብ፡</foreign> <ref cRef="Jer." loc="10,25">Jer. 10,25</ref>; <ref cRef="Thren." loc="2,4">Thren. 2,4</ref>; <ref cRef="Hez." loc="20,13">Hez. 20,13</ref>; <ref cRef="Hez." loc="22,31">Hez. 22,31</ref>; <foreign xml:lang="gez">ከዐዉ፡ ልበክሙ፡ ቅድሜሁ፡</foreign> <ref cRef="Ps." loc="61,8">Ps. 61,8</ref>; <ref cRef="1 Reg." loc="1,15">1 Reg. 1,15</ref>; <foreign xml:lang="gez">ምስለ፡ ገዐር፡ ወክዒወ፡ ልብ፡</foreign> <ref cRef="Chrys. Ta." loc="31">Chrys. Ta. 31</ref>; <foreign xml:lang="gez">እክዑ፡ ቅድሜሁ፡ ስእለትየ፡</foreign> <ref cRef="Ps." loc="141,2">Ps. 141,2</ref>; <ref cRef="Ps." loc="141,101">Ps. 141,101</ref> tit.; <foreign xml:lang="gez">ከዐዉ፡ ኀይለ፡ ላውለ፡ ኢየሩሳሌም፡</foreign> <ref cRef="Jer." loc="6,6">Jer. 6,6</ref>; <lbl expand="cum">c.</lbl> <gramGrp> <case value="accusativus">Acc.</case> </gramGrp> vasis <ref cRef="Apoc." loc="16,1">Apoc. 16,1</ref> <lbl expand="sequens">seq.</lbl>; etiam intransit.: aqua <cit type="translation" xml:lang="la"><quote> superfundit </quote></cit> (se) i.e. <cit type="translation" xml:lang="la"><quote> inundat </quote></cit> <ref cRef="Hen." loc="89,4">Hen. 89,4</ref>. <cit type="translation" xml:lang="la"><quote> – Voc. Ae.: </quote></cit> <foreign xml:lang="gez">ከዐወ፡ ዘ፡ አፈሰሰ፡</foreign> </sense>
               
