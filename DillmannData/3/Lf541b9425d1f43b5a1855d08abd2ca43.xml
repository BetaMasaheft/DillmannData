            <entry xml:id="Lf541b9425d1f43b5a1855d08abd2ca43" n="2692">
               <form>
                  <foreign xml:lang="gez">ስቡሕ</foreign>
               </form>
               <sense xml:lang="la" source="#dillmann" xml:id="Dmain">
                  <gramGrp>
                     <pos expand="participium">part.</pos>
                  </gramGrp>
                  <sense n="1" xml:id="D1">
                     <cit type="translation" xml:lang="la">
                        <quote>laudatus</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>laudabilis</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>celebratus</quote>
                     </cit>: <ref cRef="Ps." loc="21,3">Ps. 21,3</ref>; <ref cRef="Ps." loc="88,8">Ps. 88,8</ref>; <ref cRef="Ex. " loc="15,11">Ex. 15,11</ref>; <ref cRef="Dan. apocr." loc="3,27">Dan. apocr. 3,27</ref>-64; <ref cRef="Hen." loc="9,4">Hen. 9,4</ref>; <gramGrp>
                        <pos expand="adverbium, adverbialiter">adv.</pos>
                     </gramGrp>
                     <foreign xml:lang="gez">ስቡሐ፡</foreign> in Cantico Mosis in Psalmorum editione
                        <ref cRef="Ludolf p." loc="324 ">Ludolf p. 324 </ref>(<ref cRef="Ex." loc="15,1">Ex. 15,1</ref>). – Sensu speciali: <foreign xml:lang="gez">ቅዱስ፡
                        ወስቡሕ፡ ለእግዚአብሔር፡</foreign>
                     <foreign xml:lang="grc">αἰνετὸς τῷ Κυρίῳ</foreign>
                     <ref cRef="Lev." loc="19,24">Lev. 19,24</ref>; <ref cRef="Lev." loc="27,21">Lev. 27,21</ref>.</sense>
                  <sense n="2" xml:id="D2">
                     <cit type="translation" xml:lang="la">
                        <quote>gloriosus</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>magnificus</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>splendidus</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>praeclarus</quote>
                     </cit>, <ref cRef="Asc. Jes. " loc="9,33">Asc. Jes. 9,33</ref>; <ref cRef="Asc. Jes." loc="10,2">Asc. Jes. 10,2</ref>; <foreign xml:lang="gez">ቃል፡ ስቡሕ፡</foreign>
                     <foreign xml:lang="grc">λόγοι ἔνδοξοι</foreign>
                     <ref cRef="Prov." loc="25,27">Prov. 25,27</ref>; <foreign xml:lang="gez">ፍሥሓ፡
                        ስብሕት፡</foreign>
                     <ref cRef="1 Petr." loc="1,8">1 Petr. 1,8</ref>; <foreign xml:lang="gez">መንክር፡
                        ዐቢይ፡ ወስቡሕ፡ </foreign>
                     <ref cRef="Hen." loc="34,1">Hen. 34,1</ref>; <foreign xml:lang="gez">ስቡሕ፡
                        ክፍልክሙ፡</foreign>
                     <ref cRef="Hen." loc="58,2">Hen. 58,2</ref>; <foreign xml:lang="gez">ምድር፡ ሐዋዝ፡
                        ወስብሕት፡</foreign>
                     <ref cRef="Hen." loc="89,40">Hen. 89,40</ref>; <foreign xml:lang="gez">ስቡሕ፡
                        ራእዮሙ፡</foreign>
                     <ref cRef="Hen." loc="24,2">Hen. 24,2</ref>; (<foreign xml:lang="gez">ዕፀው፡</foreign>) <foreign xml:lang="gez">ስቡሓን፡</foreign>
                     <ref cRef="Hen." loc="32,3">Hen. 32,3</ref>.</sense>
                  <sense n="3" xml:id="D3">
                     <cit type="translation" xml:lang="la">
                        <quote>lucidus</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>splendidus</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>clarus</quote>
                     </cit>
                     <ref cRef="Hen." loc="108,4">Hen. 108,4</ref>; <ref cRef="Hen." loc="108,5">Hen. 108,5</ref>.</sense>
               </sense>
               
