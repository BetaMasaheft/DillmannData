               <sense xml:lang="la" source="#dillmann" xml:id="Dmain"> <subc>III,1</subc> et <subc>III,2</subc> <sense n="1" xml:id="D1">
                     <cit type="translation" xml:lang="la">
                        <quote>frangi</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>perfringi</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>confringi</quote>
                     </cit>: <foreign xml:lang="gez">ወይትፈተት፡ እብነ፡ በረድ፡</foreign>
                     <foreign xml:lang="grc">διεθρύβησαν</foreign>
                     <ref cRef="Sir. " loc="43,15">Sir. 43,15</ref>.</sense>
                  <sense n="2" xml:id="D2"> in specie <cit type="translation" xml:lang="la">
                        <quote>frangi</quote>
                     </cit>, de pane distribuendo: <ref cRef="Lit." loc="173,2">Lit. 173,2</ref>;
                        <foreign xml:lang="gez">ዝውእቱ፡ ሥጋየ፡ ዘበእንቲአክሙ፡ ይትፌተት፡ ለኅድገተ፡ ኀጢአት፡</foreign>
                     <cb n="1367" xml:id="c1367"/>
                     <ref cRef="Kid." loc="9">Kid. f. 9</ref>; Lit. Joh.; <ref cRef="Phlx." loc="149">Phlx. 149</ref>; <foreign xml:lang="gez">ይትፈተት፡</foreign>
                     <ref cRef="Phlx." loc="171">Phlx. 171</ref>; M.M. f. 94.</sense>
               </sense>
               
