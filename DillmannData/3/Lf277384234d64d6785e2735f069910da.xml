         <div>
            <entry xml:id="Lf277384234d64d6785e2735f069910da" n="2768">
               <form>
                  <foreign xml:lang="gez">ስታይ</foreign>
               </form>
               <sense xml:lang="la" source="#dillmann" xml:id="Dmain">
                  <gramGrp>
                     <pos expand="Substantivum">subst.</pos>
                  </gramGrp>, plerumque <gramGrp>
                     <gen value="masculinum">m.</gen>
                  </gramGrp>
                  <sense n="1" xml:id="D1">
                     <cit type="translation" xml:lang="la">
                        <quote>potatio</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>potus</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>compotatio</quote>
                     </cit>, <foreign xml:lang="grc">μέθη</foreign>, <foreign xml:lang="grc">μέθαι</foreign>
                     <ref cRef="Hez." loc="23,32">Hez. 23,32</ref>; <ref cRef="Rom." loc="13,13">Rom. 13,13</ref>; <ref cRef="Gal." loc="5,21">Gal. 5,21</ref>; <foreign xml:lang="grc">πότοι</foreign>
                     <ref cRef="1 Petr." loc="4,3">1 Petr. 4,3</ref>; <foreign xml:lang="grc">οἶνος</foreign>
                     <foreign xml:lang="gez">ስታይ፡ ወአንስት፡ ያስሕቶ፡ ለጠቢብ፡ </foreign>
                     <ref cRef="Sir." loc="19,2">Sir. 19,2</ref>; <ref cRef="Sir." loc="34,25">Sir.
                        34,25</ref>; <foreign xml:lang="gez">ዝሙት፡ ወስታይ፡ ወስካር፡ </foreign>
                     <ref cRef="Hos." loc="4,11">Hos. 4,11</ref>; <foreign xml:lang="gez">እለ፡ ይጐነድዩ፡
                        ውስተ፡ ስታየ፡ ወይን፡ </foreign>
                     <ref cRef="Prov." loc="23,30">Prov. 23,30</ref>; <foreign xml:lang="gez">ከመ፡
                        ማሕሌት፡ ውስተ፡ ስታየ፡ ወይን፡ </foreign>
                     <foreign xml:lang="grc">ἐν συμποσίῳ οἴνου</foreign>
                     <ref cRef="Sir." loc="49,1">Sir. 49,1</ref>; <foreign xml:lang="gez">እለ፡ ይውዕሉ፡
                        በስታይ፡</foreign>
                     <ref cRef="Jes." loc="5,11">Jes. 5,11</ref>; <ref cRef="Prov." loc="24,73">Prov. 24,73</ref>; <foreign xml:lang="gez">ተውኔት፡ ወበሊዕ፡ ወስታይ፡</foreign>
                     <ref cRef="Clem." loc="17">Clem. f. 17</ref>; <ref cRef="Did." loc="4.">Did.
                        4.</ref>
                  </sense>
                  <sense n="2" xml:id="D2"> rarius <cit type="translation" xml:lang="la">
                        <quote>potio</quote>
                     </cit> (id quod bibitur), ut: <foreign xml:lang="gez">ዘይሠርባ፡ ለኀጢአት፡ ከመ፡
                        ስታይ፡</foreign>
                     <ref cRef="Job" loc="15,16">Job 15,16</ref>. <cb n="0366" xml:id="c0366"/>
                  </sense>
               </sense>
               
