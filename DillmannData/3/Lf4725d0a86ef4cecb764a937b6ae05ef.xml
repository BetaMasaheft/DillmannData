                     <foreign xml:lang="gez">ዋጠ፡</foreign>; Arabes habent <foreign xml:lang="ar">حَطَّ</foreign> (<foreign xml:lang="syr">ܢܚܶܬ</foreign>) <cit type="translation" xml:lang="la">
                     <quote>descendit</quote>
                  </cit>, II <cit type="translation" xml:lang="la">
                     <quote>comedit</quote>
                  </cit>; <foreign xml:lang="ar">غَاطَ</foreign>
                  <cit type="translation" xml:lang="la">
                     <quote>demersus fuit</quote>
                  </cit>, II <cit type="translation" xml:lang="la">
                     <quote>deglutivit</quote>
                  </cit>] <gramGrp>
                     <pos expand="Subjunctivus">Subj.</pos>
                  </gramGrp>
                  <foreign xml:lang="gez">የኀጥ፡</foreign>, <cit type="translation" xml:lang="la">
                     <quote>vorare</quote>
                  </cit>, <cit type="translation" xml:lang="la">
                     <quote>devorare</quote>
                  </cit>, <cit type="translation" xml:lang="la">
                     <quote>deglutire</quote>
                  </cit>, de hominibus, cibos: <foreign xml:lang="gez">ይፈቅድ፡ የኀጣ፡</foreign>
