               <sense xml:lang="la" source="#dillmann" xml:id="Dmain"> (amh. <foreign xml:lang="gez">አሠር፡</foreign>) <gramGrp> <pos expand="not found">subst.</pos> </gramGrp>, plerumque <gramGrp> <gen value="not found">m.</gen> </gramGrp>, <lbl expand="collatio, -is vel collectivum,&#xA;                            -e">coll.</lbl>, <sense n="1" xml:id="D1"> <cit type="translation" xml:lang="la">
