               <sense xml:lang="la" source="#dillmann" xml:id="Dmain"> <gramGrp> <pos expand="Substantivum">subst.</pos> </gramGrp>, <lbl expand="pluralis">Pl.</lbl> <foreign xml:lang="gez">ገንዶራት፡</foreign> [an a <foreign xml:lang="ar">قَمْطَرَ كَمَتْرَ</foreign>? <lbl expand="videas">vid.</lbl> <foreign xml:lang="gez">ቀምጠራ፡</foreign>, <foreign xml:lang="ar">قِمَطْرٌ</foreign> <cit type="translation" xml:lang="la">
                            <quote> vinculum pedis ligneum </quote>
                        </cit>] <sense n="1" xml:id="D1"> <cit type="translation" xml:lang="la">
                                <quote> compes </quote>
                            </cit>, <cit type="translation" xml:lang="la">
