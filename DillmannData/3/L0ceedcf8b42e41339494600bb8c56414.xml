               <sense xml:lang="la" source="#dillmann" xml:id="Dmain"> <subc>I,1</subc> [<foreign xml:lang="ar">ضَفَرَ</foreign>] Subj. <foreign xml:lang="gez">ይፅፍር፡</foreign> <cit type="translation" xml:lang="la">
                            <quote>plectere</quote>
                        </cit>, <cit type="translation" xml:lang="la">
                            <quote>contorquere</quote>
                        </cit>, <cit type="translation" xml:lang="la">
                            <quote>nectere</quote>
                        </cit>, capillos <ref cRef="Jud." loc="16,13">Jud. 16,13</ref>; <ref cRef="Jud." loc="16,14">Jud. 16,14</ref>; <foreign xml:lang="gez">ፀፈረት፡ ርእሳ፡ ወተከብሰት፡</foreign> <ref cRef="Judith" loc="16,8">Judith 16,8</ref>; <foreign xml:lang="gez">ኢታንኅ፡ ሥዕርተ፡ ርእስከ፡ ወኢትፍትል፡ ወኢትፅፍር፡</foreign> <ref cRef="Did." loc="1">Did. 1</ref>; <foreign xml:lang="gez">ፀፊሮቱ፡ ለሥዕርት፡</foreign> <ref cRef="F.N." loc=" 51">F.N.  51</ref>; coronam, sertum <ref cRef="Matth." loc="27,29">Matth. 27,29</ref>; <foreign xml:lang="gez">ለዘኮነ፡ ይፀፍር፡ ፅፍረተ፡ እግዚአብሔር፡ ይባርክ፡ ለከ፡ ወይፀፍር፡ ለከ፡ አክሊለ፡</foreign> <ref cRef="Phlx. " loc="189">Phlx.  189</ref>; limbum, funem, rete, cratem: <foreign xml:lang="gez">ኅባኔ፡</foreign>(v. <foreign xml:lang="gez">ሐብለ፡</foreign>) <foreign xml:lang="gez">ፀፈረ፡ ውስተ፡ ክሳድየ፡</foreign> <ref cRef="Job" loc="30,18">Job 30,18</ref>; <foreign xml:lang="gez">ዘይፀፍር፡ መሥገርተ፡ አበሳ፡</foreign> <ref cRef="III Sal. Mar." loc="4">III Sal. Mar. 4</ref>; <foreign xml:lang="gez">አሕባለ፡</foreign> <ref cRef="Ssal. Req.&gt;*; *Phlx." loc="48 ">Ssal. Req.&gt;*; *Phlx. 48 </ref>(vid. sub <foreign xml:lang="gez">ደጓዕሌ፡</foreign>) <foreign xml:lang="gez">ፀፊረ፡ ደጓዕሌ፡</foreign> <ref cRef="Sx. Hed." loc="16.">Sx. Hed. 16.</ref> <sense n="a" xml:id="Da"> <cit type="translation" xml:lang="la">
                                <quote>implectere</quote>
                            </cit> aliquid rei, c. Acc. et <foreign xml:lang="gez">በ፡</foreign>, <ref cRef="M.M." loc=" f. 236">M.M.  f. 236</ref>.</sense> <sense n="b" xml:id="Db"> transl.: <cb n="1338" xml:id="c1338"/> <cit type="translation" xml:lang="la">
                                <quote>nectere</quote>
                            </cit> (<cit type="translation" xml:lang="la">
                                <quote>concinnare</quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote>struere</quote>
                            </cit>)&gt; dolum: <foreign xml:lang="gez">ልሳንከ፡ ፀፈራ፡ ለሕብል፡</foreign> <ref cRef="Ps." loc="49,20">Ps. 49,20</ref>; <foreign xml:lang="gez">ጽልሑተ፡ ይፀፍራ፡ እደዊክሙ፡</foreign> <ref cRef="Ps." loc="57,2">Ps. 57,2</ref>.</sense> <sense n="c" xml:id="Dc"> <cit type="translation" xml:lang="la">
                                <quote>perplexum facere</quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote>implicare</quote>
                            </cit>, <foreign xml:lang="gez">እለ፡ ይፀፍርዋ፡ ለምክር፡</foreign> <foreign xml:lang="grc">πολύπλοκοι</foreign> <ref cRef="Job" loc="5,13">Job 5,13</ref>.</sense> </sense>
               
