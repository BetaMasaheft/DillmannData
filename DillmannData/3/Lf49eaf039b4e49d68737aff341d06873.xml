               <form>
                  <foreign xml:lang="gez">ኀሳር</foreign>
               </form>
               <sense xml:lang="la" source="#dillmann" xml:id="Dmain">
                  <gramGrp>
                     <pos expand="Substantivum">subst.</pos>
                  </gramGrp> (nonnunquam <foreign xml:lang="gez">ኅሰር፡</foreign>, ut <lbl expand="Novum Testamentum">N.T.</lbl>
                  <ref cRef="Polygl. Rom." loc="1,27">Polygl. Rom. 1,27</ref>; <ref cRef="Phil." loc="3,8">Phil. 3,8</ref>; <ref cRef="1 Cor." loc="15,43">1 Cor. 15,43</ref>) <gramGrp>
                     <gen value="masculinum">m.</gen>
                  </gramGrp> et <gramGrp>
                     <gen value="femininus">fem.</gen>
                  </gramGrp>, <lbl expand="pluralis">Pl.</lbl>
                  <foreign xml:lang="gez">ኀሳራት፡</foreign>
                  <sense n="1" xml:id="D1">
                     <cit type="translation" xml:lang="la">
                        <quote>humilitas</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>vilis conditio</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>detrimentum</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>inopia</quote>
                     </cit>: <foreign xml:lang="grc">ταπείνωσις</foreign>
                     <foreign xml:lang="gez">መዋዕለ፡ ኀሳራ፡ </foreign>
                     <ref cRef="Thren." loc="1,7">Thren. 1,7</ref>; <ref cRef="Ps." loc="89,3">Ps.
                        89,3</ref>; <foreign xml:lang="grc">πτῶμα</foreign>
                     <foreign xml:lang="gez">ጽኑሕ፡ ሎቱ፡ ኀሳር፡ ዕፁብ፡</foreign>
                     <ref cRef="Job" loc="18,12">Job 18,12</ref>; (cfr. <foreign xml:lang="gez">ለኀሳርኪ፡</foreign>
                     <foreign xml:lang="grc">εἰς πρόσκομμα σεαυτῇ </foreign>
                     <ref cRef="Jer." loc="3,3">Jer. 3,3</ref>); <foreign xml:lang="grc">ζημία</foreign>
                     <ref cRef="Phil." loc="3,8">Phil. 3,8</ref>; inopia in <bibl>
                        <ptr target="bm:Ludolf1691Commentarius"/>Lud. comm. </bibl>ad hist. <ref cRef="Aeth." loc="561,">Aeth. p. 561,</ref>
                     <ref cRef="Nr." loc="18.">Nr. 18.</ref>
                  </sense>
                  <sense n="2" xml:id="D2">
                     <cit type="translation" xml:lang="la">
                        <quote>miseria</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>aerumna</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>infelicitas</quote>
                     </cit>, <foreign xml:lang="grc">ταλαιπωρία</foreign>
                     <ref cRef="Ps." loc="68,24">Ps. 68,24</ref>; <ref cRef="Job" loc="30,3">Job
                        30,3</ref>; <ref cRef="Jes." loc="47,11">Jes. 47,11</ref>; <ref cRef="Jes." loc="59,7">Jes. 59,7</ref>; <ref cRef="Jes." loc="60,18">Jes. 60,18</ref>;
                        <ref cRef="Jer." loc="6,26">Jer. 6,26</ref>; <ref cRef="Rom." loc="3,16">Rom. 3,16</ref>.</sense>
                  <sense n="3" xml:id="D3">
                     <cit type="translation" xml:lang="la">
                        <quote>ignominia</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>probrum</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>dedecus</quote>
                     </cit>, (raro: <cit type="translation" xml:lang="la">
                        <quote>pudor</quote>
                     </cit>), <cit type="translation" xml:lang="la">
                        <quote>contumelia</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>convicium</quote>
                     </cit>: <foreign xml:lang="grc">ἀτιμία </foreign>
                     <foreign xml:lang="gez">ኀሳር፡ ወክቡር፡</foreign>
                     <ref cRef="Sir." loc="5,13">Sir. 5,13</ref>; <ref cRef="Jes." loc="10,16">Jes.
                        10,16</ref>; <ref cRef="Prov." loc="3,35">Prov. 3,35</ref>; <ref cRef="Prov." loc="9,7">Prov. 9,7</ref>; <ref cRef="Prov." loc="14,35">Prov.
                        14,35</ref>; <ref cRef="Rom." loc="9,21">Rom. 9,21</ref>; <ref cRef="Ps." loc="82,15">Ps. 82,15</ref>; <ref cRef="Job" loc="12,21">Job 12,21</ref>;
                        <ref cRef="Jer." loc="13,26">Jer. 13,26</ref>; <ref cRef="Dan." loc="12,2">Dan. 12,2</ref>; <ref cRef="1 Cor." loc="15,43">1 Cor. 15,43</ref>;
                        <foreign xml:lang="gez">በኀሳር፡</foreign>
                     <foreign xml:lang="grc">ἐν ἀτιμίᾳ</foreign>
                     <ref cRef="Judith" loc="1,11">Judith 1,11</ref>; <foreign xml:lang="grc">αἰσχύνη </foreign>
                     <ref cRef="Jer." loc="20,18">Jer. 20,18</ref>; <foreign xml:lang="grc">ἀσχημοσύνη</foreign>
                     <ref cRef="Thren." loc="1,8">Thren. 1,8</ref>; <ref cRef="Rom. " loc="1,27">Rom. 1,27</ref>; <foreign xml:lang="grc">ἐντροπή</foreign>
                     <ref cRef="Ps." loc="68,23">Ps. 68,23</ref>; <foreign xml:lang="gez">ይከውነከ፡
                        ለኀሳር፡</foreign>
                     <ref cRef="Jes." loc="16,12">Jes. 16,12</ref>; <foreign xml:lang="grc">μῶμος</foreign>
                     <ref cRef="Sir. " loc="20,24">Sir. 20,24</ref>; <foreign xml:lang="grc">χατάγνοσις</foreign>
                     <foreign xml:lang="gez">እኪት፡ ኀሳሩ፡</foreign>
                     <ref cRef="Sir." loc="5,14">Sir. 5,14</ref>; <foreign xml:lang="grc">ὄνειδος</foreign>
                     <foreign xml:lang="gez">ኀሳር፡ ለሰብእ፡ አስተሐቅሮታ፡ ለእሙ፡</foreign>
                     <ref cRef="Sir." loc="3,11">Sir. 3,11</ref>; <ref cRef="Sir." loc="41,6">Sir.
                        41,6</ref>; <foreign xml:lang="grc">ὀνειδισμοί</foreign>
                     <ref cRef="Jes." loc="47,3">Jes. 47,3</ref>; <foreign xml:lang="grc">ὕβρις </foreign>
                     <foreign xml:lang="gez">ትረውጽ፡ ኀሳሩ፡ ቅድሜሁ፡</foreign>
                     <ref cRef="Job" loc="15,26">Job 15,26</ref>; <ref cRef="Jes. " loc="10,33">Jes.
                        10,33</ref>; <ref cRef="Jes." loc="28,1">Jes. 28,1</ref>; praeterea <foreign xml:lang="gez">ኀሳር፡ ዐቢይ፡</foreign>
                     <ref cRef="Kuf." loc="12">Kuf. 12</ref>; <ref cRef="4                 Esr." loc="6,62">4 Esr. 6,62</ref>; proverbium <foreign xml:lang="gez">ትዕቢት፡ አቡሁ፡
                        ወወልዱ፡ ኀሳር፡ </foreign>
                     <cit type="translation" xml:lang="la">
                        <quote>superbia gignit ignominiam</quote>
                     </cit>, Lud.; <foreign xml:lang="gez">ኀበ፡ ሀሎ፡ ትዕቢት፡ ሀለወ፡ ኀሳር፡</foreign> Lud. ex
                        <ref cRef="Sx. Sen." loc="27 ">Sx. Sen. 27 </ref>Enc.; <foreign xml:lang="gez">ፍትወት፡</foreign> (concupiscentia) <foreign xml:lang="gez">ይእቲ፡
                        እሞን፡ ለኀሳራት፡ ወለድካማት፡</foreign> F.N. 24,1.</sense>
                  <sense n="4" xml:id="D4">
                     <cit type="translation" xml:lang="la">
                        <quote>abominandum</quote>
                     </cit>: <foreign xml:lang="gez">ኀሳሩ፡ ለሙስና፡</foreign>
                     <foreign xml:lang="grc">βδέλυγμα τῆς ἐρημώσεως</foreign>
                     <ref cRef="Matth." loc="24,15">Matth. 24,15</ref>; <foreign xml:lang="gez">ኀሳርኪ፡</foreign>
                     <foreign xml:lang="grc">τὰ προςοχθίσματά σου </foreign>
                     <ref cRef="Hez." loc="5,11">Hez. 5,11</ref>. <cb n="0592" xml:id="c0592"/>
                  </sense>
               </sense>
               
            </entry>
