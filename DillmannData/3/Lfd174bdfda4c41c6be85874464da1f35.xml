            <entry xml:id="Lfd174bdfda4c41c6be85874464da1f35" n="6432">
               <form>
                  <foreign xml:lang="gez">ተወልደ</foreign>
               </form>
               <sense xml:lang="la" source="#dillmann" xml:id="Dmain"> <subc>III,1</subc> <sense n="1" xml:id="D1"> <cit type="translation" xml:lang="la">
                                <quote> nasci </quote>
                            </cit>: <foreign xml:lang="gez">ተወሊዶ፡</foreign> <ref cRef="Matth." loc="2,1">Matth. 2,1</ref>; <ref cRef="Matth." loc="2,4">Matth. 2,4</ref>; <ref cRef="Matth. " loc="26,24">Matth.  26,24</ref>; <ref cRef="Joh." loc="9,2">Joh. 9,2</ref>; <ref cRef="Joh." loc="9,19">Joh. 9,19</ref>; <ref cRef="Hebr." loc="11,23">Hebr. 11,23</ref>; <ref cRef="Jer." loc="16,3">Jer. 16,3</ref>; <ref cRef="Job" loc="39,1">Job 39,1</ref>; <ref cRef="Hez." loc="28,13">Hez. 28,13</ref>; <foreign xml:lang="gez">ወልድ፡ ጻድቅ፡ ይትወለድ፡ ለሕይወት፡</foreign> <ref cRef="Prov." loc="11,19">Prov. 11,19</ref>; <foreign xml:lang="gez">መዋዕልየ፡ ዘእምአመ፡ ተወለድኩ፡</foreign> <ref cRef="Judith" loc="12,18">Judith 12,18</ref>; <foreign xml:lang="gez">ዕለተ፡ ተወለድከ፡</foreign> (<cit type="translation" xml:lang="la">
                                <quote> dies natalis tuus </quote>
                            </cit>) <ref cRef="Sir." loc="23,14">Sir. 23,14</ref>; <ref cRef="Jer. " loc="20,14">Jer.  20,14</ref>; <foreign xml:lang="gez">ቀዳሚ፡ ተወልዶ፡</foreign> vel <foreign xml:lang="gez">ቀዲመ፡ ተወልዶ፡</foreign> <cit type="translation" xml:lang="la">
                                <quote> primogenitura </quote>
                            </cit> <ref cRef="Kuf." loc="24">Kuf. 24</ref>; <foreign xml:lang="gez">ዘቀደመ፡ ተወልዶ፡</foreign> <cit type="translation" xml:lang="la">
                                <quote> primogenitus </quote>
                            </cit> <ref cRef="Kuf." loc="26">Kuf. 26</ref>; <lbl expand="construitur cum">c.c.</lbl> <foreign xml:lang="gez">ለ፡</foreign> patris, <ref cRef="Gen." loc="21,5">Gen. 21,5</ref>; <ref cRef="Gen." loc="24,24">Gen. 24,24</ref>; <ref cRef="1 Par." loc="2,3">1 Par. 2,3</ref>; <ref cRef="Hen." loc="6,1">Hen. 6,1</ref>; etiam: <foreign xml:lang="gez">እስመ፡ ሕፃን፡ ተወልደ፡ ለነ፡</foreign> <ref cRef="Jes." loc="9,6">Jes. 9,6</ref>; <lbl expand="cum">c.</lbl> <foreign xml:lang="gez">እምነ፡</foreign> matris: <ref cRef="1 Par." loc="2,3">1 Par. 2,3</ref>; <ref cRef="4 Esr." loc="3,12">4 Esr. 3,12</ref>; <ref cRef="Luc." loc="1,35">Luc. 1,35</ref>; <foreign xml:lang="gez">መዋቲ፡ ዘይትወለድ፡ እምአንስት፡</foreign> <ref cRef="Job" loc="14,1">Job 14,1</ref>; <ref cRef="Job" loc="15,14">Job 15,14</ref>; <foreign xml:lang="gez">ተወልዱ፡ እምከርሠ፡ እሞሙ፡</foreign> <ref cRef="Matth." loc="19,12">Matth. 19,12</ref>; <foreign xml:lang="gez">ዘተወልደ፡ እምሥጋ፡</foreign> <ref cRef="Joh." loc="3,6">Joh. 3,6</ref>; et viri: <foreign xml:lang="gez">ወእምኔሁ፡ ኵሉ፡ ጻድቃን፡ ተወልዱ፡</foreign> <ref cRef="4 Esr." loc="1,13">4 Esr. 1,13</ref> <lbl expand="varia lectio">var.</lbl>; <ref cRef="4 Esr." loc="4,64">4 Esr. 4,64</ref>; et causae: <cb n="0886" xml:id="c0886"/> <foreign xml:lang="gez">ኢተወለድነ፡ እምዝሙት፡</foreign> Joh.8,41; <lbl expand="cum">c.</lbl> <foreign xml:lang="gez">እምላዕለ፡</foreign> matris: <ref cRef="Matth." loc="1,16">Matth. 1,16</ref>; <ref cRef="Matth." loc="1,20">Matth. 1,20</ref>; vel viri <ref cRef="Rom. " loc="9,5">Rom.  9,5</ref>; <ref cRef="4 Esr." loc="4,15">4 Esr. 4,15</ref>; vel <lbl expand="cum">c.</lbl> <foreign xml:lang="gez">በላዕለ፡</foreign> mulieris, <ref cRef="Hen. " loc="15,5">Hen.  15,5</ref>. <sense n="a" xml:id="D1a"> <cit type="translation" xml:lang="la">
                                    <quote> natum esse e Deo </quote>
                                </cit>: <foreign xml:lang="gez">እምእግዚአብሔር፡ ተወልዱ፡</foreign> <ref cRef="Joh. " loc="1,13">Joh.  1,13</ref>; <ref cRef="Joh." loc="3,6">Joh. 3,6</ref>; <ref cRef="1 Joh." loc="3,9">1 Joh. 3,9</ref>.</sense> <sense n="b" xml:id="D1b"> <cit type="translation" xml:lang="la">
                                    <quote> regenerari </quote>
                                </cit>: <ref cRef="1 Petr." loc="1,23">1 Petr. 1,23</ref>; <foreign xml:lang="gez">ዘኢተወልደ፡ ዳግመ፡</foreign> <ref cRef="Joh." loc="3,3">Joh. 3,3</ref>; <ref cRef="Joh." loc="3,7">Joh. 3,7</ref>; <foreign xml:lang="gez">እለ፡ ተወልዱ፡ ዳግመ፡ በቅድስት፡ ጥምቀት፡</foreign> <ref cRef="Clem." loc="95.">Clem. f. 95.</ref> </sense> <sense n="c" xml:id="D1c"> <cit type="translation" xml:lang="la">
                                    <quote> gigni </quote>
                                </cit>, de Filio Dei: <foreign xml:lang="gez">ቃል፡</foreign> (<foreign xml:lang="grc">ὁ Λόγος</foreign>) <foreign xml:lang="gez">ዘበአማን፡ ተወልደ፡ ወኢተገብረ፡</foreign> <ref cRef="Cyr." loc="119.">Cyr. f. 119.</ref> </sense> </sense> <sense n="2" xml:id="D2"> de rebus: <cit type="translation" xml:lang="la">
                                <quote> nasci </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> gigni </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> oriri </quote>
                            </cit>, <ref cRef="4 Esr." loc="2,38">4 Esr. 2,38</ref>; <foreign xml:lang="gez">ይትወለድ፡ ብሩር፡ እምጸበለ፡ ምድር፡</foreign> <ref cRef="Hen." loc="65,7">Hen. 65,7</ref>; <foreign xml:lang="gez">በተካሕዶ፡ ይትወለድ፡ ለነ፡ ባቍዕ፡</foreign> <ref cRef="Cyr. c. Pall. " loc="68">Cyr. c. Pall.  f. 68</ref>; <foreign xml:lang="gez">እምስካር፡ ይትወለድ፡ ምርዐት፡ ወዝሙት፡</foreign> <ref cRef="Clem." loc="139">Clem. f. 139</ref>; <foreign xml:lang="gez">እምልብ፡ ይትወለድ፡ ኵሉ፡ ኀጢአት፡</foreign> <ref cRef="F.M." loc=" 2,11">F.M.  2,11</ref>; <foreign xml:lang="gez">እምዛቲ፡ ግብር፡ ይትወለድ፡ ቀትል፡</foreign> F.M. 24,1; <ref cRef="Sx. Masc." loc="11 Enc.">Sx. Masc. 11 Enc.</ref>; <foreign xml:lang="gez">ሰዐል፡ ይትወለድ፡ እምነ፡ ቍር፡</foreign> Lud. ex <lbl expand="auctore, auctoritate">auct.</lbl> Gregorii.</sense> <sense n="3" xml:id="D3"> <cit type="translation" xml:lang="la">
                                <quote> prolem nancisci </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> liberos suscipere </quote>
                            </cit>, <ref cRef="Gen." loc="30,3">Gen. 30,3</ref>.</sense> </sense>
               
