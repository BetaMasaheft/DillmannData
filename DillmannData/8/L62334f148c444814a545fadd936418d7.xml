         <div>
            <entry xml:id="L62334f148c444814a545fadd936418d7" n="9392">
               <form>
                  <foreign xml:lang="gez">ጸንዐ</foreign>
               </form>
               <sense xml:lang="la" source="#dillmann" xml:id="Dmain"> <subc>I,2</subc> <cit type="translation" xml:lang="la">
                     <quote>firmum facere</quote>
                  </cit>, <cit type="translation" xml:lang="la">
                     <quote>firmare</quote>
                  </cit>, <sense n="a" xml:id="Da">
                     <cit type="translation" xml:lang="la">
                        <quote>stabilire</quote>
                     </cit>: <foreign xml:lang="gez">ጸንዖ፡</foreign> (v. <foreign xml:lang="gez">አጽንዖ፡</foreign>) <foreign xml:lang="gez">በኀጺን፡</foreign>
                     <foreign xml:lang="grc">ἀσφαλισάμενος</foreign>
                     <ref cRef="Sap." loc="13,15">Sap. 13,15</ref>; <foreign xml:lang="gez">ዘይጼንዑ፡
                        መፍቅዶሙ፡ ለብሔር፡</foreign>
                     <foreign xml:lang="grc">στηρίσουσι</foreign>
                     <ref cRef="Sir. " loc="38,34">Sir. 38,34</ref>; <foreign xml:lang="gez">ጸንዖሙ፡
                        በምሕረቱ፡</foreign>
                     <foreign xml:lang="grc">ἐστηρίσατο</foreign>
                     <ref cRef="Jes." loc="59,16">Jes. 59,16</ref>.</sense>
                  <sense n="b" xml:id="Db">
                     <cit type="translation" xml:lang="la">
                        <quote>sustentare</quote>
                     </cit>, <ref cRef="Jud." loc="5,11">Jud. 5,11</ref>
                     <lbl expand="varia lectio">var.</lbl>; <foreign xml:lang="gez">ይጼንዕዋ፡ ለምክርኪ፡ </foreign>
                     <ref cRef="Hez." loc="27,9">Hez. 27,9</ref>
                     <lbl expand="versio antiqua">vers. ant.</lbl>
                  </sense>
                  <sense n="c" xml:id="Dc">
                     <cit type="translation" xml:lang="la">
                        <quote>solidare</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>reficere</quote>
                     </cit>, aedificia: <foreign xml:lang="gez">አንትሙ፡ ትጼንዑ፡ አብያቲክሙ፡</foreign> (v.
                        <foreign xml:lang="gez">ታጸንዑ፡</foreign>) <ref cRef="Hagg." loc="1,9">Hagg.
                        1,9</ref>; <foreign xml:lang="gez">በዘይጼንዑ፡ ቤዴቀ፡</foreign>
                     <ref cRef="4 Reg." loc="22,5">4 Reg. 22,5</ref>; <ref cRef="4 Reg." loc="22,6">4 Reg. 22,6</ref>.</sense>
                  <sense n="d" xml:id="Dd">
                     <cit type="translation" xml:lang="la">
                        <quote>confortare</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>roborare</quote>
                     </cit>: <ref cRef="Jud." loc="5,12">Jud. 5,12</ref>; <ref cRef="Act." loc="18,23">Act. 18,23</ref> rom; <foreign xml:lang="gez">ዘደክመ፡
                        ኢትጼንዑ፡</foreign>
                     <ref cRef="Hez." loc="34,4">Hez. 34,4</ref>; <ref cRef="Hez." loc="34,16">Hez.
                        34,16</ref>; <ref cRef="Luc." loc="22,32">Luc. 22,32</ref>; <ref cRef="Luc. " loc="22,43">Luc. 22,43</ref>.</sense>
               </sense>
               
