               <sense xml:lang="la" source="#dillmann" xml:id="Dmain"> <subc>II,1</subc> <cit type="translation" xml:lang="la">
                     <quote>liberare</quote>
                  </cit>, <cit type="translation" xml:lang="la">
                     <quote>servare</quote>
                  </cit>, <cit type="translation" xml:lang="la">
                     <quote>eripere</quote>
                  </cit>, <cit type="translation" xml:lang="la">
                     <quote>salvare</quote>
                  </cit>: <foreign xml:lang="gez">ያድኅን፡ በዕለተ፡ ምንዳቤ፡</foreign>
                  <ref cRef="Sir." loc="2,11">Sir. 2,11</ref>; <foreign xml:lang="gez">ስእኑ፡
                     አድኅኖ፡</foreign>
                  <ref cRef="Job" loc="4,20">Job 4,20</ref>; <lbl expand="cum">c.</lbl>
                  <gramGrp>
                     <case value="accusativus">Acc.</case>
                  </gramGrp>: <ref cRef="Jud." loc="10,14">Jud. 10,14</ref>; <ref cRef="Mich." loc="4,10">Mich. 4,10</ref>; <ref cRef="Jer. " loc="1,8">Jer. 1,8</ref>; <ref cRef="Jer." loc="2,27">Jer. 2,27</ref>; <ref cRef="Jer." loc="2,28">Jer.
                     2,28</ref>; <ref cRef="Koh." loc="9,15">Koh. 9,15</ref>; <ref cRef="Sir." loc="29,17">Sir. 29,17</ref>; <ref cRef="Sir." loc="31,13">Sir. 31,13</ref>;
                     <foreign xml:lang="gez">አድኅኑ፡ ግፉዐ፡</foreign>
                  <ref cRef="Jes." loc="1,17">Jes. 1,17</ref>; <ref cRef="Jes." loc="19,20">Jes.
                     19,20</ref>; <ref cRef="Jes." loc="36,14">Jes. 36,14</ref>; <ref cRef="Jes." loc="36,15">Jes. 36,15</ref>; <foreign xml:lang="gez">ያድኀኖ፡ እግዚአብሔር፡ ለእስራኤል፡
                     መድኀኒተ፡ ዘለዓለም፡</foreign>
                  <ref cRef="Jes." loc="45,17">Jes. 45,17</ref>; <ref cRef="Ps." loc="53,1">Ps.
                     53,1</ref>; <ref cRef="Ps." loc="53,4">Ps. 53,4</ref>; <ref cRef="Matth." loc="8,25">Matth. 8,25</ref>; <ref cRef="Matth." loc="14,30">Matth.
                  14,30</ref>; <foreign xml:lang="gez">አድኅን፡ ርእሰከ፡ </foreign>
                  <ref cRef="Matth." loc="27,40">Matth. 27,40</ref>; <ref cRef="Gen." loc="19,19">Gen. 19,19</ref>; <foreign xml:lang="gez">ነፍሰከ፡</foreign>
                  <ref cRef="Gen." loc="19,17">Gen. 19,17</ref>; <ref cRef="Jer." loc="31,6">Jer.
                     31,6</ref>; <ref cRef="Hez." loc="3,19">Hez. 3,19</ref>; <ref cRef="Hez." loc="3,21">Hez. 3,21</ref>; <ref cRef="Hez." loc="33,9">Hez. 33,9</ref>; <lbl expand="cum">c.</lbl>
                  <foreign xml:lang="gez">እምነ፡</foreign>, <foreign xml:lang="gez">እምውስተ፡</foreign>
                  <ref cRef="Ps." loc="17,20">Ps. 17,20</ref>; <ref cRef="Ps." loc="33,5">Ps.
                     33,5</ref>; <ref cRef="Ps." loc="53,7">Ps. 53,7</ref>; <ref cRef="Ps." loc="58,1">Ps. 58,1</ref>; <ref cRef="Ps." loc="58,2">Ps. 58,2</ref>; <ref cRef="Ps." loc="68,18">Ps. 68,18</ref>; <ref cRef="Ex." loc="14,30">Ex.
                     14,30</ref>; <ref cRef="Ex." loc="18,9">Ex. 18,9</ref>; <ref cRef="Ex." loc="18,10">Ex. 18,10</ref>; <foreign xml:lang="gez">አድኀነኒ፡ እምነ፡ ማየ፡
                     አይኅ፡</foreign>
                  <ref cRef="Kuf." loc="7">Kuf. 7</ref>; <ref cRef="Job" loc="5,19">Job 5,19</ref>;
                     <ref cRef="Job" loc="29,2">Job 29,2</ref>; <ref cRef="Job" loc="33,30">Job
                     33,30</ref>; <ref cRef="Jer." loc="20,13">Jer. 20,13</ref>; <ref cRef="Sap." loc="16,8">Sap. 16,8</ref>; <ref cRef="Joh." loc="12,27">Joh. 12,27</ref>; <ref cRef="Hebr." loc="5,7">Hebr. 5,7</ref>; <foreign xml:lang="gez">አድኅኖ፡</foreign>
                  vel <foreign xml:lang="gez">አድኅኖት፡</foreign>
                  <cit type="translation" xml:lang="la">
                     <quote>salus</quote>
                  </cit>: <ref cRef="Ps." loc="3,8">Ps. 3,8</ref>; <ref cRef="Ps." loc="9,15">Ps.
                     9,15</ref>; <ref cRef="Ps." loc="97,5">Ps. 97,5</ref>. <sense n="a" xml:id="Da">
                     <cit type="translation" xml:lang="la">
                        <quote>libertati restituere</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>redimere</quote>
                     </cit>, <ref cRef="Ex." loc="21,8">Ex. 21,8</ref>; <ref cRef="Jes." loc="41,14">Jes. 41,14</ref>; <ref cRef="Jes." loc="43,1">Jes. 43,1</ref>; <ref cRef="Jes. " loc="44,23">Jes. 44,23</ref>.</sense>
                  <sense n="b" xml:id="Db">
                     <cit type="translation" xml:lang="la">
                        <quote>servare</quote>
                     </cit> vel <cit type="translation" xml:lang="la">
                        <quote>reservare</quote>
                     </cit>
                     <ref cRef="4 Esr." loc="9,24">4 Esr. 9,24</ref>.</sense>
                  <sense n="c" xml:id="Dc"> sensu theologico: <cit type="translation" xml:lang="la">
                        <quote>salvare</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>salvum reddere</quote>
                     </cit>, <ref cRef="Matth." loc="18,11">Matth. 18,11</ref>; <ref cRef="Joh." loc="12,47">Joh. 12,47</ref>; <ref cRef="Rom." loc="11,14">Rom. 11,14</ref>;
                        <ref cRef="1 Cor." loc="1,21">1 Cor. 1,21</ref>; <ref cRef="1 Cor." loc="7,16">1 Cor. 7,16</ref>; <ref cRef="1 Cor." loc="9,22">1 Cor.
                        9,22</ref> al.; <foreign xml:lang="gez">እምኀጣውእ፡</foreign>
                     <ref cRef="Matth." loc="1,21">Matth. 1,21</ref>.</sense>
                  <sense n="d" xml:id="Dd">
                     <cit type="translation" xml:lang="la">
                        <quote>culpa liberare</quote>
                     </cit> vel <cit type="translation" xml:lang="la">
                        <quote>excusare</quote>
                     </cit>, Lud. e Vit. Barbarae.</sense>
                  <sense n="e" xml:id="De"> sensu forensi: fide interposita liberare aliquem i.e. <cit type="translation" xml:lang="la">
                        <quote>vadem fieri</quote>
                     </cit> vel <cit type="translation" xml:lang="la">
                        <quote>vadimonium facere</quote>
                     </cit> (<lbl expand="videas">vid.</lbl>
                     <foreign xml:lang="gez">ተድኅነ፡</foreign> et <foreign xml:lang="gez">መድኅን፡</foreign>): <foreign xml:lang="gez">በእንተ፡ ሕቢት፡ ወአድኅኖ፤ ሎሙ፡ ፪ ፍናዋት፤
                        አድኅኖሰ፡ ዘነፍስ፡ ውእቱ፡ ወተሐብዮ፡ ዘንዋይ፡</foreign> F.N. 27.</sense>
               </sense>
               
