            <entry xml:id="L60759b3f71b844c7af98278cb68aabea" n="4986">
               <form>
                  <foreign xml:lang="gez">ንኅለ</foreign>
               <rs type="root"/>
                    </form>
               <sense xml:lang="la" source="#dillmann" xml:id="Dmain">, rarius <foreign xml:lang="gez">ነኀለ፡</foreign> (etiam per <foreign xml:lang="gez">ህ</foreign> et <foreign xml:lang="gez">ሕ</foreign>) <subc>I,1</subc> [significatione respondet <foreign xml:lang="ar">هار</foreign>; etymo ad radices <foreign xml:lang="he">חָלַל</foreign> <foreign xml:lang="syr">ܚܰܠ</foreign> <foreign xml:lang="ar">خَلَّ حَلَّ</foreign>, quibus significatus <cit type="translation" xml:lang="la">
                            <quote> perforandi </quote>
                        </cit> et <cit type="translation" xml:lang="la">
                            <quote> solvendi </quote>
                        </cit> inest, referendum videtur] <gramGrp> <pos expand="Subjunctivus">Subj.</pos> </gramGrp> <foreign xml:lang="gez">ይንኀል፡</foreign>, <sense n="1" xml:id="D1"> <cit type="translation" xml:lang="la">
                                <quote> corruere </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> collabi </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> dirui </quote>
                            </cit>, de monte: <foreign xml:lang="gez">ደብር፡ ዘንኅለ፡ ወወድቀ፡</foreign> <foreign xml:lang="grc">ὄρος πίπτον διαπεσεῖται</foreign> <ref cRef="Job" loc="14,18">Job 14,18</ref>; de aedificiis, muris, al.: <foreign xml:lang="gez">ከመ፡ ሀገር፡ ዘንኅለ፡ ጥቅማ፡</foreign> <ref cRef="Prov." loc="25,28">Prov. 25,28</ref>; <foreign xml:lang="gez">ንኅለ፡ ላዕሌሁ፡ አረፍተ፡ ጸወንከ፡</foreign> <ref cRef="Jes." loc="25,12">Jes. 25,12</ref>; <foreign xml:lang="gez">ንኅለ፡</foreign> (v. <foreign xml:lang="gez">ነኀለ፡</foreign>) <foreign xml:lang="gez">አረፋቲሃ፡</foreign> <ref cRef="Jer. " loc="27,15">Jer.  27,15</ref>; <ref cRef="Jer." loc="40,4">Jer. 40,4</ref>; <foreign xml:lang="gez">ንኅለ፡ ጠፈር፡ ላዕሌሁ፡</foreign> <ref cRef="Phlx." loc="81">Phlx. 81</ref>; <foreign xml:lang="gez">እምቅድመ፡ ይንኀል፡ ዝቅጽር፡ ሣልሳይ፡</foreign> <ref cRef="Jsp." loc="360">Jsp. p. 360</ref>; <ref cRef="Jsp. " loc="333.">Jsp.  p. 333.</ref> </sense> <sense n="2" xml:id="D2"> <cit type="translation" xml:lang="la">
                                <quote> ruinis stratum </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> devastatum esse, </quote>
                            </cit>de urbe: <foreign xml:lang="gez">አህጉሪሃ፡ ይንኅል፡</foreign> <ref cRef="Hez." loc="29,12">Hez. 29,12</ref> (<foreign xml:lang="grc">ἠρημωμέμων</foreign>); <foreign xml:lang="gez">ንኅለ፡ ሀገረ፡ መቅደስከ፡</foreign> (aliâs <foreign xml:lang="gez">መዝበረ፡</foreign>) <ref cRef="Jes." loc="64,10">Jes. 64,10</ref>.</sense> </sense>
               
