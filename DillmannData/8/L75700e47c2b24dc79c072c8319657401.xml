               </form>
               <sense xml:lang="la" source="#dillmann" xml:id="Dmain"> <subc>III,2</subc> <cit type="translation" xml:lang="la">
                     <quote>exspectare</quote>
                  </cit>, <cit type="translation" xml:lang="la">
                     <quote>sperare</quote>
                  </cit>, (<cit type="translation" xml:lang="la">
                     <quote>confidere</quote>
                  </cit>), <lbl expand="construitur cum">c.c.</lbl>
                  <gramGrp>
                     <case value="accusativus">Acc.</case>
                  </gramGrp> rei vel <lbl expand="persona">pers.</lbl>: <foreign xml:lang="gez">እሴፎ፡
                     ሕይወትየ፡</foreign>
                  <ref cRef="Job" loc="2,9">Job 2,9</ref>; <ref cRef="Job" loc="7,2">Job 7,2</ref>;
                     <foreign xml:lang="gez">ምድር፡ እንተ፡ ትሴፎ፡ ዝናመ፡</foreign>
                  <ref cRef="Job" loc="29,23">Job 29,23</ref>; <foreign xml:lang="gez">ተሰፎኩ፡
                     ብርሃነ፡</foreign>
                  <ref cRef="Job" loc="30,26">Job 30,26</ref>; <foreign xml:lang="gez">ኪያየ፡ ይሴፈዉ፡
                     አሕዛብ፡ </foreign>
                  <ref cRef="Jes." loc="51,5">Jes. 51,5</ref>; <ref cRef="Jes." loc="60,9">Jes.
                     60,9</ref>; <foreign xml:lang="gez">ይሴፈዉ፡ ሣህለከ፡</foreign>
                  <ref cRef="Jes. " loc="38,18">Jes. 38,18</ref>; <foreign xml:lang="gez">ተሰፈውነ፡
                     ሰላመ፡</foreign>
                  <ref cRef="Jer." loc="8,15">Jer. 8,15</ref>; <ref cRef="Jer." loc="14,19">Jer.
                     14,19</ref>; <foreign xml:lang="gez">ተሰፎናከ፡</foreign>
                  <ref cRef="Jer." loc="14,22">Jer. 14,22</ref>; <ref cRef="Ps." loc="24,2">Ps.
                     24,2</ref>; <ref cRef="Ps." loc="24,5">Ps. 24,5</ref>; <ref cRef="Ps." loc="70,6">Ps. 70,6</ref>; <ref cRef="Ps." loc="70,15">Ps. 70,15</ref>; <ref cRef="Ps." loc="144,16">Ps. 144,16</ref>; <ref cRef="Sir." loc="2,9">Sir.
                     2,9</ref>; <ref cRef="Sir." loc="34,7">Sir. 34,7</ref>; <ref cRef="Sir." loc="34,15">Sir. 34,15</ref>; <foreign xml:lang="gez">ተሰፈው፡ ጻማከ፡ </foreign>
                  <cit type="translation" xml:lang="la">
                     <quote>spera</quote>
                  </cit> (laborem i.e.) <cit type="translation" xml:lang="la">
                     <quote>fructum laboris tuum</quote>
                  </cit>
                  <foreign xml:lang="grc">ἔμμενε τῷ πόνῳ σου</foreign>
                  <ref cRef="Sir." loc="11,21">Sir. 11,21</ref>; <foreign xml:lang="gez">ተሰፊዎሙ፡
                     ንዋየ፡</foreign>
                  <ref cRef="Hebr." loc="11,1">Hebr. 11,1</ref>; <ref cRef="Judae" loc="21">Judae
                     21</ref>; <ref cRef="Joh." loc="5,45">Joh. 5,45</ref>; <ref cRef="Rom." loc="8,24">Rom. 8,24</ref>; <ref cRef="Rom." loc="8,25">Rom. 8,25</ref>;
                     <foreign xml:lang="gez">በተሰፍዎ፡ ከንቱ፡</foreign>
                  <foreign xml:lang="grc">ἐν κενῇ ἐλπίδι </foreign>
                  <ref cRef="Job" loc="7,6">Job 7,6</ref> (cfr. <ref cRef="Jes." loc="29,8">Jes.
                     29,8</ref>); <lbl expand="cum">c.</lbl>
                  <foreign xml:lang="gez">በ፡</foreign>
                  <ref cRef="Ps." loc="117,9">Ps. 117,9</ref>; <lbl expand="cum">c.</lbl>
                  <foreign xml:lang="gez">ኀበ፡</foreign>
                  <ref cRef="Ps." loc="103,28">Ps. 103,28</ref>; <gramGrp>
                     <pos expand="absolute">absol.</pos>
                  </gramGrp>
                  <ref cRef="Hen." loc="96,1">Hen. 96,1</ref>; <foreign xml:lang="gez">ከንቱ፡</foreign> (v. <foreign xml:lang="gez">ከንቶ፡</foreign>) <foreign xml:lang="gez">ተሰፈወ፡</foreign>
                  <ref cRef="Jes." loc="29,8">Jes. 29,8</ref>. Deinde verba subjunguntur, vel ope
                  Infin. in <gramGrp>
                     <case value="accusativus">Acc.</case>
                  </gramGrp>: <foreign xml:lang="gez">ኢይሴፎ፡ ሐይወ፡</foreign>
                  <ref cRef="Job" loc="15,31">Job 15,31</ref>; <ref cRef="Job" loc="24,23">Job
                     24,23</ref>, vel <foreign xml:lang="gez">ከመ፡</foreign> seq. <ref cRef="Imperf. Hen." loc="98,10">Imperf. Hen. 98,10</ref> (<ref cRef="Hen." loc="46,6">Hen. 46,6</ref>); vel <foreign xml:lang="gez">ከመ፡</foreign> seq. <gramGrp>
                     <pos expand="Subjunctivus">Subj.</pos>
                  </gramGrp>
                  <ref cRef="Hen." loc="10,10">Hen. 10,10</ref>; vel (omisso <foreign xml:lang="gez">ከመ፡</foreign>) <lbl expand="sequens">seq.</lbl>
                  <gramGrp>
                     <pos expand="Subjunctivus">Subj.</pos>
                  </gramGrp> nudo: <foreign xml:lang="gez">ኢይሴፈዉ፡ እንከ፡ ይርአዩኒ፡</foreign>
                  <ref cRef="Tob." loc="10,7">Tob. 10,7</ref>; <foreign xml:lang="gez">አዕይንተ፡ እስራኤል፡
                     ይሴፈዋከ፡ ታይዶዖሙ፡ </foreign>
                  <ref cRef="3 Reg." loc="1,20">3 Reg. 1,20</ref>; <ref cRef="Hen." loc="103,11">Hen. 103,11</ref>; <ref cRef="Act." loc="3,5">Act. 3,5</ref>; <ref cRef="Rom." loc="15,24">Rom. 15,24</ref>; <ref cRef="Phil." loc="2,23">Phil. 2,23</ref>,
                  vel <lbl expand="sequens">seq.</lbl> Imperf. nudo: <foreign xml:lang="gez">ተሰፈወ፡
                     ዳኅነ፡ ይነብር፡</foreign>
                  <lbl expand="vel">v.</lbl>
                  <foreign xml:lang="gez">ይንበር፡</foreign>
                  <ref cRef="Job" loc="15,21">Job 15,21</ref>; <foreign xml:lang="gez">ተሰፈወ፡
                     ያዐርፍ፡</foreign>
                  <lbl expand="vel">v.</lbl>
                  <foreign xml:lang="gez">ያዕርፍ፡</foreign>
                  <ref cRef="Sir." loc="11,19">Sir. 11,19</ref>. </sense>
               
