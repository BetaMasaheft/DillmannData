                  <foreign xml:lang="gez">ቀብጸ</foreign>
               <rs type="root"/>
                    </form>
               <sense xml:lang="la" source="#dillmann" xml:id="Dmain"> <subc>I,1</subc> [primitus significat <cit type="translation" xml:lang="la">
                            <quote> comprehendere </quote>
                        </cit>, <cit type="translation" xml:lang="la">
                            <quote> contrahere </quote>
                        </cit>, <lbl expand="conferas">cfr.</lbl> <foreign xml:lang="he">קָבַץ קָפַץ קָפַד</foreign> et radices aliarum linguarum cognatas; deinde <cb n="0439" xml:id="c0439"/> intr. <cit type="translation" xml:lang="la">
                            <quote> se contrahere </quote>
                        </cit>] <gramGrp> <pos expand="Subjunctivus">Subj.</pos> </gramGrp> <foreign xml:lang="gez">ይቅበጽ፡</foreign> et <foreign xml:lang="gez">ይቅብጽ፡</foreign>, <sense n="1" xml:id="D1"> <cit type="translation" xml:lang="la">
                                <quote> concidere </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> corrugari </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> deficere </quote>
                            </cit>, de utero: <foreign xml:lang="gez">ቀብጸ፡ ማሕፀና፡</foreign> Ssal. Req.; de animo: <foreign xml:lang="gez">ቀብጸተኒ፡ ነፍስየ፡</foreign> <foreign xml:lang="grc">ἠκηδίασεν</foreign> (suff. <gramGrp> <case value="dativus">Dat.</case> </gramGrp>) <ref cRef="Ps." loc="142,4">Ps. 142,4</ref>; <ref cRef="Ps." loc="60,2">Ps. 60,2</ref>.</sense> <sense n="2" xml:id="D2"> <cit type="translation" xml:lang="la">
                                <quote> abjicere </quote>
                            </cit> vel <cit type="translation" xml:lang="la">
                                <quote> demittere </quote>
                            </cit> vel <cit type="translation" xml:lang="la">
                                <quote> despondere animum </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> spem </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> desperare </quote>
                            </cit>: <foreign xml:lang="gez">ተዐገሡ፡ ወኢትቅበጹ፡</foreign> <ref cRef="Herm." loc="5">Herm. p. 5</ref>; <ref cRef="Herm." loc="20">Herm. p. 20</ref>; <ref cRef="Clem." loc="221">Clem. f. 221</ref>; <ref cRef="Rom." loc="4,20">Rom. 4,20</ref>; <foreign xml:lang="gez">ኢትኅድገነ፡ ንቅብጽ፡</foreign> <ref cRef="Kedr f." loc="34">Kedr f. 34</ref>; <ref cRef="Kedr f." loc="50">Kedr f. 50</ref>; saepe cum <gramGrp> <case value="accusativus">Acc.</case> </gramGrp> <foreign xml:lang="gez">ተስፋ፡</foreign> <cit type="translation" xml:lang="la">
                                <quote> spem </quote>
                            </cit>, ut: <foreign xml:lang="gez">እለ፡ ቀብጹ፡ ተስፋሆሙ፡</foreign> <ref cRef="Sir." loc="17,24">Sir. 17,24</ref>; (ne rejicias peccantem) <foreign xml:lang="gez">ከመ፡ ኢይቅብጽ፡ ተስፋሁ፡</foreign> <ref cRef="Clem." loc="180">Clem. f. 180</ref>; <ref cRef="Jsp." loc="368">Jsp. p. 368</ref>; <ref cRef="Jsp." loc="371.">Jsp. p. 371.</ref> Deinde subjungitur <sense n="a" xml:id="D2a"> <gramGrp> <case value="accusativus">Acc.</case> </gramGrp> rei <sense n="α" xml:id="D2aα"> <cit type="translation" xml:lang="la">
                                        <quote> desperare aliquid </quote>
                                    </cit>: <foreign xml:lang="gez">ቀብጸት፡ ነፍስየ፡ ሰላመ፡</foreign> <ref cRef="Thren." loc="3,17">Thren. 3,17</ref>; <foreign xml:lang="gez">እስከ፡ ቀብጸ፡ ሕይወተ፡</foreign> (restitutionem sanitatis) <ref cRef="Jsp." loc="368">Jsp. p. 368</ref>; <foreign xml:lang="gez">ታሀጕሉ፡ ተስፋክሙ፡ ወትቀብጹ፡ መድኀኒተክሙ፡</foreign> (salutem aeternam) <ref cRef="Did. " loc="12.">Did.  12.</ref> </sense> <sense n="β" xml:id="D2aβ"> <cit type="translation" xml:lang="la">
                                        <quote> renuere </quote>
                                    </cit>, <cit type="translation" xml:lang="la">
                                        <quote> respuere </quote>
                                    </cit>, <cit type="translation" xml:lang="la">
                                        <quote> recusare </quote>
                                    </cit> (rem tamquam desperatam): <foreign xml:lang="gez">ቀብጸት፡ ነፍስየ፡ ትፍሥሕተ፡</foreign> <foreign xml:lang="grc">ἀπηνήνατο παρακληθῆναι</foreign> <ref cRef="Ps." loc="76,3">Ps. 76,3</ref>; <foreign xml:lang="gez">ከመ፡ ኢትቅበጽዋ፡ ለዛቲ፡ ሕማምክሙ፡</foreign> <ref cRef="1 Thess." loc="3,3">1 Thess. 3,3</ref> <lbl expand="romanae editionis">rom.</lbl> </sense> </sense> <sense n="b" xml:id="D2b"> <gramGrp> <case value="accusativus">Acc.</case> </gramGrp> <lbl expand="persona">pers.</lbl> <sense n="α" xml:id="D2bα"> <cit type="translation" xml:lang="la">
                                        <quote> desperare </quote>
                                    </cit> alicui vel de aliquo: <foreign xml:lang="gez">አቡየ፡ ወእምየ፡ ቀብጹኒ፡ ወኢይሴፈዉ፡ እንከ፡ ይርአዩኒ፡</foreign> <ref cRef="Tob." loc="10,7">Tob. 10,7</ref>; <foreign xml:lang="gez">ይቀብጽዎ፡</foreign> (<foreign xml:lang="gez">ለዘከሠተ፡ ምክረ፡ ቢጹ፡</foreign>) <ref cRef="Sir." loc="27,21">Sir. 27,21</ref>; <foreign xml:lang="gez">ኢትቅብጾ፡</foreign> <ref cRef="Sir." loc="22,21">Sir. 22,21</ref>; <ref cRef="Sir." loc="22,22">Sir. 22,22</ref> (v. <foreign xml:lang="gez">ትትቀበጽ፡</foreign>).</sense> <sense n="β" xml:id="D2bβ"> <cit type="translation" xml:lang="la">
                                        <quote> respuere </quote>
                                    </cit>, <cit type="translation" xml:lang="la">
                                        <quote> rejicere </quote>
                                    </cit> et <cit type="translation" xml:lang="la">
                                        <quote> destituere </quote>
                                    </cit>, <cit type="translation" xml:lang="la">
                                        <quote> derelinquere </quote>
                                    </cit>, <cit type="translation" xml:lang="la">
                                        <quote> deserere </quote>
                                    </cit> aliquem tamquam desperatum: <foreign xml:lang="gez">ቀብጹኒ፡ ለዓለም፡</foreign> <foreign xml:lang="grc">εἰς τὸν αἰῶνα ἀπεποιήσαντό με</foreign> <ref cRef="Job" loc="19,18">Job 19,18</ref>; <ref cRef="Ps." loc="37,12">Ps. 37,12</ref>; <foreign xml:lang="gez">ቀብጹኒ፡ እምልብ፡ ከመ፡ ዘሞተ፡</foreign> <ref cRef="Ps." loc="30,15">Ps. 30,15</ref>; <ref cRef="Job" loc="17,8">Job 17,8</ref> <lbl expand="varia lectio">var.</lbl> – <lbl expand="Vocabularium Aethiopicum, -a -a">Voc. Ae.</lbl>: <foreign xml:lang="gez">ቀብጸ፡ ብ፡ አጣ።</foreign> </sense> </sense> </sense> </sense>
               
