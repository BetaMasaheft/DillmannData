               <sense xml:id="Dmain" xml:lang="la" source="#dillmann"> <subc>III,1</subc> <sense n="1" xml:id="D1"> <gramGrp> <pos expand="reflexivum">refl.</pos> </gramGrp> <cit type="translation" xml:lang="la"><t:quote xmlns:t="http://www.tei-c.org/ns/1.0"> se oblinere </t:quote></cit>, <cit type="translation" xml:lang="la"><t:quote xmlns:t="http://www.tei-c.org/ns/1.0"> ungere </t:quote></cit>, <cit type="translation" xml:lang="la"><t:quote xmlns:t="http://www.tei-c.org/ns/1.0"> perungere </t:quote></cit>: <ref cRef="Ruth" loc="3,3">Ruth 3,3</ref>; <lbl expand="cum">c.</lbl> <gramGrp> <case value="accusativus">Acc.</case> </gramGrp> unguenti: <ref cRef="Deut." loc="28,40">Deut. 28,40</ref>; <ref cRef="Ex." loc="30,32">Ex. 30,32</ref>; <foreign xml:lang="gez">ይትቀብኡ፡ ዕፍረተ፡</foreign> <ref cRef="Jes." loc="25,7">Jes. 25,7</ref>; <ref cRef="Judith" loc="10,3">Judith 10,3</ref>; <ref cRef="Am." loc="6,6">Am. 6,6</ref>; <lbl expand="cum">c.</lbl> <gramGrp> <case value="accusativus">Acc.</case> </gramGrp> ejus, quod ungitur: <foreign xml:lang="gez">ተቀብኦቶን፡ ሥዕርተ፡ ርእሶን፡</foreign> F.M. 34,8; <lbl expand="cum">c.</lbl> <lbl expand="duplex">dupl.</lbl> <gramGrp> <case value="accusativus">Acc.</case> </gramGrp> <foreign xml:lang="gez">ተቀብአት፡ ገጻ፡ ዕፍረተ፡</foreign> <ref cRef="Judith" loc="16,7">Judith 16,7</ref>; <foreign xml:lang="gez">ተቀባእ፡</foreign> <foreign xml:lang="grc">κατάπλασαι</foreign> <ref cRef="Jes." loc="38,21">Jes. 38,21</ref>. 
</sense> <sense n="2" xml:id="D2"> <gramGrp> <pos expand="passivum">pass.</pos> </gramGrp> <cit type="translation" xml:lang="la"><t:quote xmlns:t="http://www.tei-c.org/ns/1.0"> illini </t:quote></cit>, <cit type="translation" xml:lang="la"><t:quote xmlns:t="http://www.tei-c.org/ns/1.0"> ungi </t:quote></cit>, <cit type="translation" xml:lang="la"><t:quote xmlns:t="http://www.tei-c.org/ns/1.0"> perungi </t:quote></cit>, <cit type="translation" xml:lang="la"><t:quote xmlns:t="http://www.tei-c.org/ns/1.0"> ungendo consecrari </t:quote></cit>, <lbl expand="cum">c.</lbl> <gramGrp> <case value="accusativus">Acc.</case> </gramGrp> vel <foreign xml:lang="gez">በ፡</foreign> unguenti: <ref cRef="Ps. " loc="140,6">Ps.  140,6</ref>; <ref cRef="Ex." loc="30,31">Ex. 30,31</ref>; <ref cRef="Ex." loc="38,25">Ex. 38,25</ref>; <ref cRef="Ex." loc="40,7">Ex. 40,7</ref>; <foreign xml:lang="gez">ከመ፡ ይትቀባእ፡ ቅዱሰ፡ ቅዱሳን፡</foreign> <ref cRef="Dan." loc="9,24">Dan. 9,24</ref>; <ref cRef="Gad. Ad." loc="123">Gad. Ad. f. 123</ref>; <ref cRef="Gad. Ad." loc="124">Gad. Ad. f. 124</ref>; <foreign xml:lang="gez">መሬተ፡ ሥጋ፡ ሶበ፡ ተቀብአ፡</foreign> <ref cRef="Sx. Mag." loc="7 ">Sx. Mag. 7 </ref>Enc.; <lbl expand="cum">c.</lbl> <gramGrp> <case value="accusativus">Acc.</case> </gramGrp> praedicati: <foreign xml:lang="gez">ተቀብአ፡ ዳዊት፡ ንጉሠ፡</foreign> <ref cRef="1 Par." loc="14,8">1 Par. 14,8</ref>. 
</sense> <sense n="3" xml:id="D3"> <cit type="translation" xml:lang="la"><t:quote xmlns:t="http://www.tei-c.org/ns/1.0"> oleum fieri </t:quote></cit>: <foreign xml:lang="gez">ይትቀባእ፡ ቆዑ፡ በምዕር፡</foreign> <foreign xml:lang="grc">τρυγηθείη ὡς ὄμφαξ πρὸ ὥρας</foreign> (var. <foreign xml:lang="gez">ይትበላዕ፡</foreign>) <ref cRef="Job" loc="15,33">Job 15,33</ref>. 
</sense> </sense>
               
