               <sense xml:lang="la" source="#dillmann" xml:id="Dmain"> <subc>I,1</subc>; <gramGrp> <pos expand="Subjunctivus">Subj.</pos> </gramGrp> <foreign xml:lang="gez">ይስኪ፡</foreign> (raro <foreign xml:lang="gez">ይስከይ፡</foreign>, ut <ref cRef="Hen." loc="89,15">Hen. 89,15</ref> in cod. Abbad. XXXV) <sense n="1" xml:id="D1"> <cit type="translation" xml:lang="la">
                                <quote> confugere </quote>
                            </cit> in locum vel ad aliquem: <foreign xml:lang="gez">ይሰኪ፡ ህየ፡ ቀታሊ፡</foreign> <ref cRef="Num." loc="35,11">Num. 35,11</ref>; <foreign xml:lang="gez">ከመ፡ ይስኪ፡ ህየ፡</foreign> <ref cRef="Num." loc="35,15">Num. 35,15</ref>; <ref cRef="Deut." loc="4,41">Deut. 4,41</ref>; <ref cRef="Deut." loc="19,4">Deut. 19,4</ref> <lbl expand="sequens">seq.</lbl>; <ref cRef="Jos." loc="20,4">Jos. 20,4</ref> <lbl expand="sequens">seq.</lbl>; <cit type="translation" xml:lang="la">
                                <quote> precibus </quote>
                            </cit> vel auxilium petendo <cit type="translation" xml:lang="la">
                                <quote> adire </quote>
                            </cit> aliquem, <lbl expand="cum">c.</lbl> <foreign xml:lang="gez">ኀበ፡</foreign>, ut: <foreign xml:lang="gez">ሰከይኩ፡ ኀበ፡ እግዚአብሔር፡ ወሰአልክዎ፡</foreign> (<foreign xml:lang="grc">ἐνέτυχον</foreign>) <ref cRef="Sap." loc="8,21">Sap. 8,21</ref>; <foreign xml:lang="gez">ይሰክዩ፡ ኀቤሁ፡</foreign> <ref cRef="Clem." loc="194">Clem. f. 194</ref>; <lbl expand="cum">c.</lbl> <gramGrp> <case value="accusativus">Acc.</case> </gramGrp> <ref cRef="Hen." loc="45,3">Hen. 45,3</ref>.</sense> <sense n="2" xml:id="D2"> <cit type="translation" xml:lang="la">
                                <quote> queri </quote>
                            </cit>; <cit type="translation" xml:lang="la">
                                <quote> incusare </quote>
                            </cit>; <cit type="translation" xml:lang="la">
                                <quote> criminari </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> reum facere </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> accusare </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> agere in </quote>
                            </cit> aliquem <foreign xml:lang="ar">شَكَا</foreign> et <foreign xml:lang="ar">شَكِى</foreign>; <gramGrp> <pos expand="absolute">absol.</pos> </gramGrp> <ref cRef="Hen." loc="22,5">Hen. 22,5</ref>; <ref cRef="Hen." loc="22,6">Hen. 22,6</ref>; <foreign xml:lang="gez">ትትፈደዩ፡ ዕዳክሙ፡ ወትሰክዩ፡ ወትትዋቀሡ፡ ወትትላኰዩ፡ ወትትጋአዙ፡</foreign> <ref cRef="Jes." loc="58,3">Jes. 58,3</ref>; <ref cRef="Jes." loc="58,4">Jes. 58,4</ref>; <foreign xml:lang="gez">ለእመ፡ ሰከዩ፡ ለለ፩እምኔሆሙ፡</foreign> <cit type="translation" xml:lang="la">
                                <quote> si uterque actionem instituit </quote>
                            </cit> F.N. 43; <ref cRef="Did." loc="10">Did. 10</ref>; <foreign xml:lang="gez">ዘይሰኪ፡</foreign> <cit type="translation" xml:lang="la">
                                <quote> actor </quote>
                            </cit> F.N. 43,6; <foreign xml:lang="gez">አምጽኦ፡ ሰማዕት፡ ለጥያቄ፡ ላዕለ፡ ዘይሰኪ፡</foreign> <cit type="translation" xml:lang="la">
                                <quote> testes producere actoris est </quote>
                            </cit> F.N. 43,6; <lbl expand="cum">c.</lbl> <foreign xml:lang="gez">ለ፡</foreign> vel plerumque <foreign xml:lang="gez">ኀበ፡</foreign> pers., apud quam queruntur vel actionem instituunt: <ref cRef="Hen." loc="9,3">Hen. 9,3</ref>; <ref cRef="Hen." loc="89,15">Hen. 89,15</ref>; <ref cRef="Hen." loc="103,14">Hen. 103,14</ref>; <foreign xml:lang="gez">እመቦ፡ ዘሰከይዎ፡ ኀበ፡ ፈታሒ፡</foreign> F.N. 43,9; <foreign xml:lang="gez">ሰከያ፡</foreign> (<foreign xml:lang="gez">ለወለቱ፡</foreign>) <foreign xml:lang="gez">ለመኳንንት፡</foreign> <ref cRef="Sx. Haml." loc="25">Sx. Haml. 25</ref>; <foreign xml:lang="gez">ሰከዩ፡ ሎቱ፡</foreign> (<cit type="translation" xml:lang="la">
                                <quote> Augusto </quote>
                            </cit>) <foreign xml:lang="gez">ዘገብረ፡ ላዕሌሆሙ፡ ሄሮድስ፡</foreign> <ref cRef="Jsp." loc="340">Jsp. p. 340</ref>; <lbl expand="cum">c.</lbl> <gramGrp> <case value="accusativus">Acc.</case> </gramGrp> causae vel <lbl expand="persona">pers.</lbl>, quam queruntur vel accusant: <ref cRef="Hen. " loc="7,6">Hen.  7,6</ref>; <ref cRef="Hen." loc="22,7">Hen. 22,7</ref>; <ref cRef="Rom." loc="11,2">Rom. 11,2</ref>; <ref cRef="Lit." loc="172,4">Lit. 172,4</ref>; <foreign xml:lang="gez">እለ፡ ይሰክይዎ፡</foreign> <cit type="translation" xml:lang="la">
                                <quote> ejus accusatores </quote>
                            </cit> <ref cRef="Jsp." loc="315">Jsp. p. 315</ref>; Alexandra <foreign xml:lang="gez">ትሰክዩ፡ ለሄሮድስ፡</foreign> (Herodem) <ref cRef="Jsp." loc="322">Jsp. p. 322</ref>; <foreign xml:lang="gez">ከመ፡ ይስክይዎ፡ ኀበ፡ ቄሳር፡</foreign> <ref cRef="Jsp." loc="363">Jsp. p. 363</ref>; <foreign xml:lang="gez">ዘሰከየ፡ ግብረ፡ ዘኢይትሌበው፡</foreign> <cit type="translation" xml:lang="la">
                                <quote> qui actionem haud perspicuam instituit </quote>
                            </cit> F.N. 43,6; <foreign xml:lang="gez">ዘሰከይዎ፡</foreign> <cit type="translation" xml:lang="la">
                                <quote> reus </quote>
                            </cit> F.N. 43,6; vel <lbl expand="cum">c.</lbl> <foreign xml:lang="gez">በእንተ፡</foreign> causae, F.N. 43,6; vel <lbl expand="cum">c.</lbl> <foreign xml:lang="gez">በ፡</foreign>, ut: <foreign xml:lang="gez">ይስክይዎሙ፡ ለአርድእት፡ በዘ፡ ሐሰዉ፡</foreign> <foreign xml:lang="grc">ἐγκαλείτωσαν αὐτοῖς τὴν ψευδηγορίαν</foreign> Cyr. c. <ref cRef="Pall." loc="84.">Pall. f. 84.</ref> – <lbl expand="Vocabularium Aethiopicum, -a -a">Voc. Ae.</lbl>: <foreign xml:lang="gez">ሰከየ፡ ዘ፡ ተማሕፀነ፡ አጸላሂ፡ ይ።</foreign> </sense> </sense>
               
