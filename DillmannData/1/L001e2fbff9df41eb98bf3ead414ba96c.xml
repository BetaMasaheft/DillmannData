         <div>
            <entry xml:id="L001e2fbff9df41eb98bf3ead414ba96c" n="7139">
               <form>
                  <foreign xml:lang="gez">ዕቅፍት</foreign>
               </form>
               <sense xml:lang="la" source="#dillmann" xml:id="Dmain">
                  <gramGrp>
                     <pos expand="Substantivum">subst.</pos>
                  </gramGrp>, <lbl expand="pluralis">Pl.</lbl>
                  <foreign xml:lang="gez">ዕቅፍታት፡</foreign>
                  <sense n="1" xml:id="D1">
                     <cit type="translation" xml:lang="la">
                        <quote>offensio</quote>
                     </cit> pedis: (<foreign xml:lang="gez">ዘያስተሰግል፡</foreign>) <foreign xml:lang="gez">በዕቅፍተ፡ እግር፡</foreign> (inter alia divinationis genera) <ref cRef="Clem. " loc="178">Clem. f. 178</ref>; <cit type="translation" xml:lang="la">
                        <quote>lapsus </quote>
                     </cit>offensione pedis effectus: <foreign xml:lang="gez">ዕቅፍቱ፡ </foreign>
                     <foreign xml:lang="grc">ὑποσκέλισμα αὐτοῦ</foreign>
                     <ref cRef="Prov. " loc="24,17">Prov. 24,17</ref>.</sense>
                  <sense n="2" xml:id="D2">
                     <cit type="translation" xml:lang="la">
                        <quote>causa offensionis</quote>
                     </cit> et <cit type="translation" xml:lang="la">
                        <quote>lapsus</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>offendiculum</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>scandalum</quote>
                     </cit>, <foreign xml:lang="grc">πρόσκομμα</foreign> vel <foreign xml:lang="grc">σκάνδαλον</foreign>, <foreign xml:lang="gez">ኢትደይ፡ ዕቅፍተ፡ ቅድመ፡ ዕዉር፡ </foreign>
                     <ref cRef="Lev." loc="19,14">Lev. 19,14</ref>; <ref cRef="Ps." loc="68,27">Ps.
                        68,27</ref>; <ref cRef="Ps." loc="139,6">Ps. 139,6</ref>; <foreign xml:lang="gez">የዐቅበከ፡ እምዕቅፍት፡ </foreign>
                     <ref cRef="Sir." loc="31,19">Sir. 31,19</ref>; <ref cRef="Sir." loc="39,24">Sir. 39,24</ref>; <ref cRef="Judith" loc="5,20">Judith 5,20</ref>; <ref cRef="Hos." loc="4,17">Hos. 4,17</ref>; <foreign xml:lang="gez">ዕፀ፡
                        ዕቅፍት፡</foreign>
                     <ref cRef="Sir." loc="34,7">Sir. 34,7</ref>; <foreign xml:lang="gez">እብነ፡ ዕቅፍት፡ </foreign>
                     <ref cRef="Jes." loc="8,14">Jes. 8,14</ref>; <ref cRef="Rom." loc="9,32">Rom.
                        9,32</ref>; <foreign xml:lang="gez">ትከውኖ፡ ዕቅፍተ፡</foreign>
                     <ref cRef="1 Reg. " loc="18,21">1 Reg. 18,21</ref>; <ref cRef="Ex." loc="23,33">Ex. 23,33</ref>; <ref cRef="Jud." loc="2,3">Jud. 2,3</ref>; <foreign xml:lang="gez">ዕቅፍታት፡</foreign> F.N. 5.</sense>
                  <sense n="3" xml:id="D3">
                     <cit type="translation" xml:lang="la">
                        <quote>remora</quote>
                     </cit>, Lud. ex <ref cRef="Org." loc="3.">Org. 3.</ref>
                  </sense>
               </sense>
               
