               <sense xml:lang="la" source="#dillmann" xml:id="Dmain">
                  <gramGrp>
                     <pos expand="Substantivum">subst.</pos>
                  </gramGrp>, <gramGrp>
                     <gen value="femininus">fem.</gen>
                  </gramGrp> et <gramGrp>
                     <gen value="masculinum">m.</gen>
                  </gramGrp>, <lbl expand="pluralis">Pl.</lbl>
                  <foreign xml:lang="gez">አሰክት፡</foreign> (sec. Lud. <foreign xml:lang="gez">አሳክት፡</foreign>?), fortasse proprie: <cit type="translation" xml:lang="la">
                     <quote>pensile</quote>
                  </cit> [<foreign xml:lang="he">אֶשֶׁךְ</foreign>
                  <foreign xml:lang="syr">ܐܶܫܟܳܐ</foreign>
                  <cit type="translation" xml:lang="la">
                     <quote>testiculi</quote>
                  </cit>] i.e. <cit type="translation" xml:lang="la">
                     <quote>pudenda</quote>
                  </cit> viri, <cit type="translation" xml:lang="la">
                     <quote>scrotum</quote>
                  </cit>, <cit type="translation" xml:lang="la">
                     <quote>testiculi</quote>
                  </cit>
                  <cit type="translation" xml:lang="la">
                     <quote>,</quote>
                  </cit>
                  <cit type="translation" xml:lang="la">
                     <quote>testiculus</quote>
                  </cit>, <cit type="translation" xml:lang="la">
                     <quote>penis</quote>
                  </cit>: <foreign xml:lang="gez">ደቂቀ፡ ፋርስ፡ እለ፡ መጠነ፡ ነፍስተ፡ አድግ፡ ነፍስቶሙ፡ ወከመ፡ እስኪት፡
                     ፈረስ፡ እስኪቶሙ፡</foreign>
                  <foreign xml:lang="grc">αἰδοῖα </foreign>
                  <ref cRef="Hez." loc="23,20">Hez. 23,20</ref>; <foreign xml:lang="gez">ዘአሐቲ፡
                     እስኪቱ፡</foreign>
                  <foreign xml:lang="grc">μονόρχις</foreign>
                  <ref cRef="Lev." loc="21,20">Lev. 21,20</ref>; <foreign xml:lang="gez">ክልኤሆን፡
                     አሰክቲሁ፡ </foreign>
                  <ref cRef="Deut." loc="25,11">Deut. 25,11</ref>; <foreign xml:lang="gez">ዘሞተ፡
                     እስኪቱ፡ በሥራይ፡</foreign> M. F.; <foreign xml:lang="gez">ሠጠቁ፡ መላእክት፡ እስኪቶ፡ ለማር፡
                     ኤልያስ፡ ሊቀ፡ ምኔተ፡ መነኮሳይያት፡ ሶበ፡ ኮነ፡ ምስሌሁ፡ በህየ፡ ፍትወት፡</foreign>
                  <ref cRef="Phlx." loc="25">Phlx. 25</ref>; <foreign xml:lang="gez">ለእለ፡ ይዜምዉ፡
                     በእንስሳ፡ ይትመተር፡ እስኪቶሙ፡</foreign> F.M. 7,5; F.N. 48; <foreign xml:lang="gez">መተሩ፡
                     እስኪቶ፡</foreign> Sx. <ref cRef="Tachs." loc="1 ">Tachs. 1 </ref>Enc.; <ref cRef="Sen." loc="7">Sen. 7</ref>; <ref cRef="Fal." loc="12">Fal. f. 12</ref>;
                     (<lbl expand="videas">vid. sub</lbl>
                  <foreign xml:lang="gez">ሕንብርት፡ </foreign> col. 108); <foreign xml:lang="gez">ብእሲ፡
                     ዘይፌኑ፡ እስኪቶ፡ ውስተ፡ መንደቅ፡ አው፡ ውስተ፡ ምድር፡ አው፡ ኀበ፡ ስቍረት፡ አው፡ ውስተ፡ እዴሁ፡ ወይወጽእ፡ እምኔሁ፡
                     ፍትወት፡</foreign>
                  <ref cRef="Syn." loc="395 ">Syn. f. 395 </ref>(de onania). – <lbl expand="Vocabularium Aethiopicum, -a -a.">Voc. Ae.</lbl>: <foreign xml:lang="gez">ምትፋይ፡</foreign>
                  <lbl expand="vel">v.</lbl>
                  <foreign xml:lang="gez">ንትፋይ፡ ዘ፡ እስኪት፡</foreign>
               </sense>
               
