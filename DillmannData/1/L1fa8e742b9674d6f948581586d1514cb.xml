               <sense xml:lang="la" source="#dillmann" xml:id="Dmain"> <gramGrp> <pos expand="not found">subst.</pos> </gramGrp>, <lbl expand="pluralis">Pl.</lbl> <foreign xml:lang="gez">ድካማት፡</foreign> <sense n="1" xml:id="D1"> <cit type="translation" xml:lang="la">
