            <entry xml:id="L1fe8529c668a4b5e971f3bf5fde29822" n="4993">
               <form>
                  <foreign xml:lang="gez">ምንኀር</foreign>
               </form>
               <sense xml:lang="la" source="#dillmann" xml:id="Dmain"> vel <foreign xml:lang="gez">መንኀር፡</foreign> (Lud. <foreign xml:lang="gez">መንኅር፡</foreign> emendare voluit) <gramGrp> <pos expand="Substantivum">subst.</pos> </gramGrp> <sense n="1" xml:id="D1"> <cit type="translation" xml:lang="la">
                                <quote> antlia </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> haustrum </quote>
                            </cit> [cfr. <foreign xml:lang="ar">نَاعُورةٌ</foreign>, <foreign xml:lang="syr">ܢܳܥܽܘܪܳܐ</foreign>] <foreign xml:lang="gez">ከመ፡ መንኀር፡</foreign> <lbl expand="vel">v.</lbl> <foreign xml:lang="gez">ምንኀር፡ ቦእኩ፡ ውስተ፡ ገነት፡</foreign> (<foreign xml:lang="grc">ὑδραγωγός</foreign>) <ref cRef="Sir." loc="24,30">Sir. 24,30</ref>.</sense> <sense n="2" xml:id="D2"> <cit type="translation" xml:lang="la">
                                <quote> follis </quote>
                            </cit>, Th. Petr. ad <ref cRef="Jon." loc="1,5">Jon. 1,5</ref>. – <cit type="translation" xml:lang="la">
                                <quote> Voc. Ae. </quote>
                            </cit> (ad Sir.): <foreign xml:lang="gez">ምንኀር፡ ዘ፡ ፋፋታ፡</foreign> (v. <foreign xml:lang="gez">ፈፍሐት፡</foreign>) <foreign xml:lang="gez">ምንጭይሂ፡ ይ፡</foreign>; <cit type="translation" xml:lang="la">
                                <quote> aliud </quote>
                            </cit>: <foreign xml:lang="gez">ውሒዝ፡ ምንኀር፡ ፈለግ፡ ተከዚ፡ ዘ፡ ፈሳሽ።</foreign> </sense> </sense>
               
            <sense xml:lang="en" source="#traces" xml:id="Tmain"> <cit type="transcription" xml:lang="gez">
                            <quote> mǝnḫar </quote>
                        </cit> <cit type="translation" xml:lang="en">
                            <quote> channel </quote>
                        </cit>, ‘<cit type="translation" xml:lang="it">
                            <quote> canale </quote>
                        </cit>’ <foreign xml:lang="gez">ንግባእ፡ ኀበ፡ ውዳሴሁ፡ ለአፈ፡ ወርቅ፡ ለምንኀረ፡ ጽድቅ፡</foreign> ‘<cit type="translation" xml:lang="it">
                            <quote> Ci addentriamo nella lode di colui che ha la bocca dʼoro, canale di giustizia </quote>
                        </cit>ʼ <bibl>
                            <ptr target="bm:Tedros2009TawaldaMadehnVittore"/>
                            <citedRange unit="page"> 102 (182) l. 3 (ed.); 103 (183) l. 3-4 (tr.)</citedRange>
                        </bibl> (‘L’agiografo usa <foreign xml:lang="gez">ምንኀር፡</foreign> un termine abbastanza raro e non chiarissimo.’ <bibl>
                            <ptr target="bm:Tedros2009TawaldaMadehnVittore"/>
                            <citedRange unit="page"> 103 (183) n. 74</citedRange>
                        </bibl>) <sense n="L" xml:id="TL"> <cit type="transcription" xml:lang="gez">
                                <quote> mǝnhār </quote>
                            </cit> <foreign xml:lang="gez">ምንሃር</foreign>, <cit type="transcription" xml:lang="gez">
                                <quote> manhar </quote>
                            </cit> <foreign xml:lang="gez">መንሀር</foreign> (<bibl>
                                <ptr target="bm:KidaneWaldGeezGramDict"/>
                            </bibl>), <cit type="transcription" xml:lang="gez">
                                <quote> mǝnḫar </quote>
                            </cit> <foreign xml:lang="gez">ምንኀር</foreign>, <cit type="transcription" xml:lang="gez">
                                <quote> manḫar </quote>
                            </cit> <foreign xml:lang="gez">መንኀር</foreign> (<bibl>
                                <ptr target="bm:Dillmann1865Lexicon"/>
                            </bibl>) ʻ<cit type="translation" xml:lang="en">
                                <quote> irrigation channel </quote>
                            </cit>, <cit type="translation" xml:lang="en">
                                <quote> machine for drawing water </quote>
                            </cit>, <cit type="translation" xml:lang="en">
                                <quote> pump </quote>
                            </cit>, <cit type="translation" xml:lang="en">
                                <quote> conduit </quote>
                            </cit>, (<bibl>
                                <ptr target="bm:AlaqaTayya1965MSawasew"/>
                            </bibl>) <cit type="translation" xml:lang="en">
                                <quote> waterfall </quote>
                            </cit>, <cit type="translation" xml:lang="en">
                                <quote> spring </quote>
                            </cit> (<bibl>
                                <ptr target="bm:KidaneWaldGeezGramDict"/>
                            </bibl>) <cit type="translation" xml:lang="en">
                                <quote> afterbirth </quote>
                            </cit> </sense> <nd/> </sense>
