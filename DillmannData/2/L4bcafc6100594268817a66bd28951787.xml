               <sense xml:lang="la" source="#dillmann" xml:id="Dmain"> <subc>I,2</subc> <sense n="1" xml:id="D1"> <cit type="translation" xml:lang="la">
                                <quote> fortem </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> potentem esse </quote>
                            </cit> vel <cit type="translation" xml:lang="la">
                                <quote> fieri </quote>
                            </cit>; <cit type="translation" xml:lang="la">
                                <quote> roborari </quote>
                            </cit>; <cit type="translation" xml:lang="la">
                                <quote> valere </quote>
                            </cit> et <cit type="translation" xml:lang="la">
                                <quote> invalescere </quote>
                            </cit>; <ref cRef="Hebr." loc="11,34">Hebr. 11,34</ref>; <foreign xml:lang="gez">ጽንዑ፡ ወኀይሉ፡</foreign> <ref cRef="2 Reg." loc="10,12">2 Reg. 10,12</ref>; <foreign xml:lang="gez">ውሕጠ፡ ሞት፡ ወኀየለ፡</foreign> <ref cRef="Jes." loc="25,8">Jes. 25,8</ref>; auxilium <foreign xml:lang="gez">በዘ፡ ይኄይል፡ ድካምየ፡</foreign> <cit type="translation" xml:lang="la">
