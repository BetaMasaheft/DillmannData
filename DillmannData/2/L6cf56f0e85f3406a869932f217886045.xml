               <sense xml:lang="la" source="#dillmann" xml:id="Dmain">
                  <gramGrp>
                     <pos expand="participium">part.</pos>
                  </gramGrp> et <gramGrp>
                     <pos expand="adjectivum">adj.</pos>
                     <gen value="femininus">fem.</gen>
                  </gramGrp>
                  <foreign xml:lang="gez">ክብድ፡</foreign> (<ref cRef="Deut." loc="30,11">Deut.
                     30,11</ref> FH; at in <gramGrp>
                     <case value="accusativus">Acc.</case>
                  </gramGrp>
                  <foreign xml:lang="gez">ክብድተ፡</foreign>
                  <ref cRef="Deut." loc="30,11">Deut. 30,11</ref> FH.) <sense n="1" xml:id="D1">
                     <cit type="translation" xml:lang="la">
                        <quote>gravis</quote>
                     </cit>: <foreign xml:lang="gez">ጾር፡ ክቡድ፡</foreign>
                     <ref cRef="Sir." loc="21,16">Sir. 21,16</ref>; <ref cRef="Sir." loc="40,1">Sir.
                        40,1</ref>; <ref cRef="Ps. " loc="37,4">Ps. 37,4</ref>; <ref cRef="Matth." loc="23,4">Matth. 23,4</ref>; <foreign xml:lang="gez">ክቡድ፡ እብን፡</foreign>
                     <ref cRef="Prov." loc="27,3">Prov. 27,3</ref>; <foreign xml:lang="gez">ኢታንሥእ፡
                        ክቡደ፡</foreign>
                     <cit type="translation" xml:lang="la">
                        <quote>grave quid</quote>
                     </cit>
                     <ref cRef="Sir." loc="13,2">Sir. 13,2</ref>; <foreign xml:lang="gez">ጾረት፡
                        ክቡደ፡</foreign>
                     <ref cRef="Thren." loc="1,9">Thren. 1,9</ref>; <foreign xml:lang="gez">አንቀጽ፡
                        ዐቢይ፡ ወክቡድ፡</foreign>
                     <ref cRef="Jsp." loc="376">Jsp. p. 376</ref>; <foreign xml:lang="gez">ብእሲ፡
                        ክቡድ፡</foreign> 1 <ref cRef="Reg." loc="4,18">Reg. 4,18</ref>; <lbl expand="translate">transl.</lbl> de rebus, quae oneris instar deprimunt
                        <foreign xml:lang="gez">ቀኖና፡ ክቡድ፡</foreign> F.M. 23,9; <foreign xml:lang="gez">ክቡድ፡ ቅኔሆሙ፡</foreign>
                     <ref cRef="2                 Esr." loc="15,18">2 Esr. 15,18</ref>; <foreign xml:lang="gez">ጌጋይ፡ ክቡድ፡</foreign>
                     <ref cRef="Act." loc="25,7">Act. 25,7</ref>. <sense n="a" xml:id="D1a">
                        <cit type="translation" xml:lang="la">
                           <quote>numerosus</quote>
                        </cit>
                        <ref cRef="Ps." loc="34,21">Ps. 34,21</ref>; <ref cRef="Num." loc="20,20">Num. 20,20</ref>; <ref cRef="Nah." loc="2,9">Nah. 2,9</ref>.</sense>
                     <sense n="b" xml:id="D1b">
                        <cit type="translation" xml:lang="la">
                           <quote>vehemens</quote>
                        </cit>: <foreign xml:lang="gez">ክቡድ፡ ሕማም፡</foreign>
                        <ref cRef="Sir." loc="34,2">Sir. 34,2</ref>; <foreign xml:lang="gez">ክቡድ፡
                           ድቀት፡</foreign>
                        <ref cRef="Nah." loc="3,3">Nah. 3,3</ref>; <foreign xml:lang="gez">ብፃው፡
                           ክቡድ፡</foreign>
                        <cit type="translation" xml:lang="la">
                           <quote>som</quote>
                        </cit>
                        <cit type="translation" xml:lang="la">
                           <quote>n</quote>
                        </cit>
                        <cit type="translation" xml:lang="la">
                           <quote>us gravis</quote>
                        </cit> M.M. f. 333.</sense>
                     <sense n="c" xml:id="D1c">
                        <cit type="translation" xml:lang="la">
                           <quote>gravis</quote>
                        </cit> i.e. <cit type="translation" xml:lang="la">
                           <quote>magni momenti</quote>
                        </cit>: <foreign xml:lang="gez">ሰላም፡ እብል፡ ለተኣምሪሁ፡ ክቡድ፡</foreign>
                        <ref cRef="Sx. Nah." loc="14 ">Sx. Nah. 14 </ref>Enc.</sense>
                     <sense n="d" xml:id="D1d">
                        <cit type="translation" xml:lang="la">
                           <quote>molestus</quote>
                        </cit>, <cit type="translation" xml:lang="la">
                           <quote>oneri</quote>
                        </cit>: <foreign xml:lang="gez">ክቡድ፡ ወዕጹብ፡ </foreign>
                        <foreign xml:lang="grc">δύςχρηστος</foreign>
                        <ref cRef="Jes." loc="3,10">Jes. 3,10</ref>; <foreign xml:lang="gez">ክቡድ፡
                           ውእቱ፡ ለነ፡</foreign>
                        <ref cRef="Sap." loc="2,12">Sap. 2,12</ref>.</sense>
                     <sense n="e" xml:id="D1e">
                        <cit type="translation" xml:lang="la">
                           <quote>difficilis</quote>
                        </cit>, <cit type="translation" xml:lang="la">
                           <quote>arduus</quote>
                        </cit>, <ref cRef="Deut." loc="30,11">Deut. 30,11</ref>; <ref cRef="1 Joh." loc="5,3">1 Joh. 5,3</ref>;</sense>
                     <sense n="f" xml:id="D1f">
                        <cit type="translation" xml:lang="la">
                           <quote>gravitate iners</quote>
                        </cit>, <cit type="translation" xml:lang="la">
                           <quote>impeditus</quote>
                        </cit>, <foreign xml:lang="gez">እደዊሁ፡ ለሙሴ፡ ክቡድ፡</foreign>
                        <ref cRef="Ex." loc="17,12">Ex. 17,12</ref>; <foreign xml:lang="gez">አዕይንቲሆሙ፡ ክቡዳት፡ </foreign> (somno graves) <ref cRef="Matth." loc="26,43">Matth. 26,43</ref>; <foreign xml:lang="gez">ዘክቡድ፡ ልሳኑ፡ </foreign>
                        <ref cRef="Hez." loc="3,5">Hez. 3,5</ref>.</sense>
                     <sense n="2" xml:id="D12">
                        <cit type="translation" xml:lang="la">
                           <quote>oneratus</quote>
                        </cit>, <cit type="translation" xml:lang="la">
                           <quote>onustus</quote>
                        </cit>: <foreign xml:lang="gez">ክቡዳነ፡ ጾር፡ </foreign>
                        <ref cRef="Clem." loc="201">Clem. f. 201</ref>; <ref cRef="Clem." loc="233.">Clem. f. 233.</ref>
                     </sense>
                  </sense>
               </sense>
               
