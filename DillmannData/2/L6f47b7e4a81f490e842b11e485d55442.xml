               <sense xml:lang="la" source="#dillmann" xml:id="Dmain"> et <foreign xml:lang="gez">ጌሠ፡</foreign> <subc>I,1</subc> [<foreign xml:lang="ar">غَدَا</foreign>] <gramGrp> <pos expand="Subjunctivus">Subj.</pos> </gramGrp> <foreign xml:lang="gez">ይጊስ፡</foreign> <cit type="translation" xml:lang="la">
                            <quote> mane surgere </quote>
                        </cit>, <cit type="translation" xml:lang="la">
                            <quote> mane agere </quote>
                        </cit> aliquid, <cit type="translation" xml:lang="la">
                            <quote> mane venire </quote>
                        </cit>, <cit type="translation" xml:lang="la">
                            <quote> mane pergere </quote>
                        </cit> vel <cit type="translation" xml:lang="la">
                            <quote> mane proficisci </quote>
                        </cit>: <foreign xml:lang="grc">ὀρθρίζειν</foreign> <ref cRef="Gen." loc="19,2">Gen. 19,2</ref>; <ref cRef="Ex." loc="8,16">Ex. 8,16</ref>; <foreign xml:lang="gez">ገይሰክሙ፡ በጽባሕ፡</foreign> <ref cRef="Ex." loc="19,16">Ex. 19,16</ref>; <foreign xml:lang="gez">ገይሶ፡ በጽባሕ፡ ሐነጸ፡</foreign> <ref cRef="Ex." loc="24,4">Ex. 24,4</ref>; <ref cRef="Ex." loc="32,">Ex. 32,</ref> 6; <ref cRef="1 Reg." loc="5,3">1 Reg. 5,3</ref>; <ref cRef="1 Reg." loc="5,4">1 Reg. 5,4</ref>; <ref cRef="1 Reg. " loc="15,12">1 Reg.  15,12</ref>; <ref cRef="1 Reg." loc="29,10">1 Reg. 29,10</ref>; <ref cRef="1 Reg." loc="29,11">1 Reg. 29,11</ref>; <ref cRef="2 Reg." loc="15,2">2 Reg. 15,2</ref>; <ref cRef="Kuf." loc="17">Kuf. 17</ref>; <ref cRef="Kuf." loc="27">Kuf. 27</ref>; <ref cRef="Kuf." loc="32">Kuf. 32</ref>; <foreign xml:lang="gez">ጌስኩ፡ በሌሊት፡</foreign> <ref cRef="2 Esr." loc="12,12">2 Esr. 12,12</ref>; <ref cRef="Job" loc="1,5">Job 1,5</ref>; <ref cRef="Job" loc="8,5">Job 8,5</ref>; <foreign xml:lang="gez">ለገዪስ፡</foreign> <ref cRef="Ps." loc="118,148">Ps. 118,148</ref>; <foreign xml:lang="gez">ከንቱ፡ ገይሶትክሙ፡</foreign> <ref cRef="Ps." loc="126,3">Ps. 126,3</ref>; <foreign xml:lang="gez">ዘይገይስ፡</foreign> <ref cRef="Sir." loc="40,7">Sir. 40,7</ref>; <ref cRef="Tob." loc="9,6">Tob. 9,6</ref>; <ref cRef="Dan. " loc="6,19">Dan.  6,19</ref>; <foreign xml:lang="gez">አሌለያ፡ ገይሰ፡</foreign> <ref cRef="Luc." loc="24,1">Luc. 24,1</ref>; <ref cRef="Act." loc="5,21">Act. 5,21</ref>. <sense n="a" xml:id="Da"> <lbl expand="construitur cum">c.c.</lbl> <gramGrp> <case value="accusativus">Acc.</case> </gramGrp> loci, quo quis mane pergit: <ref cRef="Luc." loc="21,">Luc. 21,</ref> 38; <foreign xml:lang="gez">ጌሰ፡ ገዳመ፡</foreign> <ref cRef="Kuf." loc="26">Kuf. 26</ref>; <ref cRef="Job" loc="29,7">Job 29,7</ref>; <foreign xml:lang="gez">እለ፡ ይገይሱ፡ በጽባሕ፡ ቤተ፡ መያሲ፡</foreign> <ref cRef="Jes." loc="5,11">Jes. 5,11</ref>; <cb n="1196" xml:id="c1196"/> <foreign xml:lang="gez">ነቂሀነ፡ እምንዋም፡ ንጊሳ፡ ለቤተ፡ ክርስቲያን፡</foreign> Deg.; <lbl expand="cum">c.</lbl> <gramGrp> <pos expand="praepositio">praep.</pos> </gramGrp> ut <foreign xml:lang="gez">ውስተ፡</foreign> loci <ref cRef="Cant." loc="7,13">Cant. 7,13</ref>; <foreign xml:lang="gez">ኀበ፡</foreign> loci vel <lbl expand="persona">pers.</lbl>: <ref cRef="Luc." loc="21,38">Luc. 21,38</ref>; <ref cRef="Luc. " loc="24,22">Luc.  24,22</ref>; <foreign xml:lang="gez">ጊስ፡ ኀበ፡ እግዚአብሔር፡ ወጸሊ፡</foreign> <ref cRef="Job" loc="8,5">Job 8,5</ref>; <foreign xml:lang="gez">ዘይገይስ፡ ኀቤሃ፡</foreign> <ref cRef="Sir." loc="4,12">Sir. 4,12</ref>; <ref cRef="Sir." loc="6,36">Sir. 6,36</ref>; <ref cRef="Sir." loc="35,14">Sir. 35,14</ref>; <ref cRef="Sir. " loc="39,5">Sir.  39,5</ref>; <ref cRef="Jes." loc="26,9">Jes. 26,9</ref>; <foreign xml:lang="gez">ጌስኩ፡ ኀቤክሙ፡ ወነገርኩ፡</foreign> <ref cRef="Jer." loc="25,3">Jer. 25,3</ref>; <ref cRef="Sap. " loc="6,15">Sap.  6,15</ref>; <ref cRef="Ps." loc="62,1">Ps. 62,1</ref>.</sense> <sense n="b" xml:id="Db"> in genere: <cit type="translation" xml:lang="la">
                                <quote> proficisci </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> pergere </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> venire </quote>
                            </cit>: <foreign xml:lang="gez">ጌሰ፡ ወመጽአ፡</foreign> <foreign xml:lang="grc">ᾤχετο</foreign> <ref cRef="Jer. " loc="48,10">Jer.  48,10</ref>; <foreign xml:lang="gez">አዘዞሙ፡ ይጊሱ፡ ኀበ፡ እግዚአብሔር፡</foreign> <ref cRef="1 Reg." loc="10,17">1 Reg. 10,17</ref>; Sx. <ref cRef="Haml." loc="15 ">Haml. 15 </ref>Enc.; <foreign xml:lang="gez">ገይስ፡</foreign> <cit type="translation" xml:lang="la">
                                <quote> profectio </quote>
                            </cit>: <foreign xml:lang="gez">ከመ፡ ይግበር፡ ገይሰ፡</foreign> <ref cRef="Kuf." loc="165.">Kuf. p. 165.</ref> </sense> </sense>
               
