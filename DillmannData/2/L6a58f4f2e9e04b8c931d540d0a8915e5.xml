               <sense xml:lang="la" source="#dillmann" xml:id="Dmain"> <subc>I,2</subc> [<foreign xml:lang="he">עזז</foreign>, <foreign xml:lang="ar">عزّ</foreign>; <lbl expand="videas">vid.</lbl> <foreign xml:lang="gez">አዘዘ፡</foreign>] <sense n="1" xml:id="D1"> <cit type="translation" xml:lang="la">
                                <quote> validum </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> fortem </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> robustum esse </quote>
                            </cit>vel <cit type="translation" xml:lang="la">
                                <quote> fieri </quote>
                            </cit>, <sense n="a" xml:id="D1a"> <cit type="translation" xml:lang="la">
                                    <quote> invalescere </quote>
                                </cit>: <foreign xml:lang="gez">ዐዘዘኒ፡</foreign> (mihi) <foreign xml:lang="gez">ማይ፡ እስከ፡ ነፍስየ፡</foreign> <ref cRef="Or. Jonae" loc="6 ">Or. Jonae 6 </ref>(in <ref cRef="Psalterio Ludolfi p. " loc="528">Psalterio Ludolfi p.  528</ref>). At <ref cRef="Ex." loc="14,24">Ex. 14,24</ref> <foreign xml:lang="gez">አዘዘ፡</foreign> <foreign xml:lang="grc">συνέταξε</foreign> (non <foreign xml:lang="gez">ዐዘዘ፡</foreign> <cit type="translation" xml:lang="la">
                                    <quote> invaluit </quote>
                                </cit>, Lud.) legendum est.</sense> <sense n="b" xml:id="D1b"> <cit type="translation" xml:lang="la">
                                    <quote> viribus firmari </quote>
                                </cit>, <cit type="translation" xml:lang="la">
                                    <quote> vires recolligere </quote>
                                </cit>, <ref cRef="Hen." loc="81,6">Hen. 81,6</ref>. – Lud. in lexico statuit </sense> </sense> <sense n="2" xml:id="D2"> <cit type="translation" xml:lang="la">
                                <quote> corroborare </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> confirmare </quote>
                            </cit>, at ut <foreign xml:lang="gez">አዝዝ፡ ለነ፡ ቦሙ፡</foreign> <ref cRef="Lit." loc="159,4">Lit. 159,4</ref> (quem locum in lexico), ita <foreign xml:lang="gez">ዘይኤዝዝ፡ ትፍሥሕተ፡ ለኵሉ፡ ምድር፡</foreign> <ref cRef="Ps." loc="47,2">Ps. 47,2</ref> (quem locum in sua Psalterii editione huc refert) ad <foreign xml:lang="gez">አዘዘ፡</foreign> <cit type="translation" xml:lang="la">
                                <quote> jubere </quote>
                            </cit> referendum est.</sense> </sense>
               
