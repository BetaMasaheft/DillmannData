               <sense xml:lang="la" source="#dillmann" xml:id="Dmain">
                  <gramGrp>
                     <pos expand="Substantivum">subst.</pos>
                  </gramGrp>, <gramGrp>
                     <gen value="masculinum">m.</gen>
                  </gramGrp> et <gramGrp>
                     <gen value="femininus">fem.</gen>
                  </gramGrp>, <sense n="1" xml:id="D1">
                     <cit type="translation" xml:lang="la">
                        <quote>rus</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>ager</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>agri</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>arvum</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>praedium</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>fundus</quote>
                     </cit>: <foreign xml:lang="gez">ዘይትቀነይ፡ ወፍረ፡</foreign>
                     <ref cRef="Kuf." loc="50">Kuf. 50</ref>; <foreign xml:lang="gez">ሐረሳዌ፡
                        ወወፍሮ፡</foreign>
                     <ref cRef="Jer." loc="28,23">Jer. 28,23</ref>; <foreign xml:lang="gez">ወፍረ፡
                        ሐረስት፡ </foreign>
                     <ref cRef="Koh." loc="5,8">Koh. 5,8</ref>; <foreign xml:lang="grc">γεώργιον</foreign>
                     <ref cRef="Prov." loc="6,7">Prov. 6,7</ref>; <ref cRef="Prov. " loc="9,12">Prov. 9,12</ref>; <foreign xml:lang="gez">ወፍር፡ ዐባይ፡</foreign>
                     <ref cRef="Prov." loc="16,32">Prov. 16,32</ref>; <foreign xml:lang="gez">ወፍር፡
                        ዐቢይ፡</foreign>
                     <ref cRef="Prov." loc="24,5">Prov. 24,5</ref>; <ref cRef="Prov." loc="29,34">Prov. 29,34</ref>; <foreign xml:lang="grc">ἀγρός</foreign>
                     <ref cRef="Prov." loc="24,42">Prov. 24,42</ref>; <ref cRef="Luc." loc="17,7">Luc. 17,7</ref>; <foreign xml:lang="gez">ዐጸደ፡ ወፍር፡</foreign>
                     <foreign xml:lang="grc">ἄρουρα</foreign>
                     <ref cRef="1 Reg." loc="22,6">1 Reg. 22,6</ref>; <foreign xml:lang="gez">አዕጻደ፡
                        ወፍር፡</foreign>
                     <foreign xml:lang="grc">ἀγροί </foreign>
                     <ref cRef="1 Reg." loc="22,7">1 Reg. 22,7</ref>; <ref cRef="Jud." loc="1,14">Jud. 1,14</ref>.</sense>
                  <sense n="2" xml:id="D2">
                     <cit type="translation" xml:lang="la">
                        <quote>ager pascuus</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>pascua</quote>
                     </cit>: <foreign xml:lang="gez">ዐብረ፡ ኵሉ፡ ወፍር፡ ዘውስተ፡ ገዳም፡ </foreign>
                     <foreign xml:lang="grc">αἱ νομαὶ τῆς ἐρήμου</foreign>
                     <ref cRef="Jer." loc="23,10">Jer. 23,10</ref>; <foreign xml:lang="gez">አዕጻዳተ፡
                        ወፍርነ፡</foreign>
                     <foreign xml:lang="grc">βουκόλια ἡμῶν</foreign>
                     <ref cRef="Judith" loc="3,3">Judith 3,3</ref>; <ref cRef="Am." loc="6,4">Am.
                        6,4</ref>; <ref cRef="Jes." loc="65,10">Jes. 65,10</ref> (v. <foreign xml:lang="gez">ወፍረ፡ ላህም፡</foreign>); <ref cRef="Luc." loc="15,15">Luc.
                        15,15</ref>.</sense>
                  <sense n="3" xml:id="D3"> res rustica, <cit type="translation" xml:lang="la">
                        <quote>oeconomia</quote>
                     </cit>: episcopus <foreign xml:lang="gez">ዘኢይዘሩ፡ ንዋየ፡ ቤተ፡ ክርስቲያን፡ ለወፍረ፡
                        ቤቱ፡</foreign>
                     <ref cRef="Clem." loc="86.">Clem. f. 86.</ref>
                  </sense>
               </sense>
               
