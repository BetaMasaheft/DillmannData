         <div>
            <entry xml:id="L4eb2408cdb274ad98b49940280c675a4" n="3667">
               <form>
                  <foreign xml:lang="gez">አስተቍጥዐ</foreign>
               </form>
               <sense xml:lang="la" source="#dillmann" xml:id="Dmain"> <subc>IV,1</subc> et crebrius <foreign xml:lang="gez">አስተቈጥዐ፡</foreign> <subc>IV,2</subc> <sense n="1" xml:id="D1">
                     <cit type="translation" xml:lang="la">
                        <quote>impatientem reddere</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>taedio</quote>
                     </cit> vel <cit type="translation" xml:lang="la">
                        <quote>fastidio afficere</quote>
                     </cit>: <foreign xml:lang="gez">ፈድፋደ፡ አስተቈጥዐኒ፡ ሕይወቶሙ፡</foreign>
                     <ref cRef="Sir." loc="25,2">Sir. 25,2</ref>; <foreign xml:lang="gez">ነገሮሙ፡
                        ለአብዳን፡ ያስተቌጥዕ፡</foreign>
                     <ref cRef="Sir." loc="27,13">Sir. 27,13</ref>; <foreign xml:lang="gez">ለሕዝብየ፡
                        ያስተቌጥዕዎሙ፡ እምከዊነ፡ ሕዝብ፡</foreign>
                     <ref cRef="Jer." loc="40,24">Jer. 40,24</ref> vers. nov.</sense>
                  <sense n="2" xml:id="D2">
                     <cit type="translation" xml:lang="la">
                        <quote>ad indignationem concitare</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>ad iram provocare</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>stomachum movere</quote>
                     </cit> alicui; <ref cRef="Tit." loc="3,2">Tit. 3,2</ref>
                     <lbl expand="romanae editionis">rom.</lbl>; <foreign xml:lang="gez">መንሱት፡
                        ያስተቈጥዕ፡</foreign>
                     <foreign xml:lang="grc">ἐκ τῆς ὀργῆς μῆνις</foreign>
                     <ref cRef="Herm." loc="31">Herm. p. 31</ref>; <lbl expand="cum">c.</lbl>
                     <gramGrp>
                        <case value="accusativus">Acc.</case>
                     </gramGrp>
                     <ref cRef="Col." loc="3,21">Col. 3,21</ref>; <ref cRef="Eph. " loc="6,4">Eph.
                        6,4</ref>; <ref cRef="Org." loc="4">Org. 4</ref>; <foreign xml:lang="gez">አስተቍጥዕዎ፡</foreign> (Perf.) <ref cRef="Jsp." loc="332">Jsp. p. 332</ref>;
                        <foreign xml:lang="gez">ያስተቌጥዖሙ፡</foreign>
                     <ref cRef="Jsp." loc="344">Jsp. p. 344</ref>; <lbl expand="cum">c.</lbl>
                     <gramGrp>
                        <case value="accusativus">Acc.</case>
                     </gramGrp>
                     <lbl expand="persona">pers.</lbl> et <foreign xml:lang="gez">ላዕለ፡</foreign>
                     <cit type="translation" xml:lang="la">
                        <quote>contra</quote>
                     </cit>: <ref cRef="Jsp." loc="296">Jsp. p. 296</ref>; <ref cRef="Jsp. " loc="334.">Jsp. p. 334.</ref>
                  </sense>
               </sense>
               
