               <sense xml:lang="la" source="#dillmann" xml:id="Dmain"> <subc>II,1</subc> <sense n="1" xml:id="D1">
                     <cit type="translation" xml:lang="la">
                        <quote>demergere</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>submergere</quote>
                     </cit>, <ref cRef="Koh." loc="10,12">Koh. 10,12</ref>; <ref cRef="Sir." loc="23,16">Sir. 23,16</ref>
                     <lbl expand="varia lectio">var.</lbl>; <ref cRef="Thren." loc="2,2">Thren.
                        2,2</ref>; <ref cRef="Thren. " loc="2,5">Thren. 2,5</ref>; <ref cRef="Ps." loc="68,3">Ps. 68,3</ref>; <foreign xml:lang="gez">ያሰጥሞን፡ ለኀጣውኢነ፡</foreign>
                     <ref cRef="Mich." loc="7,19">Mich. 7,19</ref>; <foreign xml:lang="gez">አስጥም፡
                        ጽንዖሙ፡</foreign>
                     <ref cRef="Judith" loc="9,8">Judith 9,8</ref>; (Christus) <foreign xml:lang="gez">ያሰጥም፡ መራዕየ፡ አሕርው፡</foreign> Lit. Joh.; <lbl expand="cum">c.</lbl>
                     <foreign xml:lang="gez">ውስተ፡</foreign>, ut: <foreign xml:lang="gez">አስጠምኮሙ፡
                        ውስተ፡ ቀላይ፡</foreign>
                     <ref cRef="2 Esr. " loc="19,11">2 Esr. 19,11</ref>; <ref cRef="Matth." loc="18,6">Matth. 18,6</ref>; <ref cRef="1 Tim." loc="6,9">1 Tim. 6,9</ref>;
                        <lbl expand="cum">c.</lbl>
                     <foreign xml:lang="gez">በ፡</foreign>
                     <ref cRef="Ps." loc="123,3">Ps. 123,3</ref>. 2)<cit type="translation" xml:lang="la">
                        <quote> inundare et aqua obruere</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>absorbere</quote>
                     </cit>, de fluviis: <foreign xml:lang="gez">አፍላግ፡ ኢያሰጥሙከ፡</foreign>
                     <ref cRef="Jes." loc="43,2">Jes. 43,2</ref>; <foreign xml:lang="gez">ውኂዝ፡
                        ዘይደፍና፡ ወያሰጥማ፡ ለምድር፡</foreign>
                     <ref cRef="Jer." loc="29,2">Jer. 29,2</ref>; <foreign xml:lang="gez">ያሰጥሞ፡
                        ሐዝሐዘ፡ ፈለግ፡</foreign>
                     <ref cRef="Job" loc="21,33">Job 21,33</ref>, et <lbl expand="translate">transl.</lbl> de iis, quae fluviis comparantur: <foreign xml:lang="gez">አስጠሞሙ፡ ወይን፡</foreign>
                     <ref cRef="Jes." loc="28,7">Jes. 28,7</ref>; de somno, Lit. Jac.</sense>
               </sense>
               
