            <entry xml:id="L4ddcfa4a88c0441281b9c0b7bb7ae32b" n="2989">
               <form>
                  <foreign xml:lang="gez">ተስዕረ</foreign>
               </form>
               <sense xml:lang="la" source="#dillmann" xml:id="Dmain"> <subc>III,1</subc> <sense n="1" xml:id="D1"> <cit type="translation" xml:lang="la">
                                <quote> dissolvi </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> discindi </quote>
                            </cit>: <foreign xml:lang="gez">ይሰዐር፡ ተዋስቦ፡</foreign> (matrimonium) F.N. 24,1; <foreign xml:lang="gez">ዝግብሮሙ፡ የኀልፍ፡ ወይሰዐር፡</foreign> <ref cRef="Act." loc="5,38">Act. 5,38</ref>; <foreign xml:lang="gez">ተስዕረት፡ ምክርከ፡</foreign> <ref cRef="Jes. " loc="14,10">Jes.  14,10</ref>.</sense> <sense n="2" xml:id="D2"> <cit type="translation" xml:lang="la">
                                <quote> solvi </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> aboleri </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> abrogari </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> irritum fieri </quote>
                            </cit> <ref cRef="4 Esr." loc="10,28">4 Esr. 10,28</ref>; <foreign xml:lang="gez">ተስዕረት፡ መንግሥትከ፡</foreign> <ref cRef="Dan." loc="5,28">Dan. 5,28</ref>; <ref cRef="Hebr." loc="7,24">Hebr. 7,24</ref>; <ref cRef="1 Cor. " loc="15,24">1 Cor.  15,24</ref>; <ref cRef="2 Cor." loc="3,11">2 Cor. 3,11</ref>.</sense> <sense n="3" xml:id="D3"> <cit type="translation" xml:lang="la">
                                <quote> abscindi </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> auferri </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> tolli </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> removeri </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> cessare </quote>
                            </cit>: <foreign xml:lang="gez">ተስዕረ፡ አርዑቶሙ፡</foreign> <ref cRef="Jes." loc="9,4">Jes. 9,4</ref>; <ref cRef="Jes." loc="10,27">Jes. 10,27</ref>; <ref cRef="Jes." loc="14,25">Jes. 14,25</ref>; <foreign xml:lang="gez">ይሰዐር፡ ቅንአተ፡ ኤፍሬም፡</foreign> <ref cRef="Jes." loc="11,13">Jes. 11,13</ref>; <ref cRef="Jes." loc="16,10">Jes. 16,10</ref>; <foreign xml:lang="gez">ተስዕረ፡ ኵሉ፡ ትፍሥሕተ፡ ምድር፡</foreign> <ref cRef="Jes." loc="24,11">Jes. 24,11</ref>; <ref cRef="4 Esr." loc="7,13">4 Esr. 7,13</ref>; <foreign xml:lang="gez">ከመ፡ ትሰዐር፡ ዐመፃ፡</foreign> <ref cRef="Dan." loc="9,24">Dan. 9,24</ref>; <foreign xml:lang="gez">ሊቃናት፡ እምነ፡ አንቀጽ፡ ተስዕሩ፡</foreign> <foreign xml:lang="grc">κατέπαυσαν</foreign> <ref cRef="Thren." loc="5,14">Thren. 5,14</ref>; <foreign xml:lang="gez">እፎ፡ ተስዕረ፡ ዘያቀቱ፡ ወተስዕረ፡ ዘያሜምዕ፡</foreign> <foreign xml:lang="grc">ἀναπέπαυται</foreign> <ref cRef="Jes." loc="14,4">Jes. 14,4</ref>; <foreign xml:lang="gez">ይሰዐር፡</foreign> (v. <foreign xml:lang="gez">ይስዐር፡</foreign>) <foreign xml:lang="gez">እምግብሩ፡</foreign> <foreign xml:lang="grc">ἀποστήσεται</foreign> <ref cRef="Num." loc="8,25">Num. 8,25</ref>;</sense> <sense n="4" xml:id="D4"> in specie: <cit type="translation" xml:lang="la">
                                <quote> removeri </quote>
                            </cit> vel <cit type="translation" xml:lang="la">
                                <quote> expelli </quote>
                            </cit> munere (<cit type="translation" xml:lang="la">
                                <quote> depponi </quote>
                            </cit>), <ref cRef="Act." loc="24,27">Act. 24,27</ref>; <foreign xml:lang="gez">ትሰዐር፡ እምግብከ፡ ወእምኵናንከ፡</foreign> <ref cRef="Jes." loc="22,19">Jes. 22,19</ref>; creberrime in canonibus ecclesiasticis: <ref cRef="Can. Ap." loc="19 ">Can. Ap. 19 </ref>(<ref cRef="Can. Ap." loc="18">Can. Ap. 18</ref>); <foreign xml:lang="gez">ይሰዐር፡ እማዕርጊሁ፡</foreign> <ref cRef="Clem." loc="147">Clem. f. 147</ref>; <foreign xml:lang="gez">ይሰዐር፡ እምሢመቱ፡</foreign> <ref cRef="Clem." loc="204,">Clem. f. 204,</ref> al.</sense> <sense n="5" xml:id="D5"> <cit type="translation" xml:lang="la">
                                <quote> absolvi </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> dimitti </quote>
                            </cit>: <foreign xml:lang="gez">ከመ፡ እሰዐር፡</foreign> <foreign xml:lang="grc">ὅπως ἀπολυθῶ</foreign> <ref cRef="Tob." loc="3,6">Tob. 3,6</ref>; <foreign xml:lang="gez">ተስዕርኪ፡ እምደዌኪ፡</foreign> <ref cRef="Luc." loc="13,12">Luc. 13,12</ref>; <foreign xml:lang="grc">καταργεῖσθαι ἀπὸ τοῦ νόμου</foreign> <ref cRef="Rom." loc="7,2">Rom. 7,2</ref>; <ref cRef="Rom." loc="7,6">Rom. 7,6</ref>.</sense> <sense n="6" xml:id="D6"> <cit type="translation" xml:lang="la">
                                <quote> exscindi </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> exstingui </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> deleri </quote>
                            </cit>: <foreign xml:lang="gez">ተስዕረ፡ ሐሤት፡ ወፍሥሓ፡ እምአፉክሙ፡</foreign> <ref cRef="Joel" loc="1,5">Joel 1,5</ref>; <foreign xml:lang="gez">ባሕርኒ፡ ተስዕረት፡ እንከ፡</foreign> <ref cRef="Apoc." loc="21,1">Apoc. 21,1</ref>.</sense> </sense>
               
            <sense xml:lang="en" source="#traces" xml:id="Tmain">
                        <cit type="transcription" xml:lang="gez">
                            <quote>tasǝʿra</quote>
                        </cit> <sense n="7" xml:id="T7"> <cit type="translation" xml:lang="en">
                                <quote>be taken</quote>
                            </cit>, <cit type="translation" xml:lang="en">
                                <quote>be captured</quote>
                            </cit>, <cit type="translation" xml:lang="en">
                                <quote>be seized</quote>
                            </cit> <foreign xml:lang="gez">ዘንተሰ፡ ቃልነ፡ ያጸድቅ፡ አመ፡ ዕለተ፡ ተስዕረ፡ አምባ፡ ተወርዋቱ፡ መትሕተ፡ ጸድፍ፡ በፈቃደ፡ ርዕሱ።</foreign> ‘<cit type="translation" xml:lang="fr">
                                <quote>Mais notre parole démontrera cela le jour de la prise de l’ambā, car il se jeta volontairement au fond d’un précipice</quote>
                            </cit>’ <bibl>
                                <ptr target="bm:ContiRossini1907SarsaDengel"/>
                                <citedRange unit="page"> 103 l. 34-36 (ed.)</citedRange>
                            </bibl>, <bibl>
                                <ptr target="bm:ContiRossini1907SarsaDengelV"/>
                                <citedRange unit="page"> 118 l. 9-10 (tr.)</citedRange>
                            </bibl>, <foreign xml:lang="gez">እምድኅረ፡ ተስዕሮቱ፡ ለሽከና፡ አምባ፡ አስተሓመመ፡ ጥቀ፡ ዝንቱ፡ ንጉሥ፡ መሲሓዊ፡ በእንተ፡ ሥዒሮቱ፡ ለወርቅ፡ አምባ፡</foreign> ‘<cit type="translation" xml:lang="fr">
                                <quote>Après la prise de Šekanā Ambā, le roi chrétien réfléchit beaucoup comment il pouvait prendre Warq Ambā</quote>
                            </cit>’ <bibl>
                                <ptr target="bm:ContiRossini1907SarsaDengel"/>
                                <citedRange unit="page"> 106 l. 20-21 (ed.)</citedRange>
                            </bibl>, <bibl>
                                <ptr target="bm:ContiRossini1907SarsaDengelV"/>
                                <citedRange unit="page"> 121 l. 12-14 (tr.)</citedRange>
                            </bibl> </sense> <sense n="8" xml:id="T8"> (of a soldier) <cit type="translation" xml:lang="en">
                                <quote>desert</quote>
                            </cit> <foreign xml:lang="gez">እምድኅረ፡ ኃለፈ፡ ሰዐተ፡ ንዋም፡ ተስዕሩ፡ ኵሉ፡ ሠራዊቱ፡ ለፋሲሎ፡ እለ፡ የዐቅቡ፡ ካበ፡ በበ፡ በሩ።</foreign> ‘<cit type="translation" xml:lang="fr">
                                <quote>après l’heure de sommeil, tous les soldats de Fāsilo, qui gardaient chacun une des entrées de la forteresse, désertèrent</quote>
                            </cit>’ <bibl>
                                <ptr target="bm:ContiRossini1907SarsaDengel"/>
                                <citedRange unit="page"> 36 l. 15-17 (ed.)</citedRange>
                            </bibl>, <bibl>
                                <ptr target="bm:ContiRossini1907SarsaDengelV"/>
                                <citedRange unit="page"> 42 l. 12-14 (tr.)</citedRange>
                            </bibl> </sense> </sense>
                </entry>
