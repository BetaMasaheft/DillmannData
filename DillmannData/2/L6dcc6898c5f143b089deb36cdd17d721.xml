               <sense xml:lang="la" source="#dillmann" xml:id="Dmain"> <subc>I,1</subc> [<foreign xml:lang="he">צוּק</foreign> <foreign xml:lang="ar">ضاق</foreign> <foreign xml:lang="he">עוּק</foreign> <foreign xml:lang="syr">ܥܳܩ</foreign> <foreign xml:lang="gez">ዐጠቀ፡ ጸዐቀ፡ ጠዐቀ፡</foreign>] <sense n="1" xml:id="D1"> <cit type="translation" xml:lang="la">
                                <quote> angustum </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> coarctatum esse </quote>
                            </cit>: <foreign xml:lang="gez">ትጠውቅ፡ ነፍስየ፡</foreign> (var. pro <foreign xml:lang="gez">ትጠወቅ፡</foreign>) <ref cRef="Job" loc="10,1">Job 10,1</ref>; <foreign xml:lang="gez">ልብየኒ፡ ይጠውቀኒ፡</foreign> (v. <foreign xml:lang="gez">ይጠወቀኒ፡</foreign>) <ref cRef="Thren." loc="1,22">Thren. 1,22</ref>.</sense> <sense n="2" xml:id="D2"> <cit type="translation" xml:lang="la">
                                <quote> coangustare </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> comprimere </quote>
                            </cit>: <foreign xml:lang="gez">እጠውቃ፡ ለነፍስየ፡ በእዴየ፡</foreign> (var. pro <foreign xml:lang="gez">ኣጠውቃ፡</foreign>) <ref cRef="Job" loc="13,14">Job 13,14</ref>; <lbl expand="videas">vid.</lbl> <ref cRef="1 Cor. " loc="9,27">1 Cor.  9,27</ref> ant.</sense> </sense>
               
