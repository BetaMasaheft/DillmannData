                    </form>
               <sense xml:lang="la" source="#dillmann" xml:id="Dmain"> <subc>I,1</subc> [contiguum videtur <foreign xml:lang="gez">ዐቀበ፡</foreign>, an <foreign xml:lang="ar">وَقَى</foreign>?] <gramGrp> <pos expand="Subjunctivus">Subj.</pos> </gramGrp> <foreign xml:lang="gez">ይዑቅ፡</foreign> <sense n="1" xml:id="D1"> <cit type="translation" xml:lang="la">
                                <quote> servare </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> observare </quote>
                            </cit>: <foreign xml:lang="gez">ነፍሶ፡ ዑቅ፡</foreign> <foreign xml:lang="grc">τὴν ψυχὴν αὐτοῦ διαφύλαξον</foreign> <ref cRef="Job" loc="2,6">Job 2,6</ref>; <foreign xml:lang="gez">ዑቅ፡ ትእዛዘ፡ ወግበር፡</foreign> <ref cRef="Kuf." loc="77.">Kuf. p. 77.</ref> <sense n="a" xml:id="D1a"> <cit type="translation" xml:lang="la">
                                    <quote> respicere </quote>
                                </cit>, <cit type="translation" xml:lang="la">
                                    <quote> attendere </quote>
                                </cit>, <cit type="translation" xml:lang="la">
                                    <quote> videre </quote>
                                </cit>, <cit type="translation" xml:lang="la">
                                    <quote> prospicere </quote>
                                </cit>, <lbl expand="cum">c.</lbl> <gramGrp> <case value="accusativus">Acc.</case> </gramGrp> <lbl expand="persona">pers.</lbl> vel rei: <foreign xml:lang="gez">ዑቀኒ፡</foreign> <foreign xml:lang="grc">ἐπιγνῶναί με</foreign> <ref cRef="Ruth" loc="2,10">Ruth 2,10</ref>; <foreign xml:lang="grc">εἰδέναι</foreign> <ref cRef="1 Thess." loc="5,12">1 Thess. 5,12</ref>; <foreign xml:lang="gez">ዘኢየዐውቅ፡ ቤቶ፡ ይወርሶ፡ ነፋስ፡</foreign> <ref cRef="Prov." loc="11,29">Prov. 11,29</ref>; <foreign xml:lang="grc">βλέπειν</foreign> <ref cRef="Col." loc="4,17">Col. 4,17</ref>; diaconi <foreign xml:lang="gez">ይዑቅዎሙ፡ ለእለ፡ አልቦሙ፡</foreign> (prospiciant iis qui nihil habent)<bibl>
                                    <ptr target="bm:Ludolf1691Commentarius"/>
                                </bibl> p. 321; etiam <lbl expand="cum">c.</lbl> <foreign xml:lang="gez">ላዕለ፡</foreign> vel <foreign xml:lang="gez">ለ፡</foreign> pers.: <foreign xml:lang="gez">ዑቅኬ፡ ላዕለ፡</foreign> (v. <foreign xml:lang="gez">ለ</foreign>)<foreign xml:lang="gez">ኢዮብ፡</foreign> <foreign xml:lang="grc">προσέχειν</foreign> <ref cRef="Job" loc="2,3">Job 2,3</ref>.</sense> <sense n="b" xml:id="D1b"> <cit type="translation" xml:lang="la">
                                    <quote> cavere </quote>
                                </cit> <sense n="α" xml:id="D1bα"> aliquem vel aliquid i.e. <cit type="translation" xml:lang="la">
                                        <quote> vitare </quote>
                                    </cit>, <lbl expand="cum">c.</lbl> <gramGrp> <case value="accusativus">Acc.</case> </gramGrp>: <foreign xml:lang="gez">ዑቅዎሙ፡ ለከለባት፡</foreign> <foreign xml:lang="grc">βλέπετε τοὺς κύνας</foreign> <ref cRef="Phil." loc="3,2">Phil. 3,2</ref>; <foreign xml:lang="gez">ዘንተ፡ ዑቅ፡ ከመ፡ ኢታግብኦ፡ ለወልድየ፡</foreign> <ref cRef="Gen." loc="24,8">Gen. 24,8</ref>; <lbl expand="sequens">seq.</lbl> <foreign xml:lang="gez">እምነ፡</foreign> rei, ut: <foreign xml:lang="gez">ዑቅ፡ ርእሰከ፡</foreign> (v. <foreign xml:lang="gez">ዑቅ፡</foreign>) <foreign xml:lang="gez">እምእኩይ፡</foreign> <ref cRef="Sir." loc="4,20">Sir. 4,20</ref>.</sense> <sense n="β" xml:id="D1bβ"> <cit type="translation" xml:lang="la">
                                        <quote> providere </quote>
                                    </cit>, <cit type="translation" xml:lang="la">
                                        <quote> videre </quote>
                                    </cit>, <cit type="translation" xml:lang="la">
                                        <quote> cautionem adhibere </quote>
                                    </cit>, sive addito <foreign xml:lang="gez">ርእሶ፡</foreign> (proprie: <cit type="translation" xml:lang="la">
                                        <quote> semet custodire </quote>
                                    </cit>) sive omisso; <gramGrp> <pos expand="absolute">absol.</pos> </gramGrp>: <foreign xml:lang="gez">ዑቅ፡ ርእሰከ፡</foreign> <ref cRef="1 Reg. " loc="19,2">1 Reg.  19,2</ref>; <ref cRef="Deut." loc="4,9">Deut. 4,9</ref>; <ref cRef="Deut." loc="8,11">Deut. 8,11</ref>; <ref cRef="Marc." loc="13,9">Marc. 13,9</ref>; <ref cRef="Luc." loc="17,3">Luc. 17,3</ref>; <foreign xml:lang="gez">ዑቅ፡ ወተዐቀብ፡</foreign> <ref cRef="Sir." loc="13,13">Sir. 13,13</ref>; <foreign xml:lang="gez">ዑቂ፡ ወለብዊ፡</foreign> <ref cRef="Did." loc="3">Did. 3</ref>; <foreign xml:lang="gez">ዑቁ፡ ወኢታፅርዑ፡</foreign> <ref cRef="2 Esr." loc="7,23">2 Esr. 7,23</ref>; <lbl expand="cum">c.</lbl> <foreign xml:lang="gez">በእንተ፡</foreign> rei <ref cRef="1 Tim." loc="4,16">1 Tim. 4,16</ref>; <lbl expand="sequens">seq.</lbl> <foreign xml:lang="gez">ኢ፡</foreign> cum <gramGrp> <pos expand="Subjunctivus">Subj.</pos> </gramGrp> verbi (<cit type="translation" xml:lang="la">
                                        <quote> ne </quote>
                                    </cit>): <ref cRef="Deut." loc="4,23">Deut. 4,23</ref>; <ref cRef="Deut." loc="6,12">Deut. 6,12</ref>; <ref cRef="Deut." loc="11,16">Deut. 11,16</ref>; <ref cRef="Deut." loc="12,13">Deut. 12,13</ref>; <ref cRef="Deut." loc="12,19">Deut. 12,19</ref>; <ref cRef="Deut." loc="12,30">Deut. 12,30</ref>; <ref cRef="Gen." loc="24,6">Gen. 24,6</ref>; <foreign xml:lang="gez">ዑቁ፡ ኢታርኅዉ፡</foreign> <ref cRef="2 Esr." loc="17,3">2 Esr. 17,3</ref>; <ref cRef="Ep. Jer." loc="4">Ep. Jer. 4</ref>; <foreign xml:lang="gez">ዑቅ፡ ኢያስሕቱከ፡</foreign> <ref cRef="Jes." loc="41,10">Jes. 41,10</ref>; <ref cRef="Sir." loc="21,1">Sir. 21,1</ref>; <ref cRef="Sir." loc="29,24">Sir. 29,24</ref>; <ref cRef="Luc." loc="21,34">Luc. 21,34</ref>; <ref cRef="Hebr." loc="12,25">Hebr. 12,25</ref>; raro <lbl expand="sequens">seq.</lbl> Imperf., ut: <foreign xml:lang="gez">ዑቅ፡ አልቦ፡ ዘትኄሊ፡</foreign> <ref cRef="Job" loc="1,8">Job 1,8</ref>; <ref cRef="Matth." loc="9,30">Matth. 9,30</ref>; <lbl expand="sequens">seq.</lbl> <foreign xml:lang="gez">ከመ፡</foreign> <lbl expand="cum">c.</lbl> <gramGrp> <pos expand="Subjunctivus">Subj.</pos> </gramGrp> (<cit type="translation" xml:lang="la">
                                        <quote> ut </quote>
                                    </cit>): <foreign xml:lang="gez">ዑቁ፡ ከመ፡ ትግበሩ፡</foreign> <ref cRef="Deut." loc="27,1">Deut. 27,1</ref>; <ref cRef="Hebr." loc="8,5">Hebr. 8,5</ref>; <lbl expand="sequens">seq.</lbl> <foreign xml:lang="gez">ዮጊ፡ ኢ፡</foreign> <cit type="translation" xml:lang="la">
                                        <quote> ne forte </quote>
                                    </cit>c. <gramGrp> <pos expand="Subjunctivus">Subj.</pos> </gramGrp>, <ref cRef="Apoc. " loc="19,10">Apoc.  19,10</ref> Platt.</sense> </sense> </sense> <sense n="2" xml:id="D2"> <cit type="translation" xml:lang="la">
                                <quote> animadvertere </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> cognoscere </quote>
                            </cit>: <foreign xml:lang="gez">ዑቅ፡ ከመ፡</foreign> „scito, quod“ Lud. e Vit. Pach. [quae verbi <foreign xml:lang="gez">አወቀ፡</foreign> significatio in lingua Amh. tritissima est; <lbl expand="conferas">cfr.</lbl> <foreign xml:lang="he">נָצַר</foreign> <cit type="translation" xml:lang="la">
                                <quote> custodire </quote>
                            </cit>, at <foreign xml:lang="ar">نظر</foreign> <foreign xml:lang="gez">ነጸረ፡</foreign> <cit type="translation" xml:lang="la">
                                <quote> intueri </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> aspicere </quote>
                            </cit>].</sense> <sense n="3" xml:id="D3"> intrans.: <cit type="translation" xml:lang="la">
                                <quote> notum </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> manifestum esse </quote>
                            </cit>vel <cit type="translation" xml:lang="la">
                                <quote> fieri </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> apparere </quote>
                            </cit>: <foreign xml:lang="gez">ድኅረ፡ የዐውቅ፡ እከይሁ፡</foreign> (nisi <foreign xml:lang="gez">ያዐውቅ፡</foreign> <foreign xml:lang="grc">ἐκφαίνει τὴν κακίαν</foreign> legere malueris) <ref cRef="Sir." loc="14,7">Sir. 14,7</ref>; <foreign xml:lang="gez">አዕረፈ፡</foreign> (<foreign xml:lang="gez">ያሬድ፡ ማሕሌታዊ፡</foreign>) <foreign xml:lang="gez">በሰላም፡ ወመቃብሪሁሰ፡ ኢተዐውቅ፡</foreign> (ni <foreign xml:lang="gez">ተዐውቀ፡</foreign> legendum est) Sx. <ref cRef="Genb." loc="11">Genb. 11</ref>; <foreign xml:lang="gez">ናሁኬ፡ የዐውቅ፡ ከመ፡</foreign> … („elucet“, ni <foreign xml:lang="gez">ያዐውቅ፡</foreign> „indicat“ interpretandum est) M.M. f. 109.</sense> </sense>
               
