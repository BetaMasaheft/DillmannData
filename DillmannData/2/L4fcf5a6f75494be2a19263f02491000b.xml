               <sense xml:lang="la" source="#dillmann" xml:id="Dmain"> <subc>I,2</subc> [vid. <foreign xml:lang="gez">ጸለወ፡</foreign>] (Imperf. <foreign xml:lang="gez">ይጸሊ፡</foreign> quod passim occurrit, ut <ref cRef="Clem." loc="161">Clem. f. 161</ref>; <ref cRef="Matth." loc="26,36">Matth. 26,36</ref> Platt, reprobandum est) <cit type="translation" xml:lang="la">
                            <quote> precari </quote>
                        </cit> ad Deum, <cit type="translation" xml:lang="la">
                            <quote> preces facere </quote>
                        </cit>, <cit type="translation" xml:lang="la">
                            <quote> orare </quote>
                        </cit>: <ref cRef="Matth." loc="6,5">Matth. 6,5</ref>; <ref cRef="Matth." loc="6,7">Matth. 6,7</ref>; <ref cRef="Matth." loc="6,9">Matth. 6,9</ref>; <ref cRef="Matth." loc="26,36">Matth. 26,36</ref>; <ref cRef="Matth." loc="26,39">Matth. 26,39</ref>; <ref cRef="Matth." loc="26,41">Matth. 26,41</ref>; <ref cRef="Marc. " loc="1,35">Marc.  1,35</ref>; <ref cRef="Luc." loc="18,11">Luc. 18,11</ref>; <ref cRef="Judae" loc="20">Judae 20</ref>; <ref cRef="4 Esr." loc="9,27">4 Esr. 9,27</ref>; <foreign xml:lang="gez">ፈጸመ፡ ጸልዮ፡</foreign> <ref cRef="Kuf." loc="12">Kuf. 12</ref>; <gramGrp> <pos expand="infinitivus&#xA;                            verbalis">Infin. verb.</pos> </gramGrp> <foreign xml:lang="gez">ጸሊዮ፡</foreign> Rel. Bar.; <cb n="1263" xml:id="c1263"/> <lbl expand="cum">c.</lbl> <foreign xml:lang="gez">ኀበ፡</foreign> numinis <ref cRef="Job" loc="33,26">Job 33,26</ref>; <ref cRef="Sir." loc="50,19">Sir. 50,19</ref>; <ref cRef="Jes. " loc="44,17">Jes.  44,17</ref>; <ref cRef="Jer." loc="39,16">Jer. 39,16</ref>; <ref cRef="Num." loc="11,2">Num. 11,2</ref>; <ref cRef="1 Reg." loc="1,11">1 Reg. 1,11</ref>; <ref cRef="Dan." loc="9,4">Dan. 9,4</ref>; <ref cRef="Ps." loc="31,7">Ps. 31,7</ref>; <ref cRef="1 Cor." loc="11,13">1 Cor. 11,13</ref>; <lbl expand="cum">c.</lbl> <foreign xml:lang="gez">ቅድመ፡</foreign> <ref cRef="Sir." loc="39,5">Sir. 39,5</ref>; <lbl expand="cum">c.</lbl> <foreign xml:lang="gez">በእንተ፡</foreign> <cit type="translation" xml:lang="la">
                            <quote> pro </quote>
                        </cit> aliquo (<cit type="translation" xml:lang="la">
                            <quote> intercedere </quote>
                        </cit>) <ref cRef="Jer." loc="7,16">Jer. 7,16</ref>; <foreign xml:lang="gez">ይጼሊ፡ በእንቲአክሙ፡</foreign> <ref cRef="Job" loc="42,8">Job 42,8</ref>; <ref cRef="Jes." loc="37,4">Jes. 37,4</ref>; <foreign xml:lang="gez">ጸልዩ፡ በእንተ፡ ሕይወቱ፡</foreign> <ref cRef="Bar." loc="1,11">Bar. 1,11</ref>; <foreign xml:lang="gez">ጸልዩ፡ በእንቲአነ፡</foreign> Mavâs 60; vel <lbl expand="cum">c.</lbl> <foreign xml:lang="gez">ለ፡</foreign> pers.: <ref cRef="Jer." loc="11,14">Jer. 11,14</ref>; <ref cRef="Jer." loc="14,11">Jer. 14,11</ref>; <ref cRef="Bar." loc="1,13">Bar. 1,13</ref>; <foreign xml:lang="gez">ጸሊ፡ ለነ፡ ኀበ፡ እግዚአብሔር፡ በእንተ፡</foreign> … <ref cRef="Jer." loc="49,2">Jer. 49,2</ref>; vel <lbl expand="cum">c.</lbl> <foreign xml:lang="gez">ላዕለ፡</foreign> pers. (<cit type="translation" xml:lang="la">
                            <quote> super </quote>
                        </cit> aliquem et <cit type="translation" xml:lang="la">
                            <quote> pro </quote>
                        </cit> aliquo): <ref cRef="Gen." loc="20,7">Gen. 20,7</ref>; <ref cRef="Deut." loc="9,20">Deut. 9,20</ref>; <ref cRef="1 Tim." loc="2,1">1 Tim. 2,1</ref>; <foreign xml:lang="gez">ጸለየ፡ ላዕሌሆሙ፡ ወባረኮሙ፡</foreign> <ref cRef="Clem." loc="15">Clem. f. 15</ref>; <ref cRef="Clem." loc="16">Clem. f. 16</ref>; vel <foreign xml:lang="gez">ዲበ፡</foreign> <ref cRef="Ex." loc="8,5">Ex. 8,5</ref>; <ref cRef="Ex." loc="8,24">Ex. 8,24</ref>; <lbl expand="cum">c.</lbl> <gramGrp> <case value="accusativus">Acc.</case> </gramGrp> precum: <ref cRef="2 Reg." loc="7,27">2 Reg. 7,27</ref>; <lbl expand="sequens">seq.</lbl> <foreign xml:lang="gez">ከመ፡</foreign> <lbl expand="cum">c.</lbl> <gramGrp> <pos expand="Subjunctivus">Subj.</pos> </gramGrp>: <ref cRef="Kuf." loc="11">Kuf. 11</ref>; <foreign xml:lang="gez">ጸሊ፡ ኀበ፡ ልዑል፡ ከመ፡ ያርትዓ፡ ለፍኖትከ፡</foreign> <ref cRef="Sir. " loc="37,15">Sir.  37,15</ref>; <ref cRef="Phil." loc="1,9">Phil. 1,9</ref>; vel <lbl expand="sequens">seq.</lbl> <gramGrp> <pos expand="Subjunctivus">Subj.</pos> </gramGrp>: <ref cRef="1 Thess." loc="3,10">1 Thess. 3,10</ref>; <ref cRef="Sx. Teq." loc="3 ">Sx. Teq. 3 </ref>Enc. <sense n="a" xml:id="Da"> <cit type="translation" xml:lang="la">
                                <quote> exorare </quote>
                            </cit> aliquid: <foreign xml:lang="gez">ዘጸለይከኒ፡ ኀቤሁ፡ ይሰምዐከ፡</foreign> <ref cRef="Job " loc="22,27">Job  22,27</ref>; <ref cRef="Marc." loc="11,24">Marc. 11,24</ref>; <foreign xml:lang="gez">ትጼሊ፡ ዳኅና፡</foreign> <ref cRef="Sx. Haml." loc="18 ">Sx. Haml. 18 </ref> Enc.</sense> <sense n="b" xml:id="Db"> <cit type="translation" xml:lang="la">
                                <quote> vovere </quote>
                            </cit>: <foreign xml:lang="gez">ጸልዮት፡</foreign> <ref cRef="Koh." loc="5,4">Koh. 5,4</ref>; <ref cRef="Num." loc="30,11">Num. 30,11</ref>; <ref cRef="Num. " loc="30,13">Num.  30,13</ref>; <ref cRef="Jon." loc="2,10">Jon. 2,10</ref> <lbl expand="varia lectio">var.</lbl>; <lbl expand="cum">c.</lbl> <foreign xml:lang="gez">ለ፡</foreign> Dei: <ref cRef="Koh." loc="5,3">Koh. 5,3</ref>; <foreign xml:lang="gez">ጸሎተ፡ ጸለየ፡ ለእግዚአብሔር፡ እንዘ፡ ይብል፡</foreign> <ref cRef="Kuf." loc="27">Kuf. 27</ref>; <ref cRef="Kuf. " loc="31.">Kuf.  31.</ref> </sense> </sense>
               
