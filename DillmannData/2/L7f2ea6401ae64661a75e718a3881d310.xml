         <div>
            <entry xml:id="L7f2ea6401ae64661a75e718a3881d310" n="5333">
               <form>
                  <foreign xml:lang="gez">ናፈቀ</foreign>
               </form>
               <sense xml:lang="la" source="#dillmann" xml:id="Dmain"> <subc>I,3</subc> <sense n="1" xml:id="D1">
                     <cit type="translation" xml:lang="la">
                        <quote>dividere</quote>
                     </cit>
                     <ref cRef="Gen." loc="33,1">Gen. 33,1</ref> F; <ref cRef="Jes." loc="30,28">Jes. 30,28</ref>
                     <lbl expand="varia lectio">var.</lbl>; Canaan <foreign xml:lang="gez">ዘይናፍቅ፡
                        መንግሥተ፡ እምደቂቀ፡ ሴም፡</foreign> (qui regni Semitici partem abripit) <ref cRef="Kebr. Nag." loc="9">Kebr. Nag. 9</ref>; in specie <cit type="translation" xml:lang="la">
                        <quote>in duas partes</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>dimidiare</quote>
                     </cit>, <ref cRef="Gen." loc="32,8">Gen. 32,8</ref> F; <foreign xml:lang="gez">ኢይናፍቁ፡ መዋዕሊሆሙ፡</foreign>
                     <cit type="translation" xml:lang="la">
                        <quote>ad dimidium perducent dies suos</quote>
                     </cit>
                     <ref cRef="Ps. " loc="54,27">Ps. 54,27</ref>.</sense>
                  <sense n="2" xml:id="D2">
                     <cit type="translation" xml:lang="la">
                        <quote>ambiguum esse</quote>
                     </cit> animo, <cit type="translation" xml:lang="la">
                        <quote>inconstantem esse</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>haesitare</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>dubitare</quote>
                     </cit>, in specie: <cit type="translation" xml:lang="la">
                        <quote>hypocritam agere</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>simulatorem</quote>
                     </cit> vel <cit type="translation" xml:lang="la">
                        <quote>incredulum esse</quote>
                     </cit>; <foreign xml:lang="gez">ኢንናፍቅ፡</foreign>
                     <cit type="translation" xml:lang="la">
                        <quote>ne dubitemus</quote>
                     </cit>
                     <ref cRef="Jsp." loc="381">Jsp. p. 381</ref>; <foreign xml:lang="gez">ኢናፈቀ፡
                        በእንተ፡ መልእክተ፡ ንጉሥ፡ </foreign>
                     <ref cRef="Jsp." loc="302">Jsp. p. 302</ref>; <foreign xml:lang="gez">እንዘ፡
                        ንናፍቅ፡ እመ፡ ውእቱሂ፡ </foreign> (<cit type="translation" xml:lang="la">
                        <quote>dubitans</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>an sit</quote>
                     </cit>) <ref cRef="Kid." loc="34">Kid. f. 34</ref>; <foreign xml:lang="gez">ተአመነ፡ ወኢናፈቀ፡</foreign>
                     <ref cRef="Rom." loc="4,19">Rom. 4,19</ref>; <ref cRef="Rom." loc="4,20">Rom.
                        4,20</ref>; <ref cRef="Matth." loc="14,31">Matth. 14,31</ref>; <ref cRef="Matth." loc="28,17">Matth. 28,17</ref>; <ref cRef="Jac. " loc="1,6">Jac. 1,6</ref>; <foreign xml:lang="gez">ሰአሎ፡ እንዘ፡ ኢትናፍቅ፡</foreign>
                     <ref cRef="Herm." loc="37">Herm. p. 37</ref>; <ref cRef="Sir." loc="1,28">Sir.
                        1,28</ref>; <foreign xml:lang="gez">ኢትትወላወል፡ ወኢትናፍቅ፡</foreign>
                     <ref cRef="Sir." loc="2,2">Sir. 2,2</ref>; <foreign xml:lang="gez">ልብ፡
                        ዘይናፍቅ፡</foreign>
                     <foreign xml:lang="grc">καρδίαι δειλαί </foreign>
                     <ref cRef="Sir." loc="2,12">Sir. 2,12</ref>; <foreign xml:lang="grc">ὁ
                        ἀσθενῶν</foreign> (<lbl expand="oppositum, -o, -nitur">opp.</lbl>
                     <foreign xml:lang="grc">πιστεύων</foreign>) <ref cRef="Rom. " loc="14,2">Rom.
                        14,2</ref>; <foreign xml:lang="gez">ዘይናፍቅ፡</foreign>
                     <cit type="translation" xml:lang="la">
                        <quote>hypocrita</quote>
                     </cit> (<foreign xml:lang="grc">ὁ ὑποκρινόμενος</foreign>) <ref cRef="Sir." loc="35,15">Sir. 35,15</ref>; <ref cRef="Sir." loc="36,2">Sir. 36,2</ref>;
                        <foreign xml:lang="grc">δίψυχος</foreign>
                     <ref cRef="Jac." loc="4,8">Jac. 4,8</ref>; <ref cRef="Herm." loc="38">Herm. p.
                        38</ref>; <ref cRef="Herm." loc="72">Herm. p. 72</ref>; <foreign xml:lang="gez">ናፍቆ፡</foreign>
                     <foreign xml:lang="grc">ὑποκρίσεις</foreign> 1 <ref cRef="Petr." loc="2,1">Petr. 2,1</ref>; <foreign xml:lang="grc">διψυχία</foreign>
                     <ref cRef="Herm." loc="5">Herm. p. 5</ref>; <foreign xml:lang="gez">ናፍቆት፡</foreign>
                     <foreign xml:lang="grc">διχοστασίαι </foreign>
                     <ref cRef="Herm." loc="17">Herm. p. 17</ref>; <foreign xml:lang="gez">እመቦ፡
                        ዘይናፍቁ፡ ወይክሕዱ፡ ሃይማኖቶሙ፡ </foreign>
                     <ref cRef="Kedr f." loc="77.">Kedr f. 77.</ref>
                  </sense>
               </sense>
               
