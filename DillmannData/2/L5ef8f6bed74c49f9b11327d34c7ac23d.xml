            <entry xml:id="L5ef8f6bed74c49f9b11327d34c7ac23d" n="6452">
               <form>
                  <foreign xml:lang="gez">ተወለጠ</foreign>
               </form>
               <sense xml:lang="la" source="#dillmann" xml:id="Dmain"> <subc>III,2</subc> <cit type="translation" xml:lang="la">
                     <quote>mutari</quote>
                  </cit>, <cit type="translation" xml:lang="la">
                     <quote>commutari</quote>
                  </cit>, <cit type="translation" xml:lang="la">
                     <quote>permutari</quote>
                  </cit>, <cit type="translation" xml:lang="la">
                     <quote>immutari</quote>
                  </cit>, <cit type="translation" xml:lang="la">
                     <quote>transformari</quote>
                  </cit>: <foreign xml:lang="gez">እፎ፡ ተወለጠት፡ </foreign>
                  <ref cRef="Jer." loc="31,39">Jer. 31,39</ref>; <foreign xml:lang="gez">ኢይትዌለጥ፡</foreign>
                  <ref cRef="Mal." loc="3,6">Mal. 3,6</ref>; <ref cRef="Ps." loc="101,28">Ps.
                     101,28</ref>; <ref cRef="Hebr." loc="1,12">Hebr. 1,12</ref>; <foreign xml:lang="gez">እመ፡ ይትዌለጥ፡ ሥርዐተ፡ መዓልት፡ ወሥርዐተ፡ ሌሊት፡</foreign>
                  <ref cRef="Jer. " loc="40,20">Jer. 40,20</ref>
                  <lbl expand="versio altera">vers. alt.</lbl>; <foreign xml:lang="gez">ይትዌለጥ፡ ኀይሎሙ፡
                     ለመላእክት፡</foreign>
                  <ref cRef="Jes." loc="41,1">Jes. 41,1</ref>; <foreign xml:lang="gez">ተወለጠ፡ ርእየተ፡
                     ገጹ፡</foreign>
                  <ref cRef="Dan." loc="3,19">Dan. 3,19</ref>; <ref cRef="Judith" loc="10,7">Judith
                     10,7</ref>; <ref cRef="Jes. " loc="29,22">Jes. 29,22</ref>; <ref cRef="Hen." loc="39,14">Hen. 39,14</ref>; <foreign xml:lang="gez">ተወለጠ፡ ገጾሙ፡
                     ወጸልመ፡</foreign>
                  <ref cRef="Jer." loc="37,6">Jer. 37,6</ref>; <ref cRef="Lev. " loc="13,3">Lev.
                     13,3</ref>; <ref cRef="Lev." loc="13,17">Lev. 13,17</ref>
                  <lbl expand="sequens">seq.</lbl>; <ref cRef="Hen." loc="67,11">Hen. 67,11</ref>;
                     <foreign xml:lang="gez">መንፈስየ፡ ተወለጠ፡</foreign>
                  <ref cRef="Hen. " loc="71,11">Hen. 71,11</ref>; <foreign xml:lang="gez">ተወለጠ፡ ወኮነ፡
                     ከመ፡ መልአክ፡</foreign>
                  <ref cRef="Asc. Jes." loc="9,30">Asc. Jes. 9,30</ref>; <foreign xml:lang="gez">ተወልጦቱ፡</foreign>
                  <ref cRef="Asc. Jes." loc="3,13">Asc. Jes. 3,13</ref>; <foreign xml:lang="gez">ይትዌለጥ፡ ልቦሙ፡ </foreign>
                  <ref cRef="4 Esr." loc="4,31">4 Esr. 4,31</ref>; <lbl expand="cum">c.</lbl>
                  <foreign xml:lang="gez">እምነ፡</foreign> formae prioris: <foreign xml:lang="gez">ይትወለጥ፡ ልቡ፡ እምሰብእ፡</foreign>
                  <ref cRef="Dan." loc="4,13">Dan. 4,13</ref>; <ref cRef="Job" loc="37,4">Job
                     37,4</ref>; <lbl expand="cum">c.</lbl>
                  <foreign xml:lang="gez">ኀበ፡</foreign>
                  <cit type="translation" xml:lang="la">
                     <quote>in</quote>
                  </cit> aliquid: <cb n="0890" xml:id="c0890"/>
                  <foreign xml:lang="gez">ተወለጠ፡ ፍሥሓሆሙ፡ ኀበ፡ ሐዘን፡</foreign>
                  <ref cRef="Sx. Teq." loc="14.">Sx. Teq. 14.</ref>
                  <sense n="a" xml:id="Da">
                     <cit type="translation" xml:lang="la">
                        <quote>transfigurari</quote>
                     </cit>: <ref cRef="Marc." loc="9,2">Marc. 9,2</ref>; <ref cRef="Matth." loc="17,2">Matth. 17,2</ref>; <ref cRef="1 Cor." loc="15,51">1 Cor.
                        15,51</ref>; <ref cRef="1 Cor." loc="15,52">1 Cor. 15,52</ref>.</sense>
                  <sense n="b" xml:id="Db">
                     <cit type="translation" xml:lang="la">
                        <quote>transferri ad</quote>
                     </cit> alium, <lbl expand="cum">c.</lbl>
                     <foreign xml:lang="gez">ለ፡</foreign>, ut <foreign xml:lang="gez">ይትዌለጥ፡ አብያቲሆሙ፡
                        ለካልኣን፡</foreign>
                     <ref cRef="Jer." loc="6,12">Jer. 6,12</ref>.</sense>
                  <sense n="c" xml:id="Dc">
                     <cit type="translation" xml:lang="la">
                        <quote>habitum</quote>
                     </cit> vel <cit type="translation" xml:lang="la">
                        <quote>vestitum</quote>
                     </cit> suum <cit type="translation" xml:lang="la">
                        <quote>mutare</quote>
                     </cit>: Herodes <foreign xml:lang="gez">ያንሶሱ፡ ማእከለ፡ ሰብእ፡ ተወሊጦ፡</foreign>
                     (explorandi causa) <ref cRef="Jsp. " loc="328.">Jsp. p. 328.</ref>
                  </sense>
                  <sense n="d" xml:id="Dd">
                     <cit type="translation" xml:lang="la">
                        <quote>diversum esse</quote>
                     </cit>, <lbl expand="cum">c.</lbl>
                     <foreign xml:lang="gez">እምነ፡</foreign>, <ref cRef="Hen." loc="24,2">Hen.
                        24,2</ref>; <ref cRef="Hen. " loc="33,1">Hen. 33,1</ref>.</sense>
               </sense>
               
