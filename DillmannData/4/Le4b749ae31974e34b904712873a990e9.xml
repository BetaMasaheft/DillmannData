            <entry xml:id="Le4b749ae31974e34b904712873a990e9" n="8156">
               <form>
                  <foreign xml:lang="gez">ዲፓ</foreign>
               </form>
               <sense xml:lang="la" source="#dillmann" xml:id="Dmain">, etiam <foreign xml:lang="gez">ዲባ፡</foreign>
                  <gramGrp>
                     <pos expand="Substantivum">subst.</pos>
                  </gramGrp>
                  <cit type="translation" xml:lang="la">
                     <quote>insidiae</quote>
                  </cit>, <sense n="a" xml:id="Da"> in <gramGrp>
                        <case value="accusativus">Acc.</case>
                     </gramGrp>: <foreign xml:lang="gez">ነበረ፡ ዲፓ፡</foreign> (v. <foreign xml:lang="gez">ዲባ፡</foreign>) <foreign xml:lang="grc">ἐνήδρευσεν </foreign>
                     <cit type="translation" xml:lang="la">
                        <quote>insidiatus est</quote>
                     </cit>
                     <ref cRef="1 Reg." loc="15,5">1 Reg. 15,5</ref>; <foreign xml:lang="gez">ቆመ፡
                        ዲፓ፡ ኀበ፡ </foreign> … <ref cRef="Jud." loc="9,44">Jud. 9,44</ref>; femina
                        <foreign xml:lang="gez">ጸንሐቶ፡ ዲፓ፡ ለንጉሥ፡</foreign> Jsp. p. 276.</sense>
                  <sense n="b" xml:id="Db">
                     <foreign xml:lang="gez">በዲፓ፡</foreign>, ut: <foreign xml:lang="gez">ይጸንሕዎ፡ ሎቱ፡
                        በዲፓ፡ </foreign>
                     <ref cRef="Jsp." loc="297">Jsp. p. 297</ref>; <foreign xml:lang="gez">በዲፓ፡
                        መሠጥዎ፡</foreign> Lud. e Mss. Colb. — <cit type="translation" xml:lang="la">
                        <quote>Voc</quote>
                     </cit>. <cit type="translation" xml:lang="la">
                        <quote>Ae</quote>
                     </cit>.: <foreign xml:lang="gez">ዲፓ፡</foreign> (v. <foreign xml:lang="gez">ዲባ፡</foreign>) <foreign xml:lang="gez">ዘ፡ አርዋ።</foreign>
                  </sense>
               </sense>
               
