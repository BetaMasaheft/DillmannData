                    </form>
               <sense xml:lang="la" source="#dillmann" xml:id="Dmain"> <subc>I,1</subc> [<foreign xml:lang="ar">طَفِئَ</foreign> <foreign xml:lang="he">טְפָא</foreign>] <gramGrp> <pos expand="Subjunctivus">Subj.</pos> </gramGrp> <foreign xml:lang="gez">ይጥፋእ፡</foreign> <sense n="1" xml:id="D1"> <cit type="translation" xml:lang="la">
                                <quote> exstingui </quote>
                            </cit>, de lumine, lucerna: <ref cRef="Prov." loc="24,20">Prov. 24,20</ref>; <foreign xml:lang="gez">ይጠፍእ፡ ብርሃነ፡ ኃጥኣን፡</foreign> <ref cRef="Job" loc="18,5">Job 18,5</ref>; <foreign xml:lang="gez">ማኅቶቶሙ፡ ይጠፍእ፡</foreign> <ref cRef="Job" loc="18,6">Job 18,6</ref>; <ref cRef="Jes." loc="43,17">Jes. 43,17</ref>; <ref cRef="4 Esr." loc="10,29">4 Esr. 10,29</ref>; <ref cRef="1 Tim." loc="6,16">1 Tim. 6,16</ref>; <ref cRef="Matth." loc="25,8">Matth. 25,8</ref>; de pruna <ref cRef="Sir." loc="28,12">Sir. 28,12</ref>; de igne <ref cRef="Lev." loc="6,5">Lev. 6,5</ref>; <ref cRef="Lev." loc="6,6">Lev. 6,6</ref>; <ref cRef="Jes." loc="34,10">Jes. 34,10</ref>; <ref cRef="Jer." loc="17,27">Jer. 17,27</ref>; <foreign xml:lang="gez">እሳቶሙ፡ ዘኢይጠፍእ፡</foreign> <ref cRef="Jes." loc="66,24">Jes. 66,24</ref>; <ref cRef="Sir." loc="28,23">Sir. 28,23</ref>; <ref cRef="Marc." loc="9,44">Marc. 9,44</ref>; <ref cRef="Marc. " loc="9,46">Marc.  9,46</ref>; <ref cRef="Clem." loc="207.">Clem. f. 207.</ref> <sense n="a" xml:id="D1a"> <cit type="translation" xml:lang="la">
                                    <quote> caligare </quote>
                                </cit>, <cit type="translation" xml:lang="la">
                                    <quote> deficere </quote>
                                </cit>: <foreign xml:lang="gez">ይጠፍእ፡ ብርሃኖሙ፡ ለከዋክብት፡</foreign> <ref cRef="Joel" loc="2,10">Joel 2,10</ref>; de sole: <foreign xml:lang="gez">ፀሓይ፡ ዘያበርህ፡ ወይጠፍእ፡</foreign> <ref cRef="Job" loc="31,26">Job 31,26</ref>; <foreign xml:lang="gez">ምንተ፡ ይበርህ፡ እምፀሓይ፡ ወውእቱሂ፡ ይጠፍእ፡</foreign> <ref cRef="Sir." loc="17,31">Sir. 17,31</ref>; de oculis (<cit type="translation" xml:lang="la">
                                    <quote> hebetari </quote>
                                </cit>): <foreign xml:lang="gez">ጠፍአ፡ አዕይንቲነ፡</foreign> <foreign xml:lang="grc">ἐξέλιπον</foreign> <ref cRef="Thren. " loc="4,17">Thren.  4,17</ref>.</sense> <sense n="b" xml:id="D1b"> <cit type="translation" xml:lang="la">
                                    <quote> linqui animo </quote>
                                </cit> vel <cit type="translation" xml:lang="la">
                                    <quote> tabescere </quote>
                                </cit>: <foreign xml:lang="gez">ይጠፍኣ፡ ደናግል፡ ላሕያት፡</foreign> <ref cRef="Am." loc="8,13">Am. 8,13</ref>;</sense> </sense> <sense n="2" xml:id="D2"> <cit type="translation" xml:lang="la">
                                <quote> evanescere </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> disparere </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> invisibilem fieri </quote>
                            </cit>, de rebus: <foreign xml:lang="gez">ይጠፍእ፡ ከመ፡ ጊሜ፡</foreign> <foreign xml:lang="grc">ἀφανὴς γίνεται</foreign> <ref cRef="Job" loc="24,20">Job 24,20</ref>; <foreign xml:lang="gez">ጠፍአ፡ ጥበቦሙ፡</foreign> <foreign xml:lang="grc">ᾤχετο</foreign> <ref cRef="Jer." loc="29,8">Jer. 29,8</ref>; de sono (<cit type="translation" xml:lang="la">
                                <quote> conticescere </quote>
                            </cit>) <ref cRef="Job" loc="4,10">Job 4,10</ref>; <foreign xml:lang="grc">ἔφυγε</foreign> <ref cRef="Apoc." loc="16,20">Apoc. 16,20</ref>; <foreign xml:lang="gez">ይጠፍኡ፡ አድባር፡ ወአውግር፡</foreign> <foreign xml:lang="grc">ἀποσβεσθήσεται</foreign> <ref cRef="Jes." loc="10,18">Jes. 10,18</ref>; <foreign xml:lang="gez">ጠፍአ፡ እምኔሆሙ፡</foreign> <foreign xml:lang="grc">ἄφαντος ἐγένετο ἀπ᾽ αὐτῶν</foreign> <ref cRef="Luc." loc="24,31">Luc. 24,31</ref>.</sense> <sense n="3" xml:id="D3"> <cit type="translation" xml:lang="la">
                                <quote> exstingui </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> deleri </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> perdi </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> perire </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> interire </quote>
                            </cit>: <foreign xml:lang="gez">ከመ፡ ኢይጥፋእ፡ ስሙ፡</foreign> <foreign xml:lang="grc">ἐξολοθρευθήσεται</foreign> <ref cRef="Ruth" loc="4,10">Ruth 4,10</ref>; <foreign xml:lang="gez">ጠፊአ፡ ይጠፍኡ፡</foreign> <foreign xml:lang="grc">ἀπολοῦνται</foreign> <ref cRef="Judith" loc="6,4">Judith 6,4</ref>; <ref cRef="Job" loc="4,7">Job 4,7</ref>; <lbl expand="cum">c.</lbl> <foreign xml:lang="gez">እምነ፡</foreign> auctoris <ref cRef="Judith" loc="16,12">Judith 16,12</ref>; <foreign xml:lang="grc">ἐρημωθήσεται</foreign> <ref cRef="Jes." loc="60,12">Jes. 60,12</ref>; <foreign xml:lang="gez">ኢይጠፍእ፡ ለዝሉፉ፡</foreign> <foreign xml:lang="grc">μὴ ἐκλίπω</foreign> <ref cRef="Sir. " loc="24,9">Sir.  24,9</ref>; <foreign xml:lang="gez">የኀልቁ፡ ወይጠፍኡ፡</foreign> <ref cRef="Sir." loc="40,14">Sir. 40,14</ref>; <foreign xml:lang="gez">ትጠፍእ፡ ደማስቆ፡ እምነ፡ አህጉር፡</foreign> <foreign xml:lang="grc">ἀρθήσεται</foreign> <ref cRef="Jes." loc="17,1">Jes. 17,1</ref>; de rebus, ut: <foreign xml:lang="gez">ጠፍአት፡ ንዋዮሙ፡</foreign> <foreign xml:lang="grc">ἠφανίσθη</foreign> <ref cRef="Job" loc="22,20">Job 22,20</ref>; <foreign xml:lang="gez">ብሩር፡ ለይጥፋእ፡</foreign> <ref cRef="Tob." loc="5,19">Tob. 5,19</ref>; <foreign xml:lang="gez">ለትጥፋእ፡ ይእቲ፡ ዕለት፡</foreign> <ref cRef="Job" loc="3,2">Job 3,2</ref>; <foreign xml:lang="gez">ጠፍአት፡ ሕይወትየ፡</foreign> <ref cRef="Job" loc="9,21">Job 9,21</ref>; de spe <ref cRef="Job" loc="8,13">Job 8,13</ref>; <ref cRef="Act." loc="27,20">Act. 27,20</ref> <lbl expand="romanae editionis">rom.</lbl>; morte <ref cRef="4 Esr." loc="8,63">4 Esr. 8,63</ref>; vino (<foreign xml:lang="grc">πέπαυται</foreign>) <ref cRef="Jes." loc="16,10">Jes. 16,10</ref>; <foreign xml:lang="gez">ይጠፍእ፡ እምድር፡ ዝክሩ፡</foreign> <ref cRef="Job" loc="18,17">Job 18,17</ref>; <foreign xml:lang="gez">ፍኖቶሙ፡ ትጠፍእ፡</foreign> <ref cRef="Ps." loc="1,7">Ps. 1,7</ref>; <foreign xml:lang="gez">በኀቤክሙ፡ ጠፍአት፡ ጥበብ፡</foreign> <foreign xml:lang="grc">τελευτήσει</foreign> <ref cRef="Job" loc="12,2">Job 12,2</ref>; <foreign xml:lang="gez">ጠፍአ፡ ልብከ፡</foreign> (i.e. <cit type="translation" xml:lang="la">
                                <quote> insanus es </quote>
                            </cit>) <ref cRef="Sx. Hed." loc="25.">Sx. Hed. 25.</ref> </sense> </sense>
               
