            <entry xml:id="Le45a28472bc9483c98cab1eb7795ac9c" n="8286">
               <form>
                  <foreign xml:lang="gez">አግሐሠ</foreign>
               </form>
               <sense xml:lang="la" source="#dillmann" xml:id="Dmain"> <subc>II,1</subc> <sense n="1" xml:id="D1"> de personis <sense n="a" xml:id="D1a"> <cit type="translation" xml:lang="la">
                                    <quote> seducere </quote>
                                </cit>, <cit type="translation" xml:lang="la">
                                    <quote> seorsim ducere </quote>
                                </cit>, <cit type="translation" xml:lang="la">
                                    <quote> solum prendere </quote>
                                </cit>, <cit type="translation" xml:lang="la">
                                    <quote> sevocare </quote>
                                </cit> (<note>bei Seite nehmen</note>): <foreign xml:lang="gez">አግሐሦሙ፡ እንዘ፡ ባሕቲቶሙ፡</foreign> <ref cRef="Tob. " loc="12,6">Tob.  12,6</ref>; <ref cRef="2 Reg." loc="3,27">2 Reg. 3,27</ref>; <ref cRef="3 Reg." loc="11,29">3 Reg. 11,29</ref>; <foreign xml:lang="gez">አግሐሦሙ፡ እምፍኖት፡</foreign> <ref cRef="Matth." loc="20,17">Matth. 20,17</ref>; <ref cRef="Marc." loc="7,33">Marc. 7,33</ref>.</sense> <sense n="b" xml:id="D1b"> <cit type="translation" xml:lang="la">
                                    <quote> recedere jubere </quote>
                                </cit>, <cit type="translation" xml:lang="la">
                                    <quote> removere </quote>
                                </cit> (<note>abtreten lassen</note>): <foreign xml:lang="gez">አግሐሦ፡ ኪያሁ፡</foreign> <foreign xml:lang="grc">μεταστήσας</foreign> <ref cRef="Dan. apocr." loc="1,56">Dan. apocr. 1,56</ref>.</sense> <sense n="c" xml:id="D1c"> <cit type="translation" xml:lang="la">
                                    <quote> devertere facere </quote>
                                </cit>, <lbl expand="cum">c.</lbl> <gramGrp> <case value="accusativus">Acc.</case> </gramGrp> et <foreign xml:lang="gez">ውስተ፡</foreign> 2 <ref cRef="Reg." loc="6,10">Reg. 6,10</ref>; <cit type="translation" xml:lang="la">
                                    <quote> apud se </quote>
                                </cit> i.e. <cit type="translation" xml:lang="la">
                                    <quote> hospitio excipere </quote>
                                </cit>, <cit type="translation" xml:lang="la">
                                    <quote> ad devertendum </quote>
                                </cit> <cit type="translation" xml:lang="la">
                                    <quote> invitare </quote>
                                </cit>: <ref cRef="Luc." loc="19,6">Luc. 19,6</ref>; <ref cRef="Jud." loc="19,18">Jud. 19,18</ref>.</sense> <sense n="d" xml:id="D1d"> <cit type="translation" xml:lang="la">
                                    <quote> seducere </quote>
                                </cit>, <cit type="translation" xml:lang="la">
                                    <quote> sevocare </quote>
                                </cit> i.e. <cit type="translation" xml:lang="la">
                                    <quote> deflectere facere </quote>
                                </cit>, <cit type="translation" xml:lang="la">
                                    <quote> abducere </quote>
                                </cit>, <cit type="translation" xml:lang="la">
                                    <quote> avertere </quote>
                                </cit>, <foreign xml:lang="gez">ፍናወ፡ ሕይወት፡ ያግሕሣ፡ እምእኩይ፡</foreign> <ref cRef="Prov." loc="16,17">Prov. 16,17</ref>; <foreign xml:lang="gez">አግሕሦ፡</foreign> <ref cRef="Asc. Jes." loc="2,4">Asc. Jes. 2,4</ref>; <foreign xml:lang="gez">ያግሕሥከ፡ እምነ፡ ርኵሶሙ፡</foreign> <ref cRef="Kuf." loc="81.">Kuf. p. 81.</ref> </sense> </sense> <sense n="2" xml:id="D2"> de rebus: <sense n="a" xml:id="D2a"> <cit type="translation" xml:lang="la">
                                    <quote> seponere </quote>
                                </cit>, <cit type="translation" xml:lang="la">
                                    <quote> reponere </quote>
                                </cit>, <ref cRef="4 Reg." loc="4,4">4 Reg. 4,4</ref>; <ref cRef="Matth." loc="14,20">Matth. 14,20</ref>; <ref cRef="Matth. " loc="15,37">Matth.  15,37</ref>.</sense> <sense n="b" xml:id="D2b"> <cit type="translation" xml:lang="la">
                                    <quote> semovere </quote>
                                </cit>, <cit type="translation" xml:lang="la">
                                    <quote> amovere </quote>
                                </cit>, <cit type="translation" xml:lang="la">
                                    <quote> removere </quote>
                                </cit>, <ref cRef="2 Reg." loc="20,12">2 Reg. 20,12</ref>; <foreign xml:lang="gez">እለ፡ አግሕሦ፡ አግሐሠ፡ እግዚአብሔር፡</foreign> <ref cRef="1 Par." loc="5,25">1 Par. 5,25</ref>; <foreign xml:lang="gez">አግሕሥ፡ እከየ፡ እምሥጋከ፡</foreign> <foreign xml:lang="grc">ἀπόστησον</foreign> <ref cRef="Koh." loc="11,10">Koh. 11,10</ref>; <cit type="translation" xml:lang="la">
                                    <quote> transferre </quote>
                                </cit> sedem, familiam alio, Lud. ex <ref cRef="Sx. Nah. " loc="23 Enc.">Sx. Nah.  23 Enc.</ref>
                            </sense> <sense n="c" xml:id="D2c"> <cit type="translation" xml:lang="la">
                                    <quote> avertere </quote>
                                </cit>: <foreign xml:lang="gez">አግሕሥ፡ እገሪከ፡ እምአሠሮሙ፡</foreign> <foreign xml:lang="grc">ἔκκλινον</foreign> <ref cRef="Prov." loc="1,15">Prov. 1,15</ref>; <foreign xml:lang="gez">ተጋደሉ፡ ለአግሕሦ፡ ጽዕለት፡ እምኔክሙ፡</foreign> <ref cRef="Jsp." loc="371.">Jsp. p. 371.</ref> </sense> <sense n="d" xml:id="D2d"> <cit type="translation" xml:lang="la">
                                    <quote> flectere </quote>
                                </cit> jus: <foreign xml:lang="gez">አግሕሦ፡ ጽድቅ፡ በፍትሕ፡</foreign> <ref cRef="Prov. " loc="18,5">Prov.  18,5</ref>.</sense> <sense n="e" xml:id="D2e"> <cit type="translation" xml:lang="la">
                                    <quote> tollere </quote>
                                </cit>, <cit type="translation" xml:lang="la">
                                    <quote> auferre </quote>
                                </cit> utensilia (post usum), <note>abräumen</note>: <foreign xml:lang="gez">አስተፋጠኑ፡ ወአግሐሡ፡</foreign> <foreign xml:lang="grc">ἀπολύειν</foreign> (v. <foreign xml:lang="gez">አስተግሐሡ፡</foreign>) <ref cRef="Judith " loc="13,1">Judith  13,1</ref>.</sense> </sense> </sense>
               
