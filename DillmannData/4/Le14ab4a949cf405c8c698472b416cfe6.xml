            <entry xml:id="Le14ab4a949cf405c8c698472b416cfe6" n="3113">
               <form>
                  <foreign xml:lang="gez">ስፉሕ</foreign>
               </form>
               <sense xml:lang="la" source="#dillmann" xml:id="Dmain">
                  <gramGrp>
                     <pos expand="participium">part.</pos>
                  </gramGrp> et <gramGrp>
                     <pos expand="adjectivum">adj.</pos>
                  </gramGrp>
                  <sense n="1" xml:id="D1">
                     <cit type="translation" xml:lang="la">
                        <quote>extensus</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>expansus</quote>
                     </cit>: <foreign xml:lang="gez">ክነፊሆሙ፡ ስፉሓን፡</foreign> 2 <ref cRef="Par." loc="3,13">Par. 3,13</ref>; <ref cRef="Hez." loc="1,11">Hez. 1,11</ref>;
                        <foreign xml:lang="gez">ሰይፉ፡ ስፍሕት፡ ውስተ፡ ኢየሩሳሌም፡</foreign> 1 <ref cRef="Par." loc="21,16">Par. 21,16</ref>; <foreign xml:lang="gez">ስፉሕ፡ እደዊሆን፡</foreign>
                     <ref cRef="Herm." loc="76">Herm. p. 76</ref>; <ref cRef="Herm." loc="84">Herm.
                        p. 84</ref>; <foreign xml:lang="gez">ስፍሕተ፡ እድ፡</foreign>
                     <cit type="translation" xml:lang="la">
                        <quote>extensam habens manum</quote>
                     </cit>
                     <ref cRef="Org. " loc="5.">Org. 5.</ref>
                  </sense>
                  <sense n="2" xml:id="D2">
                     <cit type="translation" xml:lang="la">
                        <quote>patens</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>latus</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>amplus</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>spatiosus</quote>
                     </cit>: <foreign xml:lang="gez">ምድር፡ ቡርክት፡ ወስፍሕት፡</foreign>
                     <ref cRef="Kuf." loc="8">Kuf. 8</ref>; <ref cRef="Jes." loc="8,8">Jes.
                        8,8</ref>
                     <lbl expand="varia lectio">var.</lbl>; <ref cRef="2 Esr." loc="19,35">2 Esr.
                        19,35</ref>; <foreign xml:lang="gez">ርሒብ፡ ወስፉሕ፡</foreign>
                     <foreign xml:lang="grc">παλτεῖς καὶ εὐρύχωροι</foreign>
                     <ref cRef="Jes." loc="33,21">Jes. 33,21</ref>; <ref cRef="4 Esr." loc="5,3">4
                        Esr. 5,3</ref>; <foreign xml:lang="gez">ስፍሕት፡ ፍኖት፡ </foreign>
                     <ref cRef="Matth." loc="7,13">Matth. 7,13</ref>; <ref cRef="Clem." loc="167">Clem. f. 167</ref>; hinc <foreign xml:lang="gez">ስፉሕ፡ </foreign>
                     <cit type="translation" xml:lang="la">
                        <quote>spatium amplum</quote>
                     </cit>: <foreign xml:lang="gez">አሐውር፡ ስፉሐ፡ </foreign>
                     <foreign xml:lang="grc">ἐν πλατυσμῷ</foreign>
                     <ref cRef="Ps." loc="118,45">Ps. 118,45</ref>.</sense>
                  <sense n="3" xml:id="D3"> (<lbl expand="oppositum, -o, -nitur">opp.</lbl> implicito, contorto,
                     simulato) <cit type="translation" xml:lang="la">
                        <quote>simplex</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>integer</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>sincerus</quote>
                     </cit>, <foreign xml:lang="grc">ἁπλοῦς</foreign>
                     <ref cRef="Matth." loc="6,22">Matth. 6,22</ref>; <foreign xml:lang="gez">በስፉሕ፡
                        ልብ፡</foreign>
                     <foreign xml:lang="grc">ἐν ἁπλότητι καρδίας </foreign>
                     <ref cRef="Sap." loc="1,1">Sap. 1,1</ref>; <ref cRef="Eph." loc="6,5">Eph.
                        6,5</ref>; <ref cRef="Col." loc="3,22">Col. 3,22</ref>; <ref cRef="Herm." loc="6">Herm. p. 6</ref>; <ref cRef="Herm." loc="25">Herm. p. 25</ref>; <ref cRef="Clem." loc="174">Clem. f. 174</ref>; <foreign xml:lang="gez">በንጹሕ፡ ልብ፡
                        ወበስፉሕ፡ ኅሊና፡</foreign> Org.; hinc <foreign xml:lang="gez">ስፍሕት፡</foreign>
                     <cit type="translation" xml:lang="la">
                        <quote>simplicitias</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>sinceritas</quote>
                     </cit>
                     <ref cRef="Herm. " loc="15">Herm. p. 15</ref>; <ref cRef="Herm." loc="91,">Herm. p. 91,</ref> et <foreign xml:lang="gez">ስፉሐ፡</foreign>
                     <cit type="translation" xml:lang="la">
                        <quote>simpliciter</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>sincere</quote>
                     </cit>: <foreign xml:lang="gez">ዘይኔጽር፡ ስፉሐ፡</foreign>
                     <foreign xml:lang="grc">ὁ πλέπων λεῖα</foreign>
                     <ref cRef="Prov." loc="12,13">Prov. 12,13</ref>; vel <foreign xml:lang="gez">በስፉሕ፡</foreign>
                     <ref cRef="Jac." loc="1,5">Jac. 1,5</ref>.</sense>
                  <sense n="4" xml:id="D4">
                     <cit type="translation" xml:lang="la">
                        <quote>liberalis</quote>
                     </cit> (proprie: qui apertam habet manum, <lbl expand="videas">vid.</lbl>
                     <foreign xml:lang="gez">ሰፍሐ፡</foreign>) <foreign xml:lang="gez">እግዚአብሔር፡ ስፉሐ፡
                        ሀብት፡</foreign>
                     <ref cRef="Genz. " loc="87.">Genz. f. 87.</ref>
                  </sense>
                  <sense n="5" xml:id="D5">
                     <cit type="translation" xml:lang="la">
                        <quote>obductus</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>obtentus</quote>
                     </cit> (<lbl expand="videas">vid.</lbl>
                     <foreign xml:lang="gez">ተሰፍሐ፡</foreign>
                     <gramGrp>
                        <pos expand="nomen">n.</pos>
                     </gramGrp> 3): <foreign xml:lang="gez">ዘሕብረ፡ በረድ፡ ስፉሕ፡ ዲበ፡ ክነፊሆሙ፡</foreign>
                     <ref cRef="Hez." loc="1,22">Hez. 1,22</ref>.</sense>
               </sense>
               
