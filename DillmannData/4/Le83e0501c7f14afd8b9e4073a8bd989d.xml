         <div>
            <entry xml:id="Le83e0501c7f14afd8b9e4073a8bd989d" n="6665">
               <form>
                  <foreign xml:lang="gez">ተወነየ</foreign>
               </form>
               <sense xml:lang="la" source="#dillmann" xml:id="Dmain"> <subc>III,2</subc> (<cit type="translation" xml:lang="la">
                     <quote>oblectari</quote>
                  </cit>) <cit type="translation" xml:lang="la">
                     <quote>ludere</quote>
                  </cit>: <foreign xml:lang="gez">ደቅ፡ ወአዋልድ፡ ዘይትዌነዩ፡</foreign> (v. <foreign xml:lang="gez">ዘይትዋነዩ፡</foreign>) <foreign xml:lang="grc">παίζοντες</foreign>
                  <ref cRef="Zach." loc="8,5">Zach. 8,5</ref>; <foreign xml:lang="gez">እለ፡ ይትዌነዩ፡
                     በእሳት፡</foreign>
                  <ref cRef="Nah." loc="2,4">Nah. 2,4</ref>; <lbl expand="cum">c.</lbl>
                  <foreign xml:lang="gez">ምስለ፡</foreign>
                  <ref cRef="Gen." loc="21,9">Gen. 21,9</ref>. <sense n="a" xml:id="Da">
                     <cit type="translation" xml:lang="la">
                        <quote>jocando</quote>
                     </cit>: <ref cRef="Gen." loc="26,8">Gen. 26,8</ref>.</sense>
                  <sense n="b" xml:id="Db">
                     <cit type="translation" xml:lang="la">
                        <quote>saltando</quote>
                     </cit> et <cit type="translation" xml:lang="la">
                        <quote>canendo</quote>
                     </cit>: <ref cRef="Ex." loc="32,6">Ex. 32,6</ref>; <ref cRef="Jud." loc="16,25">Jud. 16,25</ref>; <ref cRef="Jud." loc="16,27">Jud. 16,27</ref>; <foreign xml:lang="gez">እንዘ፡ ይትዌነያ፡ በመሰንቆ፡</foreign>
                     <ref cRef="Jud." loc="21,21">Jud. 21,21</ref>; <ref cRef="Jud." loc="21,23">Jud. 21,23</ref>; <ref cRef="Jer." loc="38,4">Jer. 38,4</ref>
                     <lbl expand="varia lectio">var.</lbl>
                  </sense>
                  <sense n="c" xml:id="Dc">
                     <cit type="translation" xml:lang="la">
                        <quote>libidinem explendo</quote>
                     </cit> in aliquo, <lbl expand="cum">c.</lbl>
                     <gramGrp>
                        <case value="accusativus">Acc.</case>
                     </gramGrp>
                     <lbl expand="persona">pers.</lbl>: <ref cRef="Jud." loc="19,25">Jud.
                        19,25</ref>; <ref cRef="Jud." loc="20,5">Jud. 20,5</ref>.</sense>
               </sense>
               
