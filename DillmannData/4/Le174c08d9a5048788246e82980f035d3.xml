                           <foreign xml:lang="grc">ἐξάπινα </foreign>
                        <ref cRef="Sir." loc="11,21">Sir. 11,21</ref>; <ref cRef="Jes." loc="48,3">Jes. 48,3</ref>; <ref cRef="Ps." loc="63,4">Ps. 63,4</ref>; <ref cRef="Num." loc="4,20">Num. 4,20</ref>; <foreign xml:lang="grc">ἐξαπίνης</foreign>
