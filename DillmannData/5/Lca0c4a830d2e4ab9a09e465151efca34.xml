               <sense xml:lang="la" source="#dillmann" xml:id="Dmain"> et <foreign xml:lang="gez">ዳፍንታ፡</foreign>
                  (quae primitus forma collectivi fuisse videtur) <gramGrp>
                     <pos expand="Substantivum">subst.</pos>
                  </gramGrp> (semel inveni formam <foreign xml:lang="gez">ዳፍናት፡ </foreign>
                  <ref cRef="Ex." loc="12,39">Ex. 12,39</ref>
                  <lbl expand="varia lectio">var.</lbl>), <lbl expand="singularis">sing.</lbl> et
                     <lbl expand="collatio, -is vel collectivum, -e">coll.</lbl>
                  <cit type="translation" xml:lang="la">
                     <quote>panis subcinericius</quote>
                  </cit>, <cit type="translation" xml:lang="la">
                     <quote>orbiculus</quote>
                  </cit> vel <cit type="translation" xml:lang="la">
                     <quote>placenta</quote>
                  </cit> panis sub cinere fervido cocti, <foreign xml:lang="grc">ἐγκρυφίας </foreign>
                  <ref cRef="Gen." loc="18,6">Gen. 18,6</ref>; <ref cRef="Gen." loc="19,3">Gen.
                     19,3</ref>; <ref cRef="Ex." loc="12,39">Ex. 12,39</ref>; <ref cRef="Num." loc="11,8">Num. 11,8</ref>; <ref cRef="Jud." loc="6,19">Jud. 6,19</ref>; <ref cRef="Kuf." loc="22">Kuf. 22</ref>; <ref cRef="3 Reg." loc="17,13">3 Reg.
                     17,13</ref>; <ref cRef="3 Reg." loc="19,6">3 Reg. 19,6</ref>; <foreign xml:lang="gez">ዳፍንተ፡</foreign> (v. <foreign xml:lang="gez">ዳፍንታ፡
                  ሰገም፡</foreign>) <foreign xml:lang="gez">ትሴሰይ፡</foreign>
                  <ref cRef="Hez." loc="4,12">Hez. 4,12</ref>; <foreign xml:lang="gez">ከመ፡ ይግበራ፡
                     ዳፍንተ፡</foreign> (v. <foreign xml:lang="gez">ዳፍንታ፡</foreign>) <foreign xml:lang="grc">καυῶνας</foreign>
                  <ref cRef="Jer." loc="7,18">Jer. 7,18</ref>; <foreign xml:lang="gez">ግበሩ፡ ኅብስተ፡
