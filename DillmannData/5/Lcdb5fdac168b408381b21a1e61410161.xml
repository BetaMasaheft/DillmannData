               <sense xml:id="Dmain" xml:lang="la" source="#dillmann"> <gramGrp> <pos expand="Substantivum">subst.</pos> </gramGrp>, <gramGrp> <gen value="not found">m.</gen> </gramGrp> et <gramGrp> <gen value="femininus">fem.</gen> </gramGrp> (ut <ref cRef="Jac." loc="4,6">Jac. 4,6</ref>), <cit type="translation" xml:lang="la"><t:quote xmlns:t="http://www.tei-c.org/ns/1.0"> gratia </t:quote></cit>, <foreign xml:lang="grc">χάρις</foreign>, <sense n="a" xml:id="Da"> <cit type="translation" xml:lang="la"><t:quote xmlns:t="http://www.tei-c.org/ns/1.0"> favor </t:quote></cit>, <cit type="translation" xml:lang="la"><t:quote xmlns:t="http://www.tei-c.org/ns/1.0"> benevolentia </t:quote></cit>, <ref cRef="Act." loc="2,47">Act. 2,47</ref>; <ref cRef="Act." loc="4,33">Act. 4,33</ref>; <foreign xml:lang="gez">ጸጋሆሙ፡ ለአብዳን፡ አልባቲ፡ ሞገስ፡</foreign> <ref cRef="Sir." loc="20,13">Sir. 20,13</ref>; <ref cRef="Sir." loc="24,17">Sir. 24,17</ref>; <foreign xml:lang="gez">ከመ፡ ሞገስ፡ ይኩን፡ ውስተ፡ ክሳድከ፡</foreign> <ref cRef="Prov." loc="3,22">Prov. 3,22</ref>; <ref cRef="Prov." loc="22,1">Prov. 22,1</ref>; <ref cRef="Koh. " loc="10,12">Koh.  10,12</ref>; <foreign xml:lang="gez">ረከበ፡ ሞገሰ፡</foreign> <cit type="translation" xml:lang="la"><t:quote xmlns:t="http://www.tei-c.org/ns/1.0"> consecutus est favorem </t:quote></cit> alicujus <lbl expand="sequens">seq.</lbl> <foreign xml:lang="gez">ቅድመ፡</foreign> vel <foreign xml:lang="gez">በቅድመ፡</foreign> <ref cRef="Kuf." loc="4">Kuf. 4</ref>; <ref cRef="Gen." loc="32,6">Gen. 32,6</ref>; <ref cRef="Gen." loc="33,8">Gen. 33,8</ref>; <ref cRef="Gen." loc="33,10">Gen. 33,10</ref>; <ref cRef="Gen." loc="39,4">Gen. 39,4</ref>; <ref cRef="Ruth" loc="2,10">Ruth 2,10</ref>; <ref cRef="1 Reg." loc="20,3">1 Reg. 20,3</ref>; <ref cRef="Esth." loc="8,5">Esth. 8,5</ref> vel <lbl expand="sequens">seq.</lbl> <foreign xml:lang="gez">ኀበ፡</foreign> et <foreign xml:lang="gez">በኀበ፡</foreign> pers., ut <ref cRef="Ruth" loc="2,2">Ruth 2,2</ref>; <ref cRef="Ex." loc="33,12">Ex. 33,12</ref>; <ref cRef="Ex." loc="33,13">Ex. 33,13</ref>; <ref cRef="Deut. " loc="24,1">Deut.  24,1</ref>; <ref cRef="Sir." loc="42,24">Sir. 42,24</ref>; <ref cRef="Luc." loc="1,30">Luc. 1,30</ref>; <foreign xml:lang="gez">ወሀቦ፡ ሞገሰ፡</foreign> <cit type="translation" xml:lang="la"><t:quote xmlns:t="http://www.tei-c.org/ns/1.0"> conciliavit ei favorem apud </t:quote></cit> <lbl expand="sequens">seq.</lbl> <foreign xml:lang="gez">በኀበ፡</foreign> pers.: <ref cRef="Gen." loc="39,21">Gen. 39,21</ref>; <ref cRef="Gen." loc="43,14">Gen. 43,14</ref> vel <foreign xml:lang="gez">ቅድመ፡</foreign> et <foreign xml:lang="gez">በቅድመ፡</foreign> <ref cRef="Ex." loc="3,21">Ex. 3,21</ref>; <ref cRef="Ex." loc="11,3">Ex. 11,3</ref>; <ref cRef="Ex." loc="12,36">Ex. 12,36</ref>. <sense n="α" xml:id="Daα"> Dei, <ref cRef="Jac." loc="4,6">Jac. 4,6</ref>; <ref cRef="2 Petr." loc="1,2">2 Petr. 1,2</ref>. 
</sense> <sense n="β" xml:id="Daβ"> <foreign xml:lang="grc">παῤῥησία</foreign> <ref cRef="Hebr." loc="10,19">Hebr. 10,19</ref>; <ref cRef="Hebr." loc="10,35">Hebr. 10,35</ref>; <ref cRef="Eph. " loc="3,12">Eph.  3,12</ref>. 
</sense> 
</sense> <sense n="b" xml:id="Db"> <cit type="translation" xml:lang="la"><t:quote xmlns:t="http://www.tei-c.org/ns/1.0"> suavitas </t:quote></cit>, <cit type="translation" xml:lang="la"><t:quote xmlns:t="http://www.tei-c.org/ns/1.0"> venustas </t:quote></cit>, <cit type="translation" xml:lang="la"><t:quote xmlns:t="http://www.tei-c.org/ns/1.0"> elegantia </t:quote></cit>, <cit type="translation" xml:lang="la"><t:quote xmlns:t="http://www.tei-c.org/ns/1.0"> decor </t:quote></cit>: <foreign xml:lang="gez">ገጽከ፡ ምሉእ፡ ሞገሰ፡</foreign> <foreign xml:lang="grc">χαρίτων μεστόν</foreign> <ref cRef="Esth." loc="5,2">Esth. 5,2</ref>; <ref cRef="Prov." loc="10,32">Prov. 10,32</ref>; <foreign xml:lang="gez">ሞገሰ፡ ፍቅራ፡ ያረውየከ፡</foreign> <ref cRef="Prov." loc="5,19">Prov. 5,19</ref>; <foreign xml:lang="gez">ምገሳ፡ ለብእሲት፡ ያስተፌሥሖ፡ ለምታ፡</foreign> <foreign xml:lang="grc">χάρις</foreign> <ref cRef="Sir." loc="26,13">Sir. 26,13</ref>; <ref cRef="Sir. " loc="35,10">Sir.  35,10</ref>; <foreign xml:lang="gez">ሠናይ፡ ሞገሶን፡ ለከናፍረ፡ ጠቢብ፡</foreign> <ref cRef="Sir." loc="21,16">Sir. 21,16</ref>; <foreign xml:lang="gez">አልቦ፡ ሞገሰ፡ ቃሉ፡</foreign> <ref cRef="Sir." loc="20,19">Sir. 20,19</ref>; <ref cRef="Ps." loc="44,3">Ps. 44,3</ref>; <ref cRef="Luc. " loc="4,22">Luc.  4,22</ref>; <ref cRef="Herm." loc="109.">Herm. p. 109.</ref> <cb n="0938" xml:id="c0938"/> <sense n="α" xml:id="Dbα"> <cit type="translation" xml:lang="la"><t:quote xmlns:t="http://www.tei-c.org/ns/1.0"> suave </t:quote></cit> quid vel <cit type="translation" xml:lang="la"><t:quote xmlns:t="http://www.tei-c.org/ns/1.0"> jucundum </t:quote></cit>: <foreign xml:lang="gez">ፍሬሁ፡ ሞገስ፡ ጥቀ፡ ለርእየ፡ ገጽ፡</foreign> <ref cRef="Hen." loc="24,5">Hen. 24,5</ref>. 
</sense> <sense n="β" xml:id="Dbβ"> decus: Agathon <foreign xml:lang="gez">አክሊላ፡ ወሞገሳ፡ ለሀገረ፡ ትንሣ፡</foreign> (corona et decus urbis Tensae) <ref cRef="Sx. Masc." loc="14 Enc.">Sx. Masc. 14 Enc.</ref> 
</sense> 
</sense> </sense>
               
