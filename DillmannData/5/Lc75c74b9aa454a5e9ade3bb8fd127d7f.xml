            <entry xml:id="Lc75c74b9aa454a5e9ade3bb8fd127d7f" n="7473">
               <form>
                  <foreign xml:lang="gez">ተዛለፈ</foreign>
               </form>
               <sense xml:lang="la" source="#dillmann" xml:id="Dmain"> <subc>III,3</subc> <sense n="1" xml:id="D1">
                     <cit type="translation" xml:lang="la">
                        <quote>jurgari</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>argutari</quote>
                     </cit>: <foreign xml:lang="gez">ኢትዛለፍ፡ በማእከለ፡ ሊቃውንት፡ </foreign>
                     <foreign xml:lang="grc">μὴ ἀδολέσχει</foreign>
                     <ref cRef="Sir." loc="7,14">Sir. 7,14</ref>; <foreign xml:lang="gez">ለማርያም፡
                        እኅተ፡ ሙሴ፡ ሶበ፡ ተዛለፈት፡ አውፅእዋ፡ እምትዕይንት፡</foreign>
                     <ref cRef="Did." loc="31 ">Did. 31 </ref>(ni <foreign xml:lang="gez">ተዘለፈት፡</foreign> legere malueris).</sense>
                  <sense n="2" xml:id="D2">
                     <cit type="translation" xml:lang="la">
                        <quote>arguere</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>coarguere</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>redarguere</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>convincere</quote>
                     </cit>, <foreign xml:lang="gez">እምድኅረ፡ ተዛለፉ፡ በስምዕ፡ ከመ፡ ቀታሌ፡ ነፍስ፡ ውእቱ፡</foreign>
                     <ref cRef="Clem." loc="248">Clem. f. 248</ref>; <lbl expand="cum">c.</lbl>
                     <gramGrp>
                        <case value="accusativus">Acc.</case>
                     </gramGrp>
                     <lbl expand="persona">pers.</lbl>: <foreign xml:lang="gez">አነ፡ እዛለፎሙ፡</foreign>
                     <foreign xml:lang="grc">ἐγὼ μάρτυς</foreign>
                     <ref cRef="Jer." loc="36,23">Jer. 36,23</ref>; <foreign xml:lang="gez">ይዛለፍኪ፡
                        እከይኪ፡</foreign>
                     <ref cRef="Jer. " loc="2,19">Jer. 2,19</ref>; <ref cRef="Jes." loc="11,4">Jes.
                        11,4</ref>; <foreign xml:lang="gez">እመ፡ ተዛለፍዎ፡ በስምዕ፡</foreign>
                     <ref cRef="Clem." loc="123">Clem. f. 123</ref>; <ref cRef="Matth." loc="12,27">Matth. 12,27</ref>
                     <lbl expand="romanae editionis">rom.</lbl>; <lbl expand="sequens">seq.</lbl>
                     <foreign xml:lang="gez">ከመ፡</foreign>
                     <ref cRef="Jac." loc="2,9">Jac. 2,9</ref>; <lbl expand="cum">c.</lbl>
                     <foreign xml:lang="gez">በ፡</foreign> rei <ref cRef="2 Petr." loc="2,16">2 Petr.
                        2,16</ref>; <lbl expand="cum">c.</lbl>
                     <foreign xml:lang="gez">በእንተ፡ </foreign> rei <ref cRef="Joh." loc="8,46">Joh.
                        8,46</ref>; <ref cRef="Joh." loc="16,8">Joh. 16,8</ref>; <ref cRef="4 Esr." loc="12,38">4 Esr. 12,38</ref>; vel <lbl expand="cum">c.</lbl>
                     <lbl expand="duplex">dupl.</lbl>
                     <gramGrp>
                        <case value="accusativus">Acc.</case>
                     </gramGrp>
                     <ref cRef="4 Esr." loc="12,36">4 Esr. 12,36</ref>; <ref cRef="4                 Esr." loc="13,41">4 Esr. 13,41</ref> Laur. (In his
                     locis omnibus sicut <ref cRef="4 Esr." loc="12,39">4 Esr. 12,39</ref> Laur.
                     quidem <foreign xml:lang="gez">ተዘለፈ፡</foreign> <subc>III,1</subc> praebet, sed mei codices
                        <foreign xml:lang="gez">ተዛለፈ፡</foreign> exhibent).</sense>
                  <sense n="3" xml:id="D3">
                     <cit type="translation" xml:lang="la">
                        <quote>reprehendere</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>objurgare</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>corripere</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>corrigere</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>castigare</quote>
                     </cit>, <lbl expand="cum">c.</lbl>
                     <gramGrp>
                        <case value="accusativus">Acc.</case>
                     </gramGrp>: <foreign xml:lang="gez">ይዛለፎሙ፡ ለብዙኃን፡ አሕዛብ፡ </foreign>
                     <ref cRef="Jes." loc="2,4">Jes. 2,4</ref>; <foreign xml:lang="gez">በተዛልፎቱ፡</foreign>
                     <foreign xml:lang="grc">ὑπ</foreign>ʼ <foreign xml:lang="grc">αὐτοῦ ἐλεγχομενος </foreign>
                     <ref cRef="Prov." loc="3,11">Prov. 3,11</ref>; <ref cRef="Ps." loc="38,14">Ps.
                        38,14</ref>; <ref cRef="Ps." loc="140,6">Ps. 140,6</ref>; <ref cRef="Lev." loc="19,17">Lev. 19,17</ref>; <ref cRef="Num." loc="25,24">Num. 25,24</ref>
                        (<foreign xml:lang="grc">παραδειγματίζειν</foreign>) <ref cRef="Hen." loc="14,3">Hen. 14,3</ref>; <ref cRef="Tit." loc="2,15">Tit. 2,15</ref>;
                        <foreign xml:lang="gez">በእንተ፡ ተዛልፎተ፡ ዘይነብብ፡ ዘኢመፍትው፡</foreign>
                     <bibl>
                        <ptr target="bm:Ludolf1691Commentarius"/>Lud. Comm. </bibl>hist. p.
                     305.</sense>
               </sense>
               
