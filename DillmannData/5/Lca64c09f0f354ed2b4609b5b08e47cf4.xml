            <entry xml:id="Lca64c09f0f354ed2b4609b5b08e47cf4" n="6027">
               <form>
                  <foreign xml:lang="gez">ከሓ</foreign>
               <rs type="root"/>
                    </form>
               <sense xml:lang="la" source="#dillmann" xml:id="Dmain"> et <foreign xml:lang="gez">ከሐ፡</foreign>, <foreign xml:lang="gez">ከሓከ፡</foreign> et <foreign xml:lang="gez">ከሐከ፡</foreign>, <foreign xml:lang="gez">ከሐክ፡</foreign>, (nec non <foreign xml:lang="gez">ካሕከ፡</foreign> <ref cRef="2 Reg." loc="2,13">2 Reg. 2,13</ref> <lbl expand="varia lectio">var.</lbl>; 3 <ref cRef="Reg." loc="20,40">Reg. 20,40</ref> <lbl expand="varia lectio">var.</lbl>; et <foreign xml:lang="gez">ክሕከ፡</foreign> <ref cRef="Matth." loc="24,23">Matth. 24,23</ref> <lbl expand="romanae editionis">rom.</lbl>), etiam per <foreign xml:lang="gez">ሀ፡</foreign>, <gramGrp> <pos expand="adverbium, adverbialiter">adv.</pos> </gramGrp> pronominale (<bibl>
                            <ptr target="bm:Dillmann1857Gramm"/>gramm.</bibl> § 62,b; <bibl>
                            <ptr target="bm:Dillmann1857Gramm"/>gramm.</bibl> § 160,b) <sense n="1" xml:id="D1"> <cit type="translation" xml:lang="la">
                                <quote> illuc </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> illic </quote>
                            </cit>; <foreign xml:lang="gez">ዝርዎ፡ ከሐ፡</foreign> <foreign xml:lang="grc">ἐκεῖ</foreign> <ref cRef="Num." loc="17,2">Num. 17,2</ref>; <foreign xml:lang="gez">ሑር፡ ከሐክ፡</foreign> <ref cRef="Gen." loc="19,9">Gen. 19,9</ref>; <foreign xml:lang="gez">ረሐቁ፡ ከሐከ፡</foreign> (v. <foreign xml:lang="gez">ከሐ፡</foreign>) <ref cRef="1 Reg." loc="14,9">1 Reg. 14,9</ref>; <ref cRef="Matth." loc="26,36">Matth. 26,36</ref>; <ref cRef="Jac." loc="2,3">Jac. 2,3</ref>; <foreign xml:lang="gez">ነዋ፡ ዝየ፡ ወነዋ፡ ከሐክ፡</foreign> <ref cRef="Luc." loc="17,23">Luc. 17,23</ref>; <ref cRef="Marc." loc="13,21">Marc. 13,21</ref>; <ref cRef="Matth. " loc="24,23">Matth.  24,23</ref>; <foreign xml:lang="gez">እስከ፡ እትመየጥ፡ ለፌ፡ ወከሐከ፡</foreign> (v. <foreign xml:lang="gez">ወካሕከ፡</foreign>) <ref cRef="3 Reg." loc="20,40">3 Reg. 20,40</ref>; subjungere licet praepositionibus: <sense n="a" xml:id="D1a"> <foreign xml:lang="gez">በከሐ፡</foreign> vel <foreign xml:lang="gez">በከሐከ፡</foreign> <cit type="translation" xml:lang="la">
                                    <quote> illic </quote>
                                </cit>, <ref cRef="Hebr." loc="7,8">Hebr. 7,8</ref>; <foreign xml:lang="gez">በዝ፡ ዓለም፡ ወበከሐከ፡ በኀበ፡ እግዚአብሔር፡</foreign> <ref cRef="Did." loc="14">Did. 14</ref>; <foreign xml:lang="gez">በከሃከ፡</foreign> (in illo mundo) <ref cRef="Genz." loc="1">Genz. f. 1</ref>; <ref cRef="Genz." loc="3 ">Genz. f. 3 </ref> <lbl expand="sequens">seq.</lbl> </sense> <sense n="b" xml:id="D1b"> <foreign xml:lang="gez">ኀበ፡ ከሐ፡</foreign> <cit type="translation" xml:lang="la">
                                    <quote> illuc </quote>
                                </cit>: Matth.17,20.</sense> <sense n="c" xml:id="D1c"> <foreign xml:lang="gez">እምከሓ፡</foreign>, <foreign xml:lang="gez">እምከሐክ፡</foreign>, <foreign xml:lang="gez">እምከሐከ፡</foreign> <cit type="translation" xml:lang="la">
                                    <quote> illinc </quote>
                                </cit>, <cit type="translation" xml:lang="la">
                                    <quote> ab </quote>
                                </cit> <cit type="translation" xml:lang="la">
                                    <quote> illa </quote>
                                </cit> <cit type="translation" xml:lang="la">
                                    <quote> parte </quote>
                                </cit>: <ref cRef="Jos." loc="8,22">Jos. 8,22</ref>; <ref cRef="2 Reg." loc="2,13">2 Reg. 2,13</ref>; <ref cRef="Apoc." loc="22,2">Apoc. 22,2</ref>.</sense> <sense n="2" xml:id="D12"> <cit type="translation" xml:lang="la">
                                    <quote> ultra </quote>
                                </cit>, <cit type="translation" xml:lang="la">
                                    <quote> ulterius </quote>
                                </cit>, <cit type="translation" xml:lang="la">
                                    <quote> supra </quote>
                                </cit>, <cit type="translation" xml:lang="la">
                                    <quote> a </quote>
                                </cit> <cit type="translation" xml:lang="la">
                                    <quote> m </quote>
                                </cit> <cit type="translation" xml:lang="la">
                                    <quote> plius </quote>
                                </cit> (i.q. <foreign xml:lang="he">הָלְאָה</foreign> <foreign xml:lang="syr">ܠܗܰܠ</foreign>, et <foreign xml:lang="he">מַעְלָה</foreign>): <foreign xml:lang="gez">ብጹሓነ፡ ፴ዓም፡ ወከሐክ፡</foreign> <foreign xml:lang="grc">ἀπὸ τριακονταετοῦς καὶ ἐπάνω</foreign> <ref cRef="1 Par." loc="23,3">1 Par. 23,3</ref>; <ref cRef="1 Par." loc="23,24">1 Par. 23,24</ref>; <ref cRef="1 Par." loc="23,27">1 Par. 23,27</ref>; id ultra vel supra quod quid est, additur <sense n="α" xml:id="D12α"> interjecta praepos. <foreign xml:lang="gez">እምነ፡</foreign>, ut: <foreign xml:lang="gez">ወከሐክ፡ እምኔሁ፡</foreign> <foreign xml:lang="grc">καὶ ἐπέκεινα</foreign> <ref cRef="Num." loc="32,19">Num. 32,19</ref>; <foreign xml:lang="gez">መለኮት፡ ዘእምኵሉ፡ ልብ፡ ከሐከ፡</foreign> <foreign xml:lang="grc">ὁ θεὸς ὁ παντὸς ἐπέκεινα νοός</foreign> Cyr. ad <ref cRef="Theod." loc="10">Theod. f. 10</ref>; <foreign xml:lang="gez">በመንክር፡ ወከሐ፡ እምልብ፡ ውእቱ፡</foreign> <foreign xml:lang="grc">ξένως τε καὶ ὑπὲρ νοῦν</foreign> Cyr. c. <ref cRef="Pall." loc="77">Pall. f. 77</ref>; <foreign xml:lang="gez">ከመ፡ እሞት፡ ከሐከ፡ ውእቱ፡</foreign> <foreign xml:lang="grc">ὅτι θανάτου κρείττων</foreign> Cyr. ad <ref cRef="Theod." loc="24">Theod. f. 24</ref>; <foreign xml:lang="gez">ተኀሥሦት፡ እምጥንቃቄ፡ ከሐክ፡</foreign> <cit type="translation" xml:lang="la">
                                        <quote> disquirere </quote>
                                    </cit> <foreign xml:lang="grc">ὑπερμέτροις ἀκριβείαις</foreign>, Cyr. ad <ref cRef="Theod. " loc="13">Theod.  f. 13</ref>; <foreign xml:lang="gez">ወዓዲ፡ ዘእምዝ፡ ከሐከ፡</foreign> <foreign xml:lang="grc">εἰ χρὴ τούτων τὰ ἐπέκεινα λέγειν</foreign> (<cit type="translation" xml:lang="la">
                                        <quote> et </quote>
                                    </cit> <cit type="translation" xml:lang="la">
                                        <quote> ut </quote>
                                    </cit> <cit type="translation" xml:lang="la">
                                        <quote> his </quote>
                                    </cit> <cit type="translation" xml:lang="la">
                                        <quote> amplius </quote>
                                    </cit> <cit type="translation" xml:lang="la">
                                        <quote> quiddam </quote>
                                    </cit> <cit type="translation" xml:lang="la">
                                        <quote> addam </quote>
                                    </cit>) Cyr. ad <ref cRef="Theod." loc="9.">Theod. f. 9.</ref> </sense> <sense n="β" xml:id="D12β"> nulla <gramGrp> <pos expand="praepositio">praep.</pos> </gramGrp> interposita (voce <foreign xml:lang="gez">ከሓ፡</foreign> ipsa praepositionis vices explente): <cit type="translation" xml:lang="la">
                                        <quote> ultra </quote>
                                    </cit>, <cit type="translation" xml:lang="la">
                                        <quote> trans </quote>
                                    </cit>: <foreign xml:lang="gez">አሐውር፡ ከሓ፡ ዝየ፡</foreign> <ref cRef="Matth." loc="26,36">Matth. 26,36</ref> <lbl expand="romanae editionis">rom.</lbl>; <foreign xml:lang="gez">፩እምለፌ፡ ድንጋገ፡ ፈለግ፡ ወ፩እምከሐ፡ ድንጋገ፡ ፈለግ፡</foreign> (v. <foreign xml:lang="gez">እምከሐከ፡ በድንጋገ፡</foreign>) <ref cRef="Dan." loc="12,5">Dan. 12,5</ref>. – <lbl expand="Vocabularium Aethiopicum, -a -a">Voc. Ae.</lbl>: <foreign xml:lang="gez">ከሀ፡ ብ፡ እዝያ፤ ከሐከ፡ ብ፡ ወድያ።</foreign> </sense> </sense> </sense> </sense>
               
