            <entry xml:id="Lc922eaa3eb6542efbc79dd31b80c4844" n="8572">
               <form>
                  <foreign xml:lang="gez">ተግዕዘ</foreign>
               </form>
               <sense xml:lang="la" source="#dillmann" xml:id="Dmain"> (sec. Lud. etiam <foreign xml:lang="gez">ተገዐዘ፡</foreign>) <subc>III,1</subc> <sense n="1" xml:id="D1">
                     <cit type="translation" xml:lang="la">
                        <quote>libertatem sibi sumere</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>emancipationem appetere</quote>
                     </cit>: <foreign xml:lang="gez">ይትገዐዙ፡</foreign> (v. <foreign xml:lang="gez">ይትጋአዙ፡</foreign>) <foreign xml:lang="gez">ወይፈቅዱ፡ ይፃኡ፡ እምኔከ፡</foreign>
                     <ref cRef="Sir." loc="30,34">Sir. 30,34</ref>.</sense>
                  <sense n="2" xml:id="D2">
                     <cit type="translation" xml:lang="la">
                        <quote>manumitti</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>libertatem adipisci</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>liberari</quote>
                     </cit>, F.N. 31; <foreign xml:lang="gez">ተግዕዞ፡</foreign>
                     <ref cRef="1 Cor." loc="7,21">1 Cor. 7,21</ref> ant.; <foreign xml:lang="gez">ተግዕዝክሙ፡ እምኀጢአት፡</foreign>
                     <ref cRef="Rom." loc="6,18">Rom. 6,18</ref>; <ref cRef="Rom." loc="6,22">Rom.
                        6,22</ref>; <lbl expand="cum">c.</lbl>
                     <gramGrp>
                        <case value="accusativus">Acc.</case>
                     </gramGrp> libertatis: <foreign xml:lang="gez">ግዕዛነ፡ ኢተግዕዘት፡</foreign>
                     <ref cRef="Lev." loc="19,20">Lev. 19,20</ref>.</sense>
               </sense>
               
