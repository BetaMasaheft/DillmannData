               <sense xml:lang="la" source="#dillmann" xml:id="Dmain"> <gramGrp> <pos expand="Substantivum">subst.</pos> </gramGrp>, <lbl expand="pluralis">Pl.</lbl> <foreign xml:lang="gez">ሙፃኣት፡</foreign> <sense n="1" xml:id="D1"> <cit type="translation" xml:lang="la">
                                <quote> locus unde quid exit </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> exitus </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> egressus </quote>
                            </cit>, <ref cRef="Jes." loc="37,28">Jes. 37,28</ref>; <foreign xml:lang="gez">አልቦ፡ ሙፃአ፡ እምነ፡ ማእከሎሙ፡</foreign> <foreign xml:lang="grc">οὐκ ἦν διαφυγεῖν ἐκ μέσου αὐτῶν</foreign> <ref cRef="Judith" loc="7,19">Judith 7,19</ref>; inferna <foreign xml:lang="gez">እንተ፡ አልባቲ፡ ሙፃአ፡ እስከ፡ ለዓለም፡</foreign> <ref cRef="Clem." loc="91">Clem. f. 91</ref>; <ref cRef="Clem." loc="95">Clem. f. 95</ref>; <ref cRef="Clem." loc="180.">Clem. f. 180.</ref> <sense n="a" xml:id="D1a"> <cit type="translation" xml:lang="la">
                                    <quote> ortus </quote>
                                </cit> siderum <ref cRef="Ps." loc="18,6">Ps. 18,6</ref>; <ref cRef="Hen." loc="33,5">Hen. 33,5</ref>; <ref cRef="Hen." loc="73,3">Hen. 73,3</ref>; <foreign xml:lang="gez">ሙጻኣት፡</foreign> <ref cRef="Hen." loc="35.">Hen. 35.</ref> </sense> <sense n="b" xml:id="D1b"> <cit type="translation" xml:lang="la">
                                    <quote> fontes </quote>
                                </cit> aquarum: <foreign xml:lang="gez">ከመ፡ ዘይደፍን፡ ዐዘቅተ፡ ወያማስን፡ ሙፃአ፡ ማይ፡</foreign> <ref cRef="Prov." loc="25,26">Prov. 25,26</ref>; <foreign xml:lang="gez">አስተርአየ፡ ሙፃኣተ፡ ማያት፡</foreign> 2 <ref cRef="Reg." loc="22,16">Reg. 22,16</ref>.</sense> <sense n="c" xml:id="D1c"> <cit type="translation" xml:lang="la">
                                    <quote> os </quote>
                                </cit>, <cit type="translation" xml:lang="la">
                                    <quote> ostium </quote>
                                </cit> fluvii, <ref cRef="Kuf." loc="8.">Kuf. 8.</ref> </sense> <sense n="d" xml:id="D1d"> <cit type="translation" xml:lang="la">
                                    <quote> porta </quote>
                                </cit>: <foreign xml:lang="gez">ሙፃኣቲሁ፡</foreign> <ref cRef="Hez." loc="43,11">Hez. 43,11</ref>.</sense> <sense n="e" xml:id="D1e"> <cit type="translation" xml:lang="la">
                                    <quote> finis </quote>
                                </cit>: <foreign xml:lang="gez">ሙፃኣታ፡ ለፍኖት፡</foreign> <ref cRef="Sir." loc="21,10">Sir. 21,10</ref> <lbl expand="varia lectio">var.</lbl>; <foreign xml:lang="grc">ἐξόδιον</foreign> <foreign xml:lang="gez">ገብረ፡ ሙፃአ፡</foreign> <ref cRef="2 Par." loc="7,9">2 Par. 7,9</ref>.</sense> <sense n="f" xml:id="D1f"> <foreign xml:lang="gez">ሙፃአ፡ ኵስሕ፡</foreign> <cit type="translation" xml:lang="la">
                                    <quote> latrina </quote>
                                </cit> <ref cRef="Sx. Nah." loc="18 ">Sx. Nah. 18 </ref>(<lbl expand="videas">vid. sub</lbl> <foreign xml:lang="gez">ኰስሐ፡</foreign>).</sense> </sense> <sense n="2" xml:id="D2"> <cit type="translation" xml:lang="la">
                                <quote> pompa elationis </quote>
                            </cit> funeris (<lbl expand="videas">vid.</lbl> <foreign xml:lang="gez">ፀአት፡</foreign>): <foreign xml:lang="gez">ገብሩ፡ ሎቱ፡ ሙፃአ፡ ቀበር፡ ዐቢይ፡ ጥቀ፡</foreign> <ref cRef="2 Par. " loc="16,14">2 Par.  16,14</ref>.</sense> </sense>
               
