               <sense xml:lang="la" source="#dillmann" xml:id="Dmain"> <subc>II,1</subc> <cit type="translation" xml:lang="la">
                     <quote>strepere</quote>
                  </cit> vel <cit type="translation" xml:lang="la">
                     <quote>sonare</quote>
                  </cit> vel <cit type="translation" xml:lang="la">
                     <quote>resonare</quote>
                  </cit>
                  <cit type="translation" xml:lang="la">
                     <quote>facere</quote>
                  </cit>: <foreign xml:lang="gez">አነ፡ እግዚአብሔር፡ ዘኣደምፃ፡ ለማዕበላ፡</foreign>
                  <ref cRef="Jes." loc="51,15">Jes. 51,15</ref>; <ref cRef="Jer." loc="5,22">Jer.
                     5,22</ref>
                  <lbl expand="varia lectio">var.</lbl>; <foreign xml:lang="gez">እማት፡ ያደምፃ፡ ቃለ፡
                     ሰቆቃው፡</foreign>
                  <ref cRef="Sap." loc="18,10">Sap. 18,10</ref>; Abba Salama <foreign xml:lang="gez">ለብሔረ፡ አግዓዚ፡ አድያሚሃ፡ አድመፅከ፡</foreign> Deg.; <ref cRef="Org." loc="3">Org.
                     3</ref>; <cb n="1092" xml:id="c1092"/> omisso <gramGrp>
                     <case value="accusativus">Acc.</case>
                  </gramGrp>: <foreign xml:lang="gez">ከመ፡ ያድምፁ፡ ልዑለ፡</foreign>
                  <foreign xml:lang="grc">τοῦ ἀκουσθῆναι ποιῆσαι</foreign>
                  <ref cRef="1 Par." loc="15,19">1 Par. 15,19</ref>; <foreign xml:lang="gez">በአድምፆ፡
                     ይቀንጹ፡</foreign>
                  <foreign xml:lang="grc">ἐπὶ σκοπὸν ἁλοῦνται </foreign> (legendum videtur <foreign xml:lang="gez">በአድምዖ፡</foreign>) <ref cRef="Sap." loc="5,22">Sap. 5,22</ref>;
                  Sx. <ref cRef="Masc." loc="10 ">Masc. 10 </ref>Enc. (<lbl expand="videas">vid.
                     sub</lbl>
                  <foreign xml:lang="gez">አሜን፡</foreign>). </sense>
               
