            <entry xml:id="Ldb31b2cbf5dd4b0792f28c114b0f84c1" n="422">
               <form>
                  <foreign xml:lang="gez">ላዕላይ</foreign>
               </form>
               <sense xml:lang="la" source="#dillmann" xml:id="Dmain"> (rarius <foreign xml:lang="gez">ላዕላዊ፡</foreign>) <gramGrp>
                     <gen value="femininus">fem.</gen>
                  </gramGrp>
                  <foreign xml:lang="gez">ላዕላይት፡</foreign>
                  <gramGrp>
                     <pos expand="adjectivum">adj.</pos>
                  </gramGrp>
                  <cit type="translation" xml:lang="la">
                     <quote>superior</quote>
                  </cit>, <cit type="translation" xml:lang="la">
                     <quote>summus</quote>
                  </cit>, <cit type="translation" xml:lang="la">
                     <quote>celsus</quote>
                  </cit>, <cit type="translation" xml:lang="la">
                     <quote>elatus</quote>
                  </cit>, <cit type="translation" xml:lang="la">
                     <quote>altus</quote>
                  </cit>: <ref cRef="Gen." loc="22,2">Gen. 22,2</ref>; <foreign xml:lang="gez">ላዕላይ፡
                     ማኅደር፡ </foreign>
                  <cit type="translation" xml:lang="la">
                     <quote>contignatio summa</quote>
                  </cit>, <ref cRef="Clem." loc="23">Clem. p. 23</ref>; <foreign xml:lang="gez">ላዕላይ፡ ፍኖት፡ </foreign>
                  <foreign xml:lang="grc">ἡ ἄνω ὁδός</foreign>
                  <ref cRef="Jes." loc="7,3">Jes. 7,3</ref>; <ref cRef="Ps." loc="73,6">Ps.
                     73,6</ref>; <foreign xml:lang="gez">ምጥማቃት፡ ላዕላይ፡</foreign>
                  <foreign xml:lang="grc">κολυμβήθρα ἡ ἄνω </foreign>
                  <ref cRef="Jes." loc="36,2">Jes. 36,2</ref>; <foreign xml:lang="gez">ላዕላይ፡
                     ገቦሁ፡</foreign>
                  <ref cRef="Hez." loc="41,7">Hez. 41,7</ref>; <ref cRef="2 Esr. " loc="13,25">2
                     Esr. 13,25</ref>; <foreign xml:lang="gez">ሀገር፡ ታሕታይት፡ ወላዕላይት፡</foreign>
                  <ref cRef="1 Par." loc="7,24">1 Par. 7,24</ref>; <ref cRef="2 Par." loc="8,5">2
                     Par. 8,5</ref>; <foreign xml:lang="gez">ሐነጸ፡ አንቀጸ፡ ላዕላየ፡</foreign> (v. <foreign xml:lang="gez">ላዕሌየ፡</foreign>) <ref cRef="4 Reg." loc="15,35">4 Reg.
                     15,35</ref>; <foreign xml:lang="gez">ላዕላይ፡ ደወለ፡ ግብጽ፡ </foreign>
                  <ref cRef="Tob." loc="8,3">Tob. 8,3</ref> vel <foreign xml:lang="gez">ላዕላይ፡
                     ግብጽ፡</foreign>
                  <ref cRef="Sx. Masc." loc="7 ">Sx. Masc. 7 </ref> (<cit type="translation" xml:lang="la">
                     <quote>Aegyptus superior</quote>
                  </cit>); <ref cRef="Act." loc="19,1">Act. 19,1</ref>. </sense>
               
