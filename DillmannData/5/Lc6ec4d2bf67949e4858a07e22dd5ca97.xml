         <div>
            <entry xml:id="Lc6ec4d2bf67949e4858a07e22dd5ca97" n="5977">
               <form>
                  <foreign xml:lang="gez">ክህነት</foreign>
               </form>
               <sense xml:lang="la" source="#dillmann" xml:id="Dmain">
                  <gramGrp>
                     <pos expand="Substantivum">subst.</pos>
                  </gramGrp>
                  <cit type="translation" xml:lang="la">
                     <quote>sacerdotium</quote>
                  </cit>, <cit type="translation" xml:lang="la">
                     <quote>munus</quote>
                  </cit>
                  <cit type="translation" xml:lang="la">
                     <quote>sacerdotale</quote>
                  </cit>: <ref cRef="Ps." loc="98,6">Ps. 98,6</ref>; <ref cRef="1 Petr." loc="2,5">1
                     Petr. 2,5</ref>; <foreign xml:lang="gez">ተኀርየ፡ ዘርአ፡ ሌዊ፡ ለክህነት፡ </foreign>
                  <ref cRef="Kuf." loc="30">Kuf. 30</ref>; <foreign xml:lang="gez">ሳዶቅ፡ ተሠይመ፡ ውስተ፡
                     ክህነት፡</foreign> (v. <foreign xml:lang="gez">ካህናት፡</foreign>) <ref cRef="1 Par." loc="29,22">1 Par. 29,22</ref>; <foreign xml:lang="gez">ሰዐርዎሙ፡
                     እምክህነቶሙ፡</foreign>
                  <ref cRef="2 Esr." loc="2,62">2 Esr. 2,62</ref>; <foreign xml:lang="gez">አኅደግዎ፡
                     ክህነቶ፡</foreign>
                  <ref cRef="1             Esr." loc="5,39">1 Esr. 5,39</ref>; <foreign xml:lang="gez">ግብረ፡ ክህነት፡</foreign>
                  <ref cRef="Luc." loc="1,8">Luc. 1,8</ref>; <foreign xml:lang="gez">ዐቢይ፡
                     ክህነት፡</foreign>
                  <cit type="translation" xml:lang="la">
                     <quote>munus</quote>
                  </cit>
                  <cit type="translation" xml:lang="la">
                     <quote>summi</quote>
                  </cit>
                  <cit type="translation" xml:lang="la">
                     <quote>sacerdotis</quote>
                  </cit>
                  <ref cRef="Sir." loc="45,24">Sir. 45,24</ref>; Christianorum: <foreign xml:lang="gez">ኢይግበር፡ ሕዝባዊ፡ ግብረ፡ ክህነት፡</foreign>
                  <ref cRef="Did." loc="16">Did. 16</ref>; <foreign xml:lang="gez">ኢይሠየም፡
                     ለክህነት፡</foreign>
                  <ref cRef="Kedr f." loc="78">Kedr f. 78</ref>; <ref cRef="Org." loc="1.">Org.
                     1.</ref>
                  <sense n="a" xml:id="Da"> in Genitivo positum: <cit type="translation" xml:lang="la">
                        <quote>sacerdotalis</quote>
                     </cit>, ut: <foreign xml:lang="gez">አልባስ፡ ዘክህነት፡</foreign>
                     <ref cRef="Ex." loc="39,19">Ex. 39,19</ref>; <ref cRef="Ex." loc="35,19">Ex.
                        35,19</ref>; <ref cRef="Kuf." loc="32">Kuf. 32</ref>; <foreign xml:lang="gez">ሕዝበ፡ ክህነት፡</foreign>
                     <ref cRef="Kuf." loc="33.">Kuf. 33.</ref>
                  </sense>
                  <sense n="b" xml:id="Db"> sacerdotium i.e. <cit type="translation" xml:lang="la">
                        <quote>sacerdotes</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>populus</quote>
                     </cit>
                     <cit type="translation" xml:lang="la">
                        <quote>sacerdotalis</quote>
                     </cit>: <foreign xml:lang="gez">ይኩን፡ መንግሥተ፡ ወክህነተ፡ ወሕዝበ፡ ቅዱሰ፡ </foreign>
                     <ref cRef="Kuf." loc="64 ">Kuf. p. 64 </ref>(at legendum videtur <foreign xml:lang="gez">ዘክህነት፡</foreign>, <lbl expand="conferas">cfr.</lbl>
                     <ref cRef="1 Petr." loc="2,5">1 Petr. 2,5</ref>).</sense>
                  <sense n="c" xml:id="Dc"> pro <foreign xml:lang="grc">σαββατισμός</foreign> usurpatur <ref cRef="Hebr." loc="4,9">Hebr. 4,9</ref>
                     <lbl expand="romanae editionis">rom.</lbl>
                  </sense>
               </sense>
               
