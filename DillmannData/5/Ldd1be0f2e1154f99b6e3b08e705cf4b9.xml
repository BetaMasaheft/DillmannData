            <entry xml:id="Ldd1be0f2e1154f99b6e3b08e705cf4b9" n="7758">
               <form>
                  <foreign xml:lang="gez">ይቡስ</foreign>
               </form>
               <sense xml:lang="la" source="#dillmann" xml:id="Dmain">
                  <gramGrp>
                     <pos expand="participium">part.</pos>
                  </gramGrp>
                  <cit type="translation" xml:lang="la">
                     <quote>siccus</quote>
                  </cit>, <cit type="translation" xml:lang="la">
                     <quote>aridus</quote>
                  </cit>, <cit type="translation" xml:lang="la">
                     <quote>exsiccatus</quote>
                  </cit>: <foreign xml:lang="gez">ይብስት፡ ባሕር፡</foreign>
                  <ref cRef="Ex." loc="14,29">Ex. 14,29</ref>; <foreign xml:lang="gez">ምድር፡ ይቡስ፡
                  </foreign> (<lbl expand="oppositum, -o, -nitur">opp.</lbl> mari) <ref cRef="Hebr." loc="11,29">Hebr. 11,29</ref>; (<lbl expand="oppositum, -o, -nitur">opp.</lbl>
                  humectato) <ref cRef="Jud." loc="6,37">Jud. 6,37</ref>; <ref cRef="Jud." loc="6,38">Jud. 6,38</ref>; <ref cRef="Jud." loc="6,39">Jud. 6,39</ref>; <ref cRef="Jud." loc="6,40">Jud. 6,40</ref>; sterilis (<lbl expand="oppositum, -o, -nitur">opp.</lbl> irriguo) <ref cRef="Num." loc="13,20">Num. 13,20</ref>; <ref cRef="Hen." loc="26,4">Hen. 26,4</ref>; <foreign xml:lang="gez">ዕፅ፡ ይቡስ፡</foreign>
                  <ref cRef="Sir." loc="6,3">Sir. 6,3</ref>; <ref cRef="Jes." loc="56,3">Jes.
                     56,3</ref>; <ref cRef="Hez." loc="17,24">Hez. 17,24</ref>; <ref cRef="Hez." loc="20,47">Hez. 20,47</ref>; <ref cRef="Hen." loc="3">Hen. 3</ref>; <ref cRef="Luc." loc="23,31">Luc. 23,31</ref>; <foreign xml:lang="gez">ሐሠር፡
                     ይቡስ፡</foreign>
                  <ref cRef="Jes." loc="9,18">Jes. 9,18</ref>; <foreign xml:lang="gez">ጽጌ፡
                     ይቡስ፡</foreign>
                  <ref cRef="Zeph." loc="2,2">Zeph. 2,2</ref>; de spicis: <foreign xml:lang="grc">ἀνεμόφθορος </foreign>
                  <ref cRef="Gen." loc="41,23">Gen. 41,23</ref>; <ref cRef="Gen." loc="41,24">Gen.
                     41,24</ref>; de ossibus, <ref cRef="Hez." loc="37,2">Hez. 37,2</ref>; de pane:
                     <ref cRef="Jos." loc="9,3">Jos. 9,3</ref>; <foreign xml:lang="gez">ኅብስት፡ ይቡስ፡
                     ወጼው፡</foreign>
                  <ref cRef="Kedr f." loc="65">Kedr f. 65</ref>; <ref cRef="Phlx." loc="74">Phlx.
                     74</ref>; <ref cRef="Sx. Teq." loc="1.">Sx. Teq. 1.</ref>
                  <sense n="a" xml:id="Da">
                     <foreign xml:lang="gez">ይቡስ፡</foreign>
                     <cit type="translation" xml:lang="la">
                        <quote>aridum</quote>
                     </cit> vel <cit type="translation" xml:lang="la">
                        <quote>siccum</quote>
                     </cit> i.e. <cit type="translation" xml:lang="la">
                        <quote>terra sicca</quote>
                     </cit>
                     <ref cRef="Jos." loc="3,17">Jos. 3,17</ref>.</sense>
                  <sense n="b" xml:id="Db">
                     <cit type="translation" xml:lang="la">
                        <quote>torpens</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>torpidus</quote>
                     </cit>, de manu: <ref cRef="Luc." loc="6,6">Luc. 6,6</ref>.</sense>
                  <sense n="c" xml:id="Dc">
                     <cit type="translation" xml:lang="la">
                        <quote>tabe confectus</quote>
                     </cit>
                     <ref cRef="Joh." loc="5,3">Joh. 5,3</ref>.</sense>
                  <sense n="d" xml:id="Dd">
                     <cit type="translation" xml:lang="la">
                        <quote>durus</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>rigidus</quote>
                     </cit>: <foreign xml:lang="gez">ክሳድ፡ ይቡስ፡</foreign>
                     <ref cRef="Kuf." loc="1">Kuf. 1</ref>; <foreign xml:lang="gez">ይቡሳነ፡
                        ልብ፡</foreign>
                     <ref cRef="Hen." loc="5,4">Hen. 5,4</ref> – (Pro <foreign xml:lang="gez">በዝብጠተ፡
                        ይቡስ፡</foreign> quod Lud. emendavit et <cit type="translation" xml:lang="la">
                        <quote>percussione aridae</quote>
                     </cit> i.e. <cit type="translation" xml:lang="la">
                        <quote>scuticae</quote>
                     </cit> vertit, legendum est <foreign xml:lang="gez">በዝብጠተ፡ ድቡስ፡</foreign>
                     <ref cRef="Sx. Sen." loc="27 ">Sx. Sen. 27 </ref>Enc.).</sense>
               </sense>
               
