                    </form>
               <sense xml:lang="la" source="#dillmann" xml:id="Dmain">, etiam <foreign xml:lang="gez">ወድዐ፡</foreign> <subc>I,2</subc> [arab. <foreign xml:lang="ar">وَدَعَ</foreign> (<foreign xml:lang="ar">ودأ</foreign>) <cit type="translation" xml:lang="la">
                            <quote> posuit </quote>
                        </cit>; <cit type="translation" xml:lang="la">
                            <quote> sivit </quote>
                        </cit>, <cit type="translation" xml:lang="la">
                            <quote> missum fecit </quote>
                        </cit>, <cit type="translation" xml:lang="la">
                            <quote> omisit </quote>
                        </cit>] <sense n="1" xml:id="D1"> <cit type="translation" xml:lang="la">
                                <quote> perficere </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> finire </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> consummare </quote>
                            </cit>, absolvere: <foreign xml:lang="gez">ወዲአ፡ መዋዕል፡</foreign> <cit type="translation" xml:lang="la">
                                <quote> consummatio </quote>
                            </cit> (<cit type="translation" xml:lang="la">
                                <quote> finis </quote>
                            </cit>) <cit type="translation" xml:lang="la">
                                <quote> dierum </quote>
                            </cit> <ref cRef="Kuf." loc="121">Kuf. p. 121</ref>; crebro usurpatur ad significandam actionem vere perfectam (<bibl>
                                <ptr target="bm:Dillmann1857Gramm"/>gramm.</bibl> § 88), quam Latini sive Perfecto nudo, sive adverbiis <cit type="translation" xml:lang="la">
                                <quote> jam </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> modo </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> dudum </quote>
                            </cit> notant; <lbl expand="construitur cum">c.c.</lbl> Infin.: <foreign xml:lang="gez">ዘወድአ፡ ተኀፅቦ፡</foreign> <foreign xml:lang="grc">ὁ λελουμένος</foreign> <ref cRef="Joh." loc="13,10">Joh. 13,10</ref> <lbl expand="romanae editionis">rom.</lbl>, vel <lbl expand="cum">c.</lbl> <gramGrp> <case value="accusativus">Acc.</case> </gramGrp> nominis actionis: <foreign xml:lang="gez">ወድአት፡ ርሥአታ፡</foreign> <ref cRef="4 Esr. " loc="3,64">4 Esr.  3,64</ref> Laur.; plerumque <lbl expand="cum">c.</lbl> verbo finito, <sense n="a" xml:id="D1a"> eoque asyndeto, <lbl expand="scilicet">sc.</lbl> <sense n="α" xml:id="D1aα"> Perf.: <foreign xml:lang="gez">እለ፡ ወድኡ፡ ሞቱ፡</foreign> <foreign xml:lang="grc">οἱ ἤδη ἀποθανόντες</foreign> <ref cRef="Koh." loc="4,2">Koh. 4,2</ref>; <foreign xml:lang="gez">ወዳእኩ፡ ጸናዕኩ፡</foreign> <ref cRef="Job" loc="20,7">Job 20,7</ref>; <foreign xml:lang="gez">ወድአ፡ አእመረ፡</foreign> <foreign xml:lang="grc">οἶδε ἤδη</foreign> <ref cRef="Job " loc="23,10">Job  23,10</ref>; <foreign xml:lang="gez">ወድአ፡ ተአዘዘ፡</foreign> <ref cRef="Dan. apocr." loc="1,55">Dan. apocr. 1,55</ref>; <ref cRef="Num. " loc="17,12">Num.  17,12</ref>; <ref cRef="Num." loc="22,29">Num. 22,29</ref>; <ref cRef="2 Tim." loc="2,18">2 Tim. 2,18</ref>; <ref cRef="Matth." loc="5,28">Matth. 5,28</ref>; <ref cRef="Luc." loc="11,7">Luc. 11,7</ref>; <ref cRef="Luc." loc="12,49">Luc. 12,49</ref>; <ref cRef="Act. " loc="4,3">Act.  4,3</ref>; <ref cRef="1 Joh." loc="2,8">1 Joh. 2,8</ref> Platt; <ref cRef="1 Joh." loc="4,3">1 Joh. 4,3</ref>; <foreign xml:lang="gez">ወዳእነ፡ ግእዝናሆሙ፡</foreign> <foreign xml:lang="grc">προῃτιασάμεθα</foreign> <ref cRef="Rom." loc="3,9">Rom. 3,9</ref>; <foreign xml:lang="grc">πάλαι</foreign> <ref cRef="Matth." loc="11,21">Matth. 11,21</ref>; <foreign xml:lang="gez">ወዳእነ፡ ሞትነ፡</foreign> <foreign xml:lang="grc">ἀπεθάνομεν</foreign> <ref cRef="Rom." loc="6,2">Rom. 6,2</ref>; <ref cRef="Lev." loc="10,16">Lev. 10,16</ref>; nec non inverso verborum ordine: <foreign xml:lang="gez">ተአዘዘ፡ ወድአ፡</foreign> <foreign xml:lang="grc">ἐντέταλται ἤδη</foreign> <ref cRef="Job" loc="15,22">Job 15,22</ref>; <ref cRef="4 Esr." loc="13,24">4 Esr. 13,24</ref> Laur.; <cb n="0933" xml:id="c0933"/> <foreign xml:lang="gez">እቤ፡ ወዳእኩ፡</foreign> <ref cRef="1 Reg." loc="25,34">1 Reg. 25,34</ref>; <ref cRef="Marc." loc="6,35">Marc. 6,35</ref>.</sense> <sense n="β" xml:id="D1aβ"> Imperf.: <ref cRef="2 Thess." loc="2,7">2 Thess. 2,7</ref> Platt; <ref cRef="Matth." loc="3,10">Matth. 3,10</ref>.</sense> <sense n="b" xml:id="D1ab"> rarius interjecta copula <foreign xml:lang="gez">ወ፡</foreign>, ut: <foreign xml:lang="gez">ወድአ፡ ወገብረ፡</foreign> <foreign xml:lang="grc">προέθετο</foreign> <ref cRef="Rom." loc="3,25">Rom. 3,25</ref> Platt; <ref cRef="Phil." loc="3,12">Phil. 3,12</ref> Platt; <ref cRef="1 Joh." loc="2,8">1 Joh. 2,8</ref> <lbl expand="romanae editionis">rom.</lbl>; <foreign xml:lang="gez">ወዳእኩ፡ ወብዕልኩ፡</foreign> <ref cRef="Hos." loc="12,8">Hos. 12,8</ref>.</sense> <sense n="2" xml:id="D1a2"> <cit type="translation" xml:lang="la">
                                        <quote> absumere </quote>
                                    </cit>, <cit type="translation" xml:lang="la">
                                        <quote> consumere </quote>
                                    </cit>, <cit type="translation" xml:lang="la">
                                        <quote> totum consumere </quote>
                                    </cit>: <foreign xml:lang="gez">እሳት፡ ዘይዌድእ፡ ውእቱ፡</foreign> <ref cRef="Deut." loc="9,3">Deut. 9,3</ref>; <foreign xml:lang="gez">ሕያው፡ ነፍስከ፡ ከመ፡ ኢትዌድእ፡ አመትከ፡ ዘሀለወ፡ ምስሌሃ፡</foreign> <ref cRef="Judith " loc="12,4">Judith  12,4</ref>; <ref cRef="Ex." loc="12,4">Ex. 12,4</ref>; <ref cRef="Ex." loc="12,9">Ex. 12,9</ref>; <foreign xml:lang="gez">ነጸረ፡ ንጉሥ፡ ዘወድኡ፡ ማእደ፡</foreign> <ref cRef="Dan. apocr." loc="13,18">Dan. apocr. 13,18</ref>. – <lbl expand="Vocabularium Aethiopicum, -a -a">Voc. Ae.</lbl>: <foreign xml:lang="gez">ወድአ፡ ዘ፡ ፈጸመ፡ ቀርበሂ፡ ይ፡</foreign>; aliud: <foreign xml:lang="gez">ወድአ፡ ዘ፡ ፈጀ።</foreign> </sense> </sense> </sense> </sense>
               
