            <entry xml:id="L61e8c8ffd33a46bcae27bbe652465132" n="4525">
               <form>
                  <foreign xml:lang="gez">አኅሰረ</foreign>
               </form>
               <sense xml:lang="la" source="#dillmann" xml:id="Dmain"> <subc>II,1</subc> <sense n="1" xml:id="D1">
                     <cit type="translation" xml:lang="la">
                        <quote>deprimere</quote>
                     </cit>; <cit type="translation" xml:lang="la">
                        <quote>humilem</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>vilem reddere</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>in conditionem deteriorem conjicere</quote>
                     </cit>: <foreign xml:lang="grc">ταπεινοῦν</foreign>
                     <ref cRef="Ruth" loc="1,21">Ruth 1,21</ref>; <cb n="0591" xml:id="c0591"/>
                     <foreign xml:lang="gez">እግዚአብሔር፡ አኅሰራ፡</foreign>
                     <ref cRef="Thren." loc="1,5">Thren. 1,5</ref>; <ref cRef="Job" loc="22,12">Job
                        22,12</ref>; <ref cRef="Job " loc="40,6">Job 40,6</ref>; <ref cRef="Jes." loc="3,17">Jes. 3,17</ref>; <ref cRef="Jes." loc="13,11">Jes. 13,11</ref>;
                        <ref cRef="Jes." loc="64,12">Jes. 64,12</ref>; <ref cRef="Dan." loc="4,34">Dan. 4,34</ref>; <foreign xml:lang="gez">ያኀስሮሙ፡ ለዕቡያን፡</foreign> (<foreign xml:lang="grc">ἀντιτάσσεται</foreign>) <ref cRef="1 Petr." loc="5,5">1 Petr.
                        5,5</ref>; <ref cRef="Job" loc="36,13">Job 36,13</ref>.</sense>
                  <sense n="2" xml:id="D2">
                     <cit type="translation" xml:lang="la">
                        <quote>imminuere</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>ad paucitatem redigere</quote>
                     </cit>; <cit type="translation" xml:lang="la">
                        <quote>mulcta</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>poena</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>miseria afficere</quote>
                     </cit>: <foreign xml:lang="grc">ταλαιπωρεῖν</foreign>
                     <ref cRef="Jes. " loc="33,1">Jes. 33,1</ref>; <ref cRef="Ps." loc="16,9">Ps.
                        16,9</ref>; <foreign xml:lang="grc">καταπίνειν</foreign>
                     <ref cRef="Jes." loc="9,16">Jes. 9,16</ref>; <foreign xml:lang="grc">ζημιοῦν</foreign>; <ref cRef="Ex." loc="21,22">Ex. 21,22</ref>; <foreign xml:lang="gez">አኅስሮቱ፡ ለብእሲ፡ ጻድቅ፡ አኮ፡ ሠናይ፡</foreign>
                     <ref cRef="Prov." loc="17,26">Prov. 17,26</ref>; <foreign xml:lang="gez">ለእንተ፡
                        ከመዝ፡ ያኀስርዋ፡</foreign>
                     <ref cRef="Sir." loc="23,24">Sir. 23,24</ref>; <foreign xml:lang="grc">ἐξουδενεῖν</foreign>
                     <ref cRef="Judith " loc="13,17">Judith 13,17</ref>.</sense>
                  <sense n="3" xml:id="D3">
                     <cit type="translation" xml:lang="la">
                        <quote>ignominia</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>dedecore</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>contumelia afficere</quote>
                     </cit>; <cit type="translation" xml:lang="la">
                        <quote>exprobrare</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>pudore suffundere</quote>
                     </cit>: <foreign xml:lang="grc">ἀτιμάζειν</foreign>
                     <foreign xml:lang="gez">ከመ፡ ኢታኅስር፡ አዝማዲከ፡</foreign>
                     <ref cRef="Sir." loc="8,4">Sir. 8,4</ref>; <ref cRef="Sir." loc="29,6">Sir.
                        29,6</ref>; <ref cRef="Luc." loc="20,11">Luc. 20,11</ref>; <ref cRef="Marc." loc="12,4">Marc. 12,4</ref>; <foreign xml:lang="grc">ὑβρίζειν</foreign>
                     <ref cRef="Jes." loc="13,3">Jes. 13,3</ref>; <foreign xml:lang="gez">ለያኅስርከ፡
                        እግዚአብሔር፡</foreign>
                     <foreign xml:lang="grc">ἐπιτιμήσαι ἐν σοί</foreign>
                     <ref cRef="Zach." loc="3,2">Zach. 3,2</ref>.</sense>
                  <sense n="4" xml:id="D4"> In specie <cit type="translation" xml:lang="la">
                        <quote>vitiare</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>corrumpere</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>stuprare</quote>
                     </cit> feminam: <foreign xml:lang="grc">ταπεινοῦν</foreign>
                     <ref cRef="Gen." loc="34,2">Gen. 34,2</ref>; <ref cRef="Deut." loc="22,24">Deut. 22,24</ref>; <ref cRef="Jud." loc="19,24">Jud. 19,24</ref>; hinc
                        <foreign xml:lang="grc">μιαίνειν τὴν γῆν</foreign>
                     <ref cRef="Jer." loc="2,7">Jer. 2,7</ref>; <foreign xml:lang="gez">ከመ፡ ያኅስሩ፡
                        ነፍስቶሙ፡</foreign>
                     <foreign xml:lang="grc">τοῦ ἀτιμάζεσθαι τὰ σώματα ἑαυτῶν ἐν ἑαυτοῖς</foreign>
                     <ref cRef="Rom." loc="1,24">Rom. 1,24</ref>.</sense>
               </sense>
               
