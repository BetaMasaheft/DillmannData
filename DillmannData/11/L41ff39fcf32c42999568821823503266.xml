               <sense xml:lang="la" source="#dillmann" xml:id="Dmain">, rarius <foreign xml:lang="gez">ተሰአለ፡</foreign> (ut <ref cRef="Kuf." loc="41">Kuf. 41</ref>; <ref cRef="Chrys. ho." loc=" 9">Chrys. ho.  9</ref>) <subc>III,2</subc> <cit type="translation" xml:lang="la">
                            <quote> interrogare </quote>
                        </cit>, <cit type="translation" xml:lang="la">
                            <quote> percontari </quote>
                        </cit>, <cit type="translation" xml:lang="la">
                            <quote> sciscitari </quote>
                        </cit>, <lbl expand="cum">c.</lbl> <gramGrp> <case value="accusativus">Acc.</case> </gramGrp> <lbl expand="persona">pers.</lbl>: <foreign xml:lang="gez">እሴአለከ፡</foreign> <ref cRef="Job" loc="38,3">Job 38,3</ref>; <ref cRef="Job" loc="8,8">Job 8,8</ref>; <ref cRef="Job" loc="12,7">Job 12,7</ref>; <foreign xml:lang="gez">እመ፡ ተስእልክዎሙ፡</foreign> <ref cRef="Jes." loc="41,28">Jes. 41,28</ref>; <foreign xml:lang="gez">ተሰአልዎሙ፡ ለአሕዛብ፡</foreign> (<foreign xml:lang="grc">ἐρωτήσατε</foreign>) <ref cRef="Jer." loc="18,13">Jer. 18,13</ref>; <ref cRef="Jer." loc="43,17">Jer. 43,17</ref>; <ref cRef="4 Esr." loc="8,4">4 Esr. 8,4</ref>; <ref cRef="4 Esr." loc="10,12">4 Esr. 10,12</ref>; <ref cRef="Gen. " loc="32,18">Gen.  32,18</ref>; <ref cRef="Hen." loc="40,8">Hen. 40,8</ref>; <ref cRef="Matth." loc="2,4">Matth. 2,4</ref>; <foreign xml:lang="gez">እለ፡ ኢተስእሉኒ፡</foreign> <ref cRef="Jes. " loc="65,1">Jes.  65,1</ref>; <ref cRef="Rom." loc="10,20">Rom. 10,20</ref>; <lbl expand="cum">c.</lbl> <gramGrp> <case value="accusativus">Acc.</case> </gramGrp> rei: <foreign xml:lang="gez">ተሰአሉ፡ ፍኖተ፡ እግዚአብሔር፡</foreign> (Imperat.) <ref cRef="Jer." loc="6,16">Jer. 6,16</ref>; <ref cRef="Gen." loc="32,30">Gen. 32,30</ref>; <ref cRef="1 Reg." loc="23,2">1 Reg. 23,2</ref>; <ref cRef="1 Reg." loc="28,6">1 Reg. 28,6</ref>; <ref cRef="2 Reg." loc="2,1">2 Reg. 2,1</ref>; <lbl expand="cum">c.</lbl> <lbl expand="duplex">dupl.</lbl> <gramGrp> <case value="accusativus">Acc.</case> </gramGrp>: <ref cRef="Ps." loc="136,3">Ps. 136,3</ref>; <foreign xml:lang="gez">ተስእልዎሙ፡ አስማቲሆሙ፡</foreign> <ref cRef="2 Esr." loc="5,4">2 Esr. 5,4</ref>; <ref cRef="Gen." loc="43,7">Gen. 43,7</ref>; <foreign xml:lang="gez">ዘይሴአሉኒ፡</foreign> <ref cRef="Chrys. ho." loc="4">Chrys. ho. 4</ref>; <lbl expand="cum">c.</lbl> <foreign xml:lang="gez">ኀበ፡</foreign> vel <foreign xml:lang="gez">በኀበ፡</foreign> pers. <ref cRef="Jud." loc="20,18">Jud. 20,18</ref>; <ref cRef="Dan." loc="2,10">Dan. 2,10</ref>; <ref cRef="Dan." loc="2,11">Dan. 2,11</ref>; <lbl expand="cum">c.</lbl> <foreign xml:lang="gez">በእንተ፡</foreign> rei vel <lbl expand="persona">pers.</lbl> (<cit type="translation" xml:lang="la">
                            <quote> de </quote>
                        </cit>): <ref cRef="Koh." loc="7,10">Koh. 7,10</ref>; <ref cRef="4 Esr." loc="2,34">4 Esr. 2,34</ref>; <foreign xml:lang="gez">ተሰአሉኒ፡ በእንተ፡ ደቂቅየ፡</foreign> <ref cRef="Jes." loc="45,11">Jes. 45,11</ref>; <ref cRef="1 Reg." loc="17,22">1 Reg. 17,22</ref>; <ref cRef="1 Reg." loc="22,10">1 Reg. 22,10</ref>; nota etiam: <foreign xml:lang="gez">ተስእልክዎ፡ ከመ፡ ያርእየኒ፡</foreign> <ref cRef="Hen." loc="60,9">Hen. 60,9</ref>. – Infin. <foreign xml:lang="gez">ተስእሎት፡</foreign> <cit type="translation" xml:lang="la">
                            <quote> quaestio </quote>
                        </cit>, <cit type="translation" xml:lang="la">
                            <quote> interrogatio </quote>
                        </cit>: <foreign xml:lang="gez">ተስእሎታት፡ ዘተስእልዎ፡ ካልኣን፡</foreign> <ref cRef="Sx. Jac." loc="10">Sx. Jac. 10</ref>; <ref cRef="Phlx." loc="6">Phlx. 6</ref>; <foreign xml:lang="gez">ተስእሎታተ፡ ዜናሆሙ፡ ለአበው፡ ቅዱሳን፡</foreign> <ref cRef="Phlx." loc=" in tit.">Phlx.  in tit.</ref> </sense>
               
