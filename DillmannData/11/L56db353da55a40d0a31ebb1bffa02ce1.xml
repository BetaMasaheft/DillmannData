         <div>
            <entry xml:id="L56db353da55a40d0a31ebb1bffa02ce1" n="8426">
               <form>
                  <foreign xml:lang="gez">ግቡር</foreign>
               </form>
               <sense xml:lang="la" source="#dillmann" xml:id="Dmain">
                  <gramGrp>
                     <pos expand="participium">part.</pos>
                  </gramGrp>
                  <cit type="translation" xml:lang="la">
                     <quote>factus</quote>
                  </cit>
                  <sense n="a" xml:id="Da">
                     <cit type="translation" xml:lang="la">
                        <quote>creatus</quote>
                     </cit>: philosophi <foreign xml:lang="gez">ለግቡራንሂ፡ ይቤልዎሙ፡ ኢኮኑ፡ ግቡራነ፡ ኢተፈጥሩሂ፡
                        አላ፡ ሀለዉ፡ ዘእንበለ፡ ጥንት፡</foreign>
                     <ref cRef="Chrys. ho." loc="22.">Chrys. ho. 22.</ref>
                  </sense>
                  <sense n="b" xml:id="Db">
                     <cit type="translation" xml:lang="la">
                        <quote>confectus</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>fabricatus</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>formatus</quote>
                     </cit>: <foreign xml:lang="grc">πεπλασμένος</foreign>
                     <ref cRef="Jer." loc="19,1">Jer. 19,1</ref>; <foreign xml:lang="gez">ኀጺን፡ ግቡር፡ </foreign>
                     <foreign xml:lang="grc">σίδηρος εἰργασμένος</foreign>
                     <ref cRef="Hez." loc="27,19">Hez. 27,19</ref>; <foreign xml:lang="gez">ማእሰ፡
                        በግዕ፡ ግቡረ፡ መሃን፡</foreign>
                     <ref cRef="Ex." loc="35,7">Ex. 35,7</ref>; <ref cRef="Ex. " loc="39,21">Ex.
                        39,21</ref>.</sense>
                  <sense n="c" xml:id="Dc">
                     <cit type="translation" xml:lang="la">
                        <quote>paratus</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>praeparatus</quote>
                     </cit>: <ref cRef="Lev." loc="7,10">Lev. 7,10</ref>; <ref cRef="Lev." loc="7,12">Lev. 7,12</ref>; <ref cRef="Lev." loc="23,13">Lev. 23,13</ref>;
                        <ref cRef="Num." loc="7,13">Num. 7,13</ref>
                     <lbl expand="sequens">seq.</lbl>; <ref cRef="Num." loc="8,8">Num. 8,8</ref>;
                        <ref cRef="Num." loc="28 ">Num. 28 </ref>et <ref cRef="Num." loc="29">Num.
                        29</ref>; <ref cRef="1 Reg." loc="25,18">1 Reg. 25,18</ref>.</sense>
                  <sense n="d" xml:id="Dd">
                     <cit type="translation" xml:lang="la">
                        <quote>ornatus</quote>
                     </cit>
                     <ref cRef="Ps." loc="67,14">Ps. 67,14</ref>.</sense>
                  <sense n="e" xml:id="De">
                     <cit type="translation" xml:lang="la">
                        <quote>perfectus</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>peractus</quote>
                     </cit>
                     <ref cRef="Ps." loc="110,7">Ps. 110,7</ref>.</sense>
                  <sense n="f" xml:id="Df">
                     <lbl expand="videas">vid. sub</lbl>
                     <foreign xml:lang="gez">ግዑር፡</foreign>
                  </sense>
               </sense>
               
            </entry>
