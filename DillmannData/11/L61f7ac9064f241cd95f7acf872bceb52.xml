               <sense xml:lang="la" source="#dillmann" xml:id="Dmain"> <subc>III,2</subc> <gramGrp> <pos expand="reflexivum">refl.</pos> </gramGrp> et <gramGrp> <pos expand="passivum">pass.</pos> </gramGrp> <sense n="1" xml:id="D1"> <cit type="translation" xml:lang="la">
                                <quote> occuli </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> operiri </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> occultum esse </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> latere </quote>
                            </cit>, <ref cRef="Matth." loc="8,24">Matth. 8,24</ref>; <lbl expand="construitur cum">c.c.</lbl> <foreign xml:lang="gez">እምነ፡</foreign> <ref cRef="Luc." loc="19,42">Luc. 19,42</ref>; <cb n="0385" xml:id="c0385"/> <foreign xml:lang="gez">ለምንት፡ ትሴወር፡ እምኔየ፡</foreign> <ref cRef="Job" loc="13,24">Job 13,24</ref>; <ref cRef="Job" loc="26,6">Job 26,6</ref>; <ref cRef="Jer." loc="23,24">Jer. 23,24</ref>; <foreign xml:lang="gez">ይሴወር፡ እምዐይን፡ ወገጽ፡</foreign> <ref cRef="Job" loc="24,15">Job 24,15</ref>; <ref cRef="Gad. Lal." loc="">Gad. Lal. </ref> (<lbl expand="videas">vid.</lbl> <foreign xml:lang="gez">ተመስጠረ፡</foreign>) vel <lbl expand="cum">c.</lbl> <foreign xml:lang="gez">እምቅድመ፡</foreign>, ut: <foreign xml:lang="gez">ኢይሴወር፡ እምቅድመ፡ አዕይንትየ፡ ኀጢአቶሙ፡</foreign> <ref cRef="Jer." loc="16,17">Jer. 16,17</ref>.</sense> <sense n="2" xml:id="D2"> <cit type="translation" xml:lang="la">
                                <quote> se abscondere </quote>
                            </cit> i.e. <cit type="translation" xml:lang="la">
                                <quote> disparere </quote>
                            </cit>: <foreign xml:lang="gez">ተሰወረ፡ አምአዕይንቲሆሙ፡</foreign> <ref cRef="Act." loc="1,9">Act. 1,9</ref>; <lbl expand="cum">c.</lbl> <gramGrp> <case value="accusativus">Acc.</case> </gramGrp> <lbl expand="persona">pers.</lbl> (ab aliquo), Lud. ex <ref cRef="Hom. Theoph." loc="">Hom. Theoph. </ref> </sense> <sense n="3" xml:id="D3"> <cit type="translation" xml:lang="la">
                                <quote> sibi operire </quote>
                            </cit> vel <cit type="translation" xml:lang="la">
                                <quote> tegere </quote>
                            </cit>: <foreign xml:lang="gez">ቤት፡ ኀበ፡ ትሴወር፡</foreign> (v. <foreign xml:lang="gez">ትሴውር፡</foreign>) <foreign xml:lang="gez">ኀፍረተከ፡</foreign> <ref cRef="Sir." loc="29,21">Sir. 29,21</ref>.</sense> </sense>
               
            
