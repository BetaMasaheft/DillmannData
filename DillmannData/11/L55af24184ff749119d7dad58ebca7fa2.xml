                  <foreign xml:lang="gez">ተሰብረ</foreign>
               </form>
               <sense xml:lang="la" source="#dillmann" xml:id="Dmain"> <subc>III,1</subc> <sense n="1" xml:id="D1"> <cit type="translation" xml:lang="la">
                                <quote> frangi </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> confringi </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> effringi </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> perfringi </quote>
                            </cit>; de olla <foreign xml:lang="gez">ለሊሃ፡ ትሰበር፡</foreign> <ref cRef="Sir." loc="13,2">Sir. 13,2</ref>; baculo <ref cRef="4 Reg." loc="18,21">4 Reg. 18,21</ref>; <ref cRef="Jes." loc="42,3">Jes. 42,3</ref> <lbl expand="varia lectio">var.</lbl>; arcu <ref cRef="Jer." loc="28,56">Jer. 28,56</ref>; malleo <foreign xml:lang="gez">እፎ፡ ተቀጥቀጠት፡ ወተሰብረት፡</foreign> <ref cRef="Jer." loc="27,23">Jer. 27,23</ref>; cornu <ref cRef="Jer." loc="31,25">Jer. 31,25</ref>; spina dorsi <ref cRef="1 Reg." loc="4,18">1 Reg. 4,18</ref>; arbore <ref cRef="Hez." loc="19,12">Hez. 19,12</ref>; munimentis: <foreign xml:lang="gez">ተሰብረ፡ ጸወንክሙ፡</foreign> <ref cRef="Jes." loc="23,14">Jes. 23,14</ref>; hominibus <foreign xml:lang="grc">διεσχίσθησαν</foreign> <ref cRef="Ps." loc="34,19">Ps. 34,19</ref>. In specie <sense n="a" xml:id="D1a"> de <cit type="translation" xml:lang="la">
                                    <quote> crurifragio </quote>
                                </cit>, <cit type="translation" xml:lang="la">
                                    <quote> fractura membrorum corporis </quote>
                                </cit> <ref cRef="Ex." loc="22,13">Ex. 22,13</ref>; <ref cRef="Sx. Nah. " loc="24">Sx. Nah.  24</ref>; <foreign xml:lang="gez">በተሰብሮ፡ ቍይጽ፡</foreign> <ref cRef="Sx. Jac." loc="21 Enc.">Sx. Jac. 21 Enc.</ref>
                            </sense> <sense n="b" xml:id="D1b"> de <cit type="translation" xml:lang="la">
                                    <quote> naufragio </quote>
                                </cit>, <ref cRef="2 Cor." loc="11,25">2 Cor. 11,25</ref> <lbl expand="romanae editionis">rom.</lbl>; <foreign xml:lang="gez">ተመንደበ፡ ሐመሮሙ፡ ከመ፡ ይሰበር፡</foreign> <ref cRef="Jon." loc="1,4">Jon. 1,4</ref>; <ref cRef="Sx." loc="">Sx. </ref> saepius.</sense> </sense> <sense n="2" xml:id="D2"> <cit type="translation" xml:lang="la">
                                <quote> profligari </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> clade affici </quote>
                            </cit> (in bello): <ref cRef="Gen." loc="14,10">Gen. 14,10</ref>; <ref cRef="1 Par." loc="19,15">1 Par. 19,15</ref>; <foreign xml:lang="gez">ተሰብሩ፡ ሕዝብ፡ በቅድመ፡ ፀሮሙ፡</foreign> <ref cRef="2 Par." loc="6,24">2 Par. 6,24</ref>; <ref cRef="2 Par. " loc="14,12">2 Par.  14,12</ref>; <ref cRef="3 Reg." loc="22,35">3 Reg. 22,35</ref>.</sense> </sense>
               
