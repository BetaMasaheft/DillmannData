               <sense xml:lang="la" source="#dillmann" xml:id="Dmain"> <subc>III,2</subc>, rarius <subc>III,1</subc> <sense n="1" xml:id="D1"> <cit type="translation" xml:lang="la">
                                <quote> se munire </quote>
                            </cit> <sense n="a" xml:id="D1a"> <cit type="translation" xml:lang="la">
                                    <quote> sibi praemunire </quote>
                                </cit>, <cit type="translation" xml:lang="la">
                                    <quote> munitionibus defendere </quote>
                                </cit>, <cit type="translation" xml:lang="la">
                                    <quote> praesidio firmare </quote>
                                </cit> locum: <foreign xml:lang="gez">ተጸወነ፡ ህየ፡</foreign> <ref cRef="Jsp." loc="298">Jsp. p. 298</ref>; <ref cRef="Jsp." loc="308">Jsp. p. 308</ref>; <ref cRef="Jes." loc="37,26">Jes. 37,26</ref>; <lbl expand="cum">c.</lbl> <gramGrp> <case value="accusativus">Acc.</case> </gramGrp> urbis: <foreign xml:lang="gez">ወቦኡ፡ ወተጸወኑ፡ ሀገረ፡</foreign> 1 <ref cRef="Reg." loc="19,15">Reg. 19,15</ref>.</sense> <sense n="b" xml:id="D1b"> <cit type="translation" xml:lang="la">
                                    <quote> se recipere </quote>
                                </cit> vel <cit type="translation" xml:lang="la">
                                    <quote> refugere in locum munitum </quote>
                                </cit> vel <cit type="translation" xml:lang="la">
                                    <quote> tutum </quote>
                                </cit>, <ref cRef="Jud. " loc="9,51">Jud.  9,51</ref>; <foreign xml:lang="gez">ተጸወኑ፡ ውስተ፡ በአታት፡</foreign> <ref cRef="Jer." loc="4,29">Jer. 4,29</ref>; <foreign xml:lang="gez">ኀበ፡ ትጸወን፡</foreign> (v. <foreign xml:lang="gez">ይጼወን፡</foreign>) <foreign xml:lang="gez">ኤፍሬም፡</foreign> <ref cRef="Jes." loc="17,3">Jes. 17,3</ref>.</sense> <sense n="c" xml:id="D1c"> <cit type="translation" xml:lang="la">
                                    <quote> confugere in asylum </quote>
                                </cit>: Josua <foreign xml:lang="gez">ፈለጠ፡ አህጉረ፡ ፮ ከመ፡ ይጸወኑ፡ ውስቴቶሙ፡ ኵሉ፡ ዘቀተለ፡ ነፍሰ፡</foreign> <ref cRef="Sx. Sen." loc="26.">Sx. Sen. 26.</ref> </sense> <sense n="d" xml:id="D1d"> <cit type="translation" xml:lang="la">
                                    <quote> se recipere in tutelam </quote>
                                </cit> vel <cit type="translation" xml:lang="la">
                                    <quote> praesidium </quote>
                                </cit> alicujus, <cit type="translation" xml:lang="la">
                                    <quote> confugere ad patronum </quote>
                                </cit>, <lbl expand="construitur cum">c.c.</lbl> <foreign xml:lang="gez">በ፡</foreign>, ut: <foreign xml:lang="gez">ይጼወኑ፡ በነገሥት፡ ወመኳንንት፡</foreign> <ref cRef="Clem." loc="154">Clem. f. 154</ref>; <foreign xml:lang="gez">ንዌጥን፡ ተጸዊነነ፡ በኀይሉ፡ በጽሒፈ፡ ዜና፡ አይሁድ፡</foreign> <ref cRef="Jsp." loc="271">Jsp. p. 271</ref>; <lbl expand="cum">c.</lbl> <gramGrp> <case value="accusativus">Acc.</case> </gramGrp> <lbl expand="persona">pers.</lbl>: <foreign xml:lang="gez">እለ፡ ተወከሉኒ፡ ኣድኅን፡ ወለእለ፡ ተጸወኑኒ፡ እትመሐፀን፡</foreign> <ref cRef="Clem." loc="274.">Clem. f. 274.</ref> </sense> </sense> <sense n="2" xml:id="D2"> <gramGrp> <pos expand="passivum">pass.</pos> </gramGrp> <sense n="a" xml:id="D2a"> <cit type="translation" xml:lang="la">
                                    <quote> muniri </quote>
                                </cit>, <cit type="translation" xml:lang="la">
                                    <quote> munitionibus defendi </quote>
                                </cit>: <foreign xml:lang="gez">ተጸዊኖን፡</foreign> <foreign xml:lang="grc">ὀχυραί</foreign> <ref cRef="Hez." loc="36,35">Hez. 36,35</ref>.</sense> <sense n="b" xml:id="D2b"> <cit type="translation" xml:lang="la">
                                    <quote> munitionibus circumdari </quote>
                                </cit> vel <cit type="translation" xml:lang="la">
                                    <quote> claudi </quote>
                                </cit>, <cit type="translation" xml:lang="la">
                                    <quote> in obsidione teneri </quote>
                                </cit>: <foreign xml:lang="gez">ይጼወኑ፡ ወይትአሰሩ፡</foreign> <ref cRef="Jes." loc="24,22">Jes. 24,22</ref>; <foreign xml:lang="gez">ተጸወነት፡ ሀገር፡</foreign> <foreign xml:lang="grc">ἦλθεν εἰς συνοχήν</foreign> <ref cRef="Jer." loc="52,5">Jer. 52,5</ref> <lbl expand="varia lectio">var.</lbl>; <ref cRef="4 Reg." loc="24,10">4 Reg. 24,10</ref>; <ref cRef="4 Reg." loc="25,2">4 Reg. 25,2</ref>.</sense> </sense> </sense>
               
