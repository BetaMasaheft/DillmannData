               <sense xml:lang="la" source="#dillmann" xml:id="Dmain"> et <foreign xml:lang="gez">ሰክበ፡</foreign> (<lbl expand="videas">vid.</lbl> <ref cRef="Gen." loc="34,2">Gen. 34,2</ref>; <ref cRef="Gen." loc="35,22">Gen. 35,22</ref>; <ref cRef="Num." loc="5,13">Num. 5,13</ref> <lbl expand="annotatio">ann.</lbl>) <subc>I,1</subc> [<foreign xml:lang="he">שָׁכַב</foreign> <foreign xml:lang="syr">ܫܟܶܒ</foreign>] <gramGrp> <pos expand="Subjunctivus">Subj.</pos> </gramGrp> <foreign xml:lang="gez">ይስከብ፡</foreign> et <foreign xml:lang="gez">ይስክብ፡</foreign> <sense n="1" xml:id="D1"> <cit type="translation" xml:lang="la">
                                <quote> cubare </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> recumbere </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> procumbere </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> discumbere </quote>
                            </cit>: <foreign xml:lang="gez">ትሰክብ፡ በገቦከ፡ ዘፀጋም፡</foreign> <ref cRef="Hez." loc="4,4">Hez. 4,4</ref>; <foreign xml:lang="gez">ሶበ፡ ይሰክብ፡ ዲበ፡ ምስካቡ፡</foreign> <ref cRef="Job" loc="33,15">Job 33,15</ref>; <foreign xml:lang="gez">ጉዜ፡ ይሰክብ፡ ዲበ፡ ምስካቢሁ፡</foreign> <ref cRef="Sir." loc="40,5">Sir. 40,5</ref>; <foreign xml:lang="gez">ሰከበ፡ ውስተ፡ ሠቅ፡ ወሐመድ፡</foreign> <ref cRef="Jes." loc="58,5">Jes. 58,5</ref>; <foreign xml:lang="grc">ἀνέπεσεν</foreign> <ref cRef="Judith" loc="12,16">Judith 12,16</ref>; <foreign xml:lang="gez">ሰከበ፡ ሰኪሮ፡</foreign> <ref cRef="Kuf." loc="7">Kuf. 7</ref>; <ref cRef="Num." loc="24,9">Num. 24,9</ref>; <ref cRef="2 Reg." loc="12,16">2 Reg. 12,16</ref>; 2 <ref cRef="Reg." loc="13,31">Reg. 13,31</ref>; <foreign xml:lang="gez">ሰከበት፡ ላዕሌሁ፡</foreign> <ref cRef="3 Reg." loc="3,19">3 Reg. 3,19</ref>; <ref cRef="4 Reg." loc="4,34">4 Reg. 4,34</ref>; <ref cRef="Luc." loc="2,16">Luc. 2,16</ref>; <ref cRef="Luc." loc="17,34">Luc. 17,34</ref>; etiam de rebus inanimatis, ut: vitis <foreign xml:lang="gez">ውስተ፡ ምድር፡ ሰከበ፡</foreign> <ref cRef="Herm." loc="50.">Herm. p. 50.</ref> <sense n="a" xml:id="D1a"> de dormituris et dormientibus: <cit type="translation" xml:lang="la">
                                    <quote> cubitum ire </quote>
                                </cit>, <cit type="translation" xml:lang="la">
                                    <quote> dormire </quote>
                                </cit>: (<foreign xml:lang="grc">κοιμᾶσθαι</foreign>, <foreign xml:lang="grc">καθεύδειν</foreign>) <ref cRef="Ruth" loc="3,4">Ruth 3,4</ref>; <ref cRef="Gen." loc="19,4">Gen. 19,4</ref>; <ref cRef="Gen." loc="28,13">Gen. 28,13</ref>; <foreign xml:lang="gez">እለ፡ ይሰክቡ፡ ውስተ፡ ዐራታት፡</foreign> <ref cRef="Am." loc="6,4">Am. 6,4</ref>; <ref cRef="1 Reg." loc="3,5">1 Reg. 3,5</ref>; <ref cRef="2 Reg." loc="11,13">2 Reg. 11,13</ref>; <ref cRef="1 Esr." loc="3,6">1 Esr. 3,6</ref>; <ref cRef="Ps." loc="3,5">Ps. 3,5</ref>; <ref cRef="Prov." loc="3,24">Prov. 3,24</ref>; <ref cRef="Matth." loc="8,24">Matth. 8,24</ref> <lbl expand="romanae editionis">rom.</lbl> </sense> <sense n="b" xml:id="D1b"> de aegrotantibus <ref cRef="2 Reg." loc="13,6">2 Reg. 13,6</ref>; <ref cRef="4 Esr." loc="6,80">4 Esr. 6,80</ref> (var.).</sense> <sense n="c" xml:id="D1c"> de morientibus et mortuis: <cit type="translation" xml:lang="la">
                                    <quote> occumbere </quote>
                                </cit>, <cit type="translation" xml:lang="la">
                                    <quote> obdormiscere </quote>
                                </cit>, <cit type="translation" xml:lang="la">
                                    <quote> dormire </quote>
                                </cit> (<foreign xml:lang="grc">κοιμᾶσθαι</foreign>): <ref cRef="2 Reg." loc="7,12">2 Reg. 7,12</ref>; 3 <ref cRef="Reg." loc="1,21">Reg. 1,21</ref>; <ref cRef="4 Reg." loc="10,35">4 Reg. 10,35</ref>; <ref cRef="4 Reg." loc="13,9">4 Reg. 13,9</ref>; <ref cRef="4 Reg." loc="14,16">4 Reg. 14,16</ref>; <ref cRef="Job" loc="3,13">Job 3,13</ref>; <ref cRef="Hez." loc="32,21">Hez. 32,21</ref>; <ref cRef="Jes. " loc="14,8">Jes.  14,8</ref> <lbl expand="varia lectio">var.</lbl>; <ref cRef="1 Cor." loc="11,30">1 Cor. 11,30</ref>.</sense> </sense> <sense n="2" xml:id="D2"> <cit type="translation" xml:lang="la">
                                <quote> concumbere </quote>
                            </cit>: conjuges <foreign xml:lang="gez">እለ፡ ሰከቡ፡ በእንተ፡ ውሉድ፡</foreign> <ref cRef="Lit." loc="174,3">Lit. 174,3</ref>; <lbl expand="construitur cum">c.c.</lbl> <foreign xml:lang="gez">ምስለ፡</foreign> <ref cRef="Gen." loc="19,33">Gen. 19,33</ref>-35; <ref cRef="Gen." loc="26,10">Gen. 26,10</ref>; <ref cRef="Gen." loc="34,2">Gen. 34,2</ref>; <ref cRef="Gen." loc="39,7">Gen. 39,7</ref>; <ref cRef="Gen." loc="39,14">Gen. 39,14</ref>; <ref cRef="Gen." loc="39,17">Gen. 39,17</ref>; <ref cRef="Ex." loc="22,15">Ex. 22,15</ref>; <ref cRef="Lev." loc="15,16">Lev. 15,16</ref>; <ref cRef="Lev." loc="18,20">Lev. 18,20</ref>; <ref cRef="Lev." loc="18,22">Lev. 18,22</ref>; <ref cRef="Lev." loc="19,20">Lev. 19,20</ref>; <ref cRef="Lev." loc="20,11">Lev. 20,11</ref>; <ref cRef="2 Reg." loc="11,11">2 Reg. 11,11</ref>; <ref cRef="2 Reg." loc="13,11">2 Reg. 13,11</ref>; <lbl expand="cum">c.</lbl> <foreign xml:lang="gez">ኀበ፡</foreign> <ref cRef="Gen." loc="19,32">Gen. 19,32</ref>; <lbl expand="cum">c.</lbl> <gramGrp> <case value="accusativus">Acc.</case> </gramGrp> (potissimum de concubitu maris cum mare et hominis cum bestia): <ref cRef="Lev." loc="18,23">Lev. 18,23</ref>; <ref cRef="Lev." loc="20,16">Lev. 20,16</ref> (quamquam etiam <lbl expand="cum">c.</lbl> <foreign xml:lang="gez">ምስለ፡</foreign> <ref cRef="Gen." loc="18,22">Gen. 18,22</ref>; <ref cRef="Gen. " loc="20,13">Gen.  20,13</ref>; <ref cRef="Deut." loc="27,21">Deut. 27,21</ref>); <foreign xml:lang="gez">እለ፡ ይሰክቡ፡ ተባዕተ፡</foreign> <foreign xml:lang="grc">ἀρσενοκοῖται</foreign> <ref cRef="Chrys. ho." loc="30">Chrys. ho. 30</ref>; F.M. 34,2; (sed etiam de concubitu maris cum muliere) <foreign xml:lang="gez">ዘይሰክብ፡ ብእሲተ፡</foreign> F.N. 5. – <lbl expand="Vocabularium Aethiopicum, -a -a">Voc. Ae.</lbl>: <foreign xml:lang="gez">ሰከበ፡ ደቀሰ፡ ዘ፡ ተኘ።</foreign> </sense> </sense>
               
