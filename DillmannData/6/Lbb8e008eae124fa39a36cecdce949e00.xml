               <form>
                  <foreign xml:lang="gez">አስተፍሥሐ</foreign>
               </form>
               <sense xml:lang="la" source="#dillmann" xml:id="Dmain"> <subc>IV,1</subc> et <foreign xml:lang="gez">አስተፈሥሐ፡</foreign> <subc>IV,1</subc> et <subc>IV,2</subc> <sense n="1" xml:id="D1"> <cit type="translation" xml:lang="la">
                                <quote> exhilarare </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> laetitia </quote>
                            </cit> vel <cit type="translation" xml:lang="la">
                                <quote> gaudio afficere </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> jucundare </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> laetificare </quote>
                            </cit>, <foreign xml:lang="grc">εὐφραίνειν</foreign> <ref cRef="Prov." loc="10,1">Prov. 10,1</ref>; <ref cRef="Ps." loc="18,9">Ps. 18,9</ref>; <ref cRef="Ps." loc="103,16">Ps. 103,16</ref>; <foreign xml:lang="gez">አስተፍሥሐ፡ ጸላዕትኪ፡</foreign> <ref cRef="Thren." loc="2,17">Thren. 2,17</ref>; <ref cRef="Koh." loc="10,19">Koh. 10,19</ref>; <ref cRef="Sir." loc="1,23">Sir. 1,23</ref>; <ref cRef="Sir." loc="4,18">Sir. 4,18</ref>; <foreign xml:lang="gez">ታስተፈሥሖ፡</foreign> (v. <foreign xml:lang="gez">ታስተፌሥሖ፡</foreign>) <foreign xml:lang="gez">ለምታ፡</foreign> <ref cRef="Sir." loc="26,2">Sir. 26,2</ref>; <ref cRef="Sir. " loc="32,25">Sir.  32,25</ref>; <foreign xml:lang="gez">ወይን፡ ወማሕሌት፡ ያስተፌሥሕ፡ ልበ፡</foreign> <ref cRef="Sir." loc="40,20">Sir. 40,20</ref>; <ref cRef="Jes. " loc="56,7">Jes.  56,7</ref>; <ref cRef="Jer." loc="38,13">Jer. 38,13</ref>; <foreign xml:lang="gez">አስተፍሣሕከኒ፡</foreign> <ref cRef="Tob." loc="8,16">Tob. 8,16</ref>; <foreign xml:lang="gez">አስተፍሥሖሙ፡</foreign> <ref cRef="2 Esr." loc="6,22">2 Esr. 6,22</ref>; <ref cRef="2 Esr." loc="22,43">2 Esr. 22,43</ref>; <ref cRef="4 Esr." loc="5,29">4 Esr. 5,29</ref>; <ref cRef="4 Esr. " loc="12,41">4 Esr.  12,41</ref>; <ref cRef="Act." loc="14,17">Act. 14,17</ref>; <ref cRef="1 Cor." loc="13,6">1 Cor. 13,6</ref>. <sense n="a" xml:id="D1a"> <cit type="translation" xml:lang="la">
                                    <quote> solatio </quote>
                                </cit> <cit type="translation" xml:lang="la">
                                    <quote> afficere </quote>
                                </cit>, <cit type="translation" xml:lang="la">
                                    <quote> consolari </quote>
                                </cit> tristem, afflictum, <foreign xml:lang="grc">παρακαλεῖν</foreign> <ref cRef="Ex." loc="15,13">Ex. 15,13</ref>; <ref cRef="Jes." loc="40,11">Jes. 40,11</ref>; <ref cRef="Jes." loc="49,13">Jes. 49,13</ref>; <ref cRef="Jes." loc="51,3">Jes. 51,3</ref>; <ref cRef="Jes. " loc="51,12">Jes.  51,12</ref>; <ref cRef="Jes." loc="57,18">Jes. 57,18</ref>; <ref cRef="Thren." loc="2,13">Thren. 2,13</ref>; <ref cRef="Ruth" loc="2,13">Ruth 2,13</ref>; <ref cRef="Judith" loc="6,20">Judith 6,20</ref>; <ref cRef="Job" loc="42,11">Job 42,11</ref>; <ref cRef="Sir." loc="17,24">Sir. 17,24</ref>; <foreign xml:lang="gez">አስተፍሥሖሙ፡ ለእለ፡ ይላሕዉ፡</foreign> <ref cRef="Sir." loc="48,24">Sir. 48,24</ref>; <ref cRef="Ps." loc="85,16">Ps. 85,16</ref>; <ref cRef="Ps." loc="118,50">Ps. 118,50</ref>; <ref cRef="4 Esr." loc="14,10">4 Esr. 14,10</ref>; <ref cRef="1 Thess." loc="5,11">1 Thess. 5,11</ref>; <ref cRef="2 Thess." loc="2,17">2 Thess. 2,17</ref>, al.; <lbl expand="sequens">seq.</lbl> <foreign xml:lang="gez">እምነ፡</foreign> rei <ref cRef="2 Cor." loc="1,4">2 Cor. 1,4</ref>; <ref cRef="Hen." loc="107,3">Hen. 107,3</ref>.</sense> <sense n="b" xml:id="D1b"> <cit type="translation" xml:lang="la">
                                    <quote> oblectare </quote>
                                </cit>: <foreign xml:lang="gez">አስተፍሥሓ፡</foreign> (v. <foreign xml:lang="gez">አስተፈሥሓ፡</foreign>) <foreign xml:lang="gez">ለነፍስከ፡</foreign> <ref cRef="Sir." loc="14,16">Sir. 14,16</ref>; <ref cRef="Sir." loc="10,28">Sir. 10,28</ref>.</sense> <sense n="c" xml:id="D1c"> <cit type="translation" xml:lang="la">
                                    <quote> refocillare </quote>
                                </cit>: <foreign xml:lang="gez">ያስተፈሥሖሙ፡</foreign> (v. <foreign xml:lang="gez">ያስተፌሖሙ፡</foreign>) <foreign xml:lang="grc">διδοὺς ζωὴν τοῖς</foreign> … <ref cRef="Jes." loc="57,15">Jes. 57,15</ref>.</sense> <sense n="d" xml:id="D1d"> <cit type="translation" xml:lang="la">
                                    <quote> delectare </quote>
                                </cit>: <foreign xml:lang="gez">ኢያስተፍሥሕከ፡ ብዝኆሙ፡</foreign> <foreign xml:lang="grc">μὴ ἔπεχε ἐπὶ</foreign> … <ref cRef="Sir." loc="16,3">Sir. 16,3</ref>.</sense> <sense n="e" xml:id="D1e"> <cit type="translation" xml:lang="la">
                                    <quote> laeto nuntio exhilarare </quote>
                                </cit>: <foreign xml:lang="gez">ኣስተፈሥሖሙ፡</foreign> (<gramGrp> <pos expand="Subjunctivus">Subj.</pos> </gramGrp>) <foreign xml:lang="gez">ለነዳያን፡</foreign> <foreign xml:lang="grc">εὐαγγελίσασθαι</foreign> <ref cRef="Jes." loc="61,1">Jes. 61,1</ref>.</sense> <sense n="f" xml:id="D1f"> <cit type="translation" xml:lang="la">
                                    <quote> laetis donis ornare </quote>
                                </cit>, <cit type="translation" xml:lang="la">
                                    <quote> beare </quote>
                                </cit>: <foreign xml:lang="gez">አስተፍሥሐ፡ ኵሎ፡ ዓመተ፡</foreign> <foreign xml:lang="grc">ἐκόσμησε</foreign> <ref cRef="Sir." loc="47,10">Sir. 47,10</ref>; <ref cRef="Jsp." loc="329 ">Jsp. p. 329 </ref> (<lbl expand="videas">vid. sub</lbl> <foreign xml:lang="gez">አስተዋደደ፡</foreign>).</sense> <sense n="g" xml:id="D1g"> <cit type="translation" xml:lang="la">
                                    <quote> salutare </quote>
                                </cit>: <foreign xml:lang="gez">አስተፍሥሐቶሙ፡</foreign> <foreign xml:lang="grc">ἐχαιρέτισεν αὐτούς</foreign> <cb n="1350" xml:id="c1350"/> <ref cRef="Tob." loc="7,1">Tob. 7,1</ref>.</sense> <sense n="h" xml:id="D1h"> <cit type="translation" xml:lang="la">
                                    <quote> conciliare sibi </quote>
                                </cit>, <cit type="translation" xml:lang="la">
                                    <quote> affabilitate allicere </quote>
                                </cit> aliquem, <ref cRef="2 Reg." loc="15,6">2 Reg. 15,6</ref>.</sense> <sense n="i" xml:id="D1i"> <cit type="translation" xml:lang="la">
                                    <quote> laetificare </quote>
                                </cit>, de flumine quod <foreign xml:lang="gez">ያስተፌሥሕ፡ አህጉራተ፡</foreign> Lud. ex <ref cRef="Sx. Jac." loc="9 Enc.">Sx. Jac. 9 Enc.</ref>
                            </sense> </sense> <sense n="2" xml:id="D2"> <cit type="translation" xml:lang="la">
                                <quote> hilarem </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> laetum esse </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> gaudere </quote>
                            </cit>, <ref cRef="Clem." loc="174 ">Clem. f. 174 </ref>(<lbl expand="videas">vid. sub</lbl> <foreign xml:lang="gez">በርሀ፡</foreign>); <foreign xml:lang="gez">ዘአመ፡ ይጼነስ፡ ኢየሐዝን፡ አመ፡ ይዴለው፡ እፎ፡ ያስተፌሥሕ፡</foreign> <ref cRef="Sir." loc="10,31">Sir. 10,31</ref>.</sense> </sense>
               
