         <div>
            <entry xml:id="Lb213c4538b5a4734a7d0cbe4a0d4b644" n="8423">
               <form>
                  <foreign xml:lang="gez">ተገብረ</foreign>
               </form>
               <sense xml:lang="la" source="#dillmann" xml:id="Dmain"> <subc>III,1</subc> <sense n="1" xml:id="D1">
                     <cit type="translation" xml:lang="la">
                        <quote>fieri</quote>
                     </cit>
                     <sense n="a" xml:id="D1a">
                        <cit type="translation" xml:lang="la">
                           <quote>creari</quote>
                        </cit>
                        <ref cRef="Kuf." loc="2">Kuf. 2</ref>; <foreign xml:lang="gez">ተገቢሮ፡ ሠሐቅዎ፡
                           መላእክት፡</foreign>
                        <ref cRef="Job " loc="40,14">Job 40,14</ref>; <ref cRef="Hen." loc="72,1">Hen. 72,1</ref>; <foreign xml:lang="gez">ቃል፡</foreign> (<foreign xml:lang="grc">ὁ Λόγος</foreign>) <foreign xml:lang="gez">ዘበአማን፡ ተወልደ፡
                           ወኢተገብረ፡ </foreign>
                        <ref cRef="Cyr." loc="119">Cyr. f. 119</ref>; <ref cRef="Exp. Fid. Nic.">Exp. Fid. Nic.</ref> </sense>
                     <sense n="b" xml:id="D1b">
                        <cit type="translation" xml:lang="la">
                           <quote>gigni</quote>
                        </cit>: <foreign xml:lang="gez">ኀጺን፡ እምውስተ፡ ምድር፡ ይትገበር፡</foreign>
                        <foreign xml:lang="grc">γίνεται</foreign> (v. <foreign xml:lang="gez">ይትጌበር፡</foreign>) <ref cRef="Job" loc="28,2">Job 28,2</ref>; <ref cRef="Jac." loc="4,1">Jac. 4,1</ref>.</sense>
                     <sense n="c" xml:id="D1c">
                        <cit type="translation" xml:lang="la">
                           <quote>confici</quote>
                        </cit>: <foreign xml:lang="gez">ዘይትገበር፡ እምአስካል፡</foreign>
                        <ref cRef="Num." loc="6,3">Num. 6,3</ref>; <foreign xml:lang="gez">ኵሉ፡
                           ዘይትገበር፡ በእሳት፡ ንዋይ፡</foreign>
                        <ref cRef="Num." loc="31,23">Num. 31,23</ref>; <ref cRef="Ex. " loc="25,31">Ex. 25,31</ref>
                        <lbl expand="sequens">seq.</lbl>; <ref cRef="Ex." loc="26,10">Ex.
                           26,10</ref>; <ref cRef="Ex." loc="30,2">Ex. 30,2</ref>; <ref cRef="Ex." loc="30,3">Ex. 30,3</ref>; <ref cRef="Ex." loc="39,23">Ex. 39,23</ref>;
                           <foreign xml:lang="gez">ኵሉ፡ ወርቅ፡ ዘተገብረ፡ ለግብረ፡ ቅዱሳን፡</foreign>
                        <ref cRef="Ex." loc="39,1">Ex. 39,1</ref>; <ref cRef="Ex." loc="39,4">Ex.
                           39,4</ref>.</sense>
                     <sense n="d" xml:id="D1d">
                        <cit type="translation" xml:lang="la">
                           <quote>parari</quote>
                        </cit>, <cit type="translation" xml:lang="la">
                           <quote>apparari</quote>
                        </cit>: <foreign xml:lang="gez">ወዘይትገበር፡ ሲሳይነ፡ ዘለለዕለትነ፡ </foreign>
                        <ref cRef="2 Esr." loc="15,18">2 Esr. 15,18</ref>; <ref cRef="1 Reg." loc="21,6">1 Reg. 21,6</ref>; <foreign xml:lang="gez">በጼው፡ ይትገበር፡ </foreign>
                        <ref cRef="Lev." loc="2,13">Lev. 2,13</ref>; <cit type="translation" xml:lang="la">
                           <quote>ornari</quote>
                        </cit>: <foreign xml:lang="gez">እንተ፡ ተገብረት፡ በወርቅ፡ ወበዕንቍ፡</foreign>
                        <ref cRef="Apoc." loc="18,16">Apoc. 18,16</ref>;</sense>
                     <sense n="e" xml:id="D1e">
                        <cit type="translation" xml:lang="la">
                           <quote>perfici</quote>
                        </cit>, <cit type="translation" xml:lang="la">
                           <quote>perpetrari</quote>
                        </cit>, <cit type="translation" xml:lang="la">
                           <quote>agi</quote>
                        </cit>: <foreign xml:lang="gez">ተገብረ፡ ዝንቱ፡ ኵሉ፡ በእዴኪ፡</foreign>
                        <ref cRef="Judith" loc="15,10">Judith 15,10</ref>; <foreign xml:lang="gez">ትዕግልት፡ ዘይትገበር፡</foreign>
                        <ref cRef="Koh." loc="4,1">Koh. 4,1</ref>; <ref cRef="Mal." loc="1,9">Mal.
                           1,9</ref>; <foreign xml:lang="gez">ይትገበር፡ ግብረ፡ </foreign>
                        <ref cRef="4 Esr." loc="4,47">4 Esr. 4,47</ref>; <ref cRef="Hen." loc="2,2">Hen. 2,2</ref>; <ref cRef="Hen." loc="5,2">Hen. 5,2</ref>; <ref cRef="Hen." loc="6,3">Hen. 6,3</ref>; <ref cRef="Hen." loc="15,5">Hen.
                           15,5</ref>; <ref cRef="Hen." loc="91,15">Hen. 91,15</ref>; <ref cRef="Hen. " loc="106,6">Hen. 106,6</ref>; <ref cRef="Matth." loc="18,19">Matth. 18,19</ref>.</sense>
                     <sense n="f" xml:id="D1f">
                        <cit type="translation" xml:lang="la">
                           <quote>agi</quote>
                        </cit>, <cit type="translation" xml:lang="la">
                           <quote>peragi</quote>
                        </cit>, <cit type="translation" xml:lang="la">
                           <quote>haberi</quote>
                        </cit>, de festis: <foreign xml:lang="gez">ተገብረ፡ ፋሲካ፡</foreign>
                        <ref cRef="4 Reg." loc="23,22">4 Reg. 23,22</ref>; <ref cRef="Kuf." loc="26">Kuf. p. 26</ref>; <foreign xml:lang="gez">ይትገበር፡ ሎቱ፡ በዓል፡ ዐቢይ፡</foreign>
                        <ref cRef="Sx. Mij." loc="24">Sx. Mij. 24</ref>; <foreign xml:lang="gez">ሲኖዶስ፡ ይትገበር፡</foreign>
                        <ref cRef="II Can. Ap." loc="35.">II Can. Ap. 35.</ref>
                     </sense>
                     <sense n="2" xml:id="D12">
                        <cit type="translation" xml:lang="la">
                           <quote>coli</quote>
                        </cit>, de agris <ref cRef="Hen." loc="10,18">Hen. 10,18</ref>.</sense>
                     <sense n="3" xml:id="D13">
                        <cit type="translation" xml:lang="la">
                           <quote>efficacem fieri</quote>
                        </cit> vel <cit type="translation" xml:lang="la">
                           <quote>esse</quote>
                        </cit>: <foreign xml:lang="gez">ተገብረ፡ ወተሰልጠ፡ እሳት፡ ውስቴቱ፡</foreign> (<foreign xml:lang="gez">ለማይ፡</foreign>) <ref cRef="Sap." loc="16,17">Sap.
                           16,17</ref>; <ref cRef="Matth." loc="14,2">Matth. 14,2</ref> (; <ref cRef="Jac." loc="4,1">Jac. 4,1</ref>)</sense>
                  </sense>
               </sense>
               
            </entry>
         </div>
