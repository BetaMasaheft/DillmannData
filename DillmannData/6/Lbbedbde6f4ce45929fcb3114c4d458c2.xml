         <div>
            <entry xml:id="Lbbedbde6f4ce45929fcb3114c4d458c2" n="4203">
               <form>
                  <foreign xml:lang="gez">አብጽዐ</foreign>
               </form>
               <sense xml:lang="la" source="#dillmann" xml:id="Dmain"> <subc>II,1</subc> <sense n="1" xml:id="D1"> voti compotem facere
                     aliquem i.e. <cit type="translation" xml:lang="la">
                        <quote>beatum facere</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>felicem reddere</quote>
                     </cit>, <ref cRef="Ps." loc="40,2">Ps. 40,2</ref>; <foreign xml:lang="gez">አብጽዖ፡ በሠናይ፡ ሰርጕ፡</foreign>
                     <ref cRef="Sir." loc="45,7">Sir. 45,7</ref>;</sense>
                  <sense n="2" xml:id="D2">
                     <cit type="translation" xml:lang="la">
                        <quote>beatum praedicare</quote>
                     </cit>: <foreign xml:lang="gez">አብጽዖትክሙ፡</foreign>
                     <foreign xml:lang="grc">μακαρισμὸς ὑμῶν</foreign>
                     <ref cRef="Gal." loc="4,15">Gal. 4,15</ref>.</sense>
                  <sense n="3" xml:id="D3"> voto potiri i.e. <cit type="translation" xml:lang="la">
                        <quote>beatum</quote>
                     </cit> vel <cit type="translation" xml:lang="la">
                        <quote>felicem fieri</quote>
                     </cit> vel <cit type="translation" xml:lang="la">
                        <quote>esse</quote>
                     </cit>: <foreign xml:lang="gez">ለብእሲት፡ ኄርት፡ ያበጽዕ፡ ምታ፡</foreign>
                     <ref cRef="Sir." loc="26,1">Sir. 26,1</ref>; <foreign xml:lang="gez">አብጻዕኩ፡
                        አነኒ፡</foreign>
                     <ref cRef="Gen." loc="30,11">Gen. 30,11</ref>; <ref cRef="Herm." loc="1">Herm.
                        p. 1</ref>; <ref cRef="4 Esr." loc="10,71">4 Esr. 10,71</ref>; <foreign xml:lang="gez">እመ፡ ብዙኀ፡ አብጻዕኩ፡ በኀቤከ፡</foreign>
                     <ref cRef="4 Esr." loc="12,12">4 Esr. 12,12</ref>; <foreign xml:lang="gez">አብጽዖ፡</foreign>
                     <ref cRef="Tit." loc="2,3">Tit. 2,3</ref>; <ref cRef="Rom." loc="4,6">Rom.
                        4,6</ref>; <ref cRef="Rom." loc="4,7">Rom. 4,7</ref>; <ref cRef="Rom." loc="4,8">Rom. 4,8</ref>; <ref cRef="Rom. " loc="4,9">Rom.
                     4,9</ref>.</sense>
               </sense>
               
