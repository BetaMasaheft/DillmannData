                  <foreign xml:lang="gez">አብዝኀ</foreign>
               </form>
               <sense xml:lang="la" source="#dillmann" xml:id="Dmain"> <subc>II,1</subc> <cit type="translation" xml:lang="la">
                     <quote>multum facere</quote>
                  </cit>, <cit type="translation" xml:lang="la">
                     <quote>multiplicare</quote>
                  </cit>, <cit type="translation" xml:lang="la">
                     <quote>augere</quote>
                  </cit>, <ref cRef="Gen. " loc="3,16">Gen. 3,16</ref>; <ref cRef="Gen." loc="17,2">Gen. 17,2</ref>; <ref cRef="Gen." loc="26,22">Gen. 26,22</ref>; <ref cRef="Gen." loc="26,24">Gen. 26,24</ref>; <ref cRef="Gen." loc="34,12">Gen.
                     34,12</ref>; <ref cRef="Ex." loc="7,3">Ex. 7,3</ref>; <ref cRef="Deut." loc="1,10">Deut. 1,10</ref>; <ref cRef="Deut." loc="30,5">Deut. 30,5</ref>;
                     <ref cRef="Jud." loc="16,24">Jud. 16,24</ref>; <foreign xml:lang="gez">ኣበዝኆሙ፡
                     ወኢይውሕዱ፡</foreign>
                  <ref cRef="Jer." loc="37,19">Jer. 37,19</ref>; <ref cRef="Ps." loc="17,16">Ps.
                     17,16</ref>; <ref cRef="Ps. " loc="104,22">Ps. 104,22</ref>; <ref cRef="2 Esr." loc="19,23">2 Esr. 19,23</ref>; <ref cRef="Job" loc="1,10">Job 1,10</ref>; <ref cRef="Num." loc="26,54">Num. 26,54</ref>; <foreign xml:lang="gez">ከመ፡ ኆፃ፡ ኣብዝኅ፡
                     መዋዕለ፡ </foreign>
                  <ref cRef="Job" loc="29,18">Job 29,18</ref>; <foreign xml:lang="gez">አብዝኀ፡
                     ምሕረቶ፡</foreign>
                  <ref cRef="Sir." loc="18,12">Sir. 18,12</ref>; <ref cRef="4 Esr. " loc="7,40">4
                     Esr. 7,40</ref>; <foreign xml:lang="gez">አብዝኅዋ፡ለኀጢአት፡</foreign>
                  <ref cRef="Sir." loc="48,16">Sir. 48,16</ref>; <ref cRef="Rom." loc="5,20">Rom.
                     5,20</ref>; <ref cRef="Rom." loc="15,13">Rom. 15,13</ref>; <ref cRef="Ps." loc="70,23">Ps. 70,23</ref>; <ref cRef="Hen." loc="16,4">Hen. 16,4</ref>; <ref cRef="1 Thess." loc="3,12">1 Thess. 3,12</ref>; <foreign xml:lang="gez">አብዛኅኪ፡
                     ዝሙተኪ፡</foreign>
                  <ref cRef="Hez. " loc="16,25">Hez. 16,25</ref>; <foreign xml:lang="gez">አብዝኀ፡ ኀሳራ፡
                     ለወለተ፡ ይሁዳ፡ ኅስርት፡</foreign>
                  <ref cRef="Thren." loc="2,5">Thren. 2,5</ref>. <sense n="a" xml:id="Da"> passim: <cit type="translation" xml:lang="la">
                        <quote>multum comparare</quote>
                     </cit> vel <cit type="translation" xml:lang="la">
                        <quote>habere</quote>
                     </cit>: <ref cRef="Deut." loc="17,16">Deut. 17,16</ref>; Deut.17,17; <foreign xml:lang="gez">ያበዝኁ፡ አዋልደ፡ ወደቂቀ፡</foreign>
                     <ref cRef="1 Par." loc="8,40">1 Par. 8,40</ref>; <foreign xml:lang="gez">በአብዝኆ፡
                        ንዋይ፡</foreign>
                     <ref cRef="Luc." loc="12,15">Luc. 12,15</ref>; <foreign xml:lang="gez">አብዝኁ፡
                        ወርቀ፡</foreign>
                     <ref cRef="Judith" loc="5,9">Judith 5,9</ref>; <foreign xml:lang="gez">ዘአብዝኀ፡
                        ብዕለ፡</foreign>
                     <ref cRef="Koh. " loc="5,11">Koh. 5,11</ref>; <foreign xml:lang="gez">ዘአብዝኀ፡
                        ጥበበ፡ አብዝኀ፡ ሐዘነ፡</foreign>
                     <ref cRef="Koh." loc="1,18">Koh. 1,18</ref>.</sense>
                  <sense n="b" xml:id="Db">
                     <lbl expand="sequens">seq.</lbl> Infin. vel <gramGrp>
                        <pos expand="nomen">nom.</pos>
                     </gramGrp> abstracto in <gramGrp>
                        <case value="accusativus">Acc.</case>
                     </gramGrp> vocabulis <cit type="translation" xml:lang="la">
                        <quote>multum</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>plus</quote>
                     </cit>, exponere licet: <foreign xml:lang="gez">ኢታብዝኅ፡ ተናግሮ፡ ምስለ፡ አብድ፡ </foreign>
                     <cit type="translation" xml:lang="la">
                        <quote>ne multum colloquaris</quote>
                     </cit>
                     <ref cRef="Sir." loc="22,13">Sir. 22,13</ref>; <foreign xml:lang="gez">ነቢበ፡ </foreign>
                     <ref cRef="Job" loc="4,2">Job 4,2</ref>; <ref cRef="Job" loc="11,3">Job
                        11,3</ref>; <foreign xml:lang="gez">አብዝኆ፡ ነገር፡</foreign>
                     <ref cRef="Job" loc="4,2">Job 4,2</ref>; <foreign xml:lang="gez">ዘያበዝኅ፡ ሰትየ፡
                        ወይን፡</foreign> F.N. 6; <foreign xml:lang="gez">አብዝኀ፡ ሐይወ፡</foreign>
                     <cit type="translation" xml:lang="la">
                        <quote>diu vixit</quote>
                     </cit>
                     <ref cRef="Kuf." loc="23">Kuf. 23</ref>; <foreign xml:lang="gez">አብዛኅክሙ፡ ገቢረ፡
                        እኩይ፡ እምአበዊክሙ፡</foreign>
                     <ref cRef="Jer." loc="16,12">Jer. 16,12</ref>; <ref cRef="Marc." loc="12,43">Marc. 12,43</ref>; <foreign xml:lang="gez">አኮ፡ በአብዝኆ፡ አላ፡ በአውሕዶ፡</foreign>
                     <cit type="translation" xml:lang="la">
                        <quote>non multis sed paucis</quote>
                     </cit>, Lit. Cyr. <cb n="0533" xml:id="c0533"/>
                  </sense>
               </sense>
               
