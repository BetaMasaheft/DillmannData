            <entry xml:id="L4002a457b5f34b96affaa1b22401bad8" n="4696">
               <form>
                  <foreign xml:lang="gez">ኀጢአት</foreign>
               </form>
               <sense xml:lang="la" source="#dillmann" xml:id="Dmain"> <gramGrp> <pos expand="Substantivum">subst.</pos> </gramGrp> (<bibl>
                            <ptr target="bm:Dillmann1857Gramm"/>gramm.</bibl> § 127,1, a), <gramGrp> <gen value="not found">m.</gen> </gramGrp> et <gramGrp> <gen value="femininus">fem.</gen> </gramGrp>, <lbl expand="collatio, -is vel collectivum,&#xA;                            -e">coll.</lbl> et <lbl expand="">sing.</lbl>, <lbl expand="pluralis">Pl.</lbl> <foreign xml:lang="gez">ኀጣውእ፡</foreign> et <foreign xml:lang="gez">ኀጣይእ፡</foreign> (<lbl expand="videas">vid.</lbl> <lbl expand="annotatio">ann.</lbl> ad <ref cRef="Lev." loc="26,18">Lev. 26,18</ref> et <ref cRef="Num." loc="5,6">Num. 5,6</ref>; <bibl>
                            <ptr target="bm:Dillmann1857Gramm"/>gramm.</bibl> § 140,3,a <sense n="1" xml:id="D1"> rarissime <cit type="translation" xml:lang="la">
                                <quote> defectus </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> penuria </quote>
                            </cit>: <foreign xml:lang="gez">ኀጣይእከ፡</foreign> <foreign xml:lang="grc">ἡ πτωχεία σου</foreign> <ref cRef="Apoc." loc="2,9">Apoc. 2,9</ref>; <lbl expand="conferas">cfr.</lbl> <ref cRef="Phil." loc="4,11">Phil. 4,11</ref> <lbl expand="romanae editionis">rom.</lbl> </sense> <sense n="2" xml:id="D2"> plerumque: <cit type="translation" xml:lang="la">
                                <quote> peccatum </quote>
                            </cit> <sense n="a" xml:id="D2a"> in <lbl expand="">sing.</lbl> quatenus vel animi habitus vel summa peccatorum est, <cit type="translation" xml:lang="la">
                                    <quote> peccandi consuetudo </quote>
                                </cit>, <cit type="translation" xml:lang="la">
                                    <quote> impietas </quote>
                                </cit>, <cit type="translation" xml:lang="la">
                                    <quote> peccata </quote>
                                </cit> <ref cRef="Joh." loc="8,21">Joh. 8,21</ref>; <ref cRef="Joh. " loc="9,34">Joh.  9,34</ref>; <ref cRef="Rom." loc="5,12">Rom. 5,12</ref> <lbl expand="sequens">seq.</lbl>; <ref cRef="Rom." loc="6,10">Rom. 6,10</ref> <lbl expand="sequens">seq.</lbl>; <ref cRef="1 Cor." loc="15,17">1 Cor. 15,17</ref>; quare <lbl expand="">sing.</lbl> <foreign xml:lang="gez">ኀጢአት፡</foreign> crebro usurpator, ubi Graecus pluralem habet: <foreign xml:lang="grc">ἁμαρτίαι</foreign> <ref cRef="Jes." loc="5,18">Jes. 5,18</ref>; <ref cRef="Jes." loc="14,21">Jes. 14,21</ref>; <ref cRef="Jes. " loc="43,24">Jes.  43,24</ref>; <ref cRef="Jes." loc="58,1">Jes. 58,1</ref>; <ref cRef="Jer." loc="14,7">Jer. 14,7</ref>; <ref cRef="Dan." loc="9,20">Dan. 9,20</ref>; <ref cRef="Ps." loc="102,10">Ps. 102,10</ref>; <foreign xml:lang="grc">ἀνομίαι</foreign> <ref cRef="Jes." loc="6,7">Jes. 6,7</ref>; <foreign xml:lang="grc">ἀσέβειαι</foreign> <ref cRef="Thren." loc="1,5">Thren. 1,5</ref>; <foreign xml:lang="grc">παραπτώματα</foreign> <ref cRef="Rom." loc="4,25">Rom. 4,25</ref>; <foreign xml:lang="gez">ኀጢአትነ፡ ወጌጋይነ፡</foreign> <foreign xml:lang="grc">αἱ πλάναι καὶ αἱ ἀνομίαι</foreign> <ref cRef="Hez." loc="33,10">Hez. 33,10</ref>; <foreign xml:lang="gez">በእንተ፡ ኀጢአተ፡ ከናፍር፡</foreign> <foreign xml:lang="grc">διὰ φαυλισμὸν χειλέων</foreign> <ref cRef="Jes." loc="28,11">Jes. 28,11</ref>; <foreign xml:lang="grc">πονηρία</foreign> <ref cRef="Jes." loc="59,7">Jes. 59,7</ref>; <foreign xml:lang="grc">ἀσέβεια</foreign> <ref cRef="Prov." loc="1,19">Prov. 1,19</ref>; <ref cRef="Prov." loc="28,4">Prov. 28,4</ref>.</sense> <sense n="b" xml:id="D2b"> in <lbl expand="">sing.</lbl> et <lbl expand="pluralis">Pl.</lbl>: quatenus significat singulas peccandi actiones, <cit type="translation" xml:lang="la">
                                    <quote> peccatum </quote>
                                </cit> (<cit type="translation" xml:lang="la">
                                    <quote> delictum </quote>
                                </cit>, <cit type="translation" xml:lang="la">
                                    <quote> crimen </quote>
                                </cit>), <ref cRef="1 Joh." loc="3,4">1 Joh. 3,4</ref>; <ref cRef="1 Joh." loc="5,17">1 Joh. 5,17</ref>; <ref cRef="Jac." loc="1,15">Jac. 1,15</ref>; <ref cRef="Ps." loc="68,7">Ps. 68,7</ref>; <lbl expand="pluralis">Pl.</lbl> <ref cRef="Koh." loc="10,4">Koh. 10,4</ref>; <ref cRef="Job" loc="13,23">Job 13,23</ref>; <ref cRef="Job" loc="34,37">Job 34,37</ref>; <ref cRef="Sir." loc="3,27">Sir. 3,27</ref>; <ref cRef="Sir." loc="28,8">Sir. 28,8</ref>; <ref cRef="Judith" loc="7,28">Judith 7,28</ref>; <ref cRef="Ex." loc="34,7">Ex. 34,7</ref>; <ref cRef="Prov." loc="1,19">Prov. 1,19</ref>; <ref cRef="Jes." loc="43,25">Jes. 43,25</ref>; <ref cRef="Jes." loc="44,22">Jes. 44,22</ref>; <ref cRef="Jes." loc="53,4">Jes. 53,4</ref>; <ref cRef="Jes." loc="53,5">Jes. 53,5</ref>; <ref cRef="1 Petr." loc="2,24">1 Petr. 2,24</ref>; <ref cRef="1 Petr." loc="3,18">1 Petr. 3,18</ref>; <ref cRef="Judae" loc="18">Judae 18</ref>; <foreign xml:lang="gez">ክልኤቲ፡ ኀጣይእ፡</foreign> <ref cRef="Sir." loc="7,8">Sir. 7,8</ref>; <foreign xml:lang="gez">አብዕልተ፡ ኀጣውእ፡ ንኡሳት፡</foreign> <cit type="translation" xml:lang="la">
                                    <quote> possessores </quote>
                                </cit> (i.e. rei) <cit type="translation" xml:lang="la">
                                    <quote> peccatillorum </quote>
                                </cit>, <ref cRef="F.M." loc=" 1,5">F.M.  1,5</ref>; <foreign xml:lang="gez">ገብረ፡ ኀጢአተ፡</foreign> <ref cRef="Jes." loc="53,9">Jes. 53,9</ref>; <ref cRef="Sir." loc="15,20">Sir. 15,20</ref>; <ref cRef="Sir. " loc="19,4">Sir.  19,4</ref>; <ref cRef="Sir." loc="27,1">Sir. 27,1</ref> al.; <foreign xml:lang="gez">ኀጢአት፡ እንተ፡ አበሰ፡ ባቲ፡</foreign> <ref cRef="Lev. " loc="4,23">Lev.  4,23</ref>; <ref cRef="Lev." loc="5,5">Lev. 5,5</ref>; <foreign xml:lang="gez">ኀጢአት፡ እንተ፡ አበሰ፡</foreign> <ref cRef="Lev." loc="4,35">Lev. 4,35</ref>; <ref cRef="Lev." loc="5,6">Lev. 5,6</ref>; <foreign xml:lang="gez">ኀጢአተ፡ ይከውኖ፡</foreign> <ref cRef="Lev." loc="19,8">Lev. 19,8</ref>; <foreign xml:lang="gez">ይነሥእ፡ ኀጢአቶ፡</foreign> <ref cRef="Lev." loc="7,18">Lev. 7,18</ref>; <foreign xml:lang="gez">ይድኅን፡ እምኀጢአት፡</foreign> <foreign xml:lang="grc">ἀθῶος ἔσθαι</foreign> <ref cRef="Ex. " loc="21,28">Ex.  21,28</ref>.</sense> </sense> <sense n="3" xml:id="D3"> <cit type="translation" xml:lang="la">
                                <quote> sacrificium pro peccato </quote>
                            </cit> (vel <cit type="translation" xml:lang="la">
                                <quote> pro delicto </quote>
                            </cit>) <cit type="translation" xml:lang="la">
                                <quote> oblatum </quote>
                            </cit> interpretes verterunt (sc. <foreign xml:lang="gez">መሥዋዕት፡</foreign>) <foreign xml:lang="gez">ዘበእንተ፡ ኀጢአት፡</foreign> vel <foreign xml:lang="gez">ዘኀጢአት፡</foreign> (<foreign xml:lang="grc">ἁμαρτία</foreign>) <ref cRef="Lev." loc="4,8">Lev. 4,8</ref>; <ref cRef="Lev." loc="4,24">Lev. 4,24</ref>; <ref cRef="Lev." loc="5,7">Lev. 5,7</ref>; <ref cRef="Lev." loc="5,8">Lev. 5,8</ref>; <ref cRef="Lev." loc="14,19">Lev. 14,19</ref>; <ref cRef="Hez." loc="44,29">Hez. 44,29</ref>; <ref cRef="Hebr." loc="10,6">Hebr. 10,6</ref>; <ref cRef="Hebr." loc="10,18">Hebr. 10,18</ref>; (<foreign xml:lang="grc">πλημμέλεια</foreign>) <ref cRef="Sir." loc="7,31">Sir. 7,31</ref> (quamquam plurimis in locis <foreign xml:lang="grc">πλημμέλεια</foreign> <foreign xml:lang="gez">ንስሓ፡</foreign> versum et ab <foreign xml:lang="grc">ἁμαρτία</foreign> <foreign xml:lang="gez">ኀጢአት፡</foreign> distinctum est); <foreign xml:lang="grc">ἱλασμός</foreign> <ref cRef="Hez." loc="44,27">Hez. 44,27</ref>. Etiam <foreign xml:lang="gez">ኀጢአት፡</foreign> nudum (neque <foreign xml:lang="gez">ዘ፡</foreign> neque <foreign xml:lang="gez">ዘበእንተ፡</foreign> praemisso) hoc sensu usurpator, ut <ref cRef="Lev. " loc="4,21">Lev.  4,21</ref>. <cb n="0622" xml:id="c0622"/> </sense> </sense>
               
            </entry>
