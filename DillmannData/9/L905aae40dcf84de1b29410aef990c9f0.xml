         <div>
            <entry xml:id="L905aae40dcf84de1b29410aef990c9f0" n="6030">
               <form>
                  <foreign xml:lang="gez">ተኵሕለ</foreign>
               </form>
               <sense xml:lang="la" source="#dillmann" xml:id="Dmain"> et <foreign xml:lang="gez">ተኰሐለ፡</foreign>
                  <subc>III,1</subc> <sense n="1" xml:id="D1">
                     <gramGrp>
                        <pos expand="reflexivum">Refl.</pos>
                     </gramGrp>: <cit type="translation" xml:lang="la">
                        <quote>sibi</quote>
                     </cit>
                     <cit type="translation" xml:lang="la">
                        <quote>illinere</quote>
                     </cit> vel <cit type="translation" xml:lang="la">
                        <quote>oblinere</quote>
                     </cit> oculos, <cit type="translation" xml:lang="la">
                        <quote>sibi</quote>
                     </cit>
                     <cit type="translation" xml:lang="la">
                        <quote>tingere</quote>
                     </cit> vel <cit type="translation" xml:lang="la">
                        <quote>fucare</quote>
                     </cit> stibio, <lbl expand="cum">c.</lbl>
                     <lbl expand="duplex">dupl.</lbl>
                     <gramGrp>
                        <case value="accusativus">Acc.</case>
                     </gramGrp>: <foreign xml:lang="gez">እምሂ፡ ተኰሐልኪ፡ ኵሕለ፡ አዕይንተኪ፡</foreign>
                     <ref cRef="Jer." loc="4,30">Jer. 4,30</ref>; <lbl expand="cum">c.</lbl>
                     <gramGrp>
                        <case value="accusativus">Acc.</case>
                     </gramGrp> oculorum: <foreign xml:lang="gez">ይትኀፀባ፡ ወይትኰሐላ፡ አዕይንቲሆን፡</foreign>
                     <ref cRef="Hez." loc="23,40">Hez. 23,40</ref>; <foreign xml:lang="gez">ተኵሕለት፡
                        አዕይንቲሃ፡</foreign>
                     <ref cRef="4 Reg." loc="9,30">4 Reg. 9,30</ref>; <ref cRef="Apoc." loc="3,18">Apoc. 3,18</ref>; <foreign xml:lang="gez">በተኵሕሎቶን፡</foreign> F.M. 34,8;
                        <ref cRef="Hen." loc="8,1">Hen. 8,1</ref>; <foreign xml:lang="gez">ተኵሕሎ፡</foreign> (Infin.) <ref cRef="Did." loc="3.">Did. 3.</ref>
                  </sense>
                  <sense n="2" xml:id="D2"> Pass.: <cit type="translation" xml:lang="la">
                        <quote>oblini</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>illini</quote>
                     </cit>: <foreign xml:lang="gez">ሶበ፡ ተኵሕለ፡ </foreign>
                     <ref cRef="Tob." loc="11,7">Tob. 11,7</ref>; <foreign xml:lang="gez">አዕይንቲኪ፡
                        ሥን፡ ዘኢተኵሕለ፡ </foreign>
                     <cit type="translation" xml:lang="la">
                        <quote>oculi</quote>
                     </cit>
                     <cit type="translation" xml:lang="la">
                        <quote>tui</quote>
                     </cit>
                     <cit type="translation" xml:lang="la">
                        <quote>pulchritudo</quote>
                     </cit>
                     <cit type="translation" xml:lang="la">
                        <quote>non</quote>
                     </cit>
                     <cit type="translation" xml:lang="la">
                        <quote>fucata</quote>
                     </cit>, Lud.</sense>
               </sense>
               
