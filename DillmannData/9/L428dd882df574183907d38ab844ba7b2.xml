            <entry xml:id="L428dd882df574183907d38ab844ba7b2" n="8520">
               <form>
                  <foreign xml:lang="gez">ጕንዱይ</foreign>
               </form>
               <sense xml:lang="la" source="#dillmann" xml:id="Dmain">
                  <gramGrp>
                     <pos expand="participium">part.</pos>
                  </gramGrp>
                  <sense n="1" xml:id="D1">
                     <cit type="translation" xml:lang="la">
                        <quote>cunctabundus</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>tardus</quote>
                     </cit>: <foreign xml:lang="gez">ጕንዱይ፡ ለነቢብ፡</foreign>, <foreign xml:lang="gez">ጕንዱይ፡ ለመዐት፡</foreign>
                     <foreign xml:lang="grc">βραδὺς εἰς </foreign>
                     <ref cRef="Jac." loc="1,19">Jac. 1,19</ref>; <foreign xml:lang="gez">ጕንዱየ፡
                        መዐት፡</foreign>
                     <foreign xml:lang="grc">μακρόθυμος</foreign>
                     <ref cRef="2 Esr." loc="19,17">2 Esr. 19,17</ref>; <foreign xml:lang="gez">ጕንዱያነ፡ ልብ፡</foreign>
                     <ref cRef="Luc." loc="24,25">Luc. 24,25</ref>.</sense>
                  <sense n="2" xml:id="D2">
                     <cit type="translation" xml:lang="la">
                        <quote>serus</quote>
                     </cit>: <foreign xml:lang="gez">ኢይኩን፡ በአትከ፡ ጕንዱየ፡ ወፀአትከ፡ ፍጡነ፡</foreign> Clem.
                     f. 175.</sense>
                  <sense n="3" xml:id="D3">
                     <cit type="translation" xml:lang="la">
                        <quote>diuturnus</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>longus</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>longinquus</quote>
                     </cit>: <foreign xml:lang="gez">እሕየው፡ ጕንዱየ፡ ዓመተ፡</foreign>
                     <ref cRef="Job" loc="29,18">Job 29,18</ref>; <foreign xml:lang="gez">ጕንዱየ፡
                        መዋዕለ፡ </foreign>
                     <ref cRef="Jes." loc="27,10">Jes. 27,10</ref>; <ref cRef="Jos." loc="11,18">Jos. 11,18</ref>; <foreign xml:lang="gez">ጕንዱይ፡ መዋዕል፡</foreign>
                     <ref cRef="Hez. " loc="12,22">Hez. 12,22</ref>; <foreign xml:lang="gez">እምጕንዱይ፡
                        ዓመት፡</foreign>
                     <ref cRef="Zach." loc="7,3">Zach. 7,3</ref>; <ref cRef="Luc." loc="8,27">Luc.
                        8,27</ref>; <foreign xml:lang="gez">እምጕንዱይ፡ ዕለት፡</foreign>
                     <ref cRef="Rom." loc="15,23">Rom. 15,23</ref>; <ref cRef="Luc." loc="23,8">Luc.
                        23,8</ref>; <foreign xml:lang="gez">በጕንዱይ፡ መዋዕል፡</foreign>
                     <ref cRef="Jes." loc="30,27">Jes. 30,27</ref>; <ref cRef="Jes." loc="49,1">Jes.
                        49,1</ref>; <foreign xml:lang="gez">እምድኅረ፡ ጕንዱይ፡ መዋዕል፡</foreign>
                     <ref cRef="Jer." loc="13,6">Jer. 13,6</ref>; <ref cRef="Hebr." loc="4,7">Hebr.
                        4,7</ref>; <foreign xml:lang="gez">ለጕንዱይ፡ መዋዕል፡</foreign>
                     <ref cRef="Hez." loc="12,27">Hez. 12,27</ref> et omisso <foreign xml:lang="gez">መዋዕል፡</foreign>
                     <ref cRef="4 Esr." loc="12,27">4 Esr. 12,27</ref>. – Adv. <foreign xml:lang="gez">ጕንዱየ፡</foreign>
                     <sense n="a" xml:id="D3a">
                        <cit type="translation" xml:lang="la">
                           <quote>diu</quote>
                        </cit>, <cit type="translation" xml:lang="la">
                           <quote>longum tempus</quote>
                        </cit>: <ref cRef="Ex." loc="14,13">Ex. 14,13</ref>; <ref cRef="Hen." loc="39,10">Hen. 39,10</ref>.</sense>
                     <sense n="b" xml:id="D3b"> longo tempore interjecto i.e. <cit type="translation" xml:lang="la">
                           <quote>raro</quote>
                        </cit>: <foreign xml:lang="grc">σπάνιον</foreign>
                        <ref cRef="Prov." loc="25,17">Prov. 25,17</ref>. <cb n="1182" xml:id="c1182"/>
                     </sense>
                  </sense>
               </sense>
               
