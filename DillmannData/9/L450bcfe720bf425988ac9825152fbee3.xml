                     <foreign xml:lang="gez">ዲበ፡</foreign> jumenti <ref cRef="1 Reg." loc="25,20">1
                        Reg. 25,20</ref>; <lbl expand="cum">c.</lbl>
                     <foreign xml:lang="gez">በ፡</foreign> jumenti <ref cRef="Gen." loc="24,61">Gen.
                        24,61</ref>; vel navis <foreign xml:lang="gez">ሶበ፡ ከልእዎ፡ ይጸዐን፡
                        በሐመር፡</foreign>
                     <ref cRef="Sx. Haml." loc="7 ">Sx. Haml. 7 </ref>Enc.; <gramGrp>
                        <pos expand="absolute">abs.</pos>
                     </gramGrp>: <ref cRef="Matth." loc="21,7">Matth. 21,7</ref>; <foreign xml:lang="gez">ተጸዐኑ፡</foreign>
                     <foreign xml:lang="grc">ἐπίβητε </foreign>
                     <ref cRef="Jer." loc="26,4">Jer. 26,4</ref>.</sense>
                  <sense n="2" xml:id="D2">
                     <cit type="translation" xml:lang="la">
                        <quote>vehi</quote>
                     </cit>, <sense n="a" xml:id="D2a">
                        <cit type="translation" xml:lang="la">
                           <quote>equitare</quote>
                        </cit>, <lbl expand="cum">c.</lbl>
                        <foreign xml:lang="gez">ዲበ፡</foreign>, ut: <foreign xml:lang="gez">ዲበ፡
                           እንስሳ፡</foreign>
                        <ref cRef="Kuf." loc="50">Kuf. 50</ref>; <foreign xml:lang="gez">ዲበ፡ ፈረስ፡ </foreign>
                        <ref cRef="Zach." loc="1,8">Zach. 1,8</ref>; <ref cRef="Jer." loc="22,4">Jer. 22,4</ref>; <lbl expand="cum">c.</lbl>
                        <foreign xml:lang="gez">ላዕለ፡</foreign>, ut: <ref cRef="Jud." loc="5,10">Jud.
                           5,10</ref>; magus <foreign xml:lang="gez">ዐርገ፡ ሰማየ፡ እስመ፡ ይጼዐን፡ ላዕለ፡
                           አጋንንት፡ </foreign>
                        <ref cRef="Clem." loc="253">Clem. f. 253</ref>; <cb n="1306" xml:id="c1306"/>
                        <lbl expand="cum">c.</lbl>
                        <foreign xml:lang="gez">በ፡</foreign>, ut <ref cRef="Jer." loc="6,23">Jer.
                           6,23</ref>; <ref cRef="Jer." loc="17,25">Jer. 17,25</ref>; <foreign xml:lang="gez">ተጽዕነ፡ በአጋንንት፡</foreign>
                        <ref cRef="Clem. " loc="254">Clem. f. 254</ref>; <lbl expand="cum">c.</lbl>
                        <gramGrp>
                           <case value="accusativus">Acc.</case>
                        </gramGrp>: <ref cRef="Luc." loc="19,30">Luc. 19,30</ref>; <ref cRef="Apoc." loc="6,2">Apoc. 6,2</ref>; <foreign xml:lang="gez">ፈረስ፡ ወዘይጼዐኖ፡ </foreign>
                        <ref cRef="Job" loc="39,18">Job 39,18</ref>; <foreign xml:lang="gez">ፈረስ፡
                           እንተ፡ ይጼዐን፡ ንጉሥ፡ </foreign>
                        <ref cRef="Esth." loc="6,8">Esth. 6,8</ref>; <ref cRef="Esth." loc="8,14">Esth. 8,14</ref>; <ref cRef="Jes." loc="21,7">Jes. 21,7</ref>; <ref cRef="Jes." loc="36,8">Jes. 36,8</ref>; <foreign xml:lang="gez">ዘይክል፡
                           ተጽዕኖቶሙ፡</foreign>
                        <ref cRef="4 Reg." loc="18,23">4 Reg. 18,23</ref>; <foreign xml:lang="gez">ተምህረ፡ ተጽዕኖ፡ ፈረስ፡ ወነዲፈ፡ ቀስት፡</foreign>
                        <ref cRef="Sx. Sen." loc="29">Sx. Sen. 29</ref>; <ref cRef="Sx. Haml." loc="15 ">Sx. Haml. 15 </ref>(<lbl expand="videas">vid.</lbl>
                        <foreign xml:lang="gez">ጽኑዕ፡</foreign>); <foreign xml:lang="gez">ተጽዒኖ፡</foreign> cum equitaret <ref cRef="Clem." loc="253.">Clem. f.
                           253.</ref>
                     </sense>
                     <sense n="b" xml:id="D2b">
                        <cit type="translation" xml:lang="la">
                           <quote>invehi</quote>
                        </cit> curru, al., <lbl expand="cum">c.</lbl>
                        <foreign xml:lang="gez">በ፡</foreign>, ut: <foreign xml:lang="gez">በአፍራስ፡
                           ወበሰረገላ፡ ይጼዐኑ፡</foreign>
                        <ref cRef="Jer." loc="6,23">Jer. 6,23</ref>; <ref cRef="Jer." loc="17,25">Jer. 17,25</ref>; <lbl expand="cum">c.</lbl>
                        <foreign xml:lang="gez">ላዕለ፡</foreign>
                        <ref cRef="Ps." loc="17,12">Ps. 17,12</ref>; <lbl expand="cum">c.</lbl>
                        <foreign xml:lang="gez">ዲበ፡</foreign>
                        <ref cRef="Jer." loc="22,4">Jer. 22,4</ref>; <lbl expand="cum">c.</lbl>
                        <gramGrp>
                           <case value="accusativus">Acc.</case>
                        </gramGrp>: <foreign xml:lang="gez">ሰረገላ፡ ወዘይጼዐኖሙ፡ </foreign>
                        <ref cRef="Jer." loc="28,22">Jer. 28,22</ref>; <foreign xml:lang="gez">ሰረገላ፡
                           በኀበ፡ ይጼዐን፡</foreign>
                        <ref cRef="Hen. " loc="73,2">Hen. 73,2</ref>; <ref cRef="Lit." loc="172,1">Lit. 172,1</ref>.</sense>
                     <sense n="c" xml:id="D2c">
                        <cit type="translation" xml:lang="la">
                           <quote>vehi</quote>
                        </cit> in nave, <cit type="translation" xml:lang="la">
                           <quote>navigare</quote>
                        </cit>: <foreign xml:lang="gez">እለ፡ ይጼዐኑ፡ ውስተ፡ ሐመር፡</foreign>
                        <ref cRef="Sx. Masc." loc="25">Sx. Masc. 25</ref>; <foreign xml:lang="gez">ንሕነ፡ ተጽዕነ፡ ውስተ፡ ሐመር፡ </foreign>
                        <ref cRef="Sx. Pag." loc="3.">Sx. Pag. 3.</ref>
                     </sense>
                     <sense n="d" xml:id="D2d">
                        <lbl expand="translate">transl.</lbl>: <sense n="α" xml:id="D2dα">
                           <cit type="translation" xml:lang="la">
                              <quote>insidere</quote>
                           </cit>: <foreign xml:lang="gez">ይጼዐን፡ ቀመረ፡</foreign> insidet fornici,
                           Lud. e Lit. Orth.</sense>
                        <sense n="β" xml:id="D2dβ">
                           <cit type="translation" xml:lang="la">
                              <quote>obsidere</quote>
                           </cit>: <foreign xml:lang="gez">ተጽዕነ፡ ላዕሌሁ፡ ሰይጣን፡</foreign> Sx. <ref cRef="Masc." loc="19">Masc. 19</ref>; <ref cRef="Sx. Masc." loc="29 ">Sx. Masc. 29 </ref>al. – <lbl expand="Vocabularium Aethiopicum, -a -a.">Voc. Ae.</lbl>: <foreign xml:lang="gez">ተጽዕነ፡ ዘ፡ ሰረረ፡ በረረሂ፡ ይ፡</foreign>
                        </sense>
                     </sense>
                  </sense>
               </sense>
               
