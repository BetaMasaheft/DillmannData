               <sense xml:lang="la" source="#dillmann" xml:id="Dmain"> (pro <foreign xml:lang="gez">ወለድት፡</foreign>) <gramGrp> <pos expand="not found">subst.</pos> <gen value="femininus">fem.</gen> </gramGrp>, <lbl expand="pluralis">Pl.</lbl> <foreign xml:lang="gez">አዋልድ፡</foreign> <sense n="1" xml:id="D1"> <cit type="translation" xml:lang="la">
                                <quote> filia </quote>
                            </cit>, <ref cRef="Gen." loc="6,1">Gen. 6,1</ref> <lbl expand="sequens">seq.</lbl>; <ref cRef="Gen." loc="11,29">Gen. 11,29</ref>; <ref cRef="Gen." loc="24,23">Gen. 24,23</ref>; <ref cRef="Gen." loc="24,24">Gen. 24,24</ref>; <ref cRef="Ex." loc="2,1">Ex. 2,1</ref>; <ref cRef="Ex." loc="2,5">Ex. 2,5</ref>; <ref cRef="Lev." loc="18,10">Lev. 18,10</ref> <lbl expand="sequens">seq.</lbl>; <ref cRef="Job" loc="42,13">Job 42,13</ref>; <ref cRef="Jer." loc="5,17">Jer. 5,17</ref>; <ref cRef="Jer." loc="16,2">Jer. 16,2</ref>; <foreign xml:lang="gez">በከመ፡ እም፡ ከማሁ፡ ወለታ፡</foreign> <ref cRef="Hez." loc="16,44">Hez. 16,44</ref>; <ref cRef="Sir." loc="22,3">Sir. 22,3</ref>; <ref cRef="Matth." loc="9,18">Matth. 9,18</ref>; <ref cRef="Matth." loc="10,35">Matth. 10,35</ref>; <ref cRef="Matth. " loc="10,37">Matth.  10,37</ref>.</sense> <sense n="2" xml:id="D2"> <sense n="a" xml:id="D2a"> <cit type="translation" xml:lang="la">
                                    <quote> neptis </quote>
                                </cit>: <foreign xml:lang="gez">ወለተ፡ ወልድከ፡ አው፡ ወለተ፡ ወለትከ፡</foreign> <ref cRef="Lev. " loc="18,10">Lev.  18,10</ref>; <foreign xml:lang="gez">አዋልደ፡ አዋልዲሁ፡</foreign> <ref cRef="Gen." loc="46,7">Gen. 46,7</ref>; <foreign xml:lang="gez">ሰሎሜ፡ ወለተ፡ ማጣት፡</foreign> <ref cRef="Sx. Genb." loc="25.">Sx. Genb. 25.</ref> </sense> <sense n="b" xml:id="D2b"> <cit type="translation" xml:lang="la">
                                    <quote> nurus </quote>
                                </cit> <ref cRef="Ruth" loc="1,11">Ruth 1,11</ref>; <ref cRef="Ruth" loc="1,12">Ruth 1,12</ref>; <ref cRef="Ruth" loc="1,13">Ruth 1,13</ref>; <ref cRef="Ruth" loc="2,22">Ruth 2,22</ref>.</sense> <sense n="c" xml:id="D2c"> <cit type="translation" xml:lang="la">
                                    <quote> oriunda e progenie </quote>
                                </cit> alicujus, <ref cRef="Gen." loc="24,3">Gen. 24,3</ref>; <ref cRef="Gen." loc="27,46">Gen. 27,46</ref>; <ref cRef="Gen." loc="28,6">Gen. 28,6</ref>; <ref cRef="Num." loc="25,1">Num. 25,1</ref>; <ref cRef="Jud." loc="11,40">Jud. 11,40</ref>; <foreign xml:lang="gez">አዋልደ፡ ሕዝብየ፡</foreign> <ref cRef="Jer." loc="6,26">Jer. 6,26</ref>; <ref cRef="Luc." loc="1,5">Luc. 1,5</ref>; <ref cRef="Luc." loc="13,16">Luc. 13,16</ref>.</sense> </sense> <sense n="3" xml:id="D3"> Varie transfertur: <sense n="a" xml:id="D3a"> <lbl expand="sequens">seq.</lbl> Genitivo temporis: <foreign xml:lang="gez">ይእቲ፡ ወለተ፡ ፲ወ፪ ዓመት፡</foreign> <cit type="translation" xml:lang="la">
                                    <quote> duodecim annos nata </quote>
                                </cit> <ref cRef="Kuf." loc="30.">Kuf. 30.</ref> </sense> <sense n="b" xml:id="D3b"> <lbl expand="sequens">seq.</lbl> Genitivo loci: <cit type="translation" xml:lang="la">
                                    <quote> incola </quote>
                                </cit>, <cit type="translation" xml:lang="la">
                                    <quote> oriunda e </quote>
                                </cit>, <ref cRef="Luc." loc="23,28">Luc. 23,28</ref>; <ref cRef="Cant. " loc="1,5">Cant.  1,5</ref> <lbl expand="sequens">seq.</lbl>; <ref cRef="Gen." loc="34,1">Gen. 34,1</ref>.</sense> <sense n="c" xml:id="D3c"> <cit type="translation" xml:lang="la">
                                    <quote> alumna </quote>
                                </cit>: <foreign xml:lang="gez">ቦቱ፡ ወለት፡ እንተ፡ ሐፀና፡</foreign> <ref cRef="Esth. " loc="2,7">Esth.  2,7</ref>.</sense> <sense n="d" xml:id="D3d"> <cit type="translation" xml:lang="la">
                                    <quote> discipula </quote>
                                </cit>, vel <cit type="translation" xml:lang="la">
                                    <quote> filia </quote>
                                </cit>, in compellatione: <ref cRef="Ps." loc="44,12">Ps. 44,12</ref>; <ref cRef="Judith" loc="13,18">Judith 13,18</ref>; <ref cRef="Matth." loc="9,22">Matth. 9,22</ref>; <ref cRef="Marc." loc="5,34">Marc. 5,34</ref>; <ref cRef="Luc." loc="8,48">Luc. 8,48</ref>.</sense> <sense n="e" xml:id="D3e"> <cit type="translation" xml:lang="la">
                                    <quote> filiae urbis </quote>
                                </cit> i.e. <cit type="translation" xml:lang="la">
                                    <quote> vici </quote>
                                </cit> minores in ejus ditione siti ex eaque pendentes: <foreign xml:lang="gez">ቄናት፡ ወአዋልዲሃ፡</foreign> <ref cRef="1 Par." loc="2,23">1 Par. 2,23</ref>; <ref cRef="Jud. " loc="1,27">Jud.  1,27</ref>.</sense> </sense> <sense n="4" xml:id="D4"> <cit type="translation" xml:lang="la">
                                <quote> puella </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> adolescentula </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> mulier </quote>
                            </cit>: <foreign xml:lang="gez">ወለት፡ ድንግል፡</foreign> <ref cRef="Sir." loc="20,4">Sir. 20,4</ref>; <ref cRef="Ruth" loc="2,5">Ruth 2,5</ref>; <ref cRef="Judith" loc="12,13">Judith 12,13</ref>; <ref cRef="3 Reg." loc="1,2">3 Reg. 1,2</ref>; <ref cRef="Ex." loc="2,8">Ex. 2,8</ref>; <ref cRef="Deut." loc="22,23">Deut. 22,23</ref>; <ref cRef="Deut." loc="22,24">Deut. 22,24</ref>; <ref cRef="Deut." loc="22,25">Deut. 22,25</ref>; <ref cRef="Deut." loc="22,26">Deut. 22,26</ref>; <ref cRef="Deut." loc="22,27">Deut. 22,27</ref>; <ref cRef="Deut." loc="22,28">Deut. 22,28</ref>; <ref cRef="Deut." loc="22,29">Deut. 22,29</ref>; <ref cRef="Jud." loc="19,3">Jud. 19,3</ref> <lbl expand="sequens">seq.</lbl>; <ref cRef="Cant." loc="6,8">Cant. 6,8</ref>; <ref cRef="Cant." loc="6,9">Cant. 6,9</ref>. In specie praedicatur de urbibus vel gentibus: <foreign xml:lang="gez">ወለተ፡ ጽዮን፡</foreign> <ref cRef="Jes." loc="10,32">Jes. 10,32</ref>; <ref cRef="Jes." loc="37,22">Jes. 37,22</ref>, <ref cRef="Jes." loc="62,11">Jes. 62,11</ref>; <foreign xml:lang="gez">ድንግል፡ ወለተ፡ ግብጽ፡</foreign> <ref cRef="Jer. " loc="26,11">Jer.  26,11</ref>; <foreign xml:lang="gez">ወለተ፡ ሕዝብየ፡</foreign> <ref cRef="Jer." loc="8,22">Jer. 8,22</ref>; Jer.14,17; <ref cRef="Matth. " loc="21,5">Matth.  21,5</ref>; <ref cRef="Joh." loc="12,15">Joh. 12,15</ref>.</sense> <sense n="5" xml:id="D5"> <cit type="translation" xml:lang="la">
                                <quote> famula </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> ancilla </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> serva </quote>
                            </cit>: <ref cRef="Ex." loc="2,5">Ex. 2,5</ref>; <ref cRef="Ruth" loc="2,8">Ruth 2,8</ref>; <ref cRef="Ruth" loc="2,22">Ruth 2,22</ref> <lbl expand="sequens">seq.</lbl>; <ref cRef="Esth." loc="4,4">Esth. 4,4</ref>; <ref cRef="Esth." loc="4,16">Esth. 4,16</ref>; <ref cRef="Judith" loc="10,2">Judith 10,2</ref>; <ref cRef="Judith" loc="10,5">Judith 10,5</ref>; <ref cRef="Dan. apocr." loc="1,15">Dan. apocr. 1,15</ref>; <ref cRef="Matth." loc="26,69">Matth. 26,69</ref>; <ref cRef="Luc." loc="12,45">Luc. 12,45</ref>.</sense> </sense>
               
