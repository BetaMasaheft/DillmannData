               <sense xml:lang="la" source="#dillmann" xml:id="Dmain"> <gramGrp> <pos expand="Substantivum">subst.</pos> </gramGrp>, plerumque <gramGrp> <gen value="not found">m.</gen> </gramGrp> [significatione convenit cum <foreign xml:lang="ar">جِلْدٌ</foreign>, fortasse etiam etymo, quatenus <foreign xml:lang="ar">جَلُدَ</foreign> <cit type="translation" xml:lang="la">
                            <quote> durus fuit </quote>
                        </cit>, <cit type="translation" xml:lang="la">
                            <quote> robustus </quote>
                        </cit>, <cit type="translation" xml:lang="la">
                            <quote> alacer fuit </quote>
                        </cit>, <foreign xml:lang="ar">جَمُدَ</foreign> <cit type="translation" xml:lang="la">
                            <quote> concrevit </quote>
                        </cit> (<foreign xml:lang="gez">ሐመዳ፡</foreign>); (<foreign xml:lang="syr">ܓܡܕ</foreign>) <foreign xml:lang="ar">جَمَدَ</foreign> <cit type="translation" xml:lang="la">
                            <quote> duro animo ac immiti fuit, </quote>
                        </cit> <foreign xml:lang="ar">جَنَدٌ</foreign> <cit type="translation" xml:lang="la">
                            <quote> terra salebrosa </quote>
                        </cit>, <foreign xml:lang="ar">عَلِدَ</foreign> <cit type="translation" xml:lang="la">
                            <quote> durus fuit </quote>
                        </cit>, <foreign xml:lang="ar">كَلَدٌ</foreign> <cit type="translation" xml:lang="la">
                            <quote> terra </quote>
                        </cit> <cit type="translation" xml:lang="la">
                            <quote> dura </quote>
                        </cit>, <foreign xml:lang="ar">كَنُودٌ</foreign> <cit type="translation" xml:lang="la">
                            <quote> sterilis </quote>
                        </cit>, <cit type="translation" xml:lang="la">
                            <quote> avarus </quote>
                        </cit>, ejusdem familiae esse videntur; <cb n="0779" xml:id="c0779"/> de permutatione literarum palatalium et <foreign xml:lang="gez">አ</foreign> vid. etiam <foreign xml:lang="gez">አለደ፡</foreign> et <foreign xml:lang="gez">አበረ፡</foreign>] <sense n="1" xml:id="D1"> <cit type="translation" xml:lang="la">
                                <quote> cutis </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> pellis </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> tunica </quote>
                            </cit>; corporis humani, ut <ref cRef="Jer." loc="38,2">Jer. 38,2</ref> <lbl expand="varia lectio">var.</lbl>; <ref cRef="M.M." loc="f. 43">M.M. f. 43</ref>; <ref cRef="Sx. Haml." loc="5 ">Sx. Haml. 5 </ref>(<lbl expand="videas">vid. sub</lbl> <foreign xml:lang="gez">ተዝሕቀ፡</foreign>); <foreign xml:lang="gez">አነዳ፡ ገጹ፡</foreign> <ref cRef="Sx. Genb." loc="3 Enc.">Sx. Genb. 3 Enc.</ref>; <foreign xml:lang="gez">አነዳ፡ ሥጋሁ፡</foreign> <ref cRef="Jsp." loc="299">Jsp. p. 299</ref>; <ref cRef="Sx. Tachs." loc="6">Sx. Tachs. 6</ref>; bestiarum: <foreign xml:lang="gez">ዘባሒ፡ ይዝሕቅ፡ አነዳ፡ እምዲበ፡ ሥጋ፡</foreign> <ref cRef="Chrys. ho." loc="7">Chrys. ho. 7</ref>; <foreign xml:lang="gez">አነዳ፡ ላህም፡ ርጡብ፡</foreign> <ref cRef="Gad. T.H." loc="">Gad. T.H. </ref>; <foreign xml:lang="gez">መዝራዕት፡ በአነዳሁ፡</foreign> <ref cRef="Num." loc="6,19">Num. 6,19</ref> (an legit <foreign xml:lang="grc">ἀνεφθόν</foreign>?); aliarum rerum, ut granorum frumenti <ref cRef="Macc. " loc="5 ">Macc.  f. 5 </ref>(ubi <foreign xml:lang="gez">አንዳ፡</foreign> scriptum legitur); pomorum: <foreign xml:lang="gez">አነዳ፡ ትፉሕ፡</foreign> <ref cRef="Jsp." loc="340 ">Jsp. p. 340 </ref>(<lbl expand="videas">vid. sub</lbl> <foreign xml:lang="gez">ዘሐቀ፡</foreign>).</sense> <sense n="2" xml:id="D2"> <cit type="translation" xml:lang="la">
                                <quote> pellis </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> corium </quote>
                            </cit>: <foreign xml:lang="gez">አልበሶሙ፡</foreign> (Adamum et Evam) <foreign xml:lang="gez">እግዚአብሔር፡ አነዳ፡ ማእስ፤ ዝውእቱ፡</foreign> (hoc significat allegorice) <foreign xml:lang="gez">አነዳ፡ ዘመልዕልተ፡ ሥጋነ፡ ልብሰ፡ ሐዘን፡ ወሕማም፡</foreign> <ref cRef="Clem." loc="7">Clem. f. 7</ref>; <foreign xml:lang="gez">ይኩን፡ ልብሰቱ፡ እምአነዳ፡ ማእስ፡ ዘእንስሳ፡</foreign> <ref cRef="Clem." loc="22">Clem. f. 22</ref>; <foreign xml:lang="gez">ዝቅ፡ ዘአነዳ፡</foreign> <ref cRef="Sx. Haml. " loc="25.">Sx. Haml.  25.</ref> </sense> <sense n="3" xml:id="D3"> <cit type="translation" xml:lang="la">
                                <quote> segmentum </quote>
                            </cit> <cit type="translation" xml:lang="la">
                                <quote> corii </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> lorum </quote>
                            </cit>: <foreign xml:lang="gez">በሐብለ፡ አነዳ፡ ተአስሩ፡</foreign> <ref cRef="Sx. Teq." loc="17">Sx. Teq. 17</ref>; <foreign xml:lang="gez">ዘበጦ፡ በአነዳ፡ ላህም፡</foreign> F.N. 47; <foreign xml:lang="gez">ዘቀሠፈ፡ አሐደ፡ እምአግብርቲሁ፡ በአነዳ፡ አው፡ በበትር፡</foreign> F.N. 47; <foreign xml:lang="gez">እንዘ፡ ይቀሥፍዎ፡ በይቡስ፡ አነዳ፡</foreign> <ref cRef="Sx. Mag." loc="7 Enc.">Sx. Mag. 7 Enc.</ref> — Voc. Ae.: <foreign xml:lang="gez">ጠብደን፡ አነዳ፡ ብ፡ የላም፡ ደባ።</foreign> </sense> </sense>
               
