                  <foreign xml:lang="gez">ጸበተ</foreign>
               <rs type="root"/>
                    </form>
               <sense xml:lang="la" source="#dillmann" xml:id="Dmain"> (vel <foreign xml:lang="gez">ፀበተ፡</foreign>) <subc>I,1</subc> [vic. <foreign xml:lang="gez">ጸበየ፡</foreign>; etymon repetendum est a <cit type="translation" xml:lang="la">
                            <quote> fluendo </quote>
                        </cit> (<foreign xml:lang="gez">ጾበ፡</foreign> <foreign xml:lang="ar">ضَبَّ</foreign>, <foreign xml:lang="ar">صَبَّ صَابَ</foreign>), sicut etymon verbi <foreign xml:lang="he">שָׂחָה סוּחַ</foreign> et <foreign xml:lang="syr">ܛܳܦ</foreign> <foreign xml:lang="he">צוּף</foreign>] <gramGrp> <pos expand="Subjunctivus">Subj.</pos> </gramGrp> <foreign xml:lang="gez">ይጽብት፡</foreign> <cit type="translation" xml:lang="la">
                            <quote> natare </quote>
                        </cit>, <cit type="translation" xml:lang="la">
                            <quote> nare </quote>
                        </cit>: de inanimatis, ut arca <ref cRef="Noae Gad. Ad." loc="160">Noae Gad. Ad. f. 160</ref>; M.M. f. 124; cadavere: <foreign xml:lang="gez">ሥጋሁ፡ ይጸብት፡ ላዕለ፡ ማይ፡</foreign> (<cit type="translation" xml:lang="la">
                            <quote> supernatare </quote>
                        </cit>) <ref cRef="Sx. Jac." loc="6">Sx. Jac. 6</ref>; de animalibus: <foreign xml:lang="gez">እለ፡ ይትሐወሱ፡ ወይጸብቱ፡ ውስተ፡ ማያት፡</foreign> <ref cRef="Sap." loc="19,18">Sap. 19,18</ref>; <foreign xml:lang="gez">ዓሣት፡ ዘይጸብት፡</foreign> M.M. f. 242; de hominibus: <foreign xml:lang="gez">እለ፡ ያአምሩ፡ ጸቢተ፡</foreign> <ref cRef="Act." loc="27,43">Act. 27,43</ref>; F.N. 47; <foreign xml:lang="gez">ኮነ፡ ጽኑዐ፡ በተጽእኖ፡ አፍራስ፡ ወጸቢተ፡ ማይ፡</foreign> <ref cRef="Sx. Haml." loc="15">Sx. Haml. 15</ref>; <foreign xml:lang="gez">ጽብቱ፡ ምስሌሁ፡</foreign> <ref cRef="Jsp." loc="323.">Jsp. p. 323.</ref> <sense n="a" xml:id="Da"> <cit type="translation" xml:lang="la">
                                <quote> ultro citroque natare </quote>
                            </cit>: <foreign xml:lang="gez">ወዐልኩ፡ እጸብት፡ ውስተ፡ ቀላይ፡</foreign> <ref cRef="2 Cor." loc="11,25">2 Cor. 11,25</ref>. <cb n="1281" xml:id="c1281"/> </sense> <sense n="b" xml:id="Db"> <cit type="translation" xml:lang="la">
                                <quote> enatare </quote>
                            </cit>: <foreign xml:lang="gez">ከመ፡ ኢይጽብቱ፡</foreign> <ref cRef="Act." loc="27,42">Act. 27,42</ref>; <ref cRef="Act." loc="27,43">Act. 27,43</ref> Platt; <foreign xml:lang="gez">ከመ፡ ኢያምሥጥ፡ ጸቢቶ፡</foreign> <ref cRef="Act." loc="27,42">Act. 27,42</ref> <lbl expand="romanae editionis">rom.</lbl> </sense> <sense n="c" xml:id="Dc"> <cit type="translation" xml:lang="la">
                                <quote> transnatare </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> tranare </quote>
                            </cit>: <foreign xml:lang="gez">ጸበተ፡ ተጽኢኖ፡ ፈረሰ፡ ወዐደወ፡</foreign> <ref cRef="Jsp." loc="283">Jsp. p. 283</ref>; <lbl expand="cum">c.</lbl> <gramGrp> <case value="accusativus">Acc.</case> </gramGrp> aquae: Stephanus <foreign xml:lang="gez">ባሕረ፡ መጻሕፍት፡ አመ፡ ጸበተ፡ ልሳኑ፤ ተቃውሞቶ፡ መዋግድ፡ ስእኑ፡</foreign> <ref cRef="Sx. Teq." loc="17 ">Sx. Teq. 17 </ref>Enc.; abyssus <foreign xml:lang="gez">ዘኢይጸብትዎ፡ ዓሣት፡</foreign> Lit. Epiph.</sense> <sense n="d" xml:id="Dd"> <cit type="translation" xml:lang="la">
                                <quote> subnatare </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> urinari </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> urinando assequi </quote>
                            </cit>: <foreign xml:lang="gez">ይጸብት፡ በኅቡእ፡</foreign> Lit. Joh.; <foreign xml:lang="gez">ኢበጸቢተ፡ ኅሊና፡</foreign> (non profunde cogitando) Lit. Orth.; <lbl expand="cum">c.</lbl> <gramGrp> <case value="accusativus">Acc.</case> </gramGrp>: <foreign xml:lang="gez">ይፈቅድ፡ ኅሊናየ፡ ይጽብት፡ ዕመቀ፡ አብሕርቲሁ፡</foreign> <ref cRef="Lit." loc="172,1">Lit. 172,1</ref>. – <lbl expand="Vocabularium Aethiopicum, -a -a">Voc. Ae.</lbl>: <foreign xml:lang="gez">ጸበተ፡ ዘ፡ ዋኛ፡</foreign> (<lbl expand="videas">vid.</lbl> etiam scholion ad <foreign xml:lang="gez">ቀዘፈ፡</foreign> et <lbl expand="conferas">cfr.</lbl> <foreign xml:lang="he">שׁוּט</foreign> targumicum).</sense> </sense>
               
