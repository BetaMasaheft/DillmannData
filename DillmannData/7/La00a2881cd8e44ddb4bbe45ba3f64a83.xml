            <entry xml:id="La00a2881cd8e44ddb4bbe45ba3f64a83" n="4618">
               <form>
                  <foreign xml:lang="gez">ኆጻ</foreign>
               </form>
               <sense xml:lang="la" source="#dillmann" xml:id="Dmain">
                  <gramGrp>
                     <pos expand="Substantivum">subst.</pos>
                  </gramGrp> [nescio an rectius scribendum sit <foreign xml:lang="gez">ሖጻ፡</foreign>, quamquam in libris Mss. plerumque <foreign xml:lang="gez">ኆጻ፡
                  </foreign> reperitur; respondent <foreign xml:lang="ar">حصىً</foreign>
                  <cit type="translation" xml:lang="la">
                     <quote>glarca</quote>
                  </cit>, <cit type="translation" xml:lang="la">
                     <quote>calculus</quote>
                  </cit>; <foreign xml:lang="ar">حَصَبٌ</foreign>
                  <cit type="translation" xml:lang="la">
                     <quote>glarea</quote>
                  </cit>; <cit type="translation" xml:lang="la">
                     <quote>lapides</quote>
                  </cit>; <foreign xml:lang="syr">ܚܨܳܨܳܐ</foreign> et <foreign xml:lang="he">חָצָץ</foreign>
                  <lbl expand="idem">id.</lbl>, de quibus <lbl expand="videas">vid.</lbl> Ges. thes.
                  p. 511] <gramGrp>
                     <gen value="masculinum">m.</gen>
                  </gramGrp> (et <gramGrp>
                     <gen value="femininus">fem.</gen>
                  </gramGrp>), <gramGrp>
                     <pos expand="nomen">nom.</pos>
                  </gramGrp> gener. (coll.) <sense n="1" xml:id="D1">
                     <cit type="translation" xml:lang="la">
                        <quote>glarea</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>arena</quote>
                     </cit> (<foreign xml:lang="grc">ἄμμος</foreign>, raro <foreign xml:lang="grc">ψῆφος</foreign>, <foreign xml:lang="grc">χοῦς</foreign>): <foreign xml:lang="gez">ከመ፡ አሐቲ፡ ኅጠተ፡ ኆጻ፡ እምውስተ፡ ኵሉ፡ ኆጻ፡</foreign>
                     <ref cRef="Sir." loc="18,10">Sir. 18,10</ref>; <foreign xml:lang="gez">ይኄይስ፡
                        ትጾር፡ ኆጻ፡ ወጼወ፡</foreign>
                     <ref cRef="Sir." loc="22,15">Sir. 22,15</ref>; <foreign xml:lang="gez">መርገ፡ ኆጻ፡
                        ውስተ፡ አረፍት፡</foreign>
                     <ref cRef="Sir." loc="22,17">Sir. 22,17</ref>; <foreign xml:lang="gez">ዐቀበ፡
                        ኆጻ፡</foreign>
                     <foreign xml:lang="grc">ἀνάβασις ἀμμώδης</foreign>
                     <ref cRef="Sir. " loc="25,20">Sir. 25,20</ref>; <ref cRef="Ex." loc="2,12">Ex.
                        2,12</ref>; <ref cRef="Matth." loc="7,26">Matth. 7,26</ref>; <ref cRef="Jer." loc="5,22">Jer. 5,22</ref>; <ref cRef="Jer." loc="26,22">Jer.
                        26,22</ref>; <ref cRef="Thren." loc="3,16">Thren. 3,16</ref>; <ref cRef="4 Esr." loc="2,27">4 Esr. 2,27</ref>; <foreign xml:lang="gez">ዘኆጻ፡</foreign>
                     <ref cRef="Hen." loc="60,15">Hen. 60,15</ref>; saepius <foreign xml:lang="gez">ኆጻ፡ ባሕር፡</foreign>
                     <ref cRef="Gen." loc="28,14">Gen. 28,14</ref>; <ref cRef="Kuf." loc="13">Kuf.
                        13</ref>; <ref cRef="Job" loc="6,3">Job 6,3</ref>; <ref cRef="Jes." loc="10,22">Jes. 10,22</ref>; <ref cRef="Jes. " loc="48,19">Jes.
                     48,19</ref>; <ref cRef="Ps." loc="77,31">Ps. 77,31</ref> al.; <foreign xml:lang="gez">ኆጻ፡ ምድር፡</foreign>
                     <ref cRef="Sir." loc="44,21">Sir. 44,21</ref>.</sense>
                  <sense n="2" xml:id="D2">
                     <cit type="translation" xml:lang="la">
                        <quote>locus glareosus</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>syrtis</quote>
                     </cit>
                     <ref cRef="Act." loc="27,17">Act. 27,17</ref>
                     <lbl expand="romanae editionis">rom.</lbl>
                  </sense>
                  <sense n="3" xml:id="D3"> nonnumquam <cit type="translation" xml:lang="la">
                        <quote>lapillus glareae</quote>
                     </cit>: Agathon monachus, quando cum aliis congrediebatur, <foreign xml:lang="gez">ኮነ፡ ይወዲ፡ ኆጻ፡ ንስቲተ፡ ውስተ፡ አፉሁ፡ </foreign> (paulo supra <foreign xml:lang="gez">ኆጻ፡ እብነ፡</foreign>) <ref cRef="Phlx. qu." loc="69">Phlx. qu.
                        69</ref>; <ref cRef="Phlx. qu." loc="30.">Phlx. qu. 30.</ref>
                     <cb n="0607" xml:id="c0607"/>
                  </sense>
               </sense>
               
