               <sense xml:lang="la" source="#dillmann" xml:id="Dmain"> <subc>III,1</subc> <sense n="1" xml:id="D1">
                     <cit type="translation" xml:lang="la">
                        <quote>recitari</quote>
                     </cit>
                     <sense n="a" xml:id="D1a">
                        <cit type="translation" xml:lang="la">
                           <quote>cantando</quote>
                        </cit>; <cit type="translation" xml:lang="la">
                           <quote>hymno celebrari</quote>
                        </cit>: <foreign xml:lang="gez">እምነግህ፡ ይትነበብ፡ ቅድሳቱ፡</foreign>
                        <foreign xml:lang="grc">ἀπὸ πρωῒ ἠχεῖν τὸ ἁγίασμα </foreign>
                        <ref cRef="Sir." loc="47,10">Sir. 47,10</ref>.</sense>
                     <sense n="b" xml:id="D1b">
                        <cit type="translation" xml:lang="la">
                           <quote>legi</quote>
                        </cit>, <cit type="translation" xml:lang="la">
                           <quote>praelegi</quote>
                        </cit>: <foreign xml:lang="gez">በእንተ፡ ጸሎት፡ ዘይትነበብ፡ ዲበ፡ ማይ፡ </foreign> (super
                        aquam baptismatis) <ref cRef="Did." loc="40">Did. 40</ref>; <foreign xml:lang="gez">ኵሉ፡ መካናት፡ ዘተነበት፡ ውስቴታ፡ ዛቲ፡ ጸሎት፡</foreign>
                        <ref cRef="Kedr f." loc="16">Kedr f. 16</ref>; <ref cRef="Kedr f." loc="17">Kedr f. 17</ref>; <ref cRef="Lit." loc="161,3">Lit. 161,3</ref>; <ref cRef="Lit. " loc="162,2">Lit. 162,2</ref>; <lbl expand="cum">c.</lbl>
                        <foreign xml:lang="gez">በቅድመ፡</foreign>
                        <ref cRef="Hen." loc="89,71">Hen. 89,71</ref>; <ref cRef="Hen." loc="97,6">Hen. 97,6</ref>; Sx. <ref cRef="Teq." loc="9">Teq. 9</ref>; <foreign xml:lang="gez">ዘተነበ፡ ቅድመ፡ ንጉሥ፡</foreign>
                        <ref cRef="2 Par." loc="34,24">2 Par. 34,24</ref>.</sense>
                     <sense n="2" xml:id="D12">
                        <cit type="translation" xml:lang="la">
                           <quote>enuntiari</quote>
                        </cit>, <cit type="translation" xml:lang="la">
                           <quote>dici</quote>
                        </cit>: <foreign xml:lang="gez">ዘኢይትነበብ፡ </foreign>
                        <foreign xml:lang="grc">τὰ ἄδηλα</foreign>, <cit type="translation" xml:lang="la">
                           <quote>ineffabilia </quote>
                        </cit>
                        <ref cRef="Ps. " loc="50,7">Ps. 50,7</ref>.</sense>
                  </sense>
               </sense>
               
