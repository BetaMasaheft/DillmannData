foreign xml:lang="gez">በፀጋመ፡</foreign> <cit type="translation" xml:lang="la"><quote> a sinistra alicujus </quote></cit> <ref cRef="Asc. Jes." loc="7,27">Asc. Jes. 7,27</ref> <lbl expand="sequens">seq.</lbl>; <ref cRef="Asc. Jes." loc="9,36">Asc. Jes. 9,36</ref>; <ref cRef="Ges." loc="48,13">Ges. 48,13</ref>; <ref cRef="Ex. " loc="14,22">Ex.  14,22</ref>; <ref cRef="2 Par." loc="18,18">2 Par. 18,18</ref>; <ref cRef="Kuf." loc="132">Kuf. p. 132</ref>; <ref cRef="Luc." loc="23,33">Luc. 23,33</ref>; <ref cRef="Marc." loc="10,37">Marc. 10,37</ref>; <ref cRef="Matth." loc="20,21">Matth. 20,21</ref>; <ref cRef="Matth. " loc="20,23">Matth.  20,23</ref>; <ref cRef="Matth." loc="27,38">Matth. 27,38</ref> Platt; vel <foreign xml:lang="gez">እምፀጋመ፡</foreign> <ref cRef="2 Reg. " loc="16,6">2 Reg.  16,6</ref>; <ref cRef="3 Reg." loc="22,19">3 Reg. 22,19</ref>; <cb n="1337" xml:id="c1337"/> <ref cRef="2 Esr." loc="18,4">2 Esr. 18,4</ref>; <foreign xml:lang="gez">እምፀጋም፡</foreign> <ref cRef="2 Cor." loc="6,7">2 Cor. 6,7</ref>.</sense> <sense n="γ" xml:id="Dbγ"> <foreign xml:lang="gez">ኀበ፡ ፀጋመ፡</foreign> <cit type="translation" xml:lang="la"><quote> ad sinistram alicujus </quote></cit> <ref cRef="Gen." loc="48,13">Gen. 48,13</ref>; <foreign xml:lang="gez">ለፀጋም፡</foreign> <cit type="translation" xml:lang="la"><quote> sinistrorsum </quote></cit>: <ref cRef="Jes." loc="54,3">Jes. 54,3</ref>; <ref cRef="Deut." loc="2,27">Deut. 2,27</ref>; <ref cRef="Deut." loc="5,29">Deut. 5,29</ref>; <ref cRef="Deut. " loc="28,14">Deut.  28,14</ref>; <ref cRef="Num." loc="20,17">Num. 20,17</ref>; <ref cRef="Jos." loc="23,6">Jos. 23,6</ref>; <ref cRef="1 Reg." loc="6,12">1 Reg. 6,12</ref>, vel <foreign xml:lang="gez">ፀጋመ፡</foreign> <ref cRef="Gen." loc="13,9">Gen. 13,9</ref>; <ref cRef="Gen." loc="24,49">Gen. 24,49</ref>.</sense> </sense> </sense>
               
