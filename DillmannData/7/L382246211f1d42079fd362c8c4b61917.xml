               <sense xml:lang="la" source="#dillmann" xml:id="Dmain"> (<gramGrp>
                     <pos expand="participium">part.</pos>
                  </gramGrp>) <gramGrp>
                     <pos expand="adjectivum">adj.</pos>
                  </gramGrp>
                  <cit type="translation" xml:lang="la">
                     <quote>alius</quote>
                  </cit>, <cit type="translation" xml:lang="la">
                     <quote>alter</quote>
                  </cit>, et <gramGrp>
                     <pos expand="adjectivum">adj.</pos>
                  </gramGrp> et substantive: <foreign xml:lang="gez">ዘእምባዕድ፡ ዘመድ፡</foreign>
                  <ref cRef="Lev." loc="22,10">Lev. 22,10</ref>; <foreign xml:lang="gez">ባዕድ፡
                     ሰብእ፡</foreign>
