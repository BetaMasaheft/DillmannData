                  <ref cRef="Judith" loc="12,10">Judith 12,10</ref>; <ref cRef="Hen." loc="89,35">Hen. 89,35</ref>; <foreign xml:lang="gez">አልቦ፡ ባዕደ፡ እንበሌሁ፡ </foreign>
                  <ref cRef="Deut." loc="4,35">Deut. 4,35</ref>; <ref cRef="Jes." loc="64,4">Jes.
                     64,4</ref>; <foreign xml:lang="gez">ገራህተ፡ ባዕድ፡</foreign>
                  <ref cRef="Ruth" loc="2,8">Ruth 2,8</ref>; <ref cRef="Ruth" loc="2,22">Ruth
                     2,22</ref>; <foreign xml:lang="gez">ዘዘራእኩ፡ ባዕድ፡ ይብልዖ፡</foreign>
                  <ref cRef="Job" loc="31,8">Job 31,8</ref>; <ref cRef="Job" loc="30,24">Job
                     30,24</ref>; <foreign xml:lang="gez">የኀድግ፡ ኵሎ፡ ለባዕድ፡</foreign>
                  <ref cRef="Sir." loc="11,19">Sir. 11,19</ref>; <foreign xml:lang="gez">ይዘግብ፡
                     ለባዕድ፡</foreign>
                  <ref cRef="Sir." loc="14,4">Sir. 14,4</ref>; <foreign xml:lang="gez">ቤተ፡ ባዕድ፡ </foreign>
                  <ref cRef="Sir." loc="21,23">Sir. 21,23</ref>; <foreign xml:lang="gez">እንዘ፡ ባዕድ፡
                     ይነግር፡ ኢታይድዕ፡ ቃለከ፡</foreign>
                  <ref cRef="Sir. " loc="35,9">Sir. 35,9</ref>; <ref cRef="Dan." loc="5,17">Dan.
                     5,17</ref>; <ref cRef="Prov." loc="27,2">Prov. 27,2</ref>; <ref cRef="Koh." loc="7,22">Koh. 7,22</ref>; <ref cRef="Ps." loc="104,42">Ps. 104,42</ref>; <ref cRef="Jes." loc="42,8">Jes. 42,8</ref>; <foreign xml:lang="gez">መኑ፡ ባዕድ፡ ዘይመይጥ፡
                     ምክሮሙ፡</foreign>
                  <ref cRef="Jes." loc="44,25">Jes. 44,25</ref>; <ref cRef="Joh." loc="4,38">Joh.
                     4,38</ref>; <ref cRef="Marc." loc="15,31">Marc. 15,31</ref>; <ref cRef="1 Petr." loc="4,15">1 Petr. 4,15</ref>; <lbl expand="cum">c.</lbl>
                  <foreign xml:lang="gez">እምነ፡</foreign>, ut: <foreign xml:lang="gez">ኢይርአይ፡ ባዕደ፡
                     እምነ፡ ማእድ፡ወጻሕል፡</foreign> (<cit type="translation" xml:lang="la">
                     <quote>nihil aliud quam mensam</quote>
                  </cit>) Reg. Pach. <sense n="a" xml:id="Da">
                     <cit type="translation" xml:lang="la">
                        <quote>caeteri</quote>
                     </cit>: <foreign xml:lang="gez">ወባዕደኒ፡ ኵሎ፡ ተግባሮሙ፡</foreign>
                     <ref cRef="2 Esr." loc="23,16">2 Esr. 23,16</ref>; <ref cRef="Ps." loc="147,9">Ps. 147,9</ref>; <ref cRef="Asc. Jes." loc="6,16">Asc. Jes. 6,16</ref>;
                        <ref cRef="Marc." loc="4,19">Marc. 4,19</ref>; <ref cRef="1 Cor." loc="11,34">1 Cor. 11,34</ref>.</sense>
                  <sense n="b" xml:id="Db">
                     <cit type="translation" xml:lang="la">
                        <quote>alienus</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>peregrinus</quote>
                     </cit>: <foreign xml:lang="gez">አሕዛበ፡ ባዕድ፡ </foreign>
                     <foreign xml:lang="grc">ἔθνη ἀλλότρια</foreign>
                     <ref cRef="Sir." loc="29,18">Sir. 29,18</ref>; <foreign xml:lang="gez">በብሔረ፡
                        ባዕድ፡</foreign>
                     <ref cRef="Jer." loc="5,19">Jer. 5,19</ref>; <ref cRef="Judith" loc="5,18">Judith 5,18</ref>; <foreign xml:lang="gez">ኮነ፡ ለባዕድ፡ ርስትነ፡</foreign>
                     <ref cRef="Thren." loc="5,2">Thren. 5,2</ref>; <foreign xml:lang="gez">ለባዕድ፡
                        አማልክት፡</foreign>
                     <ref cRef="Jer." loc="16,13">Jer. 16,13</ref>.</sense>
                  <sense n="c" xml:id="Dc">
                     <foreign xml:lang="gez">እንተ፡ ባዕድ፡</foreign>
                     <foreign xml:lang="grc">ἄλλοθεν </foreign>
                     <cit type="translation" xml:lang="la">
                        <quote>aliunde</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>alibi</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>aliorsum</quote>
                     </cit>
                     <ref cRef="Esth." loc="4,14">Esth. 4,14</ref> (<lbl expand="videas">vid.</lbl>
                     etiam <ref cRef="Ruth" loc="2,8">Ruth 2,8</ref>).</sense>
               </sense>
               
