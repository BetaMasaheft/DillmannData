         <div>
            <entry xml:id="La4ac4bce13f34809b886daf2c23d7b31" n="10051">
               <form>
                  <foreign xml:lang="gez">ተፈወሰ</foreign>
               </form>
               <sense xml:lang="la" source="#dillmann" xml:id="Dmain"> <subc>III,2</subc> <cit type="translation" xml:lang="la">
                     <quote>sanari</quote>
                  </cit>, <cit type="translation" xml:lang="la">
                     <quote>curari</quote>
                  </cit>: <ref cRef="Jer." loc="28,8">Jer. 28,8</ref>; <ref cRef="4 Esr." loc="6,80">4 Esr. 6,80</ref>; <ref cRef="Luc." loc="9,11">Luc. 9,11</ref>; F.M. 5,5; <lbl expand="cum">c.</lbl>
                  <foreign xml:lang="gez">እምነ፡</foreign> morbi vulnerisve: <ref cRef="Luc." loc="5,5">Luc. 5,5</ref>; <ref cRef="Luc. " loc="6,17">Luc. 6,17</ref>;
                     <foreign xml:lang="gez">ከመ፡ ይትፈወስ፡ እምቍስሉ፡</foreign>
                  <ref cRef="2 Par." loc="22,6">2 Par. 22,6</ref>; F.M. 3,7; <lbl expand="cum">c.</lbl>
                  <gramGrp>
                     <case value="accusativus">Acc.</case>
                  </gramGrp> vulneris: <foreign xml:lang="gez">ኢይበቍዐከ፡ ዘተፈወስከ፡ ቍስለ፡</foreign>
                  <ref cRef="Jer." loc="37,13">Jer. 37,13</ref>; <foreign xml:lang="gez">ሀለወ፡ ይትፌወስ፡
                     ንድፈቶ፡</foreign>
                  <ref cRef="4 Reg." loc="9,16">4 Reg. 9,16</ref>; <lbl expand="cum">c.</lbl>
                  <gramGrp>
                     <case value="accusativus">Acc.</case>
                  </gramGrp> membri: <foreign xml:lang="gez">ኢተፈወሰ፡ እገሪሁ፡</foreign>
                  <ref cRef="2 Reg." loc="19,24">2 Reg. 19,24</ref>. <sense n="a" xml:id="Da">
                     <cit type="translation" xml:lang="la">
                        <quote>medicinam</quote>
                     </cit>
                     <cit type="translation" xml:lang="la">
                        <quote>adhibere</quote>
                     </cit>: <foreign xml:lang="gez">እንበለ፡ ትሕምም፡ ተፈወስ፡</foreign>
                     <foreign xml:lang="grc">πρὸ ἀῤῥωστίας θεραπεύου</foreign>
                     <ref cRef="Sir." loc="18,19">Sir. 18,19</ref>.</sense>
                  <sense n="b" xml:id="Db">
                     <cit type="translation" xml:lang="la">
                        <quote>salubrem</quote>
                     </cit>
                     <cit type="translation" xml:lang="la">
                        <quote>reddi</quote>
                     </cit>: <foreign xml:lang="gez">ተፈወሰ፡ ውእቱ፡ ማይ፡</foreign>
                     <ref cRef="4 Reg." loc="2,22">4 Reg. 2,22</ref>.</sense>
               </sense>
               
