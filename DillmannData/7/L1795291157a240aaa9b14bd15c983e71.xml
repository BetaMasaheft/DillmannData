                        <ref cRef="Dan." loc="2,34">Dan. 2,34</ref>; <ref cRef="Judith" loc="7,30">Judith 7,30</ref>; <ref cRef="Job" loc="20,7">Job 20,7</ref>; <ref cRef="Sir." loc="10,13">Sir. 10,13</ref>; <foreign xml:lang="grc">καθόλου</foreign>
                     <ref cRef="Hez. " loc="17,14">Hez. 17,14</ref>; <foreign xml:lang="grc">εἰς τὸ
                        λοιπόν</foreign>
                     <ref cRef="Judith" loc="11,3">Judith 11,3</ref>; <foreign xml:lang="grc">ἐνδελεχῶς</foreign>
                     <ref cRef="Sir." loc="20,26">Sir. 20,26</ref>; <foreign xml:lang="grc">εἰς τὸν
                        αἰῶνα</foreign> vel <foreign xml:lang="grc">ἕως αἰῶνος</foreign>
                     <ref cRef="Sir." loc="11,33">Sir. 11,33</ref>; <ref cRef="Sir." loc="24,9">Sir.
                        24,9</ref>; <ref cRef="4 Esr." loc="7,10">4 Esr. 7,10</ref>; <ref cRef="4 Esr." loc="8,35">4 Esr. 8,35</ref>; <ref cRef="4 Esr." loc="9,21">4
                        Esr. 9,21</ref>; et <foreign xml:lang="gez">እስከ፡ ለዝሉፉ፡</foreign>
                     <ref cRef="Job" loc="23,3">Job 23,3</ref>.</sense>
                  <sense n="2" xml:id="D2"> de hominibus <sense n="a" xml:id="D2a">
                        <cit type="translation" xml:lang="la">
                           <quote>pertinax</quote>
                        </cit>, <cit type="translation" xml:lang="la">
                           <quote>pervicax</quote>
                        </cit>, <cit type="translation" xml:lang="la">
                           <quote>obstinatus</quote>
                        </cit>: <foreign xml:lang="grc">ἄτακτος</foreign>
                        <ref cRef="1 Thess." loc="5,14">1 Thess. 5,14</ref>; <foreign xml:lang="grc">ἀνυπότακτοι </foreign>
                        <foreign xml:lang="gez">ዝሉፋን፡ እለ፡ ኢይትኤዘዙ፡</foreign>
                        <ref cRef="Tit." loc="1,6">Tit. 1,6</ref>; 1 <ref cRef="Tim." loc="1,9">Tim.
                           1,9</ref>; <foreign xml:lang="gez">ኢትኩን፡ ዝሉፈ፡ በልሳንከ፡</foreign>
                        <foreign xml:lang="grc">μὴ ἐνέδρευε τῇ γλώσσῃ σου</foreign>
                        <ref cRef="Sir." loc="5,14">Sir. 5,14</ref>; castiga filium, <foreign xml:lang="gez">ከመ፡ ኢይኩን፡ ዝሉፈ፡</foreign>
                        <ref cRef="Did." loc="22">Did. 22</ref>; <foreign xml:lang="gez">ዝሉፍ፡
                           ለይትገሠጽ፡</foreign>
                        <ref cRef="Kid." loc="9.">Kid. f. 9.</ref>
                     </sense>
                     <sense n="b" xml:id="D2b"> celeriter procedens i.e. <sense n="α" xml:id="D2bα">
                           <cit type="translation" xml:lang="la">
                              <quote>praeceps</quote>
                           </cit>, <cit type="translation" xml:lang="la">
                              <quote>temerarius</quote>
                           </cit>: <foreign xml:lang="grc">προπετής </foreign>
                           <ref cRef="2 Tim." loc="3,4">2 Tim. 3,4</ref>; <cb n="1036" xml:id="c1036"/>
                           <foreign xml:lang="grc">προαλής </foreign>
                           <ref cRef="Sir." loc="30,8">Sir. 30,8</ref>.</sense>
                        <sense n="β" xml:id="D2bβ">
                           <cit type="translation" xml:lang="la">
                              <quote>audax</quote>
                           </cit>, <cit type="translation" xml:lang="la">
                              <quote>insolens</quote>
                           </cit>, <foreign xml:lang="gez">ዝልፍት፡ </foreign>
                           <foreign xml:lang="grc">θρασεῖα</foreign>
                           <ref cRef="Sir." loc="22,5">Sir. 22,5</ref>; <foreign xml:lang="gez">ነፍስ፡
                              ዝልፍት፡</foreign>
                           <foreign xml:lang="grc">ψυχὴ τολμηρά </foreign>
                           <ref cRef="Sir." loc="19,3">Sir. 19,3</ref>; <foreign xml:lang="grc">ὑπερήφανος</foreign>
                           <ref cRef="Rom." loc="1,30">Rom. 1,30</ref>; <foreign xml:lang="gez">ኅቡላን፡ ወዝሉፋን፡</foreign>
                           <foreign xml:lang="grc">τολμηταὶ αὐθάδεις</foreign>
                           <ref cRef="2 Petr." loc="2,10">2 Petr. 2,10</ref>; <foreign xml:lang="grc">λαπιστής</foreign>
                           <ref cRef="Sir." loc="20,7">Sir. 20,7</ref>. – (<ref cRef="Job" loc="28,18">Job 28,18</ref>
                           <foreign xml:lang="gez">ዝሉፍ፡ ወዕንቡዝ፡</foreign> pro <foreign xml:lang="grc">μετέωρα καὶ γαβίς</foreign>).</sense>
                     </sense>
                  </sense>
