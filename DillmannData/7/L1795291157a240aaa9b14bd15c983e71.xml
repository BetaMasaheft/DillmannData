                  <sense n="1" xml:id="D1">
                     <cit type="translation" xml:lang="la">
                        <quote>continuus</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>perpetuus</quote>
                     </cit>, sed non nisi adverbialiter: <cit type="translation" xml:lang="la">
                        <quote>perpetuo</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>in perpetuum</quote>
                     </cit> (<cit type="translation" xml:lang="la">
                        <quote>aeternum</quote>
                     </cit>), <cit type="translation" xml:lang="la">
                        <quote>ad finem usque</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>prorsus</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>plane</quote>
                     </cit>, <foreign xml:lang="gez">ዝሉፈ፡</foreign>
                     <ref cRef="Ex." loc="21,6">Ex. 21,6</ref>, et plerumque <foreign xml:lang="gez">ለዝሉፉ፡</foreign> (<bibl>
                        <ptr target="bm:Dillmann1857Gramm"/>gramm.</bibl> § 163,2) <ref cRef="Ex." loc="27,21">Ex. 27,21</ref>; <foreign xml:lang="grc">διὰ παντός</foreign>
                     <ref cRef="Lev." loc="11,42">Lev. 11,42</ref>; <foreign xml:lang="grc">εἰς
                        τέλος</foreign>
