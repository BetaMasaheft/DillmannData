            <entry xml:id="L26fa38a053764cb79b8e00c4324fca11" n="5228">
               <form>
                  <foreign xml:lang="gez">እንግዳ</foreign>
               </form>
               <sense xml:lang="la" source="#dillmann" xml:id="Dmain">
                  <gramGrp>
                     <pos expand="Substantivum">subst.</pos>
                  </gramGrp> (<bibl>
                     <ptr target="bm:Dillmann1857Gramm"/>gramm.</bibl> § 137,5), <lbl expand="collatio, -is vel collectivum, -e">coll.</lbl>, deinde <lbl expand="singularis">sing.</lbl>, <lbl expand="pluralis">Pl.</lbl>
                  <foreign xml:lang="gez">አናግድ፡</foreign>, <cit type="translation" xml:lang="la">
                     <quote>peregrini</quote>
                  </cit>, <cit type="translation" xml:lang="la">
                     <quote>hospites</quote>
                  </cit>, <cit type="translation" xml:lang="la">
                     <quote>advenae</quote>
                  </cit>; <sense n="a" xml:id="Da">
                     <lbl expand="collatio, -is vel collectivum, -e">coll.</lbl>: <foreign xml:lang="grc">ξένοι</foreign>
                     <ref cRef="1 Reg." loc="9,13">1 Reg. 9,13</ref>; <ref cRef="Matth. " loc="27,7">Matth. 27,7</ref>; <ref cRef="2 Cor." loc="5,6">2 Cor. 5,6</ref> Platt;
                        <ref cRef="Kid." loc="17,">Kid. f. 17,</ref> al.; <foreign xml:lang="gez">ዘይከውን፡ እንግዳ፡ በውስቴትክሙ፡</foreign> Rel. Bar.; <foreign xml:lang="gez">ማኅደረ፡
                        እንግዳ፡</foreign>
                     <cit type="translation" xml:lang="la">
                        <quote>hospitium</quote>
                     </cit>
                     <ref cRef="II Can. Nic." loc="77.">II Can. Nic. 77.</ref>
                  </sense>
                  <sense n="b" xml:id="Db">
                     <lbl expand="singularis">sing.</lbl>
                     <foreign xml:lang="gez">እስመ፡ እንግዳ፡ አነ፡</foreign>
                     <foreign xml:lang="grc">εἰμὶ ξένη</foreign>
                     <ref cRef="Ruth" loc="2,10">Ruth 2,10</ref>; <ref cRef="2 Reg." loc="15,19">2
                        Reg. 15,19</ref>; <ref cRef="Matth." loc="25,44">Matth. 25,44</ref>; Fal. f.
                     57; <ref cRef="Sx. Masc." loc="23 ">Sx. Masc. 23 </ref>Enc.</sense>
                  <sense n="c" xml:id="Dc">
                     <lbl expand="pluralis">Pl.</lbl>: <foreign xml:lang="gez">አናግድ፡</foreign>
                     <ref cRef="F.N." loc="16">F.N. 16</ref>; <foreign xml:lang="gez">ጸሊአ፡
                        አናግድ፡</foreign>
                     <foreign xml:lang="grc">μισοξενία</foreign> (<cit type="translation" xml:lang="la">
                        <quote>inhospitalitas</quote>
                     </cit>) <ref cRef="Sap." loc="19,13">Sap. 19,13</ref>; <foreign xml:lang="gez">አክብሮ፡ አናግድ፡</foreign> Fal.</sense>
               </sense>
               
