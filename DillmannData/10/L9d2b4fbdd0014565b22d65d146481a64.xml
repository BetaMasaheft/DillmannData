               <sense xml:lang="la" source="#dillmann" xml:id="Dmain">, rarius <foreign xml:lang="gez">ፈትወ፡</foreign> (<ref cRef="Act." loc="25,25">Act. 25,25</ref> Platt; <ref cRef="Deut." loc="14,26">Deut. 14,26</ref> al.) <subc>I,1</subc> [<foreign xml:lang="he">פתה</foreign> <foreign xml:lang="syr">ܦܬܐ</foreign> <foreign xml:lang="ar">فتا</foreign>; origo est in <cit type="translation" xml:lang="la">
