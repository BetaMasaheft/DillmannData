                     <ref cRef="Sir." loc="20,26">Sir. 20,26</ref>; <ref cRef="Jer." loc="27,2">Jer.
                        27,2</ref>; <ref cRef="Jer." loc="30,12">Jer. 30,12</ref>; <ref cRef="Koh." loc="10,17">Koh. 10,17</ref>; <ref cRef="Job" loc="6,20">Job 6,20</ref>;
                        <foreign xml:lang="gez">ይሑሩ፡ ተኀፊሮሙ፡</foreign>
                     <ref cRef="Jes." loc="45,16">Jes. 45,16</ref>; <ref cRef="Ps. " loc="6,10">Ps.
                        6,10</ref>; <ref cRef="Ps." loc="69,2">Ps. 69,2</ref>; <ref cRef="Ps." loc="69,4">Ps. 69,4</ref>; <ref cRef="Ps." loc="70,1">Ps. 70,1</ref>; <ref cRef="Prov." loc="1,22">Prov. 1,22</ref>; <ref cRef="Rom." loc="9,33">Rom.
