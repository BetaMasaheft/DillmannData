         <div>
            <entry xml:id="L8c7ed627cc3f4cf7b8eba8ff2eced4cc" n="8124">
               <form>
                  <foreign xml:lang="gez">አድወየ</foreign>
               </form>
               <sense xml:lang="la" source="#dillmann" xml:id="Dmain"> <subc>II,1</subc> <cit type="translation" xml:lang="la">
                     <quote>aegrotum reddere</quote>
                  </cit>, <cit type="translation" xml:lang="la">
                     <quote>morbo afficere</quote>
                  </cit>, <cit type="translation" xml:lang="la">
                     <quote>morbum afferre</quote>
                  </cit>, <lbl expand="cum">c.</lbl>
                  <gramGrp>
                     <case value="accusativus">Acc.</case>
                  </gramGrp>
                  <lbl expand="persona">pers.</lbl>: <foreign xml:lang="gez">እንተ፡ አድወያ፡
                     ጋኔን፡</foreign>
                  <ref cRef="Luc. " loc="13,11">Luc. 13,11</ref>; <foreign xml:lang="gez">መጽአ፡ ላዕለ፡
                     ርእሱ፡ ሐሩር፡ ወአድወዮ፡ ወሞተ፡</foreign>
                  <ref cRef="Judith" loc="8,3">Judith 8,3</ref>; <foreign xml:lang="gez">ለእሉ፡ አድወዮሙ፡
                     ፍርሀት፡</foreign>
                  <ref cRef="Sap." loc="17,8">Sap. 17,8</ref>; omisso <gramGrp>
                     <case value="accusativus">Acc.</case>
                  </gramGrp>: <foreign xml:lang="gez">ያደዊ፡ ብዙኅ፡ በሊዕ፡</foreign>
                  <ref cRef="Sir." loc="37,30">Sir. 37,30</ref>; <ref cRef="1 Reg." loc="2,10">1
                     Reg. 2,10</ref>; Deus <foreign xml:lang="gez">ያደዊ፡ ወያሐዩ፡</foreign>
                  <ref cRef="Clem." loc="271.">Clem. f. 271.</ref>
                  <sense n="a" xml:id="Da">
                     <cit type="translation" xml:lang="la">
                        <quote>acerbum dolorem efficere</quote>
                     </cit>: <foreign xml:lang="gez">ይኰንንዎ፡ ኵነኔ፡ ዘያደዊ፡ </foreign> F.M. 20,3.</sense>
                  <sense n="b" xml:id="Db">
                     <cit type="translation" xml:lang="la">
                        <quote>morbiferum</quote>
                     </cit> vel <cit type="translation" xml:lang="la">
                        <quote>insalubrem esse</quote>
                     </cit>: <foreign xml:lang="gez">ምርዓይ፡ ማዩ፡ ዘያደዊ፡</foreign> F.N. 36.</sense>
               </sense>
               
