            <entry xml:id="L8ef21ee8ee7c4b9c812bb8cd5b0dc257" n="5219">
               <form>
                  <foreign xml:lang="gez">ተናገደ</foreign>
               </form>
               <sense xml:lang="la" source="#dillmann" xml:id="Dmain"> <subc>III,3</subc> <sense n="1" xml:id="D1">
                     <cit type="translation" xml:lang="la">
                        <quote>hinc illinc proficisci</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>peregrinari</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>hospitari</quote>
                     </cit>: <foreign xml:lang="gez">ተናግዶቱ፡ ለአብርሃም፡</foreign>
                     <ref cRef="Lit." loc="170,4">Lit. 170,4</ref>; <foreign xml:lang="gez">እለ፡
                        ይትናገዱ፡ አኀው፡</foreign>
                     <foreign xml:lang="grc">ἐπιξενούμενοι ἀδελφοί</foreign> II <ref cRef="Can. Ap." loc="39.">Can. Ap. 39.</ref>
                  </sense>
                  <sense n="2" xml:id="D2">
                     <cit type="translation" xml:lang="la">
                        <quote>negotiandi causa peregrinari</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>mercaturam facere</quote>
                     </cit>: <foreign xml:lang="gez">ዕደው፡ ጽሑፋን፡ ወሠየጥ፡ እለ፡ ይትናገዱ፡ ወያመጽኡ፡
                        ሎቱ፡</foreign>
                     <ref cRef="2 Par." loc="9,14">2 Par. 9,14</ref>; <foreign xml:lang="gez">ሥርዐተ፡
                        ተናግዶ፡ ወረባሕ፡</foreign> F.N. in proleg. (p. 4); <foreign xml:lang="gez">ምጽዋት፡
                        ተናግዶ፡ አምላካዊት፡ ረባሒት፡ ይእቲ፡</foreign> F.N. 16.</sense>
               </sense>
               
