            <entry xml:id="L9c429bd9562d46c1836edd69d6b86e25" n="4457">
               <form>
                  <foreign xml:lang="gez">ኀሙስ</foreign>
               </form>
               <sense xml:lang="la" source="#dillmann" xml:id="Dmain">
                  <gramGrp>
                     <pos expand="nomen">nom.</pos>
                  </gramGrp> temporale, <sense n="1" xml:id="D1">
                     <cit type="translation" xml:lang="la">
                        <quote>quinque</quote>
                     </cit> (dies) et <cit type="translation" xml:lang="la">
                        <quote>quintus</quote>
                     </cit> (dies), ut: <foreign xml:lang="gez">ተዐገሡ፡ ዓዲ፡ ኀሙሰ፡ መዋዕለ፡</foreign>
                     <ref cRef="Judith" loc="7,30">Judith 7,30</ref>; <foreign xml:lang="gez">እምድኅረ፡
                        ኀሙስ፡ መዋዕል፡ </foreign>
                     <ref cRef="Judith" loc="8,9">Judith 8,9</ref>; <foreign xml:lang="gez">መዋዕለ፡
                        ኀሙስ፡</foreign>
                     <cit type="translation" xml:lang="la">
                        <quote>quinque dies</quote>
                     </cit>
                     <ref cRef="Sx. Genb." loc="21 ">Sx. Genb. 21 </ref>Enc.; de diebus mensis
                     (supputando sive inde a novilunio sive a plenilunio): <foreign xml:lang="gez">አመ፡ ኀሙሱ፡ ለሠርቅ፡</foreign>
                     <ref cRef="Hez." loc="33,21">Hez. 33,21</ref>; <foreign xml:lang="gez">አመ፡ ዕለተ፡
                        ዐሡሩ፡ ወኀሙሱ፡ ለሠርቅ፡</foreign>
                     <ref cRef="Ex." loc="23,6">Ex. 23,6</ref>; <foreign xml:lang="gez">በኀሙሱ፡
                        ለጽልመት፡</foreign>
                     <foreign xml:lang="grc">εἰκάδι τοῦ μηνός </foreign>
                     <ref cRef="2 Esr." loc="10,9">2 Esr. 10,9</ref>; <ref cRef="Zach." loc="7,5">Zach. 7,5</ref>; de diebus hebdomadis: <foreign xml:lang="gez">አመ፡
                        ኀሙስ፡</foreign>
                     <ref cRef="4 Esr." loc="4,54">4 Esr. 4,54</ref>.</sense>
                  <sense n="2" xml:id="D2"> hebdomadis <cit type="translation" xml:lang="la">
                        <quote>feria quinta</quote>
                     </cit>, ut: <foreign xml:lang="gez">በዕለተ፡ ኀሙስ፡ </foreign>
                     <ref cRef="Did." loc="29">Did. 29</ref>; Lit. Epiph.; <foreign xml:lang="gez">ዘኀሙስ፡ ዘፋሲካ፡</foreign> Cod. Mus. Brit. XXIX; al.</sense>
               </sense>
               
