            <entry xml:id="Le717c05a96034bb5af34288fea0bbbae" n="12966">
               <form><foreign xml:lang="gez">ቅንየት</foreign></form>
               <sense xml:id="Dmain" xml:lang="en" source="#dillmann">
                  <foreign xml:lang="gez">ቅንየት፡</foreign> et <foreign xml:lang="gez">ቅኔት፡</foreign>
                  <gramGrp>
                     <pos expand="nomen actionis">n. act.</pos>
                  </gramGrp> et <gramGrp>
                     <pos expand="Substantivum">subst.</pos>
                  </gramGrp>, <lbl expand="pluralis">Pl.</lbl>
                  <foreign xml:lang="gez">ቅንየታት፡</foreign> et <foreign xml:lang="gez">ቅኔታት፡</foreign>
                  <sense n="1" xml:id="D1">
                     <cit type="translation" xml:lang="la">
                        <t:quote xmlns:t="http://www.tei-c.org/ns/1.0"> dominatio </t:quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <t:quote xmlns:t="http://www.tei-c.org/ns/1.0"> dominium </t:quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <t:quote xmlns:t="http://www.tei-c.org/ns/1.0"> possessio </t:quote>
                     </cit>: <foreign xml:lang="gez">ቅንየትከ፡ ወምልክናከ፡ ኵሎ፡</foreign>
                     <foreign xml:lang="grc">τὸ πάντων σε δεσπόξειν</foreign>
                     <ref cRef="Sap." loc="12,16">Sap. 12,16</ref>; <foreign xml:lang="gez">ቅንየት፡
                        ወምልክና፡</foreign>
                     <ref cRef="F.N." loc="40">F.N. 40</ref>; <foreign xml:lang="gez">እመ፡
                        አኀዝ፡</foreign> (pignus) <foreign xml:lang="gez">ኢወፅአ፡ እምእዴሁ፡ ኀበ፡ ቅንየተ፡
                        ካልኡ፡</foreign>
                     <ref cRef="F.N." loc="27">F.N. 27</ref>. </sense>
                  <sense n="2" xml:id="D2">
                     <cit type="translation" xml:lang="la">
                        <t:quote xmlns:t="http://www.tei-c.org/ns/1.0"> actus subjiciendi </t:quote>
                     </cit> vel <cit type="translation" xml:lang="la">
                        <t:quote xmlns:t="http://www.tei-c.org/ns/1.0"> subjugandi </t:quote>
                     </cit>: <foreign xml:lang="gez">እስከ፡ ትቀንይዋ፡ ለሀገር፡ እስመ፡ ቀርበ፡ ቅንየታ፡</foreign>
                     <ref cRef="Jsp." loc="304.">Jsp. p. 304.</ref>
                  </sense>
                  <sense n="3" xml:id="D3">
                     <cit type="translation" xml:lang="la">
                        <t:quote xmlns:t="http://www.tei-c.org/ns/1.0"> servitus </t:quote>
                     </cit>, <foreign xml:lang="grc">δουλεία</foreign>
                     <ref cRef="Esth." loc="7,4">Esth. 7,4</ref>; <ref cRef="Ex." loc="6,6">Ex.
                        6,6</ref>; <foreign xml:lang="gez">ቤተ፡ ቅንየት፡</foreign>
                     <ref cRef="Ex." loc="13,3">Ex. 13,3</ref>; <ref cRef="Ex." loc="13,14">Ex.
                        13,14</ref>; <ref cRef="Lev." loc="26,45">Lev. 26,45</ref>; <ref cRef="Deut." loc="5,6">Deut. 5,6</ref>; <ref cRef="Deut." loc="6,12">Deut.
                        6,12</ref>; <ref cRef="Deut." loc="7,8">Deut. 7,8</ref>; <ref cRef="Lit." loc="173,1">Lit. 173,1</ref>. </sense>
                  <sense n="4" xml:id="D4">
                     <cit type="translation" xml:lang="la">
                        <t:quote xmlns:t="http://www.tei-c.org/ns/1.0"> creatura serva </t:quote>
                     </cit>
                     <ref cRef="Hen." loc="75,3">Hen. 75,3</ref>
                     <lbl expand="cum">c.</lbl>
                     <lbl expand="varia lectio">var.</lbl>; <foreign xml:lang="gez">ባሕር፡ አመቱ፡
                        ወወሓይዝተ፡ አፍላግ፡ ቅንየታቲሁ፡</foreign> Lit. Epiph. </sense>
                  <ref type="hacohen" n=" 449"/>
               </sense>
