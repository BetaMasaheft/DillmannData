            <entry xml:id="L8da390837deb4e9186e29114db55358f" n="12783">
               <form><foreign xml:lang="gez">የሻለቃ</foreign></form>
               <sense xml:id="Tmain" xml:lang="en" source="#traces">
                  <cit type="transcription" xml:lang="gez">
                     <quote>yašālaqā</quote>
                  </cit>, <foreign xml:lang="gez">ሻለቃ፡</foreign>
                  <cit type="transcription" xml:lang="gez">
                     <quote>šālaqā</quote>
                  </cit>, <foreign xml:lang="gez">ሸለቃ፡</foreign>
                  <cit type="transcription" xml:lang="gez">
                     <quote>šalaqā</quote>
                  </cit>
                  <gramGrp><pos expand="Substantivum">subst.</pos></gramGrp> (title) ‘<cit type="translation" xml:lang="en">
                     <quote>major</quote>
                  </cit> (army)’ <bibl><ptr target="bm:Kane1990DictAmhVol1"/><citedRange unit="page"> 606b</citedRange></bibl>
                  <foreign xml:lang="gez">ሸለቃ፡ ተስፋ፡ ጽዮን፡</foreign> ‘<cit type="translation" xml:lang="fr">
                     <quote>le šalaqā Tasfā Ṣyon</quote>
                  </cit>’ <bibl><ptr target="bm:ContiRossini1909LiberT"/><citedRange unit="page"> 51
                        l. 28 (ed.)</citedRange></bibl>, <bibl><ptr target="bm:ContiRossini1910LiberV"/><citedRange unit="page"> 62 l. 13
                        (tr.)</citedRange></bibl>; <foreign xml:lang="gez">ሸለቃ፡ ፎላ፡ ወልደ፡ አቤቶ፡
                     ኪሩቤል፡</foreign> ‘<cit type="translation" xml:lang="fr">
                     <quote>le šalaqā Folā, fils de l’abēto Kirubēl</quote>
                  </cit>’ <bibl><ptr target="bm:ContiRossini1909LiberT"/><citedRange unit="page"> 52
                        l. 19 (ed.)</citedRange></bibl>, <bibl><ptr target="bm:ContiRossini1910LiberV"/><citedRange unit="page"> 63 l. 6
                        (tr.)</citedRange></bibl>; <foreign xml:lang="gez">ሰለዋ፡ ሸለቃ፡ ወልደ፡
                     ሚካኤል፡</foreign> ‘<cit type="translation" xml:lang="fr">
                     <quote>le Salawā šalaqā Walda Mikāʾēl</quote>
                  </cit>’ <bibl><ptr target="bm:ContiRossini1909LiberT"/><citedRange unit="page"> 53
                        l. 18 (ed.)</citedRange></bibl>, <bibl><ptr target="bm:ContiRossini1910LiberV"/><citedRange unit="page"> 64 l. 12–13
                        (tr.)</citedRange></bibl>
                  <sense n="E" xml:id="TE">from Amharic <foreign xml:lang="am">የ-ሽህ/ሺ</foreign><cit type="transcription" xml:lang="gez">
                        <quote>ya-šǝh/ši</quote>
                     </cit> ‘of thousand’ + <foreign xml:lang="am">አለቃ</foreign>
                     <cit type="transcription" xml:lang="gez">
                        <quote>ʾalaqā</quote>
                     </cit> ‘chief’</sense>
                  <nd/>
               </sense>
