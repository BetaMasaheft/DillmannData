               <form><foreign xml:lang="gez">ትሕልፍት</foreign></form>
               <sense xml:id="Tmain" xml:lang="en" source="#traces">
                  <cit type="transcription" xml:lang="gez">
                     <t:quote xmlns:t="http://www.tei-c.org/ns/1.0">tǝḥlǝft</t:quote>
                  </cit>, <foreign xml:lang="gez">ትኅልፍት፡</foreign>
