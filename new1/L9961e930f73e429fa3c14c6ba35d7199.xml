            <entry xml:id="L9961e930f73e429fa3c14c6ba35d7199" n="12767">
               <form><foreign xml:lang="gez">እቴጌ</foreign></form>
               <sense xml:id="Tmain" xml:lang="en" source="#traces">
                  <cit type="transcription" xml:lang="gez">
                     <quote>ʾǝtege</quote>
                  </cit>, <foreign xml:lang="gez">ኢቴጌ፡</foreign>
                  <cit type="transcription" xml:lang="gez">
                     <quote>ʾitege</quote>
                  </cit>, <foreign xml:lang="gez">ኢተጌ፡</foreign>
                  <cit type="transcription" xml:lang="gez">
                     <quote>ʾitage</quote>
                  </cit>
                  <gramGrp><pos expand="Substantivum">subst.</pos></gramGrp> (title) ‘consort of an
                  Ethiopian monarch, <cit type="translation" xml:lang="en">
                     <quote>Empress</quote>
                  </cit>, <cit type="translation" xml:lang="en">
                     <quote>Queen</quote>
                  </cit>
                  <bibl><ptr target="bm:Kane1990DictAmhVol2"/><citedRange unit="page">
                        1210a</citedRange></bibl>
                  <foreign xml:lang="gez">ወእምዝ፡ አዕረፉ፡ ኢቴጌ፡ ቀሾ፡</foreign> ‘<cit type="translation" xml:lang="fr">
                     <quote>Ensuite, mourut l’itēgē Qašo </quote>
                  </cit>’ <bibl><ptr target="bm:ContiRossini1909LiberT"/><citedRange unit="page"> 69
                        l. 14 (ed.)</citedRange></bibl>, <bibl><ptr target="bm:ContiRossini1910LiberV"/><citedRange unit="page"> 83 l. 31
                        (tr.)</citedRange></bibl>
                  <sense n="C" xml:id="TC">for <foreign xml:lang="gez">ቃለ፡ ኢተጌ፡</foreign>
                     <lbl expand="videas">vid.</lbl>
                     <ref type="external" target="https://betamasaheft.eu/Dillmann/lemma/L968149f7fc1748479a9f8482bc3f49c0"><foreign xml:lang="gez">ቃል፡</foreign></ref>
                  </sense>
                  <nd/>
               </sense>
