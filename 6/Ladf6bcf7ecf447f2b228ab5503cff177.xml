               <sense xml:lang="la" source="#dillmann" xml:id="Dmain"> <gramGrp> <pos expand="not found">subst.</pos> </gramGrp> <lbl expand="pluralis">Pl.</lbl> (a <lbl expand="not found">sing.</lbl> <foreign xml:lang="gez">በፂዕ፡</foreign> vel <foreign xml:lang="gez">በፂዐት፡</foreign>; <bibl>
                            <ptr target="bm:Dillmann1857Gramm"/>gramm.</bibl> § 140,3, a), rarius <foreign xml:lang="gez">በፅውዕ፡</foreign> vel <foreign xml:lang="gez">በፅዋዕ፡</foreign>, <lbl expand="pluralis">Pl.</lbl> <foreign xml:lang="gez">በፃውዓት፡</foreign>, <cit type="translation" xml:lang="la">
                            <quote> somnia </quote>
                        </cit> vel <cit type="translation" xml:lang="la">
                            <quote> commenta hariolorum </quote>
                        </cit>, in specie <cit type="translation" xml:lang="la">
                            <quote> ventriloquorum </quote>
                        </cit> vel <cit type="translation" xml:lang="la">
                            <quote> pythonicorum </quote>
                        </cit>: <foreign xml:lang="gez">ዘይገብር፡ ስብዐ፡ ወመቅሰመ፤ ሐረሰ፡ ወበፅዋዕ፡</foreign> (sic) <ref cRef="Clem." loc="144">Clem. f. 144</ref>; <foreign xml:lang="gez">እለ፡ ያነቅሃ፡ በፅውዐ፡</foreign> (v. <foreign xml:lang="gez">ያነቅሁ፡ በፃውዕ፡</foreign>) <foreign xml:lang="grc">ἐγγαστρίμυθοι</foreign> <ref cRef="Jes." loc="44,25">Jes. 44,25</ref>; <foreign xml:lang="gez">ዘበፃውዕ፡</foreign> <foreign xml:lang="grc">ἐγγαστρίμυθος</foreign> <ref cRef="Deut." loc="18,11">Deut. 18,11</ref>; <lbl expand="pluralis">Pl.</lbl> <foreign xml:lang="gez">እለ፡ በፃውዕ፡</foreign> <ref cRef="Jes." loc="8,19">Jes. 8,19</ref>; <foreign xml:lang="gez">በፃውዓት፡</foreign> <lbl expand="not found">v.</lbl> <foreign xml:lang="gez">እለ፡ በፃውዕ፡</foreign> <ref cRef="Jes." loc="19,3">Jes. 19,3</ref>; <foreign xml:lang="gez">ትቀድኁ፡ ለበፃውዕ፡</foreign> (v. <foreign xml:lang="gez">ለእለ፡ በፃውዕ፡</foreign>) <cit type="translation" xml:lang="la">
                            <quote> libatis pythonibus </quote>
                        </cit> (v. <cit type="translation" xml:lang="la">
                            <quote> pythonicis </quote>
                        </cit>) <foreign xml:lang="grc">τῇ τύχῃ</foreign> <ref cRef="Jes." loc="65,11">Jes. 65,11</ref>. </sense>
               
