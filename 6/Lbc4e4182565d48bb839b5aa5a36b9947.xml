            <entry xml:id="Lbc4e4182565d48bb839b5aa5a36b9947" n="2585">
               <form>
                  <foreign xml:lang="gez">ተሰርቀ</foreign>
               </form>
               <sense xml:lang="la" source="#dillmann" xml:id="Dmain"> <subc>III,1</subc> <sense n="1" xml:id="D1">
                     <cit type="translation" xml:lang="la">
                        <quote>furto auferri</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>surripi</quote>
                     </cit>: <ref cRef="Gen." loc="31,39">Gen. 31,39</ref>; <ref cRef="Clem." loc="225">Clem. f. 225</ref>; <foreign xml:lang="gez">ይሰረቅ፡ ንዋዩ፡</foreign>
                     <ref cRef="Sir." loc="36,30">Sir. 36,30</ref>; F.M. 17,1; <foreign xml:lang="gez">ቦ፡ ዘተሰርቀ፡ ንዋዩ፡</foreign>
                     <ref cRef="Chrys. Ta." loc="11">Chrys. Ta. 11</ref>; <foreign xml:lang="gez">ተሰርቀ፡ ንዋዩ፡ እምቤቱ፡</foreign>
                     <ref cRef="Phlx." loc="28">Phlx. 28</ref>; <foreign xml:lang="gez">ድንግልና፡
                        ዘኢይሰረቅ፡ </foreign> Ssal. Req.; <lbl expand="cum">c.</lbl>
                     <gramGrp>
                        <case value="accusativus">Acc.</case>
                     </gramGrp>
                     <lbl expand="persona">pers.</lbl>, cui aufertur: <ref cRef="Ex." loc="22,6">Ex.
                        22,6</ref>; <ref cRef="Jud." loc="17,2">Jud. 17,2</ref>.</sense>
                  <sense n="2" xml:id="D2"> reflex. <cit type="translation" xml:lang="la">
                        <quote>furtim</quote>
                     </cit> vel <cit type="translation" xml:lang="la">
                        <quote>clam se subducere</quote>
                     </cit>
                     <ref cRef="2 Reg." loc="193,">2 Reg. 193,</ref>.</sense>
                  <sense n="3" xml:id="D3">
                     <cit type="translation" xml:lang="la">
                        <quote>furto expilari</quote>
                     </cit>: <foreign xml:lang="gez">ማኅደር፡ ዘኢይሰረቅ፡</foreign> Lit. Joh.</sense>
               </sense>
               
