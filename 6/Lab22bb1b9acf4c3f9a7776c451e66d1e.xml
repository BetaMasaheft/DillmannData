            <entry xml:id="Lab22bb1b9acf4c3f9a7776c451e66d1e" n="10079">
               <form>
                  <foreign xml:lang="gez">ፈድፈደ</foreign>
               <rs type="root"/>
                    </form>
               <sense xml:lang="la" source="#dillmann" xml:id="Dmain"> quadril. I [quod equidem non cum Ludolfo et cum Hupfeldio Exerc. Aeth. p. 27 ad <foreign xml:lang="ar">فَدَّ</foreign> et <foreign xml:lang="ar">فَدْفَدَ</foreign> <cit type="translation" xml:lang="la">
                            <quote> currere </quote>
                        </cit> (<foreign xml:lang="gez">ፈጠነ፡</foreign>), sed ad <foreign xml:lang="he">פוּץ</foreign>, chald. <foreign xml:lang="he">פוּשׁ</foreign>, <foreign xml:lang="ar">فاض فضّ</foreign> (<foreign xml:lang="ar">فضل</foreign>) retulerim; de <foreign xml:lang="he">עדף</foreign> autem <lbl expand="videas">vid.</lbl> <foreign xml:lang="gez">ወደፈ፡</foreign>] <sense n="1" xml:id="D1"> <cit type="translation" xml:lang="la">
                                <quote> augescere </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> crescere </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> multiplicari </quote>
                            </cit>: <foreign xml:lang="gez">ፈድፈዱ፡ ወበዝኁ፡ ጥቀ፡</foreign> <ref cRef="Kuf." loc="46">Kuf. 46</ref>; <foreign xml:lang="gez">ከማሁ፡ ይፈደፍዱ፡</foreign> <ref cRef="Kuf." loc="46">Kuf. 46</ref>; <foreign xml:lang="gez">ኢይፈደፍዱ፡ እንከ፡</foreign> <ref cRef="Hos." loc="4,10">Hos. 4,10</ref>; <foreign xml:lang="gez">ፈድፍዶ፡</foreign> <cit type="translation" xml:lang="la">
                                <quote> incrementum </quote>
                            </cit> Org.</sense> <sense n="2" xml:id="D2"> <cit type="translation" xml:lang="la">
                                <quote> copiosiorem </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> uberiorem </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> pleniorem </quote>
                            </cit> <cit type="translation" xml:lang="la">
                                <quote> esse </quote>
                            </cit> vel <cit type="translation" xml:lang="la">
                                <quote> fieri </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> abundare </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> redundare </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> superabundare </quote>
                            </cit>: <foreign xml:lang="gez">ከመ፡ ኢትፈድፍድ፡ ኀጢአትየ፡</foreign> <foreign xml:lang="grc">πλεονάσωσι</foreign> <ref cRef="Sir." loc="23,3">Sir. 23,3</ref>; <ref cRef="Rom." loc="6,1">Rom. 6,1</ref>; 2 <ref cRef="Thess." loc="1,3">Thess. 1,3</ref>; <foreign xml:lang="grc">περισσεύειν</foreign> <ref cRef="Rom." loc="5,15">Rom. 5,15</ref>; <ref cRef="1 Thess." loc="4,10">1 Thess. 4,10</ref>; <ref cRef="Matth." loc="13,12">Matth. 13,12</ref>; <foreign xml:lang="grc">ὑπερπερισσεύειν</foreign> <ref cRef="Rom." loc="5,20">Rom. 5,20</ref>; <foreign xml:lang="grc">ὑπερπλεονάζειν</foreign> <ref cRef="1 Tim." loc="1,14">1 Tim. 1,14</ref>; <lbl expand="sequens">seq.</lbl> <foreign xml:lang="gez">እምነ፡</foreign> comparative: <ref cRef="Matth." loc="5,20">Matth. 5,20</ref>. <sense n="a" xml:id="D2a"> <cit type="translation" xml:lang="la">
                                    <quote> numero </quote>
                                </cit> <cit type="translation" xml:lang="la">
                                    <quote> superare </quote>
                                </cit>, <cit type="translation" xml:lang="la">
                                    <quote> plures </quote>
                                </cit> <cit type="translation" xml:lang="la">
                                    <quote> esse </quote>
                                </cit>, <cit type="translation" xml:lang="la">
                                    <quote> excedere </quote>
                                </cit> <cit type="translation" xml:lang="la">
                                    <quote> numerum </quote>
                                </cit>: <foreign xml:lang="gez">ፈድፈዱ፡ እምነ፡</foreign> … <foreign xml:lang="grc">πλείους ἐγένοντο ἤ</foreign> … <ref cRef="Jos." loc="10,11">Jos. 10,11</ref>; <foreign xml:lang="gez">ወኮነ፡ ኵሉ፡ መዋዕለ፡ ትንቢቱ፡ ፳ ዓመት፡ ወዘይፈደፍድ፡</foreign> (<cit type="translation" xml:lang="la">
                                    <quote> et </quote>
                                </cit> <cit type="translation" xml:lang="la">
                                    <quote> quod </quote>
                                </cit> <cit type="translation" xml:lang="la">
                                    <quote> excurrit </quote>
                                </cit> vel <cit type="translation" xml:lang="la">
                                    <quote> et </quote>
                                </cit> <cit type="translation" xml:lang="la">
                                    <quote> supra </quote>
                                </cit>, <cit type="translation" xml:lang="la">
                                    <quote> et </quote>
                                </cit> <cit type="translation" xml:lang="la">
                                    <quote> amplius </quote>
                                </cit>) <ref cRef="Sx. Ter" loc="15">Sx. Ter 15</ref>; <foreign xml:lang="gez">፶ ዓመት፡ ወዘይፈደፍድ፡</foreign> <ref cRef="Sx. Sen." loc="20">Sx. Sen. 20</ref>; <lbl expand="sequens">seq.</lbl> <foreign xml:lang="gez">እምነ፡</foreign> <ref cRef="Num." loc="3,48">Num. 3,48</ref>; <ref cRef="Num." loc="3,49">Num. 3,49</ref>; <foreign xml:lang="gez">ተወሰከ፡ ዘመኑ፡</foreign> (Hizkiae) <foreign xml:lang="gez">ወፈድፈደ፡ እምነ፡ ኍልቁ፡</foreign> <ref cRef="Sx. Nah." loc="4 ">Sx. Nah. 4 </ref>Enc.</sense> <sense n="b" xml:id="D2b"> <cit type="translation" xml:lang="la">
                                    <quote> redundare </quote>
                                </cit> <cit type="translation" xml:lang="la">
                                    <quote> longitudine </quote>
                                </cit>, <cit type="translation" xml:lang="la">
                                    <quote> longiorem </quote>
                                </cit> <cit type="translation" xml:lang="la">
                                    <quote> esse </quote>
                                </cit>, <ref cRef="Ex." loc="26,12">Ex. 26,12</ref>.</sense> <sense n="c" xml:id="D2c"> <cit type="translation" xml:lang="la">
                                    <quote> sufficere </quote>
                                </cit>, <lbl expand="cum">c.</lbl> <gramGrp> <case value="accusativus">Acc.</case> </gramGrp> Infin.: <foreign xml:lang="gez">ኢፈድፈዱ፡ ነጺሐ፡</foreign> <foreign xml:lang="grc">οὐχ ἡγνίσθησαν ἱκανοί</foreign> <ref cRef="2 Par. " loc="30,3">2 Par.  30,3</ref>.</sense> </sense> <sense n="3" xml:id="D3"> <cit type="translation" xml:lang="la">
                                <quote> superiorem </quote>
                            </cit> <cit type="translation" xml:lang="la">
                                <quote> esse </quote>
                            </cit> vel <cit type="translation" xml:lang="la">
                                <quote> fieri </quote>
                            </cit> in re, <cit type="translation" xml:lang="la">
                                <quote> excellere </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> praecellere </quote>
                            </cit>, <foreign xml:lang="gez">ትፈደፍዲ፡</foreign> <foreign xml:lang="grc">ὑπέρκεισαι</foreign> <ref cRef="Prov." loc="29,47">Prov. 29,47</ref>; <lbl expand="cum">c.</lbl> <foreign xml:lang="gez">እምነ፡</foreign> compar.: <ref cRef="Sap. " loc="7,29">Sap.  7,29</ref>; <ref cRef="Koh." loc="3,19">Koh. 3,19</ref> <lbl expand="varia lectio">var.</lbl>; <lbl expand="cum">c.</lbl> <gramGrp> <case value="accusativus">Acc.</case> </gramGrp> <lbl expand="persona">pers.</lbl>: <ref cRef="Gen." loc="41,40">Gen. 41,40</ref>; <lbl expand="cum">c.</lbl> <foreign xml:lang="gez">በ፡</foreign> <cit type="translation" xml:lang="la">
                                <quote> in </quote>
                            </cit> re: <ref cRef="Hen." loc="14,16">Hen. 14,16</ref>; <ref cRef="2 Cor." loc="8,7">2 Cor. 8,7</ref>; <cb n="1381" xml:id="c1381"/> <cit type="translation" xml:lang="la">
                                <quote> vi </quote>
                            </cit> <cit type="translation" xml:lang="la">
                                <quote> superiorem </quote>
                            </cit> <cit type="translation" xml:lang="la">
                                <quote> esse </quote>
                            </cit>: <foreign xml:lang="gez">ቃለ፡ ንጉሥ፡ ጸንዐ፡ ወፈድፈደ፡ እምቃለ፡ ኢዮአብ፡</foreign> <ref cRef="1 Par." loc="21,4">1 Par. 21,4</ref>.</sense> </sense>
               
            </entry>
