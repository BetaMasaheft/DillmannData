               <sense xml:id="Dmain" xml:lang="la" source="#dillmann"> <subc>III,1</subc> <cit type="translation" xml:lang="la"><quote> sibi calceos induere </quote></cit>, <cit type="translation" xml:lang="la"><quote> soleas subligare </quote></cit>, siv <gramGrp> <pos expand="absolute">absol.</pos> </gramGrp>, ut: <foreign xml:lang="gez">ተሠአን፡</foreign> <ref cRef="Exp. fid. Nic." loc="">Exp. fid. Nic. </ref> (f. 285), sive <lbl expand="cum">c.</lbl> <gramGrp> <case value="accusativus">Acc.</case> </gramGrp> <foreign xml:lang="gez">አሣእነ፡</foreign>, ut: <foreign xml:lang="gez">ተሥእነት፡ አሣእኒሃ፡</foreign> <ref cRef="Judith" loc="10,4">Judith 10,4</ref>; <ref cRef="Judith" loc="16,9">Judith 16,9</ref> <lbl expand="varia lectio">var.</lbl>; <ref cRef="Act." loc="12,8">Act. 12,8</ref>; <lbl expand="translate">transl.</lbl> <ref cRef="Eph." loc="6,15">Eph. 6,15</ref>. <foreign xml:lang="gez">አስተሥአነ፡</foreign> <subc>IV,1</subc> <cit type="translation" xml:lang="la"><quote> facere ut </quote></cit> aliquis <cit type="translation" xml:lang="la"><quote> calceos </quote></cit> vel <cit type="translation" xml:lang="la"><quote> soleas sibi induat </quote></cit>, <cit type="translation" xml:lang="la"><quote> calceare </quote></cit>: <foreign xml:lang="gez">አስተሠአንኩኪ፡ ያክንተ፡</foreign> <ref cRef="Hez." loc="16,9">Hez. 16,9</ref> <lbl expand="varia lectio">var.</lbl> </sense>
               
