               <form>
                  <foreign xml:lang="gez">ሃይማኖት</foreign>
               </form>
               <sense xml:id="Dmain" xml:lang="la" source="#dillmann"> (<foreign xml:lang="syr"
                     >ܗܰܝܡܳܢܽܘܬܐ</foreign>) peregrinae formationis, ab Aramaeis petitum, ab
                  Aethiopibus frequentissime usitatum, <gramGrp>
                     <pos expand="Substantivum">subst.</pos>
                  </gramGrp>, <gramGrp>
                     <gen value="not found">m.</gen>
                  </gramGrp> et <gramGrp>
                     <gen value="femininus">fem.</gen>
                  </gramGrp>: <sense n="1" xml:id="D1">
                     <cit type="translation" xml:lang="la">
                        <quote> fides qua creditur
                        </quote>
                     </cit>; <cit type="translation" xml:lang="la">
                        <quote> fidelitas </quote>
                     </cit>(<note>Glaube</note>, <note>Treue</note>): <ref cRef="Deut." loc="32,20"
                        >Deut. 32,20</ref>; <ref cRef="Jes. " loc="5,3">Jes. 5,3</ref>; <ref
                        cRef="Jes." loc="7,28">Jes. 7,28</ref>; <ref cRef="Jer." loc="15,18">Jer.
                        15,18</ref>; <ref cRef="Jer." loc="39,41">Jer. 39,41</ref>; <ref cRef="Ps."
                        loc="32,4">Ps. 32,4</ref>; <ref cRef="Sir." loc="40,12">Sir. 40,12</ref>;
                        <ref cRef="Sir." loc="45,4">Sir. 45,4</ref>; <ref cRef="Sir." loc="46,15"
                        >Sir. 46,15</ref>; <ref cRef="4 Esr." loc="4,13">4 Esr. 4,13</ref>; <ref
                        cRef="4 Esr." loc="4,32">4 Esr. 4,32</ref>; <ref cRef="4 Esr." loc="9,8">4
                        Esr. 9,8</ref>; <ref cRef="Rom." loc="1,8">Rom. 1,8</ref>; <ref cRef="Rom."
                        loc="4,12">Rom. 4,12</ref>; <ref cRef="2 Tim." loc="4,7">2 Tim. 4,7</ref>;
                        <ref cRef="Hebr." loc="11,3">Hebr. 11,3</ref>; <ref cRef="Hebr." loc="11,39"
                        >Hebr. 11,39</ref>. (synon. <foreign xml:lang="gez">አሚን፡</foreign>, <foreign
                        xml:lang="gez">ተአምኖ፡</foreign>), <ref cRef="Matth." loc="8,10">Matth.
                        8,10</ref>; <ref cRef="Jac." loc="2,14">Jac. 2,14</ref>
                     <lbl expand="sequens">seq.</lbl>; <foreign xml:lang="grc">πίστεις</foreign>
                     <ref cRef="Prov." loc="3,3">Prov. 3,3</ref>; <foreign xml:lang="gez">ሃይማኖተ፡
                        ተስፋ፡</foreign>
                     <foreign xml:lang="grc">πίστις ἐλπίδος</foreign>
                     <ref cRef="Sir." loc="49,10">Sir. 49,10</ref>; <lbl expand="construitur cum"
                        >c.c.</lbl>
                     <foreign xml:lang="gez">በ</foreign>
                     <cit type="translation" xml:lang="la">
                        <quote> in </quote>
                     </cit>
                     <ref cRef="Jac." loc="2,1">Jac. 2,1</ref>; <foreign xml:lang="gez">እለ፡ አዕረፉ፡
                        በሃይማኖት፡ በክርስቶስ፡</foreign>
                     <ref cRef="Did." loc="34">Did. 34</ref>; <foreign xml:lang="gez">ዘይገብር፡
                        ሃይማኖተ፡</foreign>
                     <foreign xml:lang="grc">ποιῶν πίστεις</foreign>
                     <ref cRef="Prov." loc="12,22">Prov. 12,22</ref>; <ref cRef="Prov. " loc="15,28"
                        >Prov. 15,28</ref>; <ref cRef="Sir." loc="15,15">Sir. 15,15</ref>; <foreign
                        xml:lang="gez">ሃይማኖተ፡ ዕቀብ፡ ምስለ፡ ቢጽከ፡</foreign> (de amicitia) <ref
                        cRef="Sir." loc="22,23">Sir. 22,23</ref>; <ref cRef="Hen." loc="41,5">Hen.
                        41,5</ref>; <ref cRef="Hen." loc="43,2">Hen. 43,2</ref>; <foreign
                        xml:lang="gez">ያጸንዑ፡ ሃይማኖቶሙ፡ በቀለም፡ ወበማየ፡ ሕመት፡</foreign>
                     <ref cRef="Hen." loc="69,10">Hen. 69,10</ref>; <foreign xml:lang="gez">ብእሲ፡
                        ዚታአምር፡ ከመ፡ ቦ፡ ሃይማኖተ፡</foreign> (<cit type="translation" xml:lang="la">
                        <quote> fidelem eum esse </quote>
                     </cit>) <ref cRef="Sir." loc="37,12">Sir. 37,12</ref>; <foreign xml:lang="gez"
                        >ሀጕለ፡ ሃይማኖቶ፡</foreign>
                     <cit type="translation" xml:lang="la">
                        <quote> fidem perdidit </quote>
                     </cit>
                     <ref cRef="Sir." loc="27,16">Sir. 27,16</ref>; <ref cRef="Luc. " loc="16,11"
                        >Luc. 16,11</ref>; <ref cRef="Luc." loc="16,12">Luc. 16,12</ref>; <foreign
                        xml:lang="gez">በሃይማኖቶሙ፡</foreign>
                     <cit type="translation" xml:lang="la">
                        <quote> in bonam fidem </quote>
                     </cit>
                     <ref cRef="4 Reg." loc="12,15">4 Reg. 12,15</ref>; <ref cRef="4 Reg."
                        loc="22,7">4 Reg. 22,7</ref>. </sense>
                  <sense n="2" xml:id="D2">
                     <cit type="translation" xml:lang="la">
                        <quote> fides quae creditur
                        </quote>
                     </cit>(<cit type="translation" xml:lang="la">
                        <quote> religio </quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote> confessio </quote>
                     </cit>); <foreign xml:lang="gez">ሃይማኖት፡ ቅድስት፡</foreign>
                     <ref cRef="Judae" loc="20">Judae 20</ref>; <foreign xml:lang="gez">ሃይማኖት፡
                        ርትዕት፡</foreign>
                     <cit type="translation" xml:lang="la">
                        <quote> fides orthodoxa </quote>
                     </cit>
                     <ref cRef="Lit." loc="165,1">Lit. 165,1</ref>: <foreign xml:lang="gez">ርቱዐ፡
                        ሃይማኖት፡</foreign>
                     <cit type="translation" xml:lang="la">
                        <quote> orthodoxus </quote>
                     </cit>(saepissime; etiam titulus libri cujusdam dogmatici); <foreign
                        xml:lang="gez">ሃይማኖተ፡ አበው፡</foreign>
                     <cit type="translation" xml:lang="la">
                        <quote> fides Patrum </quote>
                     </cit>(titulus libri celeberrimi); <foreign xml:lang="gez">ጸሎተ፡
                        ሃይማኖት፡</foreign>
                     <cit type="translation" xml:lang="la">
                        <quote> symbolum fidei </quote>
                     </cit>
                     <ref cRef="Lit." loc="163,3">Lit. 163,3</ref>; <foreign xml:lang="gez">ሃይማኖተ፡
                        እስላም፡</foreign>
                     <cit type="translation" xml:lang="la">
                        <quote> Islami religio </quote>
                     </cit>
                     <ref cRef="Abush." loc="50">Abush. 50</ref>; <foreign xml:lang="gez">ሃይማኖተ፡
                        ንስጦሮስ፡</foreign>
                     <cit type="translation" xml:lang="la">
                        <quote> confessio Nestorii
                        </quote>
                     </cit>Enc. </sense>
               </sense>

               <sense xml:id="Tmain" xml:lang="en" source="#traces">
                  <cit type="transcription" xml:lang="gez">
                     <quote>hāymānot</quote>
                  </cit>
                  <sense n="G" xml:id="TG"><foreign xml:lang="gez">ሃይማኖት፡</foreign>
                     <cit type="transcription" xml:lang="gez">
                        <quote>hāymānot</quote>
                     </cit>, <lbl expand="pluralis">Pl.</lbl>
                     <foreign xml:lang="gez">ሃይማኖታት፡</foreign>
                     <cit type="transcription" xml:lang="gez">
                        <quote>hāymānotāt</quote>
                     </cit> présente des significations très différentes : <cit type="translation"
                        xml:lang="fr">
                        <quote>foi chrétienne</quote>
                     </cit> (au sens général), <cit type="translation" xml:lang="fr">
                        <quote>foi catholique</quote>
                     </cit> (diophysite); <cit type="translation" xml:lang="fr">
                        <quote>foi monophysite de
                           Dioscore</quote>
                     </cit> (opposée à la foi catholique), (reject de l’« Epistola dogmatica ad
                     Flavianum» du pape Léon Ier, <foreign xml:lang="gez">ጦማረ፡ ልዮን፡</foreign>); <cit
                        type="translation" xml:lang="fr">
                        <quote>religion</quote>
                     </cit>, <cit type="translation" xml:lang="fr">
                        <quote>croyance</quote>
                     </cit>, etc. <foreign xml:lang="gez">አስተጋብኡ፡ ኵሎ፡ መኳንንተ፡ ወሊቃውንተ፡ ወመምህራነ፡ ወኵሎ፡
                        ዓበይተ፡ መንግሥት፡ ወመከሩ፡ ከመ፡ ይሕትትዎሙ፡ ወይጠይቅዎሙ፡ ነገረ፡ ሃይማኖት። አሜሃ፡ ተዓውቀ፡ ወተጠየቀ፡ ከመ፡
                        ኮነ፡ ሰብአ፡ ሮም፡ ወከመ፡ የአምኑ፡ በሃይማኖተ፡ ልዮን፡ ርኩስ፡ ዓላዊ፡ እስከ፡ ተናገሩ፡ በቃሎሙ፡ እንዘ፡ ይብሉ፡
                        ፪ጠባይዓት፡ ሀለዉ፡ በክርስቶስ። ወእምዝ፡ ተሠጥዉ፡ ወይቤሉ፡ ንጉሥ፡ ወሰብኡ፡ ንሕነሰ፡ ኢንብል፡ ከመዝ፡ ዳእሙ፡ ንቤ፡
                        አልቦ፡ ምንታዌ፡ በክርስቶስ፡ አምላክ፡ ድኅረ፡ ተዋሕዶተ፡ መለኮት፡ ወትስብዕት፡ እንበለ፡ ቱሳሔ፡ ወኢድማሬ። ወነአምን፡
                        በሃይማኖተ፡ ቅዱስ፡ ዲዮስቆሮስ፡ ጽዱቀ፡ ንባብ።</foreign>
                     <bibl><ptr target="bm:Guidi1903YohannesT"/><citedRange unit="page">
                           8</citedRange></bibl>. <lb/>
                     <foreign xml:lang="gez">ሃይማኖት፡</foreign> religion au sens le plus général,
                     christianisme, paganisme, doctrines ésotériques, etc. <foreign xml:lang="gez"
                        >አስተርእዮተ፡ አቴናውያን፡ ወሃይማኖቶሙ።</foreign>
                     <bibl><ptr target="bm:Zotenberg1883JohnNikiu"/><citedRange unit="page">
                           143</citedRange></bibl>. <lb/>
                     <foreign xml:lang="gez">ሃይማኖት፡ ርትዕት፡</foreign>
                     <cit type="transcription" xml:lang="gez">
                        <quote>hāymānot rǝtǝʿt</quote>
                     </cit>
                     <cit type="translation" xml:lang="fr">
                        <quote>foi orthodoxe</quote>
                     </cit> (monophysite). <foreign xml:lang="gez">ተናገረ፡ [ዮሐንስ፡ ዘሀገረ፡ ብርልስ፡] በእንተ፡
                        ሥርዓተ፡ ቤተ፡ ክርስቲያን፡ ወሕገ፡ ሃይማኖት፡ ርትዕት።</foreign> Ms. <ref type="BM"
                        target="BNFet111">BNFet111</ref>, fol. 120v. <lbl expand="videas">vid.</lbl>
                     <foreign xml:lang="gez">ርቱዕ፡</foreign>
                     <lb/>
                     <foreign xml:lang="gez">ሃይማኖት፡</foreign>
                     <cit type="translation" xml:lang="fr">
                        <quote>croyance
                           mystagogique</quote>
                     </cit>. <foreign xml:lang="gez">እልመስጦአግያ፡ ወይእቲ፡ ሃይማኖት፡ [እንተ፡] መሐሮሙ፡ እግዚእነ፡
                        ወመድኃኒነ፡ ኢየሱስ፡ ክርስቶስ፡ ለአርዳኢሁ።</foreign>
                     <bibl><ptr target="bm:Lifchitz1940textes"/><citedRange unit="page">
                           72</citedRange></bibl>. <lb/>
                     <foreign xml:lang="gez">ሃይማኖት፡ መሲሓዊት፡</foreign>
                     <cit type="transcription" xml:lang="gez">
                        <quote>hāymānot masiḥāwit</quote>
                     </cit>, <foreign xml:lang="gez">ሃይማኖት፡ ኦርቶዶክሳዊት፡</foreign>
                     <cit type="transcription" xml:lang="gez">
                        <quote>hāymānot
                           ʾortodoksāwit</quote>
                     </cit>
                     <cit type="translation" xml:lang="fr">
                        <quote>religion chrétienne</quote>
                     </cit>. <foreign xml:lang="gez">አድለወ፡ [መክስንድዮስ፡] ለሃይማኖትነ፡ ወአዘዘ፡ ከመ፡ ያቅልሉ፡ ስደተ፡
                        እምክርስቲያን።</foreign>
                     <bibl><ptr target="bm:Zotenberg1883JohnNikiu"/><citedRange unit="page">
                           193</citedRange></bibl>; <foreign xml:lang="gez">አቀመ፡ [ቈስጠንጢኖስ፡] ሃይማኖተ፡
                        ኦርቶዶክሳዊት።</foreign>
