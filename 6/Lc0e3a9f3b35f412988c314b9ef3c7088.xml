               <sense xml:id="Dmain" xml:lang="la" source="#dillmann"> <subc>I,2</subc> et <subc>I,1</subc> [prima radicis potestas est in <cit type="translation" xml:lang="la"><t:quote xmlns:t="http://www.tei-c.org/ns/1.0"> pernicitate </t:quote></cit> et <cit type="translation" xml:lang="la"><t:quote xmlns:t="http://www.tei-c.org/ns/1.0"> agilitate </t:quote></cit> (<lbl expand="videas">vid.</lbl> <foreign xml:lang="he">מָהַר</foreign>); hinc ducitur notio <cit type="translation" xml:lang="la"><t:quote xmlns:t="http://www.tei-c.org/ns/1.0"> sollertiae </t:quote></cit> (<foreign xml:lang="he">מָהִיר</foreign>, <foreign xml:lang="syr">ܡܗܺܝܪܳܐ</foreign>, <foreign xml:lang="ar">مَهَرَ</foreign>, <foreign xml:lang="ar">مَاهِرٌ</foreign>); denique in Geez stirps <subc>I,2</subc> et <subc>I,1</subc> significat <cit type="translation" xml:lang="la"><t:quote xmlns:t="http://www.tei-c.org/ns/1.0"> sollertem reddere </t:quote></cit> i.e. <cit type="translation" xml:lang="la"><t:quote xmlns:t="http://www.tei-c.org/ns/1.0"> docere </t:quote></cit>; amh. <foreign xml:lang="gez">ማረ፡</foreign> id.] Imperf. plerumque <foreign xml:lang="gez">ይሜህር፡</foreign> <subc>I,2</subc>, sed etiam: <foreign xml:lang="gez">ይመህር፡</foreign> <subc>I,1</subc> (<ref cRef="4 Esr." loc="8,15">4 Esr. 8,15</ref>) vel <foreign xml:lang="gez">ይምህር፡</foreign> (<ref cRef="Jes." loc="29,13">Jes. 29,13</ref>; <ref cRef="4 Esr." loc="10,53">4 Esr. 10,53</ref>; al.); <gramGrp> <pos expand="Subjunctivus">Subj.</pos> </gramGrp> <foreign xml:lang="gez">ይመህር፡</foreign> <cb n="0143" xml:id="c0143"/> vel <foreign xml:lang="gez">ይምህር፡</foreign> <subc>I,2</subc> (<ref cRef="Luc." loc="1,1">Luc. 1,1</ref> <lbl expand="romanae editionis">rom.</lbl>; <ref cRef="Deut." loc="6,1">Deut. 6,1</ref>; pro qua forma etiam <foreign xml:lang="gez">ይማህር፡</foreign> reperitur, ut <ref cRef="Deut." loc="4,36">Deut. 4,36</ref>) et <foreign xml:lang="gez">ይምሀር፡</foreign> <subc>I,1</subc> (<ref cRef="Ps." loc="50,14">Ps. 50,14</ref>; <ref cRef="Deut." loc="20,18">Deut. 20,18</ref>; 1 <ref cRef="Tim." loc="2,12">Tim. 2,12</ref>; al.), Imperat. <foreign xml:lang="gez">መህር፡</foreign> et <foreign xml:lang="gez">ምህር፡</foreign> <subc>I,2</subc> (<ref cRef="Matth." loc="28,19">Matth. 28,19</ref> <lbl expand="romanae editionis">rom.</lbl>; <ref cRef="Ps." loc="118,26">Ps. 118,26</ref>; <ref cRef="Ps." loc="118,33">Ps. 118,33</ref>; <ref cRef="Ps. " loc="141,11">Ps.  141,11</ref> al.; pro qua forma etiam <foreign xml:lang="gez">ማህር፡</foreign> reperitur, ut <ref cRef="Deut." loc="4,9">Deut. 4,9</ref>) et <foreign xml:lang="gez">መሀር፡</foreign> <subc>I,1</subc> (<ref cRef="Matth." loc="28,19">Matth. 28,19</ref> Platt; <ref cRef="Prov. " loc="29,17">Prov.  29,17</ref>; al.) Infin. <foreign xml:lang="gez">ምሂር፡</foreign> (<ref cRef="1 Tim." loc="4,13">1 Tim. 4,13</ref>), <foreign xml:lang="gez">ምህሮ፡</foreign> (<ref cRef="Rom." loc="1,16">Rom. 1,16</ref>; <ref cRef="1 Tim." loc="5,17">1 Tim. 5,17</ref>), <foreign xml:lang="gez">ምህሮት፡</foreign> (<ref cRef="Sir." loc="42,5">Sir. 42,5</ref>; <ref cRef="Marc." loc="11,18">Marc. 11,18</ref>) et <foreign xml:lang="gez">ምሂሮት፡</foreign> (<ref cRef="Matth." loc="7,28">Matth. 7,28</ref> <lbl expand="romanae editionis">rom.</lbl>). <sense n="1" xml:id="D1"> <cit type="translation" xml:lang="la"><t:quote xmlns:t="http://www.tei-c.org/ns/1.0"> docere </t:quote></cit> <sense n="a" xml:id="D1a"> <gramGrp> <pos expand="absolute">absol.</pos> </gramGrp> <ref cRef="1 Tim." loc="2,12">1 Tim. 2,12</ref>; <ref cRef="Matth." loc="4,23">Matth. 4,23</ref>. <ref cRef="Matth." loc="11,1">Matth. 11,1</ref>. 
</sense> <sense n="b" xml:id="D1b"> <lbl expand="cum">c.</lbl> <gramGrp> <case value="accusativus">Acc.</case> </gramGrp> <lbl expand="persona">pers.</lbl>: <ref cRef="Matth." loc="5,2">Matth. 5,2</ref>; <ref cRef="Rom." loc="2,21">Rom. 2,21</ref>; <ref cRef="Ps." loc="70,18">Ps. 70,18</ref>; <ref cRef="Ps." loc="118,102">Ps. 118,102</ref>; et <lbl expand="cum">c.</lbl> <foreign xml:lang="gez">ለ</foreign> pers.: <ref cRef="Rom." loc="2,21">Rom. 2,21</ref>; <ref cRef="Hebr." loc="8,11">Hebr. 8,11</ref>; <ref cRef="Deut. " loc="4,10">Deut.  4,10</ref>. 
</sense> <sense n="c" xml:id="D1c"> <lbl expand="cum">c.</lbl> <gramGrp> <case value="accusativus">Acc.</case> </gramGrp> rei: <foreign xml:lang="gez">ይምህሩ፡ ትምህርተ፡ ትእዛዛተ፡ ሰብእ፡</foreign> <ref cRef="Jes." loc="29,13">Jes. 29,13</ref>; <foreign xml:lang="gez">ይሜህር፡ ምክረ፡</foreign> <ref cRef="Job" loc="22,2">Job 22,2</ref>; <ref cRef="Phil." loc="1,16">Phil. 1,16</ref>; <ref cRef="Rom." loc="1,16">Rom. 1,16</ref>, et <lbl expand="cum">c.</lbl> <foreign xml:lang="gez">በእንተ፡</foreign> rei <ref cRef="4 Esr. " loc="10,53">4 Esr.  10,53</ref>; <ref cRef="Luc." loc="1,1">Luc. 1,1</ref>. 
</sense> <sense n="d" xml:id="D1d"> <lbl expand="cum">c.</lbl> <lbl expand="duplex">dupl.</lbl> <gramGrp> <case value="accusativus">Acc.</case> </gramGrp> <ref cRef="Ps." loc="50,14">Ps. 50,14</ref>; <ref cRef="Ps." loc="118,26">Ps. 118,26</ref>; <ref cRef="Ps." loc="118,33">Ps. 118,33</ref>; <ref cRef="Deut." loc="4,14">Deut. 4,14</ref>; <ref cRef="Deut. " loc="5,28">Deut.  5,28</ref>; <ref cRef="Deut." loc="6,7">Deut. 6,7</ref>; <ref cRef="4 Reg." loc="17,27">4 Reg. 17,27</ref>; <ref cRef="4 Esr." loc="8,15">4 Esr. 8,15</ref>; <ref cRef="Koh." loc="12,9">Koh. 12,9</ref>; <foreign xml:lang="gez">መሀሮ፡ አቡሁ፡ መጽሐፈ፡</foreign> <ref cRef="Kuf." loc="7">Kuf. 7</ref>; <ref cRef="Kuf." loc="47">Kuf. 47</ref>; <ref cRef="Dan." loc="1,4">Dan. 1,4</ref>; <ref cRef="Job" loc="33,33">Job 33,33</ref>; <ref cRef="Sir." loc="45,5">Sir. 45,5</ref>; <ref cRef="Joh." loc="14,26">Joh. 14,26</ref>; <ref cRef="Hebr." loc="5,12">Hebr. 5,12</ref>. 
</sense> <sense n="e" xml:id="D1e"> verba si subjunguntur, adhibetur <gramGrp> <case value="accusativus">Acc.</case> </gramGrp> Infin.: <foreign xml:lang="gez">ትሜህር፡ እገሪሃ፡ ሐዊረ፡</foreign> <ref cRef="Deut." loc="28,56">Deut. 28,56</ref>; <ref cRef="Deut." loc="20,18">Deut. 20,18</ref>; <ref cRef="Hen." loc="8,1">Hen. 8,1</ref>; <ref cRef="Kuf." loc="3,">Kuf. 3,</ref> vel <foreign xml:lang="gez">ለ፡</foreign> <lbl expand="cum">c.</lbl> Infin., ut: <foreign xml:lang="gez">ምህረኒ፡ ለገቢር፡</foreign> <ref cRef="Ps." loc="142,11">Ps. 142,11</ref>, vel <gramGrp> <pos expand="Subjunctivus">Subj.</pos> </gramGrp>, ut: <ref cRef="Matth." loc="28,19">Matth. 28,19</ref>; <ref cRef="Deut. " loc="6,1">Deut.  6,1</ref>. 
</sense> 
</sense> <sense n="2" xml:id="D2"> <cit type="translation" xml:lang="la"><t:quote xmlns:t="http://www.tei-c.org/ns/1.0"> instituere </t:quote></cit>, <cit type="translation" xml:lang="la"><t:quote xmlns:t="http://www.tei-c.org/ns/1.0"> erudire </t:quote></cit> (<foreign xml:lang="grc">παιδεύειν</foreign>, etiam <foreign xml:lang="grc">νουθετεῖν</foreign> <cit type="translation" xml:lang="la"><t:quote xmlns:t="http://www.tei-c.org/ns/1.0"> admonere </t:quote></cit>): <foreign xml:lang="gez">መሀር፡ ወልደከ፡</foreign> <ref cRef="Prov." loc="29,17">Prov. 29,17</ref>; <foreign xml:lang="gez">ምህረኒ፡ እግዚኦ፡</foreign> <ref cRef="Jer." loc="10,24">Jer. 10,24</ref>; <foreign xml:lang="gez">ገሠጾ፡ ወመሀረ፡</foreign> <ref cRef="Sir." loc="7,23">Sir. 7,23</ref>; <ref cRef="Sir." loc="18,13">Sir. 18,13</ref>; <ref cRef="Sir." loc="30,2">Sir. 30,2</ref>; <ref cRef="Prov." loc="5,13">Prov. 5,13</ref>; <ref cRef="Prov." loc="9,7">Prov. 9,7</ref>; <ref cRef="Rom." loc="15,14">Rom. 15,14</ref>; 1 <ref cRef="Cor." loc="4,14">Cor. 4,14</ref>; <ref cRef="1 Thess." loc="5,12">1 Thess. 5,12</ref>. 
</sense> </sense>
