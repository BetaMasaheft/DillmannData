            <entry xml:id="Lb61f13c1a63341389725b34a40acd317" n="7406">
               <form>
                  <foreign xml:lang="gez">ማዕጾ</foreign>
               </form>
               <sense xml:lang="la" source="#dillmann" xml:id="Dmain">, rarius <foreign xml:lang="gez">ማዕጹት፡</foreign>, et <foreign xml:lang="gez">ማዕጾት፡</foreign> (Lud. e Mss.
                  Colb.), <gramGrp>
                     <pos expand="Substantivum">subst.</pos>
                  </gramGrp>, <gramGrp>
                     <gen value="masculinum">m.</gen>
                  </gramGrp> et <gramGrp>
                     <gen value="femininus">fem.</gen>
                  </gramGrp>, <lbl expand="pluralis">Pl.</lbl>
                  <foreign xml:lang="gez">መዓጹት፡</foreign>; rarius <foreign xml:lang="gez">መዓጽው፡</foreign> (M.M. f. 312; Org.), <lbl expand="pluralis">Pl.</lbl>
                  <lbl expand="pluralis">Pl.</lbl>
                  <foreign xml:lang="gez">መዓጽዋት፡</foreign> (Lud. e Mss. Colb.) <sense n="1" xml:id="D1">
                     <cit type="translation" xml:lang="la">
                        <quote>janua</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>fores</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>valvae</quote>
                     </cit>: <foreign xml:lang="gez">ማዕጾ፡ </foreign>
                     <foreign xml:lang="grc">θύρα</foreign>
                     <ref cRef="Ep. Jer." loc="58">Ep. Jer. 58</ref>; <foreign xml:lang="gez">ከመ፡
                        ማዕጾ፡ እንተ፡ ተዐውድ፡ ውስተ፡ ድርኵኵታ፡</foreign>
                     <ref cRef="Prov." loc="26,14">Prov. 26,14</ref>; <foreign xml:lang="gez">ድርኵኵተ፡
                        ማዕጾ፡</foreign>
                     <ref cRef="Zach." loc="12,2">Zach. 12,2</ref>; <ref cRef="Sir." loc="28,25">Sir. 28,25</ref>; <foreign xml:lang="gez">ድኅረ፡ ማዕጾ፡</foreign>
                     <ref cRef="Jes." loc="57,8">Jes. 57,8</ref>; <ref cRef="Jer." loc="30,9">Jer.
                        30,9</ref>; <cb n="1023" xml:id="c1023"/>
                     <ref cRef="Ps." loc="140,3">Ps. 140,3</ref>; <foreign xml:lang="gez">ቦአ፡ እንዘ፡
                        ዕጽው፡ ማዕጹት፡</foreign>
                     <ref cRef="Cyr." loc="120">Cyr. f. 120</ref>; <foreign xml:lang="gez">ማዕጾ፡
                        ዐቢያን፡ ዘአናቅጽ፡</foreign>
                     <ref cRef="Chron. Ax." loc="89">Chron. Ax. f. 89</ref>; <foreign xml:lang="gez">መዓጹት፡</foreign>
                     <ref cRef="1 Reg." loc="23,7">1 Reg. 23,7</ref>; <ref cRef="3 Reg." loc="6,29">3 Reg. 6,29</ref>; <ref cRef="3 Reg." loc="7,36">3 Reg. 7,36</ref>; <ref cRef="4 Reg. " loc="12,13">4 Reg. 12,13</ref>; <ref cRef="Hez." loc="38,11">Hez. 38,11</ref>; <ref cRef="Hez." loc="41,23">Hez. 41,23</ref>
                     <lbl expand="sequens">seq.</lbl>; <foreign xml:lang="gez">መዓጹት፡</foreign> (v.
                        <foreign xml:lang="gez">ማዕጹት፡</foreign>) <foreign xml:lang="gez">ወመናስግት፡ </foreign>
                     <ref cRef="Job" loc="38,10">Job 38,10</ref>; <foreign xml:lang="gez">መዓጹት፡
                        ዘቅድመ፡ ገጹ፡</foreign>
                     <ref cRef="Job " loc="41,5">Job 41,5</ref>.</sense>
                  <sense n="2" xml:id="D2">
                     <cit type="translation" xml:lang="la">
                        <quote>sera</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>claustrum</quote>
                     </cit>: <foreign xml:lang="gez">ሰበረ፡ ማዕጾ፡</foreign>
                     <ref cRef="Kuf." loc="39">Kuf. 39</ref>; <foreign xml:lang="grc">σανίς</foreign>
                     <ref cRef="Cant." loc="8,9">Cant. 8,9</ref>; <foreign xml:lang="gez">በከናፍሪሁ፡
                        ማዕጾ፡ እብን፡ ወደየ፡</foreign>
                     <ref cRef="Sx. Masc." loc="16 ">Sx. Masc. 16 </ref>Enc.; <foreign xml:lang="gez">መዓጹት፡</foreign>
                     <foreign xml:lang="grc">κλεῖθρα</foreign>
                     <ref cRef="2 Esr." loc="13,3">2 Esr. 13,3</ref>; <ref cRef="2 Esr. " loc="13,13">2 Esr. 13,13</ref>.</sense>
                  <sense n="3" xml:id="D3">
                     <cit type="translation" xml:lang="la">
                        <quote>clausura </quote>
                     </cit>cordis i.e. <cit type="translation" xml:lang="la">
                        <quote>pericardia</quote>
                     </cit>: <foreign xml:lang="gez">እሠጥቅ፡ ማዕጾ፡ ልቦሙ፡</foreign>
                     <ref cRef="Hos." loc="13,8">Hos. 13,8</ref> (vers. nov., <foreign xml:lang="he">סְגִוֹר</foreign>); oculorum i.e. <cit type="translation" xml:lang="la">
                        <quote>nictus</quote>
                     </cit>: <foreign xml:lang="gez">ማዕጾተ፡ ዐይን፡</foreign> („dum oculi vicissim
                     clauduntur et aperiuntur“) Lud. e Mss. Colb. – <lbl expand="Vocabularium Aethiopicum, -a -a.">Voc. Ae.</lbl>: <foreign xml:lang="gez">ማዕጾ፡ ዘ፡ መዝግያ፡ መቆለፍያሂ፡</foreign>; <cit type="translation" xml:lang="la">
                        <quote>aliud</quote>
                     </cit>: <foreign xml:lang="gez">ዘ፡ ሳንቃ።</foreign>
                  </sense>
               </sense>
               
            </entry>
