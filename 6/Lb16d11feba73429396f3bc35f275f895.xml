            <entry xml:id="Lb16d11feba73429396f3bc35f275f895" n="5231">
               <form>
                  <foreign xml:lang="gez">ተአንገደ</foreign>
               </form>
               <sense xml:lang="la" source="#dillmann" xml:id="Dmain"> <subc>III,1</subc> <sense n="1" xml:id="D1"> <cit type="translation" xml:lang="la">
                                <quote> peregre abire </quote>
                            </cit> vel <cit type="translation" xml:lang="la">
                                <quote> esse </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> peregrinari </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> iter facere </quote>
                            </cit>: <foreign xml:lang="gez">ውስተ፡ ተአንግዶ፡</foreign> <cit type="translation" xml:lang="la">
                                <quote> in peregrinatione </quote>
                            </cit> (<foreign xml:lang="grc">ξενιτεία</foreign>) <ref cRef="Sap." loc="18,3">Sap. 18,3</ref>; <foreign xml:lang="gez">እለ፡ ሖሩ፡ ወተአንገዱ፡</foreign> <ref cRef="Lit." loc="164,3">Lit. 164,3</ref>; <foreign xml:lang="gez">ጥበብ፡ ዐርክ፡ ይእቲ፡ በውስተ፡ ተአንግዶ፡ ወሞገስ፡ ይእቲ፡ በውስተ፡ ጉባኤ፡</foreign> Fal.; <foreign xml:lang="gez">ባዕልሰ፡ በዓለ፡ ቤት፡ ውስተ፡ ተአንግዶ፡ ወነዳይሂ፡ እንግዳ፡ በቤቱ፡</foreign> <ref cRef="Fal." loc="57">Fal. f. 57</ref>; <ref cRef="Org." loc="2">Org. 2</ref>; <foreign xml:lang="grc">ὁ Λόγος</foreign> <foreign xml:lang="gez">ተአንገደ፡ ምስለ፡ ያዕቆብ፡</foreign> (<ref cRef="Gen." loc="28">Gen. 28</ref>; <ref cRef="Gen." loc="29">Gen. 29</ref>; <ref cRef="Gen." loc="30">Gen. 30</ref>; <ref cRef="Gen." loc="31">Gen. 31</ref>; <ref cRef="Gen." loc="32">Gen. 32</ref>; <ref cRef="Gen." loc="33">Gen. 33</ref>) <ref cRef="Haim. Ab. Nr." loc="2">Haim. Ab. Nr. 2</ref>; <ref cRef="M.M." loc="f. 141">M.M. f. 141</ref>.</sense> <sense n="2" xml:id="D2"> <cit type="translation" xml:lang="la">
                                <quote> hospitio excipi </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> hospitem esse </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> deversari </quote>
                            </cit> apud aliquem: <foreign xml:lang="gez">ሶበ፡ ተአንገደ፡ እግዚአብሔር፡ ውስተ፡ ቤተ፡ አብርሃም፡</foreign> (<ref cRef="Gen." loc="18">Gen. 18</ref>) <ref cRef="Sx. Nah." loc="26">Sx. Nah. 26</ref>; <foreign xml:lang="gez">እለ፡ ተአንገዱ፡ ኀቤሆሙ፡</foreign> <foreign xml:lang="grc">ξένοι</foreign> <ref cRef="Sap." loc="19,13">Sap. 19,13</ref>; <ref cRef="Org." loc="1.">Org. 1.</ref> </sense> </sense>
               
