               <sense xml:id="Dmain" xml:lang="la" source="#dillmann"> <gramGrp> <pos expand="participium">part.</pos> </gramGrp> et <gramGrp> <pos expand="adjectivum">adj.</pos> </gramGrp> <sense n="1" xml:id="D1"> <cit type="translation" xml:lang="la"><quote> durus </quote></cit>, <foreign xml:lang="grc">σκληρός</foreign>, <foreign xml:lang="grc">στερεός</foreign>, <foreign xml:lang="gez">እብን፡ ጽኑዕ፡</foreign> <ref cRef="Sap." loc="11,4">Sap. 11,4</ref>; <ref cRef="Jes. " loc="5,28">Jes.  5,28</ref>; <ref cRef="Herm." loc="81">Herm. p. 81</ref>a; <foreign xml:lang="gez">ኰኵሕ፡ ጽኑዕ፡</foreign> <ref cRef="Jes." loc="50,7">Jes. 50,7</ref>; <foreign xml:lang="gez">ኰኵሕ፡ ጽንዕት፡</foreign> <ref cRef="Jes." loc="51,1">Jes. 51,1</ref>; <ref cRef="Hen." loc="22,1">Hen. 22,1</ref>; <ref cRef="Hen." loc="31,2">Hen. 31,2</ref>. <sense n="a" xml:id="D1a"> <foreign xml:lang="gez">ጽኑዓነ፡ ገድ፡</foreign> <foreign xml:lang="grc">σκληροπόσωποι</foreign> (i.e. <cit type="translation" xml:lang="la"><quote> impudentes </quote></cit>) <ref cRef="Hez." loc="2,4">Hez. 2,4</ref>; <foreign xml:lang="gez">ጽኑዐ፡ ክሳድ፡</foreign> <foreign xml:lang="grc">σκληροτράχηλος</foreign> (i.e. <cit type="translation" xml:lang="la"><quote> pertinax </quote></cit>, <cit type="translation" xml:lang="la"><quote> obstinatus </quote></cit>) <ref cRef="Prov." loc="29,1">Prov. 29,1</ref>; <ref cRef="Bar." loc="2,30">Bar. 2,30</ref>; <ref cRef="Ex. " loc="34,9">Ex.  34,9</ref>; <foreign xml:lang="gez">ጽኑዐ፡ ልብ፡</foreign> <foreign xml:lang="grc">σκληροκάρδιος</foreign> (durus corde) <ref cRef="Prov." loc="17,20">Prov. 17,20</ref>; <ref cRef="Prov." loc="28,14">Prov. 28,14</ref>; <ref cRef="Hez. " loc="3,7">Hez.  3,7</ref>. 
</sense> <sense n="b" xml:id="D1b"> <cit type="translation" xml:lang="la"><quote> rigidus </quote></cit>, <cit type="translation" xml:lang="la"><quote> severus </quote></cit>: <foreign xml:lang="gez">ንስሓ፡ ጽኑዕ፡</foreign> F.M. 6,3; <foreign xml:lang="gez">ሐተታ፡ ጽኑዕ፡</foreign> F.M. 23,6; <ref cRef="Sx. Ter" loc="27 ">Sx. Ter 27 </ref>Enc.; <foreign xml:lang="gez">በጽኑዕ፡</foreign> <cit type="translation" xml:lang="la"><quote> (rigide) </quote></cit> <foreign xml:lang="gez">ይትዔየነኒ፡</foreign> <ref cRef="Gad. Kar." loc="">Gad. Kar. </ref> 
</sense> <sense n="c" xml:id="D1c"> <cit type="translation" xml:lang="la"><quote> arduus </quote></cit>, <cit type="translation" xml:lang="la"><quote> difficilis </quote></cit>: <ref cRef="Act." loc="26,14">Act. 26,14</ref> <lbl expand="romanae editionis">rom.</lbl> 
</sense> 
</sense> <sense n="2" xml:id="D2"> <cit type="translation" xml:lang="la"><quote> solidus </quote></cit>: <foreign xml:lang="gez">መብልዕ፡ ጽኑዕ፡</foreign> <foreign xml:lang="grc">στερεά</foreign> <ref cRef="Hebr." loc="5,12">Hebr. 5,12</ref>; <ref cRef="Hebr." loc="5,14">Hebr. 5,14</ref>; <foreign xml:lang="gez">ልብስ፡ ጽኑዕ፡</foreign> <ref cRef="Herm." loc="104">Herm. p. 104</ref>; <foreign xml:lang="grc">στερεός</foreign>, <cit type="translation" xml:lang="de"><quote> massiv </quote></cit>, <ref cRef="Ex." loc="38,14">Ex. 38,14</ref>; <ref cRef="Ex." loc="38,16">Ex. 38,16</ref>; <ref cRef="Num." loc="8,4">Num. 8,4</ref>. <sense n="a" xml:id="D2a"> <cit type="translation" xml:lang="la"><quote> nervosus </quote></cit>, <foreign xml:lang="grc">στρίφνος</foreign> <ref cRef="Job" loc="20,18">Job 20,18</ref>. 
</sense> <sense n="b" xml:id="D2b"> <cit type="translation" xml:lang="la"><quote> compactus </quote></cit>, <cit type="translation" xml:lang="la"><quote> densus </quote></cit>: <foreign xml:lang="gez">ጽኑዕ፡ ከመ፡ እብን፡</foreign> <ref cRef="Job" loc="41,15">Job 41,15</ref>. 
</sense> <sense n="c" xml:id="D2c"> <cit type="translation" xml:lang="la"><quote> sanus </quote></cit>: <foreign xml:lang="gez">ዕፀው፡ ጽኑዓን፡</foreign> <ref cRef="Kuf." loc="21">Kuf. 21</ref>; de lapidibus <ref cRef="Herm." loc="83">Herm. p. 83</ref>; de dentibus <ref cRef="Kuf." loc="35.">Kuf. 35.</ref> 
</sense> 
</sense> <sense n="3" xml:id="D3"> <cit type="translation" xml:lang="la"><quote> firmus </quote></cit>: <foreign xml:lang="gez">አመ፡ ጽኑዐ፡ ይገብር፡ መሠረታተ፡ ምድር፡</foreign> <ref cRef="Prov." loc="8,29">Prov. 8,29</ref>; <foreign xml:lang="gez">ሙቃሔ፡ ጽኑዕ፡</foreign> <ref cRef="Sx. Masc." loc="6 Enc.">Sx. Masc. 6 Enc.</ref>; <ref cRef="3 Reg." loc="8,13">3 Reg. 8,13</ref> <lbl expand="varia lectio">var.</lbl>; <foreign xml:lang="gez">ጽኑዐ፡</foreign> <cit type="translation" xml:lang="la"><quote> firme </quote></cit>, <cit type="translation" xml:lang="la"><quote> firmiter </quote></cit>; <ref cRef="Cyr." loc="129 ">Cyr. f. 129 </ref>(<lbl expand="videas">vid. sub</lbl> <foreign xml:lang="gez">ቀምጠራ፡</foreign>). <sense n="a" xml:id="D3a"> <cit type="translation" xml:lang="la"><quote> firmatus </quote></cit>: <foreign xml:lang="gez">ኵሉ፡ ጽኑዕ፡ በስብሐቲሁ፡</foreign> <ref cRef="Sir." loc="42,17">Sir. 42,17</ref>; <ref cRef="Hen." loc="24,2">Hen. 24,2</ref>. 
</sense> <sense n="b" xml:id="D3b"> <cit type="translation" xml:lang="la"><quote> fixus </quote></cit>: <ref cRef="Abush." loc="37 ">Abush. 37 </ref>(<lbl expand="videas">vid.</lbl> <foreign xml:lang="gez">ነጥብ፡</foreign>). 
</sense> <sense n="c" xml:id="D3c"> <cit type="translation" xml:lang="la"><quote> confirmatus </quote></cit>, <cit type="translation" xml:lang="la"><quote> certus </quote></cit>: <ref cRef="4 Esr." loc="8,26">4 Esr. 8,26</ref>; <ref cRef="2 Petr." loc="1,10">2 Petr. 1,10</ref>; <foreign xml:lang="gez">እለ፡ ይትናገሩ፡ ጽንዕተ፡</foreign> <ref cRef="Asc. Jes." loc="3,27">Asc. Jes. 3,27</ref>. 
</sense> <sense n="d" xml:id="D3d"> <cit type="translation" xml:lang="la"><quote> ratus </quote></cit>, <ref cRef="Hebr." loc="9,17">Hebr. 9,17</ref>; <foreign xml:lang="grc">κεκυρωμένος</foreign> <ref cRef="Gal. " loc="3,15">Gal.  3,15</ref>. 
</sense> <sense n="e" xml:id="D3e"> <cit type="translation" xml:lang="la"><quote> munitus </quote></cit>: <foreign xml:lang="gez">አህጉር፡ ጽኑዓት፡</foreign> <ref cRef="Kuf." loc="11">Kuf. 11</ref>; <ref cRef="Kuf." loc="46">Kuf. 46</ref>; <ref cRef="Sir." loc="28,14">Sir. 28,14</ref>; <ref cRef="Jes." loc="25,2">Jes. 25,2</ref>; <foreign xml:lang="gez">ሀገር፡ ጽንዕት፡</foreign> <ref cRef="Jes." loc="26,1">Jes. 26,1</ref>; <ref cRef="Jer." loc="1,18">Jer. 1,18</ref>; <ref cRef="Zeph." loc="1,16">Zeph. 1,16</ref>; <ref cRef="Prov." loc="21,22">Prov. 21,22</ref>; <foreign xml:lang="gez">ብሔር፡ ጽኑዕ፡</foreign> <ref cRef="Ps." loc="70,3">Ps. 70,3</ref>; <foreign xml:lang="gez">ማኅፈድ፡ ጽኑዕ፡</foreign> <ref cRef="Ps." loc="60,3">Ps. 60,3</ref>; <foreign xml:lang="gez">አጽዋናት፡ ጽኑዓን፡</foreign> <ref cRef="2 Cor." loc="10,4">2 Cor. 10,4</ref>. 
</sense> <sense n="f" xml:id="D3f"> <cit type="translation" xml:lang="la"><quote> tutus </quote></cit>: <foreign xml:lang="grc">ἀσφαλής</foreign> <ref cRef="Prov." loc="15,7">Prov. 15,7</ref>; <ref cRef="Prov." loc="28,17">Prov. 28,17</ref>; <ref cRef="Sap." loc="14,3">Sap. 14,3</ref>. 
</sense> 
</sense> <sense n="4" xml:id="D4"> <cit type="translation" xml:lang="la"><quote> robustus </quote></cit>: <foreign xml:lang="gez">ጽኑዕ፡ ከመ፡ ኀጺን፡</foreign> <ref cRef="Dan." loc="2,40">Dan. 2,40</ref>; de viris <ref cRef="Dan." loc="3,20">Dan. 3,20</ref>; <ref cRef="Num." loc="13,18">Num. 13,18</ref>; <foreign xml:lang="gez">ጽኑዓን፡ በኀይል፡</foreign> 1 <ref cRef="Par." loc="9,13">Par. 9,13</ref> al.; <foreign xml:lang="grc">ἀνδρωθείς</foreign> <ref cRef="Job" loc="33,25">Job 33,25</ref>; de rebus: <foreign xml:lang="gez">መሥገርት፡ ጽኑዕ፡</foreign> <foreign xml:lang="grc">ἰσχυρά</foreign> <ref cRef="Prov." loc="6,2">Prov. 6,2</ref>; <foreign xml:lang="gez">ወልታ፡ ጽኑዕ፡</foreign> <ref cRef="Sir." loc="6,29">Sir. 6,29</ref>; <foreign xml:lang="gez">ዘጽኑዕ፡ ኀይሉ፡</foreign> <ref cRef="Jos." loc="10,7">Jos. 10,7</ref>; <ref cRef="1 Reg." loc="2,9">1 Reg. 2,9</ref>. 
