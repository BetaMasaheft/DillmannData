               <sense xml:id="Dmain" xml:lang="la" source="#dillmann"> <subc>II,1</subc> <cit type="translation" xml:lang="la"><quote> venire facere </quote></cit> vel <cit type="translation" xml:lang="la"><quote> jubere </quote></cit> i.e. <sense n="a" xml:id="Da"> <cit type="translation" xml:lang="la"><quote> afferre </quote></cit>, <cit type="translation" xml:lang="la"><quote> adducere </quote></cit>, <cit type="translation" xml:lang="la"><quote> secum ferre </quote></cit> <lbl expand="cum">c.</lbl> <gramGrp> <case value="accusativus">Acc.</case> </gramGrp> <lbl expand="persona">pers.</lbl> et rei: <ref cRef="Ruth" loc="3,15">Ruth 3,15</ref>; <ref cRef="Kuf." loc="3">Kuf. 3</ref>; <ref cRef="Gen." loc="2,19">Gen. 2,19</ref>; <ref cRef="Gen." loc="24,33">Gen. 24,33</ref>; <ref cRef="Gen." loc="27,25">Gen. 27,25</ref>; <ref cRef="Gen." loc="37,2">Gen. 37,2</ref>; <ref cRef="Gen." loc="43,2">Gen. 43,2</ref>; <ref cRef="Lev." loc="4,5">Lev. 4,5</ref>; <ref cRef="2 Reg. " loc="17,28">2 Reg.  17,28</ref>; <ref cRef="Luc." loc="12,51">Luc. 12,51</ref>; <ref cRef="Luc." loc="14,21">Luc. 14,21</ref>; <ref cRef="Matth." loc="21,2">Matth. 21,2</ref>; <ref cRef="Hen." loc="90,21">Hen. 90,21</ref>; <ref cRef="Hen." loc="90,26">Hen. 90,26</ref>; <cb n="0227" xml:id="c0227"/> <foreign xml:lang="gez">ኢያምጻእኩ፡ ምስሌየ፡</foreign> <ref cRef="1 Reg." loc="21,8">1 Reg. 21,8</ref>; <ref cRef="Judith" loc="12,3">Judith 12,3</ref>; <ref cRef="1 Tim. " loc="6,17">1 Tim.  6,17</ref>; <foreign xml:lang="gez">አምጽአት፡ ቅድሜሁ፡</foreign> <ref cRef="1 Reg." loc="28,25">1 Reg. 28,25</ref>; <foreign xml:lang="gez">ኣመጽእ፡ እኪተ፡ እመደቡብ፡</foreign> <ref cRef="Jer." loc="4,6">Jer. 4,6</ref>; <foreign xml:lang="gez">ውስተ፡ ትፍሥሕት፡ ኢታምጽእ፡ ሐዘነ፡</foreign> <ref cRef="Sir." loc="18,15">Sir. 18,15</ref>; <cit type="translation" xml:lang="la"><quote> deferre </quote></cit> marito dotem: <foreign xml:lang="gez">ትሎት፡ ዘአምጽአት፡ ኀቤሁ፡</foreign> F.N. 24,7. 
</sense> <sense n="b" xml:id="Db"> <cit type="translation" xml:lang="la"><quote> offerre </quote></cit> munera, sacrificia <ref cRef="Gen." loc="4,3">Gen. 4,3</ref>; <ref cRef="Gen." loc="4,4">Gen. 4,4</ref>; <ref cRef="Lev." loc="2,11">Lev. 2,11</ref>; <ref cRef="Lev." loc="22,18">Lev. 22,18</ref>; <ref cRef="Am. " loc="5,25">Am.  5,25</ref>; <ref cRef="Ps." loc="71,10">Ps. 71,10</ref>; <ref cRef="Hen." loc="53,1">Hen. 53,1</ref>. 
</sense> <sense n="c" xml:id="Dc"> <cit type="translation" xml:lang="la"><quote> adducere super aliquem </quote></cit> aliquid, <lbl expand="cum">c.</lbl> <gramGrp> <case value="accusativus">Acc.</case> </gramGrp> rei et <foreign xml:lang="gez">ላዕለ፡</foreign> vel <foreign xml:lang="gez">ዲበ፡</foreign> pers., ut: <foreign xml:lang="gez">ኀጢአተ፡</foreign> <ref cRef="Gen." loc="26,10">Gen. 26,10</ref>; <ref cRef="Ex." loc="32,21">Ex. 32,21</ref>; <foreign xml:lang="gez">መርገመ፡</foreign> <ref cRef="Kuf." loc="26">Kuf. 26</ref>; <foreign xml:lang="gez">ማየ፡ አይኅ፡</foreign> <ref cRef="Gen." loc="6,17">Gen. 6,17</ref>; <ref cRef="Kuf." loc="4">Kuf. 4</ref>; <foreign xml:lang="gez">እኩየ፡</foreign> <ref cRef="Jer." loc="39,42">Jer. 39,42</ref>; <ref cRef="Jer." loc="51,2">Jer. 51,2</ref>; <foreign xml:lang="gez">እዴየ፡</foreign> <ref cRef="Zach." loc="2,9">Zach. 2,9</ref>; <foreign xml:lang="gez">መዋዕለ፡</foreign> <ref cRef="Jes." loc="7,17">Jes. 7,17</ref>; <ref cRef="Jes." loc="27,1">Jes. 27,1</ref>; <foreign xml:lang="gez">መቅሠፍተ፡</foreign> <ref cRef="Sir." loc="47,20">Sir. 47,20</ref>; <lbl expand="cum">c.</lbl> <foreign xml:lang="gez">ለ</foreign> pers. <ref cRef="Rom." loc="13,2">Rom. 13,2</ref>. 
</sense> <sense n="d" xml:id="Dd"> <cit type="translation" xml:lang="la"><quote> evenire jubere </quote></cit>, <cit type="translation" xml:lang="la"><quote> eventum dare </quote></cit>: <foreign xml:lang="gez">አነ፡ ኣመጽእ፡ ነገርየ፡ ለዛ፡ ሀገር፡</foreign> <ref cRef="Jer." loc="46,16">Jer. 46,16</ref>; <foreign xml:lang="gez">ነበብኩ፡ ወአምጻእኩ፡</foreign> <ref cRef="Jes." loc="46,11">Jes. 46,11</ref>; <ref cRef="Hen." loc="74,11">Hen. 74,11</ref>. 
</sense> <sense n="e" xml:id="De"> <cit type="translation" xml:lang="la"><quote> nasci </quote></cit> vel <cit type="translation" xml:lang="la"><quote> oriri facere </quote></cit> vel <cit type="translation" xml:lang="la"><quote> jubere </quote></cit>, i.e. <cit type="translation" xml:lang="la"><quote> excitare </quote></cit>, <cit type="translation" xml:lang="la"><quote> procreare </quote></cit>: <foreign xml:lang="grc">ἐξεγείρειν</foreign> <ref cRef="2 Reg." loc="12,11">2 Reg. 12,11</ref>; <foreign xml:lang="gez">አምጽአ፡ ነፋሰ፡</foreign> <ref cRef="Jon." loc="1,4">Jon. 1,4</ref>; <foreign xml:lang="gez">ታመጽእ፡ ጽልመተ፡</foreign> <ref cRef="Ps." loc="103,21">Ps. 103,21</ref>; <foreign xml:lang="gez">አዝልፎ፡ ስካር፡ ያመጽእ፡ ኮብሰ፡ ወሞተ፡ ግብት፡ ወፈጸንተ፡</foreign> F.N. 50. 
</sense> <sense n="f" xml:id="Df"> <cit type="translation" xml:lang="la"><quote> afferre </quote></cit>, <cit type="translation" xml:lang="la"><quote> proponere </quote></cit>, <cit type="translation" xml:lang="la"><quote> in medium proferre </quote></cit> sermonem, argumentum, exemplum: <ref cRef="Matth." loc="13,24">Matth. 13,24</ref>; <ref cRef="Matth." loc="13,31">Matth. 13,31</ref>; <foreign xml:lang="gez">አምጽአ፡ ጥያቄ፡</foreign> Chrys., creberrime; <foreign xml:lang="gez">አኀዘ፡ ጳውሎስ፡ ያምጽእ፡ ትእምርተ፡ እምሥርዐተ፡ ደብተራ፡</foreign> <ref cRef="Chrys. ho." loc="15.">Chrys. ho. 15.</ref> 
</sense> </sense>
               
            <sense xml:id="Tmain" xml:lang="en" source="#traces"> <cit type="transcription" xml:lang="gez"><quote>ʾamṣǝʾa</quote></cit> <sense n="G" xml:id="TG"> <foreign xml:lang="gez">አምጽአ፡</foreign> <cit type="transcription" xml:lang="gez"><quote>ʾamṣǝʾa</quote></cit> <subc>II,1</subc> <foreign xml:lang="gez">ያመጽእ፡</foreign>, <foreign xml:lang="gez">ያምጽእ፡</foreign> «<cit type="translation" xml:lang="fr"><quote>faire venir</quote></cit>, <cit type="translation" xml:lang="fr"><quote>apporter</quote></cit>» ― <foreign xml:lang="am">አመጣ፡</foreign> Ms. <ref type="BM" target="BNFabb217">BNFabb217</ref>, fol. 48r. <lb/>
 <bibl><ptr target="bm:Grebaut1952SuppLexicon"/><citedRange unit="page"> 118</citedRange></bibl> 
</sense> <sense n="L" xml:id="TL"> <foreign xml:lang="gez">አምጽአ</foreign> <cit type="transcription" xml:lang="gez"><quote>ʾamṣǝʾa</quote></cit> <cit type="translation" xml:lang="en"><quote>bring</quote></cit>, <cit type="translation" xml:lang="en"><quote>offer</quote></cit>, <cit type="translation" xml:lang="en"><quote>cause to happen</quote></cit>, <cit type="translation" xml:lang="en"><quote>bring about</quote></cit>, <cit type="translation" xml:lang="en"><quote>raise up</quote></cit>, <cit type="translation" xml:lang="en"><quote>convey</quote></cit> (a thing to a place), <cit type="translation" xml:lang="en"><quote>put forth</quote></cit> <bibl><ptr target="bm:Leslau1987dic"/><citedRange unit="page"> 369b</citedRange></bibl> 
</sense> </sense></entry>
