               <sense xml:id="Dmain" xml:lang="la" source="#dillmann"> <gramGrp> <pos expand="Substantivum">subst.</pos> </gramGrp>, plerumque <gramGrp> <gen value="not found">m.</gen> </gramGrp>, <lbl expand="pluralis">Pl.</lbl> <foreign xml:lang="gez">አቅርንት፡</foreign> [<foreign xml:lang="he">קֶרֶן</foreign> <foreign xml:lang="syr">ܩܰܪܢܳܐ</foreign> <foreign xml:lang="ar">قَرْنٌ</foreign>] 1) <cit type="translation" xml:lang="la"><quote> cornu </quote></cit> <ref cRef="Gen." loc="22,13">Gen. 22,13</ref>; <ref cRef="Ps. " loc="21,22">Ps.  21,22</ref>; <ref cRef="Dan." loc="7,">Dan. 7,</ref> 8 <lbl expand="sequens">seq.</lbl>; <ref cRef="Gen." loc="8,3">Gen. 8,3</ref> <lbl expand="sequens">seq.</lbl>; <ref cRef="Apoc." loc="6,6">Apoc. 6,6</ref>; <ref cRef="Apoc." loc="12,3">Apoc. 12,3</ref>; <ref cRef="Apoc." loc="13,1">Apoc. 13,1</ref>; <ref cRef="Hen." loc="90,9">Hen. 90,9</ref> <lbl expand="sequens">seq.</lbl>; <ref cRef="3 Reg." loc="22,11">3 Reg. 22,11</ref>. <sense n="a" xml:id="Da"> <foreign xml:lang="gez">ቀርነ፡ ነጌ፡</foreign> <cb n="0428" xml:id="c0428"/> (cornu i.e. dens elephanti) <ref cRef="Am." loc="3,15">Am. 3,15</ref>; <ref cRef="Am. " loc="6,14">Am.  6,14</ref> (alia <lbl expand="videas">vid. sub</lbl> <foreign xml:lang="gez">ነጌ፡</foreign>); etiam omisso <foreign xml:lang="gez">ነጌ፡</foreign>, ut: <foreign xml:lang="gez">ክቡዳን፡ አቅርንት፡</foreign> <ref cRef="Ps." loc="44,10">Ps. 44,10</ref>; <ref cRef="Jsp." loc="283.">Jsp. p. 283.</ref> 
</sense> <sense n="b" xml:id="Db"> <foreign xml:lang="gez">ዘአሐዱ፡ ቀርኑ፡</foreign> <cit type="translation" xml:lang="la"><quote> monoceros </quote></cit>, <cit type="translation" xml:lang="la"><quote> unicornis </quote></cit> <ref cRef="Ps. " loc="21,22">Ps.  21,22</ref>; <ref cRef="Ps." loc="28,6">Ps. 28,6</ref>; <ref cRef="Ps." loc="91,10">Ps. 91,10</ref>; <ref cRef="Num." loc="23,22">Num. 23,22</ref>; <ref cRef="Deut." loc="33,17">Deut. 33,17</ref>. 
</sense> <sense n="c" xml:id="Dc"> <foreign xml:lang="gez">ዘክልኤቱ፡ አቅርንቲሁ፡</foreign> <cit type="translation" xml:lang="la"><quote> bicornis </quote></cit>, de Alexandro M., <ref cRef="Cat. Cod. Ox. Bodl." loc="79">Cat. Cod. Ox. Bodl. p. 79</ref>; <foreign xml:lang="gez">እስክንድር፡ ዘይሰመይ፡ ዘ፪ አቅርንቲሁ፡ እስመ፡ ውእቱ፡ ነግሠ፡ በአቅርንተ፡ ፀሓይ፡ እምሥራቂሃ፡ እስከ፡ ምዕራቢሃ፡</foreign> <ref cRef="Abush." loc="48.">Abush. 48.</ref> 
</sense> <sense n="d" xml:id="Dd"> cornu est <cit type="translation" xml:lang="la"><quote> symbolum roboris et potentiae </quote></cit>: <foreign xml:lang="gez">ይበቍል፡ ቀርን፡ ለቤተ፡ እስራኤል፡</foreign> <ref cRef="Hez." loc="29,21">Hez. 29,21</ref>; <foreign xml:lang="gez">ተሰብረ፡ ቀርነ፡ ሞአብ፡</foreign> <ref cRef="Jer." loc="31,25">Jer. 31,25</ref>; <ref cRef="Sir." loc="47,7">Sir. 47,7</ref>; <foreign xml:lang="gez">አንሥአ፡ ቀርኖ፡</foreign> <ref cRef="Ps." loc="74,4">Ps. 74,4</ref>; <ref cRef="Ps." loc="74,5">Ps. 74,5</ref>; <ref cRef="Ps." loc="88,17">Ps. 88,17</ref>; <ref cRef="Ps." loc="91,10">Ps. 91,10</ref>; <ref cRef="Ps." loc="131,18">Ps. 131,18</ref>; <ref cRef="Sir." loc="47,5">Sir. 47,5</ref>; <ref cRef="1 Reg. " loc="2,1">1 Reg.  2,1</ref>. 
</sense> <sense n="e" xml:id="De"> <cit type="translation" xml:lang="la"><quote> cornua altaris </quote></cit> <ref cRef="Ex." loc="27,2">Ex. 27,2</ref> <lbl expand="sequens">seq.</lbl>; <ref cRef="Ps." loc="117,26">Ps. 117,26</ref>; <ref cRef="Joel" loc="2,17">Joel 2,17</ref>; <ref cRef="Apoc." loc="9,13">Apoc. 9,13</ref>. 
</sense> <sense n="f" xml:id="Df"> <cit type="translation" xml:lang="la"><quote> cornu </quote></cit>, <cit type="translation" xml:lang="la"><quote> buccina </quote></cit>, <cit type="translation" xml:lang="la"><quote> tuba </quote></cit>: <foreign xml:lang="grc">σάλπιγξ</foreign> <ref cRef="Ps." loc="97,7">Ps. 97,7</ref>; <ref cRef="Job" loc="39,24">Job 39,24</ref>; <ref cRef="Job" loc="39,25">Job 39,25</ref>; <foreign xml:lang="gez">ቀርነ፡ ዝብጦ፡</foreign> <ref cRef="Sir." loc="50,16">Sir. 50,16</ref>; <foreign xml:lang="gez">ቃለ፡ ቀርን፡</foreign> <ref cRef="Jes." loc="18,3">Jes. 18,3</ref>; <ref cRef="Jes. " loc="27,13">Jes.  27,13</ref>; <ref cRef="Jes." loc="44,23">Jes. 44,23</ref>; <ref cRef="Jes." loc="58,1">Jes. 58,1</ref>; <ref cRef="Jer." loc="4,5">Jer. 4,5</ref>; <ref cRef="Jer." loc="4,19">Jer. 4,19</ref>; <ref cRef="Dan." loc="3,5">Dan. 3,5</ref>; <ref cRef="Zeph." loc="1,16">Zeph. 1,16</ref>; <ref cRef="1 Esr. " loc="5,57">1 Esr.  5,57</ref>; <ref cRef="1 Par." loc="15,24">1 Par. 15,24</ref>; <ref cRef="2 Par." loc="5,13">2 Par. 5,13</ref>; <ref cRef="Matth." loc="24,31">Matth. 24,31</ref>; <ref cRef="1 Cor." loc="14,8">1 Cor. 14,8</ref>; <foreign xml:lang="grc">κερατίνη</foreign> <ref cRef="Jud." loc="7,8">Jud. 7,8</ref>; <ref cRef="2 Reg." loc="15,10">2 Reg. 15,10</ref>; <ref cRef="Jos." loc="6,4">Jos. 6,4</ref>; etiam <ref cRef="1 Par." loc="25,6">1 Par. 25,6</ref> (ubi Graecus <foreign xml:lang="grc">νάβλαι</foreign> habet). 
</sense> <sense n="g" xml:id="Dg"> cornu, quo pro <cit type="translation" xml:lang="la"><quote> vasculo </quote></cit> utuntur: <foreign xml:lang="gez">ቀርነ፡ ቅብእ፡</foreign> <foreign xml:lang="grc">κέρας</foreign> <ref cRef="1 Reg." loc="16,1">1 Reg. 16,1</ref>; <ref cRef="3 Reg." loc="1,39">3 Reg. 1,39</ref>; <foreign xml:lang="grc">φακός</foreign> <ref cRef="1 Reg." loc="10,1">1 Reg. 10,1</ref>. 
</sense> <sense n="h" xml:id="Dh"> <cit type="translation" xml:lang="la"><quote> frustum </quote></cit> panis cornu simile (de pane orbiculari): <foreign xml:lang="gez">ቀርነ፡ ኅብስት፡</foreign> <foreign xml:lang="grc">κλάσμα ἄρτου</foreign> <ref cRef="Hez." loc="13,19">Hez. 13,19</ref>. – <cit type="translation" xml:lang="la"><quote> Voc. Ae. </quote></cit>: <foreign xml:lang="gez">ቀርን፡ መጥቅዕ፡ ብ፡ ነጋሪት፡ ቀንድ፡</foreign>; aliud: <foreign xml:lang="gez">ቀርን፡ ዘ፡ ነጋሪት፡ ገንታሂ፡ ይ።</foreign> 
</sense>{<ref target="#c1430"/> <sense n="i" xml:id="Di"> <cit type="translation" xml:lang="la"><quote> pars </quote></cit> rei <cit type="translation" xml:lang="la"><quote> anterior </quote></cit> vel <cit type="translation" xml:lang="la"><quote> extrema </quote></cit> (<cit type="translation" xml:lang="de"><quote> Spitze </quote></cit>), <ref cRef="I Sal. Mar." loc=" 33">I Sal. Mar.  33</ref> (<lbl expand="videas">vid. sub</lbl> <foreign xml:lang="gez">አጽባዕት፡</foreign>) 
</sense>} </sense>
               
