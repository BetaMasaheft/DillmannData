               <sense xml:id="Dmain" xml:lang="la" source="#dillmann"> <gramGrp> <pos expand="Substantivum">subst.</pos> </gramGrp>, <gramGrp> <gen value="not found">m.</gen> </gramGrp> et <gramGrp> <gen value="femininus">fem.</gen> </gramGrp>, <lbl expand="collatio, -is vel collectivum,&#xA;                            -e">coll.</lbl> et <lbl expand="">sing.</lbl>, <lbl expand="pluralis">Pl.</lbl> <foreign xml:lang="gez">ሰረገላት፡</foreign>, qui et ipse singulariter usurpatur: <sense n="1" xml:id="D1"> <cit type="translation" xml:lang="la"><quote> currus </quote></cit> cujusvis generis (<cit type="translation" xml:lang="la"><quote> vehiculum </quote></cit>, <cit type="translation" xml:lang="la"><quote> plaustrum </quote></cit>, <cit type="translation" xml:lang="la"><quote> pilentum </quote></cit>, <cit type="translation" xml:lang="la"><quote> rheda </quote></cit>, <cit type="translation" xml:lang="la"><quote> currus bellicus </quote></cit>); <cb n="0348" xml:id="c0348"/> <foreign xml:lang="gez">ሰረገላ፡</foreign> coll. <foreign xml:lang="grc">ἅρματα</foreign> <ref cRef="Jes." loc="5,28">Jes. 5,28</ref> <lbl expand="varia lectio">var.</lbl>; <ref cRef="Jes." loc="22,7">Jes. 22,7</ref>; <ref cRef="Jes." loc="31,1">Jes. 31,1</ref> <lbl expand="varia lectio">var.</lbl>; <foreign xml:lang="gez">በብዙኅ፡ ሰረገላየ፡</foreign> <ref cRef="Jes." loc="37,24">Jes. 37,24</ref>; <ref cRef="Jes. " loc="43,17">Jes.  43,17</ref>; <ref cRef="Jes." loc="66,15">Jes. 66,15</ref>; <ref cRef="Jer." loc="4,13">Jer. 4,13</ref>; <ref cRef="Zach." loc="6,1">Zach. 6,1</ref> <lbl expand="varia lectio">var.</lbl>; <lbl expand="">sing.</lbl> <foreign xml:lang="grc">ἅρμα</foreign> <foreign xml:lang="gez">ሰረገላ፡ ሠናይ፡</foreign> <ref cRef="Jes." loc="22,18">Jes. 22,18</ref>; <ref cRef="Job" loc="39,25">Job 39,25</ref>; <ref cRef="Act." loc="8,28">Act. 8,28</ref>; <foreign xml:lang="gez">ላዕለ፡ ሰረገላ፡ ዘኪሩቤል፡</foreign> <ref cRef="Sir." loc="49,8">Sir. 49,8</ref>; <foreign xml:lang="grc">ἅμαξα</foreign> <ref cRef="1 Reg." loc="6,7">1 Reg. 6,7</ref> <lbl expand="sequens">seq.</lbl>; <ref cRef="2 Reg." loc="6,3">2 Reg. 6,3</ref>; <foreign xml:lang="gez">ሰረገላ፡ ሐዲስ፡</foreign> <ref cRef="1 Par." loc="13,7">1 Par. 13,7</ref>; <ref cRef="Sir." loc="36,5">Sir. 36,5</ref>; <foreign xml:lang="gez">ሰረገላት፡</foreign> <lbl expand="">pl.</lbl> <foreign xml:lang="grc">ἅρματα</foreign> <ref cRef="Jes." loc="2,7">Jes. 2,7</ref>; <ref cRef="Jes." loc="5,28">Jes. 5,28</ref>; <ref cRef="Jes." loc="22,7">Jes. 22,7</ref>; <ref cRef="Jes." loc="31,1">Jes. 31,1</ref>; <ref cRef="Jer." loc="27,3">Jer. 27,3</ref>; <ref cRef="Judith" loc="2,19">Judith 2,19</ref>; <foreign xml:lang="gez">ሰብአ፡ ሰረገላት፡</foreign> <ref cRef="Judith" loc="7,20">Judith 7,20</ref>; <ref cRef="1 Reg." loc="13,5">1 Reg. 13,5</ref>; <ref cRef="Ps. " loc="19,8">Ps.  19,8</ref>; <ref cRef="Apoc." loc="9,9">Apoc. 9,9</ref>; <foreign xml:lang="grc">ἅμαξαι</foreign> <ref cRef="Judith" loc="15,11">Judith 15,11</ref>; <ref cRef="Gen." loc="45,19">Gen. 45,19</ref>; <ref cRef="Gen." loc="45,21">Gen. 45,21</ref>; <foreign xml:lang="grc">λαμπηνικαί</foreign> <ref cRef="Num." loc="7,3">Num. 7,3</ref>; <lbl expand="">sing.</lbl> <foreign xml:lang="gez">ዘዐርገ፡ በነደ፡ እሳት፡ በሰረገላት፡</foreign> <ref cRef="Sir." loc="48,9">Sir. 48,9</ref>; <ref cRef="2 Reg." loc="6,3">2 Reg. 6,3</ref>.
</sense> <sense n="2" xml:id="D2"> passim: <cit type="translation" xml:lang="la"><quote> rotae </quote></cit>, <cit type="translation" xml:lang="la"><quote> rota </quote></cit>: <foreign xml:lang="gez">ሰረገላ፡</foreign> var. <foreign xml:lang="gez">ሰረገላት፡</foreign> <foreign xml:lang="grc">τροχοί</foreign> <ref cRef="Dan." loc="7,9">Dan. 7,9</ref>; <foreign xml:lang="gez">ሰረገላ፡</foreign> <foreign xml:lang="grc">τροχοί</foreign> <ref cRef="Hez." loc="3,13">Hez. 3,13</ref>; <foreign xml:lang="gez">ሰረገላ፡ ፩፡</foreign> <foreign xml:lang="grc">τροχὸς εἷς</foreign> <ref cRef="Hez." loc="1,15">Hez. 1,15</ref>; <foreign xml:lang="gez">ሰረገላት፡</foreign> <foreign xml:lang="grc">τροχοί</foreign> <ref cRef="Hez. " loc="1,16">Hez.  1,16</ref>; Titus <foreign xml:lang="gez">ገብረ፡ ዐበይተ፡ ማኅፈዳተ፡ እምዕፅ፡ ወታሕቴሁ፡ ሰረገላ፡</foreign> <ref cRef="Jsp." loc="358.">Jsp. p. 358.</ref> 
</sense> </sense>
               
            <sense xml:id="Tmain" xml:lang="en" source="#traces"> <sense n="3" xml:id="T3"> <cit type="transcription" xml:lang="gez"><quote> saragalā </quote></cit> <cit type="translation" xml:lang="en"><quote> beam </quote></cit>, ‘<cit type="translation" xml:lang="it"><quote> trave </quote></cit>’ <foreign xml:lang="gez">ወተለወጠ፡ ሰረገላቲሃ፡ ወመራናቲሃ፡ ወኵሉ፡ ንዋያ፡ ለቤተ፡ ክርስቲያን፡ ተለወጠ፡</foreign> ‘<cit type="translation" xml:lang="it"><quote> si mutarono le sue travi, si mutarono i suoi candelabri e tutte le sue suppellettili della chiesa </quote></cit>’ <bibl><ptr target="bm:Marrassini2003GabraManfasQeddusTextus"/><citedRange unit="page"> 202 (ed.)</citedRange></bibl>, <bibl><ptr target="bm:Marrassini2003GabraManfasQeddusVersio"/><citedRange unit="page"> 66 (tr.)</citedRange></bibl> (‘Cf. <bibl><ptr target="bm:Leslau1987dic"/><citedRange unit="page"> 512</citedRange></bibl>.’ <bibl><ptr target="bm:Marrassini2003GabraManfasQeddusVersio"/><citedRange unit="page"> 66 n. 283</citedRange></bibl>) 
</sense> </sense>
