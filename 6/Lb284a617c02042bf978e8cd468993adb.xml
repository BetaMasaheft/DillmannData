               <sense xml:id="Dmain" xml:lang="la" source="#dillmann"> <gramGrp> <pos expand="Substantivum">subst.</pos> </gramGrp>, <gramGrp> <gen value="femininus">fem.</gen> </gramGrp> et <gramGrp> <gen value="not found">m.</gen> </gramGrp>, <lbl expand="pluralis">Pl.</lbl> <foreign xml:lang="gez">አዕይንት፡</foreign> [<foreign xml:lang="he">עַיִן</foreign> <foreign xml:lang="syr">ܥܻܝܢ ܥܰܝܢܳܐ</foreign> <foreign xml:lang="ar">عَيْنٌ</foreign>] <sense n="1" xml:id="D1"> <cit type="translation" xml:lang="la"><quote> oculus </quote></cit>, <ref cRef="Ex. " loc="21,24">Ex.  21,24</ref>; <gramGrp> <gen value="femininus">fem.</gen> </gramGrp> <ref cRef="Job" loc="7,7">Job 7,7</ref>; <ref cRef="Job" loc="13,1">Job 13,1</ref>; <ref cRef="Job" loc="16,20">Job 16,20</ref>; <ref cRef="Job" loc="29,11">Job 29,11</ref>; <ref cRef="Sir." loc="16,5">Sir. 16,5</ref>; <ref cRef="Matth." loc="5,29">Matth. 5,29</ref>; <gramGrp> <gen value="not found">m.</gen> </gramGrp> <ref cRef="Koh. " loc="1,8">Koh.  1,8</ref>; <ref cRef="Koh." loc="4,8">Koh. 4,8</ref>; <ref cRef="Matth." loc="6,22">Matth. 6,22</ref>; <ref cRef="Matth." loc="6,23">Matth. 6,23</ref>; <ref cRef="Matth." loc="20,15">Matth. 20,15</ref>; <ref cRef="Luc." loc="11,34">Luc. 11,34</ref>; (ad sensum) <ref cRef="Job " loc="24,15">Job  24,15</ref>; <ref cRef="Sir." loc="14,9">Sir. 14,9</ref>; <ref cRef="Sir." loc="14,10">Sir. 14,10</ref>; <ref cRef="Sir." loc="23,19">Sir. 23,19</ref> al.; <foreign xml:lang="gez">በፍሡሕ፡ ዐይን፡</foreign> <ref cRef="Sir." loc="32,10">Sir. 32,10</ref>; <foreign xml:lang="gez">በሠናይ፡ ዐይን፡</foreign> <ref cRef="Sir." loc="32,12">Sir. 32,12</ref>; <foreign xml:lang="gez">ከመ፡ እኩይ፡ ዐይነ፡ ሰብእ፡</foreign> <ref cRef="Sir." loc="34,13">Sir. 34,13</ref>; <foreign xml:lang="gez">አዕይንቲሃ፡ ድኩም፡</foreign> <ref cRef="Kuf." loc="28">Kuf. 28</ref>; <ref cRef="1 Reg." loc="16,12">1 Reg. 16,12</ref>; <foreign xml:lang="gez">ትደዊ፡ አዕይንቲሃ፡</foreign> <ref cRef="Gen." loc="29,17">Gen. 29,17</ref>; <foreign xml:lang="gez">ሚጥ፡ ዐይነከ፡ እምነ፡ ብእሲት፡ ሠናይት፡</foreign> <ref cRef="Sir. " loc="9,8">Sir.  9,8</ref>; <ref cRef="Sir." loc="4,1">Sir. 4,1</ref>; <foreign xml:lang="gez">ይኔጽራ፡ አዕይንቲሁ፡</foreign> <ref cRef="Jes." loc="17,7">Jes. 17,7</ref>; <foreign xml:lang="gez">ተሀይዳ፡ አዕይንቲሁ፡</foreign> <ref cRef="Judith" loc="16,9">Judith 16,9</ref>; <foreign xml:lang="gez">ከደኑ፡ አዕይንቲሆሙ፡</foreign> <ref cRef="Matth." loc="13,15">Matth. 13,15</ref>; <foreign xml:lang="gez">ከሠተት፡ አዕይንቲሃ፡</foreign> <ref cRef="Kuf." loc="17">Kuf. 17</ref>; <foreign xml:lang="gez">ፈትሐ፡ አዕይንቲሃ፡</foreign> <ref cRef="Gen." loc="21,19">Gen. 21,19</ref>; <foreign xml:lang="gez">አንሥኡ፡ አዕይንቲሆሙ፡</foreign> <ref cRef="Matth." loc="17,8">Matth. 17,8</ref>; <foreign xml:lang="gez">አልዐለ፡ አዕይንቲሁ፡</foreign> <ref cRef="Gen." loc="13,10">Gen. 13,10</ref>; <foreign xml:lang="gez">ከብዱ፡ አዕይንቲሁ፡</foreign> <ref cRef="1 Reg. " loc="3,2">1 Reg.  3,2</ref>; <foreign xml:lang="gez">ታነብዕ፡ ዐይንየ፡</foreign> <ref cRef="Jer." loc="13,17">Jer. 13,17</ref>; <foreign xml:lang="gez">ተሀውከት፡ ዐይንየ፡</foreign> <ref cRef="Ps." loc="30,11">Ps. 30,11</ref>; <foreign xml:lang="gez">ይቀጽብ፡ በዐይን፡</foreign> <ref cRef="Prov." loc="6,13">Prov. 6,13</ref>; <foreign xml:lang="gez">ቅድመ፡ አዕይንቲከ፡</foreign> <ref cRef="2 Reg." loc="11,25">2 Reg. 11,25</ref>. <sense n="a" xml:id="D1a"> <foreign xml:lang="gez">ዐይነ፡ በዐይን፡</foreign> <cit type="translation" xml:lang="la"><quote> oculo ad oculum </quote></cit>: <foreign xml:lang="gez">ይትራአዩ፡ ዐይነ፡ በዐይን፡</foreign> <ref cRef="Jer." loc="39,4">Jer. 39,4</ref>; <ref cRef="Jes." loc="52,8">Jes. 52,8</ref>; <ref cRef="Num. " loc="14,14">Num.  14,14</ref>. 
</sense> <sense n="b" xml:id="D1b"> <foreign xml:lang="gez">ለዐይነ፡</foreign> <cit type="translation" xml:lang="la"><quote> sub oculo </quote></cit> alicujus i.e. <cit type="translation" xml:lang="la"><quote> coram </quote></cit>aliquo: <foreign xml:lang="gez">ለዐይንከ፡</foreign> <ref cRef="Sir." loc="27,23">Sir. 27,23</ref>; <ref cRef="Matth." loc="6,1">Matth. 6,1</ref> Platt; <ref cRef="Matth." loc="22,5">Matth. 22,5</ref>; <ref cRef="Luc." loc="16,15">Luc. 16,15</ref> (coll. <foreign xml:lang="gez">አዕይንቲከ፡ ለአዕይንቲሁ፡ ይሬእያ፡</foreign> <ref cRef="Jer." loc="41,3">Jer. 41,3</ref>); <ref cRef="Sx. Haml." loc="7 ">Sx. Haml. 7 </ref>Enc. 
</sense> <sense n="c" xml:id="D1c"> <foreign xml:lang="gez">ዐይን፡ ሕሙም፡</foreign> vel <foreign xml:lang="gez">ሐማሚ፡</foreign> <cit type="translation" xml:lang="la"><quote> oculus aeger </quote></cit>vel <cit type="translation" xml:lang="la"><quote> invidus </quote></cit> <ref cRef="Luc." loc="11,34">Luc. 11,34</ref> <lbl expand="romanae editionis">rom.</lbl>; <ref cRef="Matth." loc="20,15">Matth. 20,15</ref>; eundem sensum habet <foreign xml:lang="gez">ያአኪ፡ ላዕሌከ፡ ዐይኖ፡</foreign> <cit type="translation" xml:lang="la"><quote> invidebit tibi </quote></cit> <ref cRef="Deut." loc="15,9">Deut. 15,9</ref>; <foreign xml:lang="gez">በዐይን፡ እኪት፡</foreign> <foreign xml:lang="grc">ἀναιδεῖ ὀφθαλμῷ</foreign> <ref cRef="1 Reg." loc="2,29">1 Reg. 2,29</ref>; <foreign xml:lang="gez">ዐይን፡ ዕቡይ፡</foreign> <cit type="translation" xml:lang="la"><quote> oculus superbus </quote></cit> <ref cRef="Prov." loc="24,36">Prov. 24,36</ref>; <foreign xml:lang="gez">ዕቡየ፡ ዐይን፡</foreign> <ref cRef="Ps." loc="100,7">Ps. 100,7</ref>; <foreign xml:lang="gez">ዘይሬእዮሙ፡ ለሕዝብ፡ በዐይን፡ ሕጽጽት፡</foreign> <cit type="translation" xml:lang="la"><quote> qui homines </quote></cit>(oculo vel obtutu exiguo intuetur i.e.) <cit type="translation" xml:lang="la"><quote> ut multum infra despectat </quote></cit>F.N. 5. 
</sense> <sense n="d" xml:id="D1d"> <foreign xml:lang="gez">ሰብአ፡ ዐይን፡</foreign> vel (omisso <foreign xml:lang="gez">ሰብእ፡</foreign>) <foreign xml:lang="gez">ዘዐይን፡</foreign> (<ref cRef="2 Reg." loc="13,34">2 Reg. 13,34</ref>) homo vel homines oculorum i.e. <sense n="α" xml:id="D1dα"> <cit type="translation" xml:lang="la"><quote> explorator </quote></cit>, <cit type="translation" xml:lang="la"><quote> e </quote></cit> <cit type="translation" xml:lang="la"><quote> xploratore </quote></cit> <cit type="translation" xml:lang="la"><quote> s </quote></cit>, <cit type="translation" xml:lang="la"><quote> speculator </quote></cit>, <cit type="translation" xml:lang="la"><quote> speculator </quote></cit> <cit type="translation" xml:lang="la"><quote> es </quote></cit>, <foreign xml:lang="grc">σκοπός</foreign>, <foreign xml:lang="grc">σκοποί</foreign>, <foreign xml:lang="grc">κατάσκοπος</foreign>, <foreign xml:lang="grc">κατάσκοποι</foreign>, <ref cRef="Gen. " loc="42,9">Gen.  42,9</ref>; <ref cRef="Num." loc="13,2">Num. 13,2</ref> <lbl expand="sequens">seq.</lbl>; <ref cRef="Kuf." loc="42">Kuf. 42</ref>; <ref cRef="Jos." loc="2,1">Jos. 2,1</ref> <lbl expand="sequens">seq.</lbl>; <ref cRef="1 Reg." loc="26,4">1 Reg. 26,4</ref> <lbl expand="varia lectio">var.</lbl>; <ref cRef="Kuf." loc="42">Kuf. 42</ref>; <ref cRef="Jer. " loc="6,17">Jer.  6,17</ref>; <ref cRef="Hez." loc="33,6">Hez. 33,6</ref>; <ref cRef="Judith" loc="7,7">Judith 7,7</ref>; <ref cRef="Sir." loc="11,30">Sir. 11,30</ref>; <ref cRef="Sir." loc="37,14">Sir. 37,14</ref>; <ref cRef="Phil." loc="3,14">Phil. 3,14</ref>; <ref cRef="Jac." loc="2,25">Jac. 2,25</ref>; <foreign xml:lang="grc">τερατοσκόποι</foreign> <ref cRef="Zach." loc="3,8">Zach. 3,8</ref>. 
</sense> <sense n="β" xml:id="D1dβ"> <cit type="translation" xml:lang="la"><quote> custos </quote></cit> vel <cit type="translation" xml:lang="la"><quote> inspector </quote></cit>(<foreign xml:lang="grc">ἐπίσκοπος</foreign>), <lbl expand="exempli gratia">ex. gr.</lbl> beneficii testamento legati, F.N. 18. 
</sense> 
</sense> <sense n="e" xml:id="D1e"> Etiam <foreign xml:lang="gez">ዐይን፡</foreign> <cit type="translation" xml:lang="la"><quote> oculus </quote></cit>vel <foreign xml:lang="gez">አዕይንት፡</foreign> <cit type="translation" xml:lang="la"><quote> oculi </quote></cit> (sicut <foreign xml:lang="ar">عَيْنٌ</foreign>) dicitur <sense n="α" xml:id="D1eα"> <cit type="translation" xml:lang="la"><quote> speculator </quote></cit>, <cit type="translation" xml:lang="la"><quote> explorator </quote></cit> <ref cRef="1 Reg." loc="26,4">1 Reg. 26,4</ref>; <ref cRef="2 Reg." loc="10,3">2 Reg. 10,3</ref>. 
</sense> <sense n="β" xml:id="D1eβ"> <cit type="translation" xml:lang="la"><quote> insidiator </quote></cit>, <foreign xml:lang="grc">ἐπίβουλος</foreign>, <ref cRef="1 Reg. " loc="29,4">1 Reg.  29,4</ref>. 
</sense> 
</sense> <sense n="f" xml:id="D1f"> <foreign xml:lang="gez">ብንተ፡ ዐይን፡</foreign> <cit type="translation" xml:lang="la"><quote> pupilla oculi </quote></cit> <lbl expand="videas">vid. sub</lbl> <foreign xml:lang="gez">ብንት፡</foreign> <lbl expand="columna">col.</lbl> <ref target="#c0518"/>. 
</sense> <sense n="g" xml:id="D1g"> <foreign xml:lang="gez">ቅጽበተ፡ ዐይን፡</foreign> <cit type="translation" xml:lang="la"><quote> nutus oculi </quote></cit>i.e. <cit type="translation" xml:lang="la"><quote> momentum </quote></cit>temporis <lbl expand="videas">vid. sub</lbl> <foreign xml:lang="gez">ቅጽበተ፡</foreign> <lbl expand="columna">col.</lbl> <ref target="#c0475"/>. 
</sense> 
</sense> <sense n="2" xml:id="D2"> Varie transfertur oculus: <sense n="a" xml:id="D2a"> <cit type="translation" xml:lang="la"><quote> examen </quote></cit>bilancis (cfr. <foreign xml:lang="ar">عَيْنٌ</foreign>): <foreign xml:lang="gez">ከመ፡ እንተ፡ ታገብእ፡ ዐይነ፡ መዳልው፡ እሙንቱ፡</foreign> <foreign xml:lang="grc">ὡς ῥοπὴ ζυγοῦ</foreign> <ref cRef="Jes." loc="40,15">Jes. 40,15</ref>; <ref cRef="4 Esr." loc="1,35">4 Esr. 1,35</ref>. 
</sense> <sense n="b" xml:id="D2b"> <cit type="translation" xml:lang="la"><quote> species </quote></cit>vel <cit type="translation" xml:lang="la"><quote> imago insculpta </quote></cit>, <cit type="translation" xml:lang="la"><quote> sculptura </quote></cit> sigilli: <foreign xml:lang="gez">ዐይነ፡ ማኅተም፡</foreign> <foreign xml:lang="grc">γλύμμα σφραγῖδος</foreign> <ref cRef="Ex." loc="28,11">Ex. 28,11</ref>; <ref cRef="Ex." loc="28,21">Ex. 28,21</ref>; <ref cRef="Sir." loc="38,27">Sir. 38,27</ref>; <ref cRef="Sir. " loc="45,11">Sir.  45,11</ref>. 
</sense> <sense n="c" xml:id="D2c"> dubius est sensus vocis <foreign xml:lang="gez">ዐይን፡</foreign> <ref cRef="Ex. " loc="36,31">Ex.  36,31</ref>, ubi Gr. <foreign xml:lang="grc">ὤα</foreign> habet (an <cit type="translation" xml:lang="la"><quote> circulus </quote></cit>oculo similis?). 
</sense> <sense n="d" xml:id="D2d"> in libris ex Arabico translatis <foreign xml:lang="gez">ዐይን፡</foreign> seq. Gen. significat <cit type="translation" xml:lang="la"><quote> ipse </quote></cit>ille (sicut <foreign xml:lang="ar">عَيْنٌ</foreign>), ut: <foreign xml:lang="gez">እመ፡ ኢተረክበ፡ ዐይኑ፡</foreign> (sc. rei furto ablatae) <foreign xml:lang="gez">ያገብእ፡ አምሳሊሁ፡</foreign> F.N. 35 aliud exemplum <lbl expand="videas">vid. sub</lbl> <foreign xml:lang="gez">ሕስት፡</foreign> col. 891.<cb n="1007" xml:id="c1007"/> 
</sense> <sense n="e" xml:id="D2e"> <foreign xml:lang="gez">ዐይነ፡ ወርቅ፡</foreign> oculus vel species auri i.e. <cit type="translation" xml:lang="la"><quote> icterus </quote></cit>(<cit type="translation" xml:lang="la"><quote> morbus regius </quote></cit>q.d.), Lud. 
</sense> 
</sense> <sense n="3" xml:id="D3"> <cit type="translation" xml:lang="la"><quote> fons </quote></cit>, <ref cRef="Ex." loc="15,27">Ex. 15,27</ref>; <ref cRef="Jac." loc="3,11">Jac. 3,11</ref>; Deus <foreign xml:lang="gez">አቀመ፡ አዕይንቲሁ፡ ለተከዚ፡</foreign> <ref cRef="4 Esr." loc="13,46">4 Esr. 13,46</ref> et Deg.; fortasse <ref cRef="Hen." loc="21,7">Hen. 21,7</ref>; <lbl expand="translate">transl.</lbl>: <foreign xml:lang="gez">ተመክሮ፡ ዐይነ፡ ተመርሖ፡ ውእቱ፡</foreign> <ref cRef="Fal." loc="55.">Fal. f. 55.</ref> – Eodem referendum est <foreign xml:lang="gez">ዐይነ፡ ምድር፡</foreign> oculus terrae i.e. <cit type="translation" xml:lang="la"><quote> cavum </quote></cit>ut talpae, mustelae, Lud. <lbl expand="auctore, auctoritate">auct.</lbl> Greg. (cfr. etiam Isenberg <lbl expand="lexicon">lex.</lbl> amh. sub <foreign xml:lang="gez">ዐይን፡</foreign>). 
</sense> </sense>
               
