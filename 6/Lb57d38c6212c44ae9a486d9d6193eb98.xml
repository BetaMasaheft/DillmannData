                  </cit>
                  <sense n="a" xml:id="Da"> termini <ref cRef="Num." loc="34,4">Num. 34,4</ref>; <ref cRef="Jos." loc="15 ">Jos. 15 </ref>
                     <lbl expand="sequens">seq.</lbl>; <ref cRef="Kebr. Nag." loc="92">Kebr. Nag.
                        92</ref>; viae: <foreign xml:lang="gez">ሙፃእታ፡</foreign> (v. <foreign xml:lang="gez">ሙፃኣታ፡</foreign>) <foreign xml:lang="gez">ለፍኖት፡</foreign>
                     <ref cRef="Sir." loc="21,10">Sir. 21,10</ref>.</sense>
                  <sense n="b" xml:id="Db">
                     <cit type="translation" xml:lang="la">
                        <quote>finis</quote>
                     </cit>: <foreign xml:lang="gez">ሞፃእቱ፡ ለቅሥቶሙ፡</foreign>
                     <ref cRef="Hebr." loc="6,16">Hebr. 6,16</ref>.</sense>
                  <sense n="c" xml:id="Dc">
                     <cit type="translation" xml:lang="la">
                        <quote>discessus</quote>
                     </cit>
                     <ref cRef="Hebr." loc="13,7">Hebr. 13,7</ref>.</sense>
                  <sense n="d" xml:id="Dd">
                     <cit type="translation" xml:lang="la">
                        <quote>amolitio</quote>
                     </cit> i.e. <cit type="translation" xml:lang="la">
                        <quote>expiatio</quote>
                     </cit>: <foreign xml:lang="gez">ቤተ፡ ሞፃእት፡ </foreign> (v. <foreign xml:lang="gez">ሙፃእት፡</foreign>) <foreign xml:lang="grc">οἶκος
                        ἐξιλασμοῦ</foreign>
                     <ref cRef="1 Par." loc="28,11">1 Par. 28,11</ref> (at <foreign xml:lang="gez">ቤተ፡ ስርየት፡ </foreign>
                     <ref cRef="1 Par." loc="28,20">1 Par. 28,20</ref>). {<ref target="#c1433"/>
                     <lbl expand="pluralis">Pl.</lbl> forma <foreign xml:lang="gez">መዋፅእ፡ </foreign>
                     occurrit <ref cRef="Hen." loc="33,3">Hen. 33,3</ref> in <lbl expand="d'Abbadie">Abb.</lbl> LV}</sense>
               </sense>
               
