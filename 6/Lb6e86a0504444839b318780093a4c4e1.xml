               <sense xml:lang="la" source="#dillmann" xml:id="Dmain"> <subc>II,1</subc> <cit type="translation" xml:lang="la">
                            <quote> regem </quote>
                        </cit> vel <cit type="translation" xml:lang="la">
                            <quote> reginam constituere </quote>
                        </cit>, <lbl expand="cum">c.</lbl> <gramGrp> <case value="accusativus">Acc.</case> </gramGrp> regis et <lbl expand="cum">c.</lbl> <foreign xml:lang="gez">ለ፡</foreign> vel <foreign xml:lang="gez">ላዕለ፡</foreign> populi: <foreign xml:lang="gez">ንጉሥ፡ ዘአንገሡ፡</foreign> <ref cRef="Hez." loc="17,16">Hez. 17,16</ref>; <foreign xml:lang="gez">ናንግሥ፡ ሎሙ፡ ወልደ፡ ጣብሔል፡</foreign> <ref cRef="Jes." loc="7,6">Jes. 7,6</ref>; <ref cRef="Jer." loc="44,1">Jer. 44,1</ref>; <ref cRef="Job" loc="34,30">Job 34,30</ref>; <foreign xml:lang="gez">ከመ፡ ያንግሥዎ፡</foreign> <ref cRef="1 Par." loc="11,10">1 Par. 11,10</ref>; <ref cRef="1 Par." loc="29,22">1 Par. 29,22</ref>; <foreign xml:lang="gez">ከመ፡ ያንግሡከ፡ ሎሙ፡</foreign> <ref cRef="2 Esr." loc="16,6">2 Esr. 16,6</ref>; <foreign xml:lang="gez">ዘአንገሥኩከ፡ ላዕሌሆሙ፡</foreign> <ref cRef="2 Par." loc="1,11">2 Par. 1,11</ref>; <ref cRef="Joh." loc="6,15">Joh. 6,15</ref>; nonnumquam addito <gramGrp> <case value="accusativus">Acc.</case> </gramGrp> <foreign xml:lang="gez">ንጉሠ፡</foreign>, ut: <foreign xml:lang="gez">አንገሥዎ፡ ሎሙ፡ ንጉሠ፡</foreign> <ref cRef="Jud." loc="9,6">Jud. 9,6</ref>; <ref cRef="1 Reg." loc="8,22">1 Reg. 8,22</ref>; <ref cRef="1 Reg." loc="12,1">1 Reg. 12,1</ref>; passim omisso <gramGrp> <case value="accusativus">Acc.</case> </gramGrp> <foreign xml:lang="gez">ናንግሥ፡ ለነ፡</foreign> <ref cRef="Num." loc="14,4">Num. 14,4</ref>; <ref cRef="1 Reg." loc="8,19">1 Reg. 8,19</ref>; <foreign xml:lang="gez">ኢያነግሡ፡ ወኢይስዕሩ፡</foreign> <ref cRef="Ep. Jer." loc="33.">Ep. Jer. 33.</ref> <sense n="a" xml:id="Da"> <cit type="translation" xml:lang="la">
                                <quote> reginam creavit </quote>
                            </cit> rex uxorem, <lbl expand="videas">vid.</lbl> Lud. <lbl expand="lexicon">lex.</lbl> amh. c. 53.</sense> <sense n="b" xml:id="Db"> <cit type="translation" xml:lang="la">
                                <quote> proregem constituere </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> praeficere regis vice </quote>
                            </cit> <ref cRef="Act." loc="7,">Act. 7,</ref>l0 <lbl expand="romanae editionis">rom.</lbl> </sense> <sense n="c" xml:id="Dc"> <lbl expand="translate">transl.</lbl>: <cit type="translation" xml:lang="la">
                                <quote> regnare </quote>
                            </cit> vel <cit type="translation" xml:lang="la">
                                <quote> dominari facere </quote>
                            </cit>: <foreign xml:lang="gez">ኀጢአቱ፡ ለ፩ አንገሠቶ፡ ለሞት፡</foreign> <ref cRef="Rom." loc="5,17">Rom. 5,17</ref>; <ref cRef="Rom." loc="5,21">Rom. 5,21</ref>; <ref cRef="Rom." loc="6,12">Rom. 6,12</ref>; <foreign xml:lang="gez">ናንግሥ፡ ለፍቅር፡ ላዕሌነ፡</foreign> <ref cRef="Chrys. Ta." loc="19.">Chrys. Ta. 19.</ref> </sense> </sense>
               
