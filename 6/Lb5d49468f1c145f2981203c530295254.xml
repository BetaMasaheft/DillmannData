            <entry xml:id="Lb5d49468f1c145f2981203c530295254" n="5153">
               <form>
                  <foreign xml:lang="gez">አንደየ</foreign>
               </form>
               <sense xml:lang="la" source="#dillmann" xml:id="Dmain"> <subc>II,1</subc> <sense n="1" xml:id="D1">
                     <cit type="translation" xml:lang="la">
                        <quote>pauperare</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>inopem reddere</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>ad egestatem redigere</quote>
                     </cit>, <lbl expand="cum">c.</lbl>
                     <gramGrp>
                        <case value="accusativus">Acc.</case>
                     </gramGrp>: <ref cRef="Jud. " loc="14,15">Jud. 14,15</ref>; <foreign xml:lang="gez">አንደየ፡ ርእሶ፡</foreign>
                     <ref cRef="2 Cor." loc="8,9">2 Cor. 8,9</ref>; <foreign xml:lang="gez">ብሶይ፡</foreign> (Sanctus) <foreign xml:lang="gez">አንደየ፡ ርእሶ፡ በእንተ፡ መንግሥተ፡
                        ሰማያት፡</foreign> Deg.; Ssal. Req.; <foreign xml:lang="gez">እግዚአብሔር፡ ያነዲ፡
                        ወያብዕል፡</foreign>
                     <ref cRef="1 Reg." loc="2,7">1 Reg. 2,7</ref>; <ref cRef="Ep. Jer." loc="34">Ep. Jer. 34</ref>; <ref cRef="Clem." loc="170.">Clem. f. 170.</ref>
                  </sense>
                  <sense n="2" xml:id="D2"> dicendo <cit type="translation" xml:lang="la">
                        <quote>detrectare</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>extenuare</quote>
                     </cit> aliquem, ut: <foreign xml:lang="gez">ዘርክዮቶሙ፡ ወአንድዮቶሙ፡ ወአንውሮቶሙ፡
                        ወአሕጽጾቶሙ፡</foreign>
                     <ref cRef="Phlx." loc="141,">Phlx. 141,</ref> al.</sense>
               </sense>
               
