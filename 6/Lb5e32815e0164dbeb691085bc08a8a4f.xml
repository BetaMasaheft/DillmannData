               <sense xml:lang="la" source="#dillmann" xml:id="Dmain"> <subc>III,1</subc> reflex. et <gramGrp>
                     <pos expand="passivum">pass.</pos>
                  </gramGrp>
                  <sense n="1" xml:id="D1">
                     <cit type="translation" xml:lang="la">
                        <quote>occultari</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>abscondi</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>se abscondere</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>occultare</quote>
                     </cit> (se subtrahere): <foreign xml:lang="gez">ተኀብኡ፡ ውስተ፡ ግበበ፡ ምድር፡</foreign>
                     <ref cRef="Jes." loc="2,10">Jes. 2,10</ref>; <ref cRef="Jes. " loc="32,2">Jes.
                        32,2</ref>; <foreign xml:lang="gez">ተኀቢኦሙ፡ ታሕተ፡ ዖም፡</foreign>
                     <ref cRef="Kuf." loc="34">Kuf. 34</ref>; <ref cRef="Jer." loc="4,29">Jer.
                        4,29</ref>; <ref cRef="Jer. " loc="43,19">Jer. 43,19</ref>; <ref cRef="Gen." loc="3,8">Gen. 3,8</ref>; <ref cRef="1 Reg." loc="20,24">1 Reg. 20,24</ref>;
                        <ref cRef="Jos." loc="10,16">Jos. 10,16</ref>; <ref cRef="Matth." loc="5,14">Matth. 5,14</ref>; <ref cRef="Job" loc="29,8">Job 29,8</ref>; <ref cRef="Job" loc="30,7">Job 30,7</ref>; <ref cRef="Ps. " loc="9,31">Ps.
                        9,31</ref>; <ref cRef="4 Esr." loc="8,64">4 Esr. 8,64</ref> (ubi <foreign xml:lang="gez">ተኀብአት፡</foreign> legendum est); <lbl expand="construitur cum">c.c.</lbl>
                     <foreign xml:lang="gez">እምነ፡</foreign> pers., ut: <foreign xml:lang="gez">እትኀባእ፡ እምገጽከ፡</foreign>
                     <ref cRef="Job" loc="13,20">Job 13,20</ref>; <ref cRef="Sir." loc="17,15">Sir.
                        17,15</ref>; <foreign xml:lang="gez">ተኀብአት፡ እምዐይኑ፡ </foreign>
                     <ref cRef="Sx. Ter" loc="21 ">Sx. Ter 21 </ref>Enc.; et <lbl expand="cum">c.</lbl>
                     <gramGrp>
                        <case value="accusativus">Acc.</case>
                     </gramGrp>
                     <lbl expand="persona">pers.</lbl>: <foreign xml:lang="gez">አልቦ፡ ብሔር፡ ኀበ፡
                        ይትኀብእዎ፡ ገበርተ፡ ዐመፃ፡</foreign>
                     <ref cRef="Job" loc="34,22">Job 34,22</ref>; <foreign xml:lang="gez">ይትኀብአከ፡ </foreign>
                     <foreign xml:lang="grc">προςώπου σου κρυβήσεται</foreign>
                     <ref cRef="Sir." loc="6,12">Sir. 6,12</ref>; <ref cRef="Sir." loc="12,8">Sir.
                        12,8</ref>; <ref cRef="Ps." loc="54,13">Ps. 54,13</ref>; <ref cRef="Ps." loc="55,6">Ps. 55,6</ref>; <ref cRef="Joh." loc="8,59">Joh. 8,59</ref>; <ref cRef="Joh." loc="12,36">Joh. 12,36</ref>.</sense>
                  <sense n="2" xml:id="D2">
                     <cit type="translation" xml:lang="la">
                        <quote>occultum esse</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>latere</quote>
                     </cit>, <foreign xml:lang="gez">ተኀብአት፡ እምአዕዋፈ፡ ሰማይ፡ </foreign>
                     <ref cRef="Job" loc="28,21">Job 28,21</ref>; <foreign xml:lang="gez">አልቦ፡ ምንትኒ፡
                        ዘይትኀባእ፡ እምኔሁ፡</foreign>
                     <ref cRef="Job " loc="34,21">Job 34,21</ref>; <foreign xml:lang="gez">ተኀብአት፡
                        ፍኖትየ፡ እምእግዚአብሔር፡</foreign>
                     <ref cRef="Jes." loc="40,27">Jes. 40,27</ref>.</sense>
               </sense>
               
