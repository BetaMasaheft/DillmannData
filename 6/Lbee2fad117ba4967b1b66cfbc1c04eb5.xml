               <sense xml:id="Dmain" xml:lang="la" source="#dillmann"> <gramGrp> <pos expand="praepositio">praep.</pos> </gramGrp> cui <gramGrp> <pos expand="pronomen">pron.</pos> </gramGrp> I et II <lbl expand="persona">pers.</lbl> crebrius per <cit type="translation" xml:lang="la"><quote> ê </quote></cit>, <gramGrp> <pos expand="pronomen">pron.</pos> </gramGrp> III p. crebro sine <cit type="translation" xml:lang="la"><quote> ê </quote></cit> affigi solet, (<bibl><ptr target="bm:Dillmann1857Gramm"/>gramm.</bibl> § 167,3): <sense n="1" xml:id="D1"> <foreign xml:lang="gez">ማእከለ፡</foreign> <sense n="a" xml:id="D1a"> <cit type="translation" xml:lang="la"><quote> in media </quote></cit> (<cit type="translation" xml:lang="la"><quote> re </quote></cit>), vel post verba motus <cit type="translation" xml:lang="la"><quote> in mediam </quote></cit> (<cit type="translation" xml:lang="la"><quote> rem </quote></cit>): <foreign xml:lang="gez">ሐነጽኩ፡ ማኅፈደ፡ ማእከሎ፡</foreign> <ref cRef="Jes." loc="5,2">Jes. 5,2</ref>; <foreign xml:lang="gez">ሶቤሃ፡ ይሠርቅ፡ ማእከለ፡ ጽልመት፡ ብርሃንከ፡</foreign> <ref cRef="Jes." loc="58,10">Jes. 58,10</ref>; <ref cRef="Gen." loc="2,9">Gen. 2,9</ref>; <ref cRef="Gen." loc="3,3">Gen. 3,3</ref>; <ref cRef="Gen." loc="37,7">Gen. 37,7</ref>; <foreign xml:lang="gez">ማእከላ፡</foreign> <ref cRef="Hez." loc="14,14">Hez. 14,14</ref>; <ref cRef="Hez. " loc="14,16">Hez.  14,16</ref>; <ref cRef="Hez." loc="21,20">Hez. 21,20</ref>; <foreign xml:lang="gez">ማእከሌኪ፡</foreign> <ref cRef="Zach." loc="2,11">Zach. 2,11</ref>; <ref cRef="Zach. " loc="8,3">Zach.  8,3</ref>; <ref cRef="Matth." loc="14,24">Matth. 14,24</ref>; <ref cRef="Luc." loc="17,11">Luc. 17,11</ref>; <ref cRef="Luc." loc="22,55">Luc. 22,55</ref>; <foreign xml:lang="gez">ማእከለ፡ ሌሊተ፡</foreign> <ref cRef="Matth." loc="25,6">Matth. 25,6</ref>; post verba motus <ref cRef="Ex." loc="14,23">Ex. 14,23</ref>; <ref cRef="1 Reg." loc="9,14">1 Reg. 9,14</ref>; <foreign xml:lang="gez">ወወረውዋ፡ ማእከለ፡ መስፈርት፡</foreign> <ref cRef="Zach." loc="5,8">Zach. 5,8</ref>. 
</sense> <sense n="b" xml:id="D1b"> ubi refertur ad plures: <cit type="translation" xml:lang="la"><quote> inter </quote></cit>, <ref cRef="Gen." loc="23,10">Gen. 23,10</ref>; <ref cRef="Ps." loc="54,17">Ps. 54,17</ref>; <foreign xml:lang="gez">እንዘ፡ የሐውሩ፡ ማእከለ፡ ፪ አድባር፡</foreign> <ref cRef="Zach." loc="6,1">Zach. 6,1</ref>; <foreign xml:lang="gez">ይሰርር፡ ማእከለ፡ ሰማይ፡ ወምድር፡</foreign> <ref cRef="Apoc." loc="8,13">Apoc. 8,13</ref>; <foreign xml:lang="gez">መሐላ፡ እግዚአብሔር፡ ተርፈ፡ ማእከሎሙ፡</foreign> <ref cRef="Ex." loc="22,10">Ex. 22,10</ref>; <foreign xml:lang="gez">እዘርዎሙ፡ ማእከለ፡ አሕዛብ፡</foreign> <ref cRef="Kuf." loc="1">Kuf. 1</ref>; <ref cRef="Hen." loc="39,5">Hen. 39,5</ref>; <ref cRef="Matth." loc="10,16">Matth. 10,16</ref>; <ref cRef="Matth." loc="13,25">Matth. 13,25</ref>; <ref cRef="Matth." loc="18,2">Matth. 18,2</ref>; <ref cRef="Matth." loc="18,20">Matth. 18,20</ref>; <ref cRef="Joh." loc="1,26">Joh. 1,26</ref>; <ref cRef="Joh." loc="20,19">Joh. 20,19</ref>; <ref cRef="1 Thess." loc="2,7">1 Thess. 2,7</ref>; <foreign xml:lang="gez">ኢይትናገር፡ ነገረ፡ ኀፍረት፡ ኢማእከለ፡ ሥዩማን፡ ወኢማእከለ፡ ሕዝባውያን፡</foreign> F.N. 9. <sense n="α" xml:id="D1bα"> iteratur, ut: <foreign xml:lang="gez">ፈለጠ፡ እግዚአብሔር፡ ማእከለ፡ ብርሃን፡ ወማእከለ፡ ጽልመት፡</foreign> <ref cRef="Gen." loc="1,4">Gen. 1,4</ref>; <ref cRef="Gen." loc="16,14">Gen. 16,14</ref>; <ref cRef="Gen. " loc="26,28">Gen.  26,28</ref>. <ref cRef="Gen." loc="30,36">Gen. 30,36</ref>; <ref cRef="Ex." loc="14,20">Ex. 14,20</ref>; <ref cRef="Lev." loc="10,10">Lev. 10,10</ref>; <ref cRef="Lev." loc="26,46">Lev. 26,46</ref>; <ref cRef="Num." loc="30,17">Num. 30,17</ref>; <ref cRef="Deut. " loc="17,8">Deut.  17,8</ref>; <ref cRef="Jes." loc="59,2">Jes. 59,2</ref>; <ref cRef="Hen." loc="41,8">Hen. 41,8</ref>. 
</sense> <sense n="β" xml:id="D1bβ"> <foreign xml:lang="gez">ማእከለ፡</foreign> — <foreign xml:lang="gez">ለ፡</foreign> <cit type="translation" xml:lang="la"><quote> inter </quote></cit>, ut: <foreign xml:lang="gez">ማእከለ፡ ሐሪስ፡ ወለዐጺድ፡</foreign> <ref cRef="Kuf." loc="107.">Kuf. p. 107.</ref> 
</sense> 
</sense> 
</sense><sense n="2" xml:id="D2"> Componitur cum aliis praepositionibus: <sense n="a" xml:id="D2a"> <foreign xml:lang="gez">በማእከለ፡</foreign> <sense n="α" xml:id="D2aα"> <cit type="translation" xml:lang="la"><quote> in media </quote></cit> (<cit type="translation" xml:lang="la"><quote> re </quote></cit>): <foreign xml:lang="gez">በማእከሌኪ፡</foreign> <foreign xml:lang="grc">ἐν σεαυτῇ</foreign> <ref cRef="Job" loc="38,11">Job 38,11</ref>; <ref cRef="Jes. " loc="12,6">Jes.  12,6</ref>; <ref cRef="Zach." loc="2,5">Zach. 2,5</ref>; <ref cRef="Hez." loc="2,10">Hez. 2,10</ref>; <ref cRef="Sir." loc="24,1">Sir. 24,1</ref>; <foreign xml:lang="gez">በማእከለ፡ ሌሊት፡</foreign> <ref cRef="Lev." loc="23,5">Lev. 23,5</ref>. 
</sense> <sense n="β" xml:id="D2aβ"> <cit type="translation" xml:lang="la"><quote> inter </quote></cit>: <ref cRef="1 Reg." loc="10,10">1 Reg. 10,10</ref>; <ref cRef="1 Reg." loc="16,13">1 Reg. 16,13</ref>; <ref cRef="Ps." loc="21,15">Ps. 21,15</ref>; <ref cRef="Ps." loc="21,23">Ps. 21,23</ref>; <ref cRef="Ps." loc="103,13">Ps. 103,13</ref>; <ref cRef="Matth." loc="14,6">Matth. 14,6</ref>. <cb n="0785" xml:id="c0785"/> </sense>
</sense> <sense n="b" xml:id="D2b"> <foreign xml:lang="gez">ውስተ፡ ማእከለ፡</foreign> <cit type="translation" xml:lang="la"><quote> in media </quote></cit> (<cit type="translation" xml:lang="la"><quote> re </quote></cit>) vel <cit type="translation" xml:lang="la"><quote> in mediam </quote></cit> (<cit type="translation" xml:lang="la"><quote> rem </quote></cit>), <ref cRef="Apoc." loc="22,2">Apoc. 22,2</ref>; <ref cRef="Num." loc="19,6">Num. 19,6</ref>. 
</sense> <sense n="c" xml:id="D2c"> <foreign xml:lang="gez">እንተ፡ ማእከለ፡</foreign> <cit type="translation" xml:lang="la"><quote> per mediam </quote></cit> (<cit type="translation" xml:lang="la"><quote> rem </quote></cit>): <ref cRef="Gen." loc="15,17">Gen. 15,17</ref>; <ref cRef="Ps." loc="135,14">Ps. 135,14</ref>; <ref cRef="Jer." loc="41,18">Jer. 41,18</ref>; <ref cRef="Sap." loc="10,18">Sap. 10,18</ref>; <ref cRef="Judith" loc="11,19">Judith 11,19</ref>. 
</sense> <sense n="d" xml:id="D2d"> <foreign xml:lang="gez">እምነ፡ ማእከለ፡</foreign> vel <foreign xml:lang="gez">እማእከለ፡</foreign> <cit type="translation" xml:lang="la"><quote> e media </quote></cit> (<cit type="translation" xml:lang="la"><quote> re </quote></cit>), <cit type="translation" xml:lang="la"><quote> e medio </quote></cit>: <foreign xml:lang="gez">ፃኡ፡ እምነ፡ ማእከላ፡</foreign> <ref cRef="Jes." loc="52,11">Jes. 52,11</ref>; <ref cRef="Gen." loc="48,12">Gen. 48,12</ref>; <ref cRef="Ex." loc="7,5">Ex. 7,5</ref>; <ref cRef="Ex." loc="25,22">Ex. 25,22</ref>; <ref cRef="Num." loc="8,14">Num. 8,14</ref>; <ref cRef="Num." loc="27,4">Num. 27,4</ref>; <ref cRef="Deut." loc="4,15">Deut. 4,15</ref>; <ref cRef="4 Reg." loc="11,2">4 Reg. 11,2</ref>; <foreign xml:lang="gez">እማእከሌሆሙ፡</foreign> <ref cRef="Jsp." loc="322.">Jsp. p. 322.</ref> 
</sense> 
</sense> </sense>
               
            <sense xml:id="Tmain" xml:lang="en" source="#traces"> <cit type="transcription" xml:lang="gez"><quote>māʾǝkala</quote></cit> <sense n="G" xml:id="TG"> <foreign xml:lang="gez">ማእከለ፡</foreign> <cit type="transcription" xml:lang="gez"><quote>māʾǝkala</quote></cit> «<cit type="translation" xml:lang="fr"><quote>au milieu</quote></cit>, <cit type="translation" xml:lang="fr"><quote>parmi</quote></cit>» ― <foreign xml:lang="am">በማኸል፡</foreign> Ms. <ref type="BM" target="BNFabb217">BNFabb217</ref>, fol. 42r. <lb/>
 <bibl><ptr target="bm:Grebaut1952SuppLexicon"/><citedRange unit="page"> 101</citedRange></bibl> 
</sense> <sense n="L" xml:id="TL"> <foreign xml:lang="gez">ማእከለ</foreign> <cit type="transcription" xml:lang="gez"><quote>māʾǝkala</quote></cit> <cit type="translation" xml:lang="en"><quote>in the midst of</quote></cit>, <cit type="translation" xml:lang="en"><quote>in the middle of</quote></cit>, <cit type="translation" xml:lang="en"><quote>among</quote></cit> (the suffix pronouns are added to the base <cit type="transcription" xml:lang="gez"><quote>māʿǝkale</quote></cit>) <lb/>
 <foreign xml:lang="gez">ውስተ፡ ማእከለ</foreign> <cit type="transcription" xml:lang="gez"><quote>wǝsta māʾǝkala</quote></cit> <cit type="translation" xml:lang="en"><quote>in the middle of</quote></cit>, <cit type="translation" xml:lang="en"><quote>in the midst of</quote></cit> <lb/>
 <foreign xml:lang="gez">እምነ፡ ማእከለ</foreign> <cit type="transcription" xml:lang="gez"><quote>ʾǝmǝnna māʾǝkala</quote></cit> <cit type="translation" xml:lang="en"><quote>from among</quote></cit>, <cit type="translation" xml:lang="en"><quote>from</quote></cit> <lb/>
