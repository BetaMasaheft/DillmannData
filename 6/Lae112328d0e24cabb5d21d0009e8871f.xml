               <sense xml:id="Dmain" xml:lang="la" source="#dillmann"> <subc>III,1</subc> <sense n="1" xml:id="D1"> <gramGrp> <pos expand="passivum">pass.</pos> </gramGrp> <cit type="translation" xml:lang="la"><quote> evelli </quote></cit>, <cit type="translation" xml:lang="la"><quote> extrahi </quote></cit>, <cit type="translation" xml:lang="la"><quote> erui </quote></cit>, <cit type="translation" xml:lang="la"><quote> eradicari </quote></cit>, <cb n="0148" xml:id="c0148"/> <cit type="translation" xml:lang="la"><quote> exstirpari </quote></cit>, de alis: <foreign xml:lang="gez">ተመልኀ፡ ክነፊሃ፡</foreign> <ref cRef="Dan." loc="7,4">Dan. 7,4</ref>; cornibus <ref cRef="Dan." loc="7,8">Dan. 7,8</ref>; arbore <ref cRef="Luc." loc="17,6">Luc. 17,6</ref>; <foreign xml:lang="gez">በኀይለ፡ ነፋስ፡ ይትመላኅ፡</foreign> <ref cRef="Sap." loc="4,4">Sap. 4,4</ref>; de oculis: <foreign xml:lang="gez">ተመልኁ፡ አዕይንቲሆሙ፡</foreign> <ref cRef="Clem." loc="153">Clem. f. 153</ref>; Lud. e <ref cRef="Vit. Sam." loc="">Vit. Sam. </ref>; de hominibus <ref cRef="Kuf." loc="91">Kuf. p. 91</ref>; <ref cRef="Prov." loc="15,6">Prov. 15,6</ref> <lbl expand="varia lectio">var.</lbl>; de regno: <foreign xml:lang="gez">ትትመላኅ፡ መንግሥቱ፡</foreign> <ref cRef="Dan." loc="11,4">Dan. 11,4</ref>. 
</sense> <sense n="2" xml:id="D2"> <cit type="translation" xml:lang="la"><quote> evaginari </quote></cit>: <foreign xml:lang="gez">ሰይፍ፡ እንተ፡ ተመልኀት፡ ለቀቲል፡</foreign> <ref cRef="Hez. " loc="21,28">Hez.  21,28</ref>; <foreign xml:lang="gez">እምቅድመ፡ ይትመልኁ፡ መባርቅት፡</foreign> <cit type="translation" xml:lang="la"><quote> priusquam stringerentur fulmina </quote></cit> (gladiorum instar) <ref cRef="Lit. Chrys." loc="">Lit. Chrys. </ref> 
</sense> <sense n="3" xml:id="D3"> <cit type="translation" xml:lang="la"><quote> excidere </quote></cit>, <cit type="translation" xml:lang="la"><quote> effluere </quote></cit>, de ferro (<foreign xml:lang="grc">ἐκπίπτειν</foreign>) <ref cRef="Koh." loc="10,10">Koh. 10,10</ref>; de pilis: <foreign xml:lang="gez">ይበኪ፡ እስከኔ፡ ተመልኁ፡ ሥዕርተ፡ አዕይንቲሁ፡</foreign> <ref cRef="Phlx." loc="55.">Phlx. 55.</ref> 
</sense> </sense>
               
            <sense xml:id="Tmain" xml:lang="en" source="#traces"><cit type="transcription" xml:lang="gez"><quote>tamalḫa</quote></cit> <sense n="G" xml:id="TG"> <foreign xml:lang="gez">ተመልኀ፡</foreign> <cit type="transcription" xml:lang="gez"><quote>tamalḫa</quote></cit> <subc>III,1</subc> <foreign xml:lang="gez">ይትመላኅ፡</foreign> «<cit type="translation" xml:lang="fr"><quote>être dégainé</quote></cit>, <cit type="translation" xml:lang="fr"><quote>extrait</quote></cit>» ― <foreign xml:lang="am">ተመዘዘ፡</foreign> Ms. <ref type="BM" target="BNFabb217">BNFabb217</ref>, fol. 33r. <lb/>
 <bibl><ptr target="bm:Grebaut1952SuppLexicon"/><citedRange unit="page"> 75</citedRange></bibl> 
</sense> <sense n="L" xml:id="TL"> <foreign xml:lang="gez">ተመልኀ</foreign> <cit type="transcription" xml:lang="gez"><quote>tamalḫa</quote></cit> passive of <lbl expand="videas">vid.</lbl> <ref type="external" target="https://betamasaheft.eu/Dillmann/lemma/Le21b96a45db24d68aa58b9bd0644f924"><foreign xml:lang="gez">መልኀ</foreign></ref>; also <cit type="translation" xml:lang="en"><quote>fall out</quote></cit>, <cit type="translation" xml:lang="en"><quote>fall off</quote></cit>, <cit type="translation" xml:lang="en"><quote>be thrown</quote></cit>, <cit type="translation" xml:lang="en"><quote>uproot onself</quote></cit> <bibl><ptr target="bm:Leslau1987dic"/><citedRange unit="page"> 343b</citedRange></bibl> 
</sense> </sense></entry>
