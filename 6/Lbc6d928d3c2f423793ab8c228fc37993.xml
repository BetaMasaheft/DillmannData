               <sense xml:lang="la" source="#dillmann" xml:id="Dmain"> <subc>I,1</subc> [<foreign xml:lang="he">אָתָה</foreign> <foreign xml:lang="syr">ܐܶܬܳܐ</foreign> <foreign xml:lang="ar">أَتَا</foreign> et <foreign xml:lang="ar">أَتَى</foreign>] <gramGrp> <pos expand="Subjunctivus">Subj.</pos> </gramGrp> <foreign xml:lang="gez">ይእቱ፡</foreign> (raro <foreign xml:lang="gez">ይእተው፡</foreign> ut <ref cRef="4 Reg." loc="6,22">4 Reg. 6,22</ref> <lbl expand="varia lectio">var.</lbl>), Imperat. <foreign xml:lang="gez">እቱ፡</foreign> (raro <foreign xml:lang="gez">እተው፡</foreign>, ut <ref cRef="2 Reg. " loc="14,8">2 Reg.  14,8</ref>), <cit type="translation" xml:lang="la">
                            <quote> domum </quote>
                        </cit> (vel <cit type="translation" xml:lang="la">
                            <quote> in </quote>
                        </cit> <cit type="translation" xml:lang="la">
                            <quote> patriam </quote>
                        </cit>) <cit type="translation" xml:lang="la">
                            <quote> venire </quote>
                        </cit> vel <cit type="translation" xml:lang="la">
                            <quote> ire </quote>
                        </cit> vel <cit type="translation" xml:lang="la">
                            <quote> abire </quote>
                        </cit> vel <cit type="translation" xml:lang="la">
                            <quote> reverti </quote>
                        </cit>, <cit type="translation" xml:lang="la">
                            <quote> revenire </quote>
                        </cit>, <gramGrp> <pos expand="absolute">abs.</pos> </gramGrp> <ref cRef="Ruth" loc="1,11">Ruth 1,11</ref>; <ref cRef="Jes." loc="37,37">Jes. 37,37</ref>; <foreign xml:lang="gez">እቱ፡</foreign> 2 <ref cRef="Reg." loc="15,19">Reg. 15,19</ref>; <ref cRef="2 Reg." loc="19,14">2 Reg. 19,14</ref>; <foreign xml:lang="gez">ለእመ፡ ዳኅነ፡ አቶክሙ፡</foreign> <ref cRef="Tob." loc="5,16">Tob. 5,16</ref>; <foreign xml:lang="gez">አቲዎሙ፡</foreign> <foreign xml:lang="grc">ἡνίκα ἀπέτρεχεν</foreign> <ref cRef="Dan. apocr." loc="1,7">Dan. apocr. 1,7</ref>; <foreign xml:lang="gez">እስከ፡ ማእዜ፡ ትትመየጥ፡ ወማእዜ፡ ተአቱ፡</foreign> <ref cRef="2 Esr." loc="12,6">2 Esr. 12,6</ref>; <ref cRef="Luc." loc="19,13">Luc. 19,13</ref>; <ref cRef="Luc." loc="23,48">Luc. 23,48</ref>; <foreign xml:lang="gez">ይጐነዲ፡ አቲወ፡</foreign> <ref cRef="Matth." loc="24,48">Matth. 24,48</ref>; <foreign xml:lang="gez">እትዊ፡</foreign> <ref cRef="Ruth" loc="1,15">Ruth 1,15</ref>; <lbl expand="cum">c.</lbl> <gramGrp> <case value="accusativus">Acc.</case> </gramGrp> domi vel patriae: <foreign xml:lang="gez">እቱ፡ ቤተከ፡</foreign> <ref cRef="Num." loc="24,11">Num. 24,11</ref>; <ref cRef="Kuf." loc="43">Kuf. 43</ref>; <ref cRef="Ruth" loc="1,8">Ruth 1,8</ref>; 1 <ref cRef="Reg." loc="2,20">Reg. 2,20</ref>; <foreign xml:lang="gez">ይእቱ፡ ብሔሮ፡</foreign> <ref cRef="Jes." loc="13,14">Jes. 13,14</ref>; <foreign xml:lang="gez">የአትዉ፡ ጽዮንሃ፡</foreign> <ref cRef="Jes." loc="35,10">Jes. 35,10</ref>; <ref cRef="Jes." loc="51,11">Jes. 51,11</ref>; <ref cRef="Act." loc="8,39">Act. 8,39</ref>; etiam <lbl expand="cum">c.</lbl> <gramGrp> <case value="accusativus">Acc.</case> </gramGrp> viae: <foreign xml:lang="gez">አተወ፡ ፍኖቶ፡</foreign> <foreign xml:lang="grc">ᾤχετο εἰς τὴν ὁδὸν αὐτοῦ</foreign> <ref cRef="Jer." loc="35,11">Jer. 35,11</ref>; <lbl expand="cum">c.</lbl> <foreign xml:lang="gez">ውስተ፡</foreign> domi vel patriae: <ref cRef="1 Reg." loc="8,22">1 Reg. 8,22</ref>; <ref cRef="2 Reg." loc="14,8">2 Reg. 14,8</ref>; <foreign xml:lang="gez">ንእቱ፡ ኵልነ፡ ውስተ፡ ብሔርነ፡</foreign> <ref cRef="Jer." loc="28,9">Jer. 28,9</ref>; <ref cRef="Ruth" loc="1,7">Ruth 1,7</ref>; <ref cRef="1 Par." loc="16,43">1 Par. 16,43</ref>; <lbl expand="cum">c.</lbl> <foreign xml:lang="gez">ኀበ፡</foreign> pers.: <ref cRef="Ruth" loc="1,15">Ruth 1,15</ref>; <ref cRef="Ruth" loc="3,16">Ruth 3,16</ref>; <ref cRef="Ruth" loc="3,17">Ruth 3,17</ref>; <ref cRef="4 Reg." loc="6,22">4 Reg. 6,22</ref>; <lbl expand="cum">c.</lbl> <foreign xml:lang="gez">እምነ፡</foreign> loci: <ref cRef="Gen." loc="30,16">Gen. 30,16</ref>; <ref cRef="Num." loc="31,14">Num. 31,14</ref>; <ref cRef="Ruth" loc="4,3">Ruth 4,3</ref>; vel <lbl expand="cum">c.</lbl> <foreign xml:lang="gez">እምኀበ፡</foreign>, ut: <foreign xml:lang="gez">ወአተወ፡ እምኀበ፡ መዝብር፡</foreign> <ref cRef="Jer." loc="19,14">Jer. 19,14</ref>. <sense n="a" xml:id="Da"> <cit type="translation" xml:lang="la">
                                <quote> redire </quote>
                            </cit> in possessionem suam i.e. <cit type="translation" xml:lang="la">
                                <quote> recuperare </quote>
                            </cit> eam, <ref cRef="Lev." loc="25,10">Lev. 25,10</ref>; <ref cRef="Lev. " loc="25,13">Lev.  25,13</ref>.</sense> <sense n="b" xml:id="Db"> <lbl expand="translate">transl.</lbl> de morientibus: <foreign xml:lang="gez">ተአቱ፡ መቃብሪከ፡</foreign> <ref cRef="Job " loc="5,26">Job  5,26</ref>; <foreign xml:lang="gez">እቱ፡ ኀበ፡ ሕዝብከ፡</foreign> <ref cRef="Deut." loc="32,50">Deut. 32,50</ref>; item de pie defunctis, qui in patriam coelestem abeunt: <foreign xml:lang="gez">አተዉ፡ በሠናይ፡ ስም፡</foreign> Lit. Joh. (sind heimgegangen).</sense> <sense n="c" xml:id="Dc"> de animalibus, qui <cit type="translation" xml:lang="la">
                                <quote> se </quote>
                            </cit> <cit type="translation" xml:lang="la">
                                <quote> recipiunt </quote>
                            </cit> in lustra, cavernas, nidos <ref cRef="Ps." loc="103,23">Ps. 103,23</ref>.</sense> <sense n="d" xml:id="Dd"> raro: <cit type="translation" xml:lang="la">
                                <quote> venire </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> ingredi </quote>
                            </cit> in genere, ut: <foreign xml:lang="gez">ንፈቅድ፡ ወንፈቱ፤ ከመ፡</foreign> … <foreign xml:lang="gez">ውስተ፡ ማኅበርነ፡ እምሰማይ፡ ትእቱ፡</foreign> <ref cRef="Sx. Masc." loc="10 ">Sx. Masc. 10 </ref>Enc. – <lbl expand="Vocabularium Aethiopicum, -a -a">Voc. Ae.</lbl>: <foreign xml:lang="gez">አተወ፡ ቦአ፡ ዘ፡ ገባ።</foreign> </sense> </sense>
               
