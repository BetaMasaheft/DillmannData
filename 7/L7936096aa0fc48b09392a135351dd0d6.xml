               <sense xml:id="Dmain" xml:lang="la" source="#dillmann"> <gramGrp> <pos expand="Substantivum">subst.</pos> </gramGrp>, <gramGrp> <gen value="not found">m.</gen> </gramGrp> et <gramGrp> <gen value="femininus">fem.</gen> </gramGrp> (ut <ref cRef="Ex." loc="3,5">Ex. 3,5</ref>; <ref cRef="1 Esr." loc="8,75">1 Esr. 8,75</ref>; <ref cRef="Jsp." loc="328">Jsp. p. 328</ref>), <lbl expand="pluralis">Pl.</lbl> <foreign xml:lang="gez">መካናት፡</foreign> [<foreign xml:lang="ar">مَكَانٌ</foreign>] <cit type="translation" xml:lang="la"><quote> locus </quote></cit>, <foreign xml:lang="grc">τόπος</foreign> <ref cRef="Gen." loc="1,9">Gen. 1,9</ref>; <ref cRef="Gen." loc="2,21">Gen. 2,21</ref>; Gen.13,3; <ref cRef="Gen." loc="13,14">Gen. 13,14</ref>; <ref cRef="Gen." loc="22,3">Gen. 22,3</ref>; <ref cRef="Gen." loc="28,11">Gen. 28,11</ref>; <ref cRef="Ex." loc="3,5">Ex. 3,5</ref>; <ref cRef="Lev." loc="4,12">Lev. 4,12</ref>; <ref cRef="Lev." loc="6,4">Lev. 6,4</ref>; <ref cRef="Lev." loc="6,18">Lev. 6,18</ref>; <ref cRef="Lev." loc="13,28">Lev. 13,28</ref>; <ref cRef="Lev." loc="13,33">Lev. 13,33</ref>; <ref cRef="Lev." loc="14,40">Lev. 14,40</ref>; <ref cRef="Num." loc="18,31">Num. 18,31</ref>; <ref cRef="Num." loc="22,26">Num. 22,26</ref>; <foreign xml:lang="gez">በዛቲ፡ መካን፡ ቅድስት፡</foreign> <ref cRef="1 Esr." loc="8,75">1 Esr. 8,75</ref>; <ref cRef="Ps." loc="25,8">Ps. 25,8</ref>; <ref cRef="Ps." loc="36,38">Ps. 36,38</ref>; <ref cRef="Ps." loc="102,15">Ps. 102,15</ref>; <foreign xml:lang="gez">መካነ፡ እንተ፡ ኀበ፡ ግዕዙ፡ ኢትሑር፡ ህየ፡</foreign> <ref cRef="Prov." loc="4,15">Prov. 4,15</ref>; <foreign xml:lang="gez">ከመ፡ ኢይቁም፡ ውስተ፡ መካንከ፡</foreign> <ref cRef="Sir." loc="12,12">Sir. 12,12</ref>; <foreign xml:lang="gez">እሁቦሙ፡ በቤትየ፡ መካነ፡ ዘይሰመይ፡</foreign> <ref cRef="Jes." loc="56,5">Jes. 56,5</ref>; <ref cRef="Dan." loc="2,35">Dan. 2,35</ref>; <foreign xml:lang="gez">መካን፡ ዘለዓለም፡</foreign> <ref cRef="Tob." loc="3,6">Tob. 3,6</ref>; <foreign xml:lang="gez">መካነ፡ ደይን፡</foreign> <ref cRef="Kuf." loc="10">Kuf. 10</ref>; <foreign xml:lang="gez">መካን፡ ኀበ፡ እቀውም፡</foreign> <ref cRef="4 Esr." loc="4,33">4 Esr. 4,33</ref>; <foreign xml:lang="gez">መካን፡ ኅቡእ፡</foreign> <ref cRef="4 Esr. " loc="12,43">4 Esr.  12,43</ref>; <ref cRef="Hen." loc="17,1">Hen. 17,1</ref>; <ref cRef="Hen." loc="17,2">Hen. 17,2</ref>; <foreign xml:lang="gez">መካናት፡ ብሩሃን፡</foreign> <ref cRef="Hen." loc="17,3">Hen. 17,3</ref>; <foreign xml:lang="gez">መካናት፡ ሠናያት፡</foreign> <ref cRef="Hen." loc="22,1">Hen. 22,1</ref>; <ref cRef="Act." loc="7,49">Act. 7,49</ref>; <foreign xml:lang="gez">ክልኤ፡ መካናት፡ አሐቲ፡ ለሕይወት፡ ወአሐቲ፡ ለሞት፡</foreign> (pro <cit type="translation" xml:lang="la"><quote> viis </quote></cit>) Lud. comm. hist. p. 315 n. 48. <sense n="a" xml:id="Da"> <cit type="translation" xml:lang="la"><quote> spatium </quote></cit> <ref cRef="Jud." loc="20,36">Jud. 20,36</ref>; <foreign xml:lang="gez">እስመ፡ አልቦሙ፡ መካነ፡</foreign> <ref cRef="Jer. " loc="7,32">Jer.  7,32</ref>; <ref cRef="Luc." loc="2,7">Luc. 2,7</ref>; <foreign xml:lang="gez">ራሕበ፡ መካናቲሆሙ፡</foreign> <ref cRef="Hen." loc="43,2">Hen. 43,2</ref>. 
</sense> <sense n="b" xml:id="Db"> <cit type="translation" xml:lang="la"><quote> locus </quote></cit> i.e. <cit type="translation" xml:lang="la"><quote> occasio </quote></cit>: <foreign xml:lang="gez">እንዘ፡ ትሁብ፡ ኪያሆሙ፡ መካነ፡ ለንስሓ፡</foreign> <foreign xml:lang="grc">ἐδίδους τόπον μετανοίας</foreign> <ref cRef="Sap. " loc="12,10">Sap.  12,10</ref>. 
</sense> <sense n="c" xml:id="Dc"> <cit type="translation" xml:lang="la"><quote> locus habitationis </quote></cit>, <cit type="translation" xml:lang="la"><quote> regio </quote></cit>, <cit type="translation" xml:lang="la"><quote> terra </quote></cit>, <cit type="translation" xml:lang="la"><quote> sedes </quote></cit>: <ref cRef="Gen." loc="19,12">Gen. 19,12</ref>; <ref cRef="Ex. " loc="3,8">Ex.  3,8</ref>; <foreign xml:lang="gez">በዝንቱ፡ መካን፡</foreign> <ref cRef="Jer." loc="47,2">Jer. 47,2</ref>; <foreign xml:lang="gez">ለዛቲ፡ መካን፡</foreign> <ref cRef="Jer." loc="49,18">Jer. 49,18</ref>; <ref cRef="Jer." loc="49,22">Jer. 49,22</ref>; <ref cRef="Joh." loc="14,2">Joh. 14,2</ref>; <ref cRef="Joh." loc="14,3">Joh. 14,3</ref>; <foreign xml:lang="gez">መካናት፡</foreign> <ref cRef="Asc. Jes." loc="3,27">Asc. Jes. 3,27</ref>; <ref cRef="Lit." loc="176,3">Lit. 176,3</ref>; 
</sense> <sense n="d" xml:id="Dd"> <cit type="translation" xml:lang="la"><quote> plaga </quote></cit>: <foreign xml:lang="gez">እምአርባዕቱ፡ መካን፡</foreign> <ref cRef="Matth." loc="24,31">Matth. 24,31</ref>. 
</sense> <sense n="e" xml:id="De"> <cit type="translation" xml:lang="la"><quote> locus </quote></cit> vel <cit type="translation" xml:lang="la"><quote> caput </quote></cit> libri, <ref cRef="Luc." loc="4,17">Luc. 4,17</ref>. 
</sense> <sense n="f" xml:id="Df"> in arithmetica: <cit type="translation" xml:lang="la"><quote> locus </quote></cit> i.e. <cit type="translation" xml:lang="la"><quote> numerus </quote></cit>, <ref cRef="Abush." loc="11 ">Abush. 11 </ref>(<lbl expand="videas">vid. sub</lbl> <foreign xml:lang="gez">ደመረ፡</foreign>). 
</sense> <sense n="g" xml:id="Dg"> <gramGrp> <case value="accusativus">Acc.</case> </gramGrp> et st.c. <foreign xml:lang="gez">መካነ፡</foreign> <cit type="translation" xml:lang="la"><quote> in loco </quote></cit> <ref cRef="Num. " loc="9,17">Num.  9,17</ref>; loco i.e. pro <ref cRef="Deut." loc="2,12">Deut. 2,12</ref>; <ref cRef="Rom." loc="11,17">Rom. 11,17</ref>. <cit type="translation" xml:lang="la"><quote> – Voc. Ae.: </quote></cit> <foreign xml:lang="gez">መካን፡ ዘ፡ ቦታ።</foreign> 
</sense> </sense>
               
            <sense xml:id="Tmain" xml:lang="en" source="#traces"> <cit type="transcription" xml:lang="gez"><quote> makān </quote></cit>, <lbl expand="pluralis">Pl.</lbl> <cit type="transcription" xml:lang="gez"><quote>makānāt</quote></cit> <lb/>
 <foreign xml:lang="gez">መካን፡</foreign> <cit type="transcription" xml:lang="gez"><quote> makān </quote></cit> ‘<cit type="translation" xml:lang="it"><quote> luogo proprio </quote></cit>’ <foreign xml:lang="gez">ሰደኒ፡ ኀባ፡ መካኑ፡ ለአባ፡ መጣዕ፡ ኀበ፡ ተቀበረ፡ አዕፅማቲሂ፡ ወኀበ፡ አውፅአ፡ ማየ፡ በማዕተቡ፡</foreign> ‘<cit type="translation" xml:lang="it"><quote> Portami nel luogo proprio di ʾabbā Maṭāʿ, dove sono sepolte le sue ossa e dove fece scaturire l’acqua con il suo signacolo </quote></cit>’ <bibl><ptr target="bm:Bausi2003Libanos"/><citedRange unit="page"> 78 § 196 (ed.)</citedRange></bibl>, <bibl><ptr target="bm:Bausi2003LibanosTr"/><citedRange unit="page"> 49 § 196 (tr.)</citedRange></bibl> (‘«luogo proprio»: il termine è <cit type="transcription" xml:lang="gez"><quote> makān </quote></cit>, «luogo», ma anche «luogo specifico, sede» (vd. <bibl><ptr target="bm:Dillmann1865Lexicon"/><citedRange unit="page"> 865</citedRange></bibl>, ed in contesto monastico il luogo eletto ove il monaco organizza la propria attività e quella dei suoi seguaci, dunque quasi «sede»; in questo senso, nella trad. lo si è quasi sempre reso con «luogo proprio».’ <bibl><ptr target="bm:Bausi2003LibanosTr"/><citedRange unit="page"> 49 n. 10</citedRange></bibl>); <foreign xml:lang="gez">መካነ፡ ጸብእ፡</foreign> <cit type="transcription" xml:lang="gez"><quote> makāna ṣabʾ </quote></cit> <cit type="translation" xml:lang="en"><quote> battlefield </quote></cit> <foreign xml:lang="gez">ወአልቦ፡ እምኃያላነ፡ ዝንቱ፡ ንጉሥ፡ ዘኢቀተለ፡ በበ፪ወበበ፫እምነ፡ መለሳይ፡ እስከ፡ መልአ፡ አብድንቲሆሙ፡ በውእቱ፡ መካነ፡ ፀብእ።</foreign> ‘<cit type="translation" xml:lang="fr"><quote> il n’est pas un des guerriers du roi qui n’ait tué deux ou trois des musulmans (malasāy), à ce point que le champ de bataille fut rempli de leurs cadavres </quote></cit>’ <bibl><ptr target="bm:ContiRossini1907SarsaDengel"/><citedRange unit="page"> 41 l. 2-3 (ed.)</citedRange></bibl>, <bibl><ptr target="bm:ContiRossini1907SarsaDengelV"/><citedRange unit="page"> 47 l. 29-32 (tr.)</citedRange></bibl>, <foreign xml:lang="gez">ለእለኒ፡ ተረክቡ፡ በመካነ፡ ፀብእ፡ አንገፍዎሙ፡ ከመ፡ ቈጽል፡</foreign> ‘<cit type="translation" xml:lang="fr"><quote> on arracha comme des feuilles ceux qui furent trouvés sur le champ de bataille </quote></cit>’ <bibl><ptr target="bm:ContiRossini1907SarsaDengel"/><citedRange unit="page"> 119 l. 4-5 (ed.)</citedRange></bibl>, <bibl><ptr target="bm:ContiRossini1907SarsaDengelV"/><citedRange unit="page"> 135 l. 19-20 (tr.)</citedRange></bibl>, <lbl expand="videas">vid.</lbl> <foreign xml:lang="gez">ጸብእ፡</foreign> <sense n="G" xml:id="TG"> <foreign xml:lang="gez">መካን፡</foreign> <cit type="transcription" xml:lang="gez"><quote>makān</quote></cit> «<cit type="translation" xml:lang="fr"><quote>lieu</quote></cit>» ― <foreign xml:lang="am">ቦታ፡</foreign>, <foreign xml:lang="am">ስፍራ፡</foreign> Ms. <ref type="BM" target="BNFabb217">BNFabb217</ref>, fol. 43r. <lb/>
 <bibl><ptr target="bm:Grebaut1952SuppLexicon"/><citedRange unit="page"> 103</citedRange></bibl> 
</sense> <sense n="L" xml:id="TL"> <foreign xml:lang="gez">መካን</foreign> <cit type="transcription" xml:lang="gez"><quote>makān</quote></cit> (<lbl expand="pluralis">Pl.</lbl> <foreign xml:lang="gez">መካናት</foreign> <cit type="transcription" xml:lang="gez"><quote>makānāt</quote></cit>) <cit type="translation" xml:lang="en"><quote>place</quote></cit>, <cit type="translation" xml:lang="en"><quote>space</quote></cit>, <cit type="translation" xml:lang="en"><quote>room</quote></cit>, <cit type="translation" xml:lang="en"><quote>occasion</quote></cit> <lb/>
 <foreign xml:lang="gez">መካነ፡</foreign> <cit type="transcription" xml:lang="gez"><quote>makāna</quote></cit> <cit type="translation" xml:lang="en"><quote>in the place (of)</quote></cit> <lb/>
 <foreign xml:lang="gez">መካናት፡</foreign> <cit type="transcription" xml:lang="gez"><quote>makānāt</quote></cit> (<lbl expand="pluralis">Pl.</lbl> of <foreign xml:lang="gez">መካን</foreign> <cit type="transcription" xml:lang="gez"><quote>makān</quote></cit>) <cit type="translation" xml:lang="en"><quote>oratory</quote></cit> <bibl><ptr target="bm:Leslau1987dic"/><citedRange unit="page"> 299b</citedRange></bibl> 
</sense> <sense n="C" xml:id="TC"> for <foreign xml:lang="gez">አጽንዐ፡ መካነ፡</foreign> <lbl expand="videas">vid.</lbl> <foreign xml:lang="gez">አጽንዐ፡</foreign> 
</sense> </sense>
