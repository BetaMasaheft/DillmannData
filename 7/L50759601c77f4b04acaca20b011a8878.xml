                  <foreign xml:lang="gez">ጠለቀ</foreign>
               <rs type="root"/>
                    </form>
               <sense xml:lang="la" source="#dillmann" xml:id="Dmain"> et <foreign xml:lang="gez">ጠልቀ፡</foreign> <subc>I,1</subc> [vicina sunt <foreign xml:lang="ar">طَلَا</foreign> <cit type="translation" xml:lang="la">
                            <quote> illevit </quote>
                        </cit>, <cit type="translation" xml:lang="la">
                            <quote> inunxit </quote>
                        </cit>, <foreign xml:lang="ar">طَلَخَ ثَلَّخَ</foreign> <cit type="translation" xml:lang="la">
                            <quote> contaminavit </quote>
                        </cit>, <foreign xml:lang="ar">طَلَسَ</foreign> <cit type="translation" xml:lang="la">
                            <quote> delevit </quote>
                        </cit>, <foreign xml:lang="syr">ܛܘܠܝܳܐ</foreign> <cit type="translation" xml:lang="la">
                            <quote> macula </quote>
                        </cit>, <foreign xml:lang="ar">دَلِخَ</foreign> <cit type="translation" xml:lang="la">
                            <quote> pinguis fuit </quote>
                        </cit>] <sense n="1" xml:id="D1"> <cit type="translation" xml:lang="la">
                                <quote> oblini </quote>
                            </cit> i.e. <cit type="translation" xml:lang="la">
                                <quote> contaminari </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> inquinari </quote>
                            </cit>: <foreign xml:lang="grc">ὁ Λόγος</foreign> <foreign xml:lang="gez">ኢጠለቀ፡ ኀዲሮ፡ ውስተ፡ ማኅፀና፡</foreign> <foreign xml:lang="grc">οὐκ ἐμιάνθη οἰκήσας μόρια</foreign> ho. Procli in <ref cRef="Cyr. " loc="120.">Cyr.  f. 120.</ref> </sense> <sense n="2" xml:id="D2"> pinguedine repleri vel <cit type="translation" xml:lang="la">
                                <quote> satiari ad nauseam </quote>
                            </cit>: <foreign xml:lang="gez">ትጠልቅ፡ ምድር፡ እምሥብሖሙ፡</foreign> <foreign xml:lang="grc">ἐμπλησθήσεται</foreign> <ref cRef="Jes." loc="34,7">Jes. 34,7</ref> in <lbl expand="">Cod. Laur.</lbl> (in caeteris <foreign xml:lang="gez">ትጠልል፡</foreign>).</sense> <sense n="3" xml:id="D3"> <cit type="translation" xml:lang="la">
                                <quote> nausea </quote>
                            </cit> vel <cit type="translation" xml:lang="la">
                                <quote> taedio captum esse </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> fastidire </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> taedet </quote>
                            </cit> vel <cit type="translation" xml:lang="la">
                                <quote> pertaesum est </quote>
                            </cit> aliquem rei, <lbl expand="construitur cum">c.c.</lbl> <foreign xml:lang="gez">እምነ፡</foreign> rei: <foreign xml:lang="gez">ጠለቀት፡</foreign> (v. <foreign xml:lang="gez">ጠልቀት፡</foreign>) <foreign xml:lang="gez">ነፍሳ፡ እምኔሆሙ፡</foreign> <foreign xml:lang="grc">ἀπέστη</foreign> <ref cRef="Hez." loc="23,17">Hez. 23,17</ref>; <foreign xml:lang="gez">እለ፡ ጠለቀት፡</foreign> (v. <foreign xml:lang="gez">አጥለቀት፡</foreign>) <foreign xml:lang="gez">ነፍስኪ፡ እምኔሆሙ፡</foreign> <ref cRef="Hez." loc="23,22">Hez. 23,22</ref>; <ref cRef="Hez." loc="23,28">Hez. 23,28</ref>; <lbl expand="cum">c.</lbl> <gramGrp> <case value="accusativus">Acc.</case> </gramGrp> rei vel <lbl expand="persona">pers.</lbl>: <foreign xml:lang="gez">ሰቈራረ፡ ኮንክሙኒ፡ ወጠለቁክሙ፡</foreign> <ref cRef="Jes." loc="1,14">Jes. 1,14</ref>.</sense> <sense n="4" xml:id="D4"> <cit type="translation" xml:lang="la">
                                <quote> detestabilem facere </quote>
                            </cit> vel <cit type="translation" xml:lang="la">
                                <quote> calumniari </quote>
                            </cit> aliquem (dubium utrum <subc>I,1</subc> an <subc>I,2</subc>), <lbl expand="cum">c.</lbl> <gramGrp> <case value="accusativus">Acc.</case> </gramGrp> <foreign xml:lang="gez">ጠለቀቶ፡ ቅድመ፡ እግዚኡ፡</foreign> <ref cRef="Kuf." loc="139 ">Kuf. p. 139 </ref> <lbl expand="varia lectio">var.</lbl> (at <lbl expand="videas">vid.</lbl> <foreign xml:lang="gez">ጠቀለ፡</foreign>). – <lbl expand="Vocabularium Aethiopicum, -a -a">Voc. Ae.</lbl>: <foreign xml:lang="gez">ይጠልቅ፡ ብ፡ ይሰለቻል፡</foreign>; <cit type="translation" xml:lang="la">
                                <quote> aliud </quote>
                            </cit>: <foreign xml:lang="gez">ዘ፡ ዐመፃ፡ ያወርድ።</foreign> </sense> </sense>
               
