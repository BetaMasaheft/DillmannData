               <sense xml:lang="la" source="#dillmann" xml:id="Dmain"> <subc>I,1</subc> [verbum inferioris aetatis; amh. <lbl expand="idem">id.</lbl>; <foreign xml:lang="ar">دَفَرَ</foreign> <cit type="translation" xml:lang="la">
                            <quote> trusit in pectore </quote>
                        </cit>, <foreign xml:lang="ar">مِدْفَرٌ</foreign> <cit type="translation" xml:lang="la">
                            <quote> strenuus </quote>
                        </cit>, <cit type="translation" xml:lang="la">
                            <quote> fortis </quote>
                        </cit>] <gramGrp> <pos expand="Subjunctivus">Subj.</pos> </gramGrp> <foreign xml:lang="gez">ይድፍር፡</foreign> et <foreign xml:lang="gez">ይድፈር፡</foreign>, <cit type="translation" xml:lang="la">
                            <quote> audere </quote>
                        </cit>, <cit type="translation" xml:lang="la">
                            <quote> audere aggredi </quote>
                        </cit>, <cit type="translation" xml:lang="la">
                            <quote> audacem </quote>
                        </cit>, <cit type="translation" xml:lang="la">
                            <quote> protervum </quote>
                        </cit>, <cit type="translation" xml:lang="la">
                            <quote> irreverentem </quote>
                        </cit> et <cit type="translation" xml:lang="la">
                            <quote> impudentem esse </quote>
                        </cit>; <foreign xml:lang="gez">ደፊሮ፡</foreign> <cit type="translation" xml:lang="la">
                            <quote> cum auderet </quote>
                        </cit> <ref cRef="Jsp." loc="275">Jsp. p. 275</ref>; <lbl expand="sequens">seq.</lbl> <gramGrp> <pos expand="Subjunctivus">Subj.</pos> </gramGrp> verbi: <foreign xml:lang="gez">ደፈርከ፡ ትስተይ፡ በንዋየ፡ ቅድሳት፡</foreign> <ref cRef="Jsp." loc="275,">Jsp. p. 275,</ref> vel <lbl expand="sequens">seq.</lbl> <foreign xml:lang="gez">ከመ፡</foreign> <lbl expand="cum">c.</lbl> <gramGrp> <pos expand="Subjunctivus">Subj.</pos> </gramGrp>: <foreign xml:lang="gez">ኢኮኑ፡ አይሁድ፡ ይደፍሩ፡ ከመ፡ ይስዐሩ፡ ካህነ፡ እምሢመቱ፡ እስከ፡ ይመውት፡</foreign> <ref cRef="Jsp." loc="322">Jsp. p. 322</ref>; vel <lbl expand="sequens">seq.</lbl> verbo finito: <foreign xml:lang="gez">ኢይድፍር፡ ወኢይትናገር፡ መኑሂ፡</foreign> <ref cRef="Sx. Ter" loc="8">Sx. Ter 8</ref>; vel <lbl expand="sequens">seq.</lbl> <gramGrp> <case value="accusativus">Acc.</case> </gramGrp> Infin.: <foreign xml:lang="gez">ደፈረ፡ ሰትየ፡ በንዋየ፡ ቅድሳት፡</foreign> <ref cRef="Jsp." loc="275">Jsp. p. 275</ref>; <foreign xml:lang="gez">ከመ፡ ኢይድፈሩ፡ ገቢረ፡</foreign> <ref cRef="II Abt." loc="29">II Abt. 29</ref>; vel <lbl expand="sequens">seq.</lbl> <foreign xml:lang="gez">ለ፡</foreign> <lbl expand="cum">c.</lbl> Infin.: <foreign xml:lang="gez">ይደፍር፡ ለምህሮ፡</foreign> <ref cRef="Phlx." loc="229">Phlx. 229</ref>; <lbl expand="cum">c.</lbl> <gramGrp> <case value="accusativus">Acc.</case> </gramGrp> rei: <foreign xml:lang="gez">ዘንተ፡ ደፈርከ፡ በትዕቢትከ፡</foreign> <ref cRef="Macc." loc="6">Macc. f. 6</ref>; <foreign xml:lang="gez">ሐሰወ፡ ወደፈረ፡ መሐላ፡</foreign> <ref cRef="Clem." loc="210">Clem. f. 210</ref>; <lbl expand="cum">c.</lbl> <foreign xml:lang="gez">ላዕለ፡</foreign> pers.: <foreign xml:lang="gez">ይድፍሩ፡ ላዕለ፡</foreign> (<note>sich erfrechen gegen einen</note>) <ref cRef="Chrys. ho." loc="8">Chrys. ho. 8</ref>; <foreign xml:lang="gez">ወኢክህለ፡ ፩ እምኔሆሙ፡ ይድፍር፡ ላዕሌሁ፡</foreign> (sc. leonem) <ref cRef="Jsp." loc="273">Jsp. p. 273</ref>; <lbl expand="cum">c.</lbl> <gramGrp> <case value="accusativus">Acc.</case> </gramGrp> <lbl expand="persona">pers.</lbl>: peccata sacerdotis qui divulgat, <foreign xml:lang="gez">ደፈሮ፡ ለእግዚአብሔር፡</foreign> <ref cRef="Kedr f." loc="28">Kedr f. 28</ref>; <foreign xml:lang="gez">ይሁዳ፡ ደፈረከ፡</foreign> Lit. Jac. </sense>
               
