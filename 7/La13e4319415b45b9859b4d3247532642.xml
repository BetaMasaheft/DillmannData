               <form>
                  <foreign xml:lang="gez">ሰኰና</foreign>
               </form>
               <sense xml:lang="la" source="#dillmann" xml:id="Dmain"> (passim <foreign xml:lang="gez">ሰኮና፡</foreign>), <gramGrp>
                     <pos expand="Substantivum">subst.</pos>
                  </gramGrp>, <gramGrp>
                     <gen value="masculinum">m.</gen>
                  </gramGrp> et <gramGrp>
                     <gen value="femininus">fem.</gen>
                  </gramGrp>, <lbl expand="collatio, -is vel collectivum, -e">coll.</lbl> et <lbl expand="singularis">sing.</lbl>, <lbl expand="pluralis">Pl.</lbl>
                  <foreign xml:lang="gez">ሰኳንው፡</foreign> [aliud etymon conjectavit Dietrich semit.
                     <ref cRef="Wurzelforsch." loc="88">Wurzelforsch. p. 88</ref>] <sense n="1" xml:id="D1">
                     <cit type="translation" xml:lang="la">
                        <quote>planta</quote>
                     </cit> sive <cit type="translation" xml:lang="la">
                        <quote>solum pedis</quote>
                     </cit>: <foreign xml:lang="gez">ሰኰና፡ እገሪሃ፡ </foreign> (respondent <foreign xml:lang="gez">እራኃተ፡ እደዊሃ፡</foreign>) <ref cRef="4 Reg." loc="9,35">4 Reg.
                        9,35</ref>; <foreign xml:lang="gez">ኢይቀውሙ፡ በሰኰና፡ እገሪሆሙ፡</foreign> (<foreign xml:lang="grc">ἴχνος ποδῶν</foreign>) <ref cRef="Judith" loc="6,4">Judith
                        6,4</ref>; <foreign xml:lang="gez">ትከይዶሙ፡ በሰኰናከ፡ </foreign>
                     <ref cRef="Hez." loc="32,2">Hez. 32,2</ref>; <foreign xml:lang="grc">τὰ ἴχνη
                        μου</foreign>
                     <ref cRef="Ps." loc="17,20">Ps. 17,20</ref>; <foreign xml:lang="grc">ποῦς</foreign>
                     <ref cRef="Sir." loc="6,36">Sir. 6,36</ref>; <foreign xml:lang="grc">διαβήματα</foreign>
                     <ref cRef="Ps." loc="16,6">Ps. 16,6</ref>; <ref cRef="Ps." loc="36,33">Ps.
                        36,33</ref>; <ref cRef="Ps." loc="72,2">Ps. 72,2</ref> (alias <foreign xml:lang="gez">መከየድ፡</foreign>, <foreign xml:lang="gez">መካይድ፡</foreign>);
                        <foreign xml:lang="gez">ሰላም፡ ለሰኰናኪ፡</foreign> I <ref cRef="Sal. Mar." loc="32">Sal. Mar. 32</ref>; de <cit type="translation" xml:lang="la">
                        <quote>planta</quote>
                     </cit> i.e. <cit type="translation" xml:lang="la">
                        <quote>palma</quote>
                     </cit> manus <ref cRef="4 Esr." loc="4,16">4 Esr. 4,16</ref> (siquidem lectio
                     sana est).</sense>
                  <sense n="2" xml:id="D2">
                     <cit type="translation" xml:lang="la">
                        <quote>calx</quote>
                     </cit> pedis, <foreign xml:lang="grc">πτέρνα</foreign>
                     <ref cRef="Gen." loc="3,15">Gen. 3,15</ref>; <ref cRef="Gen." loc="25,26">Gen.
                        25,26</ref>; <ref cRef="Sir." loc="12,27">Sir. 12,27</ref>; <foreign xml:lang="gez">ተከሥተ፡ ሰኰናኪ፡</foreign>
                     <ref cRef="Jer." loc="13,22">Jer. 13,22</ref>; <ref cRef="Joh. " loc="13,18">Joh. 13,18</ref>.</sense>
                  <sense n="3" xml:id="D3">
                     <cit type="translation" xml:lang="la">
                        <quote>ungula</quote>
                     </cit>, equi <ref cRef="Gen." loc="49,17">Gen. 49,17</ref>; <ref cRef="Jud." loc="5,22">Jud. 5,22</ref>; <foreign xml:lang="gez">ሰኰና፡ አፍራሲሆሙ፡ </foreign>
                     <ref cRef="Jes." loc="5,28">Jes. 5,28</ref>
                     <lbl expand="varia lectio">var.</lbl>; <foreign xml:lang="grc">ἴχνος
                        κτηνῶν</foreign>
                     <foreign xml:lang="gez">ሰኰና፡ እንስሳ፡</foreign>
                     <ref cRef="Hez." loc="32,13">Hez. 32,13</ref>; <foreign xml:lang="grc">πτέρναι
                        τῶν ποιμνίων</foreign>
                     <ref cRef="Cant." loc="1,8">Cant. 1,8</ref>; <foreign xml:lang="grc">ἀκρωτήρια </foreign>
                     <ref cRef="Lev." loc="4,11">Lev. 4,11</ref>; <foreign xml:lang="grc">ὁπλή</foreign>
                     <ref cRef="Mich." loc="4,13">Mich. 4,13</ref>; <foreign xml:lang="gez">ዘንፉቅ፡
                        ሰኰናሁ፡</foreign>
                     <ref cRef="Lev." loc="11,3">Lev. 11,3</ref>-7; <ref cRef="Deut." loc="14,6">Deut. 14,6</ref>-8; <foreign xml:lang="gez">እለ፡ ሰኰና፡</foreign> (v. <foreign xml:lang="gez">ጽፍረ፡</foreign>) <foreign xml:lang="gez">ላህም፡ እግሪሆሙ፡</foreign>
                     <foreign xml:lang="grc">ὀνοκένταυροι</foreign>
                     <ref cRef="Jes." loc="13,21">Jes. 13,21</ref>; <ref cRef="Jes." loc="34,14">Jes. 34,14</ref>. – <lbl expand="Vocabularium Aethiopicum, -a -a.">Voc. Ae.</lbl>: <foreign xml:lang="gez">ሰኮና፡ ብ፡ ተረከዝ።</foreign>
                  </sense>
               </sense>
               
