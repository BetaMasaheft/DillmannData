                  <foreign xml:lang="gez">አድለቅለቀ</foreign>
               </form>
               <sense xml:lang="la" source="#dillmann" xml:id="Dmain"> II <sense n="1" xml:id="D1">
                     <cit type="translation" xml:lang="la">
                        <quote>commoveri</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>concuti</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>tremere</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>contremiscere</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>vaciliare</quote>
                     </cit>, de coelo, terra, montibus, fundamentis, locis: <foreign xml:lang="gez">ያድለቀልቅ፡ ሰማይ፡ ወምድር፡</foreign>
                     <ref cRef="Joel" loc="4,16">Joel 4,16</ref>; <ref cRef="Jes." loc="24,18">Jes.
                        24,18</ref>; <ref cRef="Jer." loc="8,16">Jer. 8,16</ref>; <ref cRef="Jer." loc="27,46">Jer. 27,46</ref>; <ref cRef="Ps." loc="17,9">Ps. 17,9</ref>;
                        <ref cRef="Ps." loc="45,2">Ps. 45,2</ref>; <ref cRef="Ps." loc="45,3">Ps.
                        45,3</ref>; <ref cRef="Ps." loc="81,5">Ps. 81,5</ref>; <ref cRef="Sir." loc="16,18">Sir. 16,18</ref>; <ref cRef="Sir." loc="16,19">Sir. 16,19</ref>;
                        <ref cRef="Sir. " loc="22,16">Sir. 22,16</ref>; <ref cRef="Sir." loc="43,16">Sir. 43,16</ref>; <ref cRef="Act." loc="4,31">Act. 4,31</ref>; <foreign xml:lang="gez">አድለቅለቁ፡ አድባር፡ እምኔሁ፡ </foreign>
                     <ref cRef="Nah." loc="1,5">Nah. 1,5</ref>; <ref cRef="4 Esr." loc="10,39">4
                        Esr. 10,39</ref>; <foreign xml:lang="gez">ታድለቀልቅ፡ ሰማየ፡ ሰማያት፡ ድልቅልቀ፡
                        ዐቢየ፡</foreign>
                     <ref cRef="Hen." loc="60,1">Hen. 60,1</ref>; de aliis rebus: <ref cRef="Jes." loc="17,4">Jes. 17,4</ref>; <ref cRef="Jes." loc="19,1">Jes. 19,1</ref>; de
                     hominibus, angelis: <foreign xml:lang="gez">አኀዙ፡ ይርዐዱ፡ ወያድለቅልቁ፡
                        እምኔሆሙ፡</foreign>
                     <ref cRef="Hen." loc="86,6">Hen. 86,6</ref>; <ref cRef="Hen." loc="1,5">Hen.
                        1,5</ref>; etiam de <foreign xml:lang="gez">ድልቅልቅ፡</foreign> ipso
                     praedicatur: <foreign xml:lang="gez">አድለቅለቀ፡ ዐቢይ፡ ድልቅልቅ፡</foreign>
                     <ref cRef="Act." loc="16,26">Act. 16,26</ref>.
                     </sense>
                     <sense n="2" xml:id="D2">
                        <cit type="translation" xml:lang="la">
                           <quote>commovere</quote>
                        </cit>, <cit type="translation" xml:lang="la">
                           <quote>concutere</quote>
                        </cit>, <cit type="translation" xml:lang="la">
                           <quote>contremiscere facere</quote>
                        </cit>: <ref cRef="Ps." loc="28,7">Ps. 28,7</ref>; <ref cRef="Ps. " loc="59,2">Ps. 59,2</ref>; <ref cRef="Ps." loc="98,1">Ps. 98,1</ref>;
                           <ref cRef="Job" loc="9,5">Job 9,5</ref>; <ref cRef="Job" loc="9,6">Job
                           9,6</ref>; <ref cRef="Jes." loc="10,14">Jes. 10,14</ref>; <ref cRef="Hagg." loc="2,6">Hagg. 2,6</ref>; <ref cRef="Hebr." loc="12,26">Hebr. 12,26</ref>; <ref cRef="4 Esr. " loc="1,19">4 Esr. 1,19</ref>;
                           <foreign xml:lang="gez">ለእመ፡ አድለቅለቀ፡ ድልቅልቅ፡ መካነ፡</foreign>
                        <ref cRef="4 Esr." loc="4,19">4 Esr. 4,19</ref>
                        <lbl expand="varia lectio">var.</lbl>; <cit type="translation" xml:lang="la">
                           <quote>movere</quote>
                        </cit> aliquid loco, <lbl expand="cum">c.</lbl>
                        <gramGrp>
                           <case value="accusativus">Acc.</case>
                        </gramGrp> et <foreign xml:lang="gez">እምነ፡ </foreign>
                        <ref cRef="Apoc." loc="2,5">Apoc. 2,5</ref>.</sense>
               </sense>
               
