            <entry xml:id="La9644c19df4e4d8ea5fe0bd9d0306c97" n="9541">
               <form>
                  <foreign xml:lang="gez">አጼነወ</foreign>
               </form>
               <sense xml:lang="la" source="#dillmann" xml:id="Dmain"> II <cit type="translation" xml:lang="la">
                     <quote>olfacere</quote>
                  </cit>, <cit type="translation" xml:lang="la">
                     <quote>odorari</quote>
                  </cit>, <lbl expand="cum">c.</lbl>
                  <gramGrp>
                     <case value="accusativus">Acc.</case>
                  </gramGrp>: <ref cRef="Gen." loc="8,21">Gen. 8,21</ref>; <ref cRef="Kuf." loc="6">Kuf. 6</ref>; <ref cRef="Kuf. " loc="26">Kuf. 26</ref>; <ref cRef="Lev." loc="26,31">Lev. 26,31</ref>; <ref cRef="Am." loc="5,21">Am. 5,21</ref>;
                     <foreign xml:lang="gez">ኢአንፎሙ፡ ለአጼንዎ፡ ነፋስ፡</foreign>
                  <ref cRef="Sap. " loc="15,15">Sap. 15,15</ref>; <ref cRef="Ex." loc="30,38">Ex.
                     30,38</ref>. <sense n="a" xml:id="Da">
                     <gramGrp>
                        <pos expand="absolute">abs.</pos>
                     </gramGrp>: <ref cRef="Deut." loc="4,28">Deut. 4,28</ref>; <ref cRef="Ps." loc="113,14">Ps. 113,14</ref>; <ref cRef="Ps." loc="134,17">Ps.
                     134,17</ref>; <foreign xml:lang="gez">ኢእሙንቱ፡ ያጼንዉ፡</foreign>
                     <ref cRef="Sir." loc="30,19">Sir. 30,19</ref>; <foreign xml:lang="gez">አጼንዎ፡ </foreign>
                     <cit type="translation" xml:lang="la">
                        <quote>olfactus</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>odoratus</quote>
                     </cit>
                     <lbl expand="videas">vid.</lbl>
                     <foreign xml:lang="gez">ሕዋስ፡</foreign>
                  </sense>
                  <sense n="b" xml:id="Db">
                     <cit type="translation" xml:lang="la">
                        <quote>odorari</quote>
                     </cit> i.e. <cit type="translation" xml:lang="la">
                        <quote>odoratu explorare</quote>
                     </cit>, <lbl expand="cum">c.</lbl>
                     <gramGrp>
                        <case value="accusativus">Acc.</case>
                     </gramGrp>: <ref cRef="Gen." loc="27,27">Gen. 27,27</ref>; <gramGrp>
                        <pos expand="absolute">abs.</pos>
                     </gramGrp>: <ref cRef="Lit. " loc="170,3">Lit. 170,3</ref>.</sense>
                  <sense n="c" xml:id="Dc">
                     <cit type="translation" xml:lang="la">
                        <quote>praesagire</quote>
                     </cit>: <foreign xml:lang="gez">እምርሑቅ፡ ያጼንዎ፡ ለቀትል፡</foreign>
                     <foreign xml:lang="grc">ὀσφαίνεται</foreign>
                     <ref cRef="Job" loc="39,25">Job 39,25</ref>.</sense>
                  <sense n="d" xml:id="Dd">
                     <lbl expand="translate">transl.</lbl>: <cit type="translation" xml:lang="la">
                        <quote>sentire</quote>
                     </cit>, <foreign xml:lang="gez">ፈትል፡ ሶበ፡ ያጼኑ፡ እሳተ፡ </foreign>
                     <ref cRef="Jud." loc="16,9">Jud. 16,9</ref>; <ref cRef="Jud." loc="15,14">Jud.
                        15,14</ref>.</sense>
               </sense>
               
