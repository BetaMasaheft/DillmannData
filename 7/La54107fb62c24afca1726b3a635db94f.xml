            <entry xml:id="La54107fb62c24afca1726b3a635db94f" n="7903">
               <form>
                  <foreign xml:lang="gez">ደረሰ</foreign>
               <rs type="root"/>
                    </form>
               <sense xml:lang="la" source="#dillmann" xml:id="Dmain"> <subc>I,1</subc> [<foreign xml:lang="he">דָּרַשׁ</foreign> <foreign xml:lang="syr">ܕܪܰܫ</foreign> <foreign xml:lang="ar">دَرَسَ</foreign>; <lbl expand="videas">vid.</lbl> Ges. thes. p. 356; Ges. thes. p. 357] <gramGrp> <pos expand="Subjunctivus">Subj.</pos> </gramGrp> <foreign xml:lang="gez">ይድርስ፡</foreign> <sense n="1" xml:id="D1"> librum vel lectionem, postquam lecta sunt, <cit type="translation" xml:lang="la">
                                <quote> tractare </quote>
                            </cit> exegetice vel homiletice i.e. <cit type="translation" xml:lang="la">
                                <quote> commentari </quote>
                            </cit> vel <cit type="translation" xml:lang="la">
                                <quote> interpretari </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> explanare oratione </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> disserere </quote>
                            </cit>, <lbl expand="cum">c.</lbl> <gramGrp> <case value="accusativus">Acc.</case> </gramGrp> lectionis interpretandae: <foreign xml:lang="gez">እፈቱ፡ አነ፡ ዘይእዜ፡ ተነበ፡ በወንጌል፡ እድርስ፡ ለክሙ፡</foreign> hom. Severiani in <ref cRef="Cyr." loc="122">Cyr. f. 122</ref>; <foreign xml:lang="gez">መጻሕፍተ፡ ሕግ፡ ደሪሶ፡</foreign> <ref cRef="Sx. Genb." loc="27 ">Sx. Genb. 27 </ref>Enc.; vel <lbl expand="cum">c.</lbl> <gramGrp> <case value="accusativus">Acc.</case> </gramGrp> interpretationis: <foreign xml:lang="gez">ቀሲስ፡</foreign> (lectione lecta) <foreign xml:lang="gez">ይድርስ፡ ድርሳነ፡ ዘመፍትው፡</foreign> <ref cRef="Kid." loc="12.">Kid. f. 12.</ref> </sense> <sense n="2" xml:id="D2"> <cit type="translation" xml:lang="la">
                                <quote> commentari </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> conscribere </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> componere </quote>
                            </cit> libros, orationes sacras, carmina: <foreign xml:lang="gez">ደረሱ፡ በእንተዝ፡ ወጸሐፉ፡</foreign> <foreign xml:lang="grc">λογίδια ἅτινα περὶ τούτων συγγράφοντες</foreign> Cyr. ad <ref cRef="Theod." loc="8">Theod. f. 8</ref>; <lbl expand="cum">c.</lbl> <gramGrp> <case value="accusativus">Acc.</case> </gramGrp> libri: Cyrillus <foreign xml:lang="gez">ደረሰ፡ ፲ወ፪ ቃላተ፡ ወከሠተ፡ ውስቴቶሙ፡ ሃይማኖተ፡ ርትዕተ፡ ወእምድኅሬሆሙ፡ ደረሰ፡ ድርሳናተ፡ ወተግሣጻተ፡ ወመልእክታተ፡</foreign> <ref cRef="Cyr." loc="2">Cyr. f. 2</ref>; <foreign xml:lang="gez">ዝንቱ፡ መጽሐፍ፡ ዘደረሰ፡</foreign> Abush., in Proleg.; <foreign xml:lang="gez">ቅዳሴ፡ ዘደረሰ፡ ህርያቆስ፡</foreign> <ref cRef="Lit." loc="170,1">Lit. 170,1</ref>; <foreign xml:lang="gez">ደሪሰ፡ ድርሳን፡</foreign> <ref cRef="Sx. Tachs." loc="8.">Sx. Tachs. 8.</ref> Enc. <cb n="1094" xml:id="c1094"/> </sense> </sense>
               
