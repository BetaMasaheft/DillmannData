               <sense xml:lang="la" source="#dillmann" xml:id="Dmain"> <subc>I,1</subc> [vicinum <foreign xml:lang="gez">ደረከ፡</foreign>; e significatu <cit type="translation" xml:lang="la">
                            <quote> calcandi </quote>
                        </cit> et <cit type="translation" xml:lang="la">
                            <quote> gradiendi </quote>
                        </cit> (<foreign xml:lang="he">דרג</foreign> <foreign xml:lang="ar">درج</foreign> <foreign xml:lang="syr">ܕܪܓ</foreign>) proficiscitur <cit type="translation" xml:lang="la">
                            <quote> sequi </quote>
                        </cit>, <cit type="translation" xml:lang="la">
                            <quote> assequi </quote>
                        </cit> (<foreign xml:lang="ar">ادرك</foreign>) et <cit type="translation" xml:lang="la">
                            <quote> sese invicem sequi </quote>
                        </cit>, <cit type="translation" xml:lang="la">
                            <quote> contineri </quote>
                        </cit> (ut <foreign xml:lang="ar">طَرْقَةٌ</foreign> <cit type="translation" xml:lang="la">
                            <quote> ordo </quote>
                        </cit>, <cit type="translation" xml:lang="la">
                            <quote> series </quote>
                        </cit>, <foreign xml:lang="ar">درّك</foreign>), <cit type="translation" xml:lang="la">
                            <quote> duplicare </quote>
                        </cit>, <cit type="translation" xml:lang="la">
                            <quote> obducere </quote>
                        </cit> (<foreign xml:lang="ar">اطرق</foreign>, <foreign xml:lang="ar">طَرَقٌ</foreign>) et <cit type="translation" xml:lang="la">
                            <quote> complicare </quote>
                        </cit>, <cit type="translation" xml:lang="la">
                            <quote> convolvere </quote>
                        </cit> (<foreign xml:lang="ar">درج</foreign> I. II. IV)] <gramGrp> <pos expand="Subjunctivus">Subj.</pos> </gramGrp> <foreign xml:lang="gez">ይድረግ፡</foreign>, <cit type="translation" xml:lang="la">
                            <quote> connexum </quote>
                        </cit>, <cit type="translation" xml:lang="la">
                            <quote> conjunctum esse inter se </quote>
                        </cit>, <cit type="translation" xml:lang="la">
                            <quote> cohaerere </quote>
                        </cit>: <foreign xml:lang="gez">ወይደርጉ፡</foreign> (v. <foreign xml:lang="gez">ወይደረጉ፡</foreign>, <lbl expand="vel">v.</lbl> <foreign xml:lang="gez">ወይዴረጉ፡</foreign>) <foreign xml:lang="gez">፪</foreign> <foreign xml:lang="grc">δύο συνεζευγμέναι πρὸς ἀλλήλας</foreign> <ref cRef="Hez. " loc="1,11">Hez.  1,11</ref>; <foreign xml:lang="gez">ይድረግ፡ ዕሩየ፡</foreign> <foreign xml:lang="grc">κατὰ τὸ αὐτὸ ἔσονται ἴσοι</foreign> <ref cRef="Ex." loc="26,24">Ex. 26,24</ref>. </sense>
               
