            <entry xml:id="La0f3d803fda543a8aa6ab928ea2f4cbf" n="3903">
               <form>
                  <foreign xml:lang="gez">ተባረከ</foreign>
               </form>
               <sense xml:lang="la" source="#dillmann" xml:id="Dmain"> <subc>III,3</subc> <sense n="1" xml:id="D1"> <cit type="translation" xml:lang="la">
                                <quote> laudari </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> celebrari </quote>
                            </cit>: <foreign xml:lang="gez">ይትባረክ፡ እግዚአብሔር፡</foreign> <foreign xml:lang="grc">εὐλογητὸς Κύριος</foreign> <ref cRef="Ruth" loc="4,14">Ruth 4,14</ref>; <ref cRef="1 Reg. " loc="25,32">1 Reg.  25,32</ref>; <ref cRef="2 Reg." loc="18,28">2 Reg. 18,28</ref>; <ref cRef="Ps." loc="27,8">Ps. 27,8</ref>; <ref cRef="Ps." loc="30,27">Ps. 30,27</ref>; <ref cRef="Ps." loc="71,19">Ps. 71,19</ref>; <ref cRef="Ps." loc="71,20">Ps. 71,20</ref>; <ref cRef="Hen." loc="39,10">Hen. 39,10</ref>; <ref cRef="Hen. " loc="61,11">Hen.  61,11</ref>; <ref cRef="Eph." loc="1,3">Eph. 1,3</ref>; <ref cRef="Luc." loc="1,68">Luc. 1,68</ref>; <ref cRef="Jes." loc="65,16">Jes. 65,16</ref>.</sense> <sense n="2" xml:id="D2"> <cit type="translation" xml:lang="la">
                                <quote> benedicere sibi </quote>
                            </cit> vel <cit type="translation" xml:lang="la">
                                <quote> sibi invicem </quote>
                            </cit> (reflex. et recipr.), <cit type="translation" xml:lang="la">
                                <quote> benedictionem petere </quote>
                            </cit> vel <cit type="translation" xml:lang="la">
                                <quote> accipere </quote>
                            </cit> (<gramGrp> <pos expand="passivum">pass.</pos> </gramGrp>): <foreign xml:lang="gez">ይትባረኩ፡ ቦቱ፡ አሕዛበ፡ ምድር፡</foreign> <ref cRef="Ps." loc="71,18">Ps. 71,18</ref>; <foreign xml:lang="gez">ተባረኩ፡ በበይናቲሆሙ፡</foreign> <ref cRef="Sx. Jac." loc="9">Sx. Jac. 9</ref>; <lbl expand="cum">c.</lbl> <foreign xml:lang="gez">እምነ፡</foreign> ejus, a quo quis benedictionem petit et accipit: <foreign xml:lang="gez">ተባረከ፡ እምኔሆሙ፡</foreign> <ref cRef="Sx. Masc." loc="14">Sx. Masc. 14</ref>; <ref cRef="Sx. Masc." loc="18">Sx. Masc. 18</ref>; Abraham <foreign xml:lang="gez">ሰገደ፡ ሎቱ፡</foreign> (Melchisedeco) <foreign xml:lang="gez">ወተባረከ፡ እምኔሁ፡</foreign> <ref cRef="Mavâs." loc=" 67">Mavâs.  67</ref>; <foreign xml:lang="gez">ሖረ፡ ሀገረ፡ ኢየሩሳሌም፡ ወተባረከ፡ እምካናት፡ ቅዱሳት፡</foreign> <ref cRef="Sx. Haml. " loc="11">Sx. Haml.  11</ref>; vel <lbl expand="cum">c.</lbl> <foreign xml:lang="gez">በኀበ፡</foreign>, ut: <foreign xml:lang="gez">ሴም፡ ተባረከ፡ በኀበ፡ ኖኅ፡</foreign> <ref cRef="Kebr. Nag." loc="9.">Kebr. Nag. 9.</ref> </sense> <sense n="3" xml:id="D3"> benedictione <cit type="translation" xml:lang="la">
                                <quote> consecrari </quote>
                            </cit> <ref cRef="Ex." loc="19,22">Ex. 19,22</ref> (<foreign xml:lang="grc">ἁγιαθήτωσαν</foreign>).</sense> <sense n="4" xml:id="D4"> <cit type="translation" xml:lang="la">
                                <quote> benedici </quote>
                            </cit> i.e. <cit type="translation" xml:lang="la">
                                <quote> fortunari </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> prosperari </quote>
                            </cit>, <ref cRef="Hen." loc="1,8">Hen. 1,8</ref>; <ref cRef="Hen." loc="67,3">Hen. 67,3</ref>; <lbl expand="cum">c.</lbl> <foreign xml:lang="gez">በ፡</foreign> <ref cRef="Gen." loc="22,18">Gen. 22,18</ref>; <ref cRef="Gen." loc="26,4">Gen. 26,4</ref>; <ref cRef="Sir." loc="44,21">Sir. 44,21</ref>; <foreign xml:lang="gez">ተባረኩ፡ በውሉዶሙ፡</foreign> <ref cRef="Tob." loc="4,12">Tob. 4,12</ref>; <lbl expand="cum">c.</lbl> <foreign xml:lang="gez">በእንተ፡</foreign> <ref cRef="Gen." loc="12,3">Gen. 12,3</ref>; <ref cRef="Gen." loc="18,18">Gen. 18,18</ref>; <ref cRef="Gen." loc="28,14">Gen. 28,14</ref>.</sense> </sense>
               
