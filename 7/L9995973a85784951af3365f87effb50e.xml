               <sense xml:lang="la" source="#dillmann" xml:id="Dmain"> <subc>I,2</subc> [<foreign xml:lang="he">עִשַּׂר</foreign> <foreign xml:lang="syr">ܥܰܣܰܪ</foreign> <foreign xml:lang="ar">عَشَرَ</foreign>; significatus <cit type="translation" xml:lang="la">
                            <quote> decem </quote>
                        </cit> quomodo cum I <foreign xml:lang="gez">ዐሠረ፡</foreign> componendus sit, <lbl expand="videas">vid.</lbl> Ges. thes. p. 1078] <sense n="1" xml:id="D1"> <cit type="translation" xml:lang="la">
                                <quote> decimam </quote>
                            </cit> rei <cit type="translation" xml:lang="la">
                                <quote> partem dare </quote>
                            </cit>: <foreign xml:lang="gez">ዐሠረ፡ እምኵሉ፡</foreign> <ref cRef="Kuf." loc="32">Kuf. 32</ref>; <foreign xml:lang="gez">ከመ፡ የዐሥር፡</foreign> et <foreign xml:lang="gez">ለዐሥሮ፡</foreign> <ref cRef="Kuf." loc="32">Kuf. 32</ref>; <ref cRef="Hebr." loc="7,9">Hebr. 7,9</ref>; <lbl expand="cum">c.</lbl> <gramGrp> <case value="accusativus">Acc.</case> </gramGrp> decimae: <foreign xml:lang="gez">ፈጸምከ፡ ዐሥሮተ፡ ኵሉ፡ ዐሥራት፡</foreign> <ref cRef="Deut." loc="26,12">Deut. 26,12</ref>; <ref cRef="Gen." loc="28,22">Gen. 28,22</ref>; <foreign xml:lang="gez">ዐሥራተ፡ ይዔሥር፡</foreign> <ref cRef="Macc." loc="15.">Macc. f. 15.</ref> </sense> <sense n="2" xml:id="D2"> <cit type="translation" xml:lang="la">
                                <quote> decimare </quote>
                            </cit>i.e. <sense n="a" xml:id="D2a"> <cit type="translation" xml:lang="la">
                                    <quote> decimam partem dare </quote>
                                </cit> rei, <lbl expand="cum">c.</lbl> <gramGrp> <case value="accusativus">Acc.</case> </gramGrp> rei: <foreign xml:lang="gez">ዐሠረ፡ ኵሎ፡ እንስሳ፡ ንጹሐ፡</foreign> <ref cRef="Kuf." loc="32">Kuf. 32</ref>; <ref cRef="Deut." loc="14,22">Deut. 14,22</ref>.</sense> <sense n="b" xml:id="D2b"> <cit type="translation" xml:lang="la">
                                    <quote> decimam partem </quote>
                                </cit> <cit type="translation" xml:lang="la">
                                    <quote> sumere </quote>
                                </cit> vel <cit type="translation" xml:lang="la">
                                    <quote> accipere </quote>
                                </cit> rei, <lbl expand="cum">c.</lbl> <gramGrp> <case value="accusativus">Acc.</case> </gramGrp> rei <ref cRef="1 Reg." loc="8,17">1 Reg. 8,17</ref> annot.; et <lbl expand="cum">c.</lbl> <gramGrp> <case value="accusativus">Acc.</case> </gramGrp> rei et <gramGrp> <case value="accusativus">Acc.</case> </gramGrp> <lbl expand="persona">pers.</lbl> (<cit type="translation" xml:lang="la">
                                    <quote> ab aliquo </quote>
                                </cit>) <ref cRef="1 Reg." loc="8,17">1 Reg. 8,17</ref>.</sense> </sense> <sense n="3" xml:id="D3"> <cit type="translation" xml:lang="la">
                                <quote> per </quote>
                            </cit> <cit type="translation" xml:lang="la">
                                <quote> decem multiplicare </quote>
                            </cit>,<bibl>
                                <ptr target="bm:Ludolf1691Commentarius"/>
                            </bibl> p. 386.</sense> </sense>
               
