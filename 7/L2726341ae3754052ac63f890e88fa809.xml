               <sense xml:id="Dmain" xml:lang="la" source="#dillmann"> <subc>II,1</subc> <sense n="1" xml:id="D1"> <cit type="translation" xml:lang="la"><quote> patefacere </quote></cit>, <cit type="translation" xml:lang="la"><quote> aperire </quote></cit>, <cit type="translation" xml:lang="la"><quote> reserare </quote></cit>, <lbl expand="cum">c.</lbl> <gramGrp> <case value="accusativus">Acc.</case> </gramGrp>, ut: <foreign xml:lang="gez">አርኀወት፡ ኆኅተ፡</foreign> <ref cRef="Tob." loc="8,13">Tob. 8,13</ref>; <foreign xml:lang="gez">ኣርኁ፡ አናቅጸ፡</foreign> <ref cRef="Jes." loc="45,1">Jes. 45,1</ref>; <foreign xml:lang="gez">አርኁ፡ ሊተ፡ አብያተ፡ ዘዕፅዋን፡</foreign> <ref cRef="4 Esr." loc="3,49">4 Esr. 3,49</ref>; <ref cRef="1 Reg." loc="3,15">1 Reg. 3,15</ref>; <ref cRef="1 Par." loc="9,27">1 Par. 9,27</ref>; <ref cRef="Ps." loc="23,7">Ps. 23,7</ref>; <ref cRef="Ps." loc="23,9">Ps. 23,9</ref>; <ref cRef="Job" loc="38,32">Job 38,32</ref>; <ref cRef="Job " loc="41,5">Job  41,5</ref>; <foreign xml:lang="gez">እመ፡ አርኀውከ፡ ሰማየ፡</foreign> <ref cRef="Jes." loc="64,1">Jes. 64,1</ref>; <foreign xml:lang="gez">አፈወ፡ አንቅዕት፡</foreign> <ref cRef="Kuf." loc="5">Kuf. 5</ref>; <foreign xml:lang="gez">መስኮተ፡</foreign> <ref cRef="Gen." loc="8,6">Gen. 8,6</ref>; <ref cRef="4 Reg." loc="13,17">4 Reg. 13,17</ref>; <foreign xml:lang="gez">አርኀዋ፡ ለታቦት፡</foreign> <ref cRef="Kuf. " loc="5">Kuf.  5</ref>; <foreign xml:lang="gez">ማሕፀነ፡</foreign> <ref cRef="Kuf." loc="28">Kuf. 28</ref>; <foreign xml:lang="gez">መቃብረ፡</foreign> <ref cRef="Hez." loc="37,13">Hez. 37,13</ref>; <foreign xml:lang="gez">መዝገበ፡</foreign> <ref cRef="Jer." loc="27,25">Jer. 27,25</ref>; <ref cRef="Hen. " loc="60,21">Hen.  60,21</ref>; <ref cRef="Matth." loc="2,11">Matth. 2,11</ref>; <foreign xml:lang="gez">አፉሃ፡</foreign> <ref cRef="Apoc." loc="12,16">Apoc. 12,16</ref>; <foreign xml:lang="gez">ጕርዔ፡</foreign> <ref cRef="Sir." loc="34,12">Sir. 34,12</ref>; <foreign xml:lang="gez">እደ፡</foreign> <ref cRef="Deut." loc="15,8">Deut. 15,8</ref>; <ref cRef="Deut." loc="15,11">Deut. 15,11</ref>; etiam omisso objecto: <foreign xml:lang="gez">ለእመ፡ አርኀወ፡ ውእቱ፡ መኑ፡ ዘየዐፁ፡</foreign> <ref cRef="Jes." loc="22,22">Jes. 22,22</ref>; <ref cRef="Job" loc="12,14">Job 12,14</ref>; <ref cRef="Apoc." loc="3,7">Apoc. 3,7</ref>; <lbl expand="cum">c.</lbl> <gramGrp> <pos expand="pronomen">pron.</pos> </gramGrp> suff. vice Dativi: <foreign xml:lang="gez">አርኅወኒ፡</foreign> <foreign xml:lang="grc">ἄνοιξόν μοι</foreign> <ref cRef="Cant." loc="5,2">Cant. 5,2</ref>; <ref cRef="Cant." loc="5,5">Cant. 5,5</ref>; <ref cRef="Deut." loc="20,11">Deut. 20,11</ref>; <ref cRef="Matth." loc="25,11">Matth. 25,11</ref>; <ref cRef="Apoc. " loc="3,20">Apoc.  3,20</ref>; <ref cRef="Jud." loc="3,25">Jud. 3,25</ref>. 
</sense> <sense n="2" xml:id="D2"> <cit type="translation" xml:lang="la"><quote> expugnare </quote></cit> (in libris ex Arabico translatis), urbem, terram: <foreign xml:lang="gez">አርኀወ፡ ሀገረ፡</foreign> <ref cRef="Jsp." loc="296">Jsp. p. 296</ref>; <ref cRef="Jsp." loc="299">Jsp. p. 299</ref>; <ref cRef="Jsp." loc="313">Jsp. p. 313</ref>; Jsp. p. 347; <ref cRef="Abush." loc="48">Abush. 48</ref>; <ref cRef="Sx. Sen." loc="26.">Sx. Sen. 26.</ref> 
</sense> </sense>
               
            <sense xml:id="Tmain" xml:lang="en" source="#traces"> <cit type="transcription" xml:lang="gez"><quote>ʾarḫawa</quote></cit> <sense n="G" xml:id="TG"> <foreign xml:lang="gez">አርኀወ፡</foreign> <cit type="transcription" xml:lang="gez"><quote>ʾarḫawa</quote></cit> <subc>II,1</subc> <foreign xml:lang="gez">ያርኁ፡</foreign> «<cit type="translation" xml:lang="fr"><quote>ouvrir</quote></cit>» ― <foreign xml:lang="am">ከፈተ፡</foreign> Ms. <ref type="BM" target="BNFabb217">BNFabb217</ref>, fol. 59v. <lb/>
 <bibl><ptr target="bm:Grebaut1952SuppLexicon"/><citedRange unit="page"> 153</citedRange></bibl> 
</sense> <sense n="L" xml:id="TL"> <cit type="transcription" xml:lang="gez"><quote>rḫw</quote></cit>, <foreign xml:lang="gez">አርኀወ</foreign> <cit type="transcription" xml:lang="gez"><quote>ʾarḫawa</quote></cit> <cit type="translation" xml:lang="en"><quote>open</quote></cit>, <cit type="translation" xml:lang="en"><quote>unlock</quote></cit>, <cit type="translation" xml:lang="en"><quote>take by assault (a city)</quote></cit>; with object suffix pronouns it also serves for the expression of an indirect object: e.g. <foreign xml:lang="gez">አርኀወኒ</foreign> <cit type="transcription" xml:lang="gez"><quote>ʾarḫawanni</quote></cit> <cit type="translation" xml:lang="en"><quote>he opened for me</quote></cit> <bibl><ptr target="bm:Leslau1987dic"/><citedRange unit="page"> 468b</citedRange></bibl> 
</sense> </sense></entry>
