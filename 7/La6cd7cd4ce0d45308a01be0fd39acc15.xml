               <sense xml:lang="la" source="#dillmann" xml:id="Dmain"> <subc>I,1</subc> [referendum videtur ad <foreign xml:lang="he">בַּר</foreign> <foreign xml:lang="syr">ܒܰܪܳܐ</foreign> <foreign xml:lang="ar">بَرٌّ</foreign> <cit type="translation" xml:lang="la">
                            <quote> campus </quote>
                        </cit>, <cit type="translation" xml:lang="la">
                            <quote> rus </quote>
                        </cit>, <foreign xml:lang="ar">أَبَرَّ</foreign>; etiam <foreign xml:lang="ar">بَرَزَ</foreign> <cit type="translation" xml:lang="la">
                            <quote> exivit in campum </quote>
                        </cit>] <gramGrp> <pos expand="Subjunctivus">Subj.</pos> </gramGrp> <foreign xml:lang="gez">ይውፍር፡</foreign> etiam <foreign xml:lang="gez">ዩፈር፡</foreign>, Imperat. <foreign xml:lang="gez">ውፍር፡</foreign> et <foreign xml:lang="gez">ፈር፡</foreign>, <cit type="translation" xml:lang="la">
                            <quote> rus ire </quote>
                        </cit>, <cit type="translation" xml:lang="la">
                            <quote> in agros exire </quote>
                        </cit> vel <cit type="translation" xml:lang="la">
                            <quote> excurrere </quote>
                        </cit>, <cit type="translation" xml:lang="la">
                            <quote> per agros </quote>
                        </cit> vel <cit type="translation" xml:lang="la">
                            <quote> campos deambulare </quote>
                        </cit>, <cit type="translation" xml:lang="la">
                            <quote> ruri esse </quote>
                        </cit>: <foreign xml:lang="gez">ወፈሩ፡ ሕዝብ፡</foreign> <ref cRef="2 Esr." loc="18,16">2 Esr. 18,16</ref>; <foreign xml:lang="gez">ጸግወነ፡ ሰላመ፡ ኑፈር፡ ወንእቱ፡</foreign> Deg.; <lbl expand="sequens">seq.</lbl> <gramGrp> <case value="accusativus">Acc.</case> </gramGrp> loci: <foreign xml:lang="gez">ወፈረ፡ ሐቅለ፡</foreign> <ref cRef="Dan. apocr." loc="13,33">Dan. apocr. 13,33</ref>; <foreign xml:lang="gez">ገዳመ፡</foreign> <ref cRef="4 Reg. " loc="4,39">4 Reg.  4,39</ref>; <lbl expand="cum">c.</lbl> <foreign xml:lang="gez">ውስተ፡</foreign> loci, ut: <foreign xml:lang="gez">ሑሩ፡ ውፍሩ፡</foreign> (v. <foreign xml:lang="gez">ወፈሩ፡</foreign>) <foreign xml:lang="gez">ውስተ፡ ደብር፡</foreign> <ref cRef="2 Esr." loc="18,15">2 Esr. 18,15</ref>; <foreign xml:lang="gez">ውስተ፡ አህጉር፡</foreign> <ref cRef="Herm." loc="4">Herm. p. 4</ref>; <lbl expand="cum">c.</lbl> <foreign xml:lang="gez">እንተ፡</foreign> <cit type="translation" xml:lang="la">
                            <quote> per </quote>
                        </cit> <ref cRef="Marc." loc="2,23">Marc. 2,23</ref>; <ref cRef="Luc." loc="6,1">Luc. 6,1</ref>; in specie: operis rustici causa, <ref cRef="Luc." loc="8,5">Luc. 8,5</ref>; <ref cRef="Ps." loc="125,7">Ps. 125,7</ref>; <foreign xml:lang="gez">ይወፍር፡ ውስተ፡ ተግባሩ፡</foreign> <ref cRef="Ps." loc="103,24">Ps. 103,24</ref>; <foreign xml:lang="gez">ወፈረ፡ ኀበ፡ አቡሁ፡ ውስተ፡ ማእረር፡</foreign> <ref cRef="4 Reg." loc="4,18">4 Reg. 4,18</ref>; <foreign xml:lang="gez">ሶበ፡ እወፍር፡ ኀበ፡ ኵዕንት፡</foreign> <ref cRef="Sx. Tachs." loc="13 ">Sx. Tachs. 13 </ref>(<lbl expand="videas">vid.</lbl> <foreign xml:lang="gez">ኵዕንት፡</foreign>); <foreign xml:lang="gez">ወዓዲ፡ ይወፍር፡ አባግዕ፡ በእደ፡ ዘይኌልቆን፡</foreign> <ref cRef="Jer." loc="40,13">Jer. 40,13</ref>; <lbl expand="translate">transl.</lbl>: <sense n="a" xml:id="Da"> <cit type="translation" xml:lang="la">
                                <quote> frequentare </quote>
                            </cit> aliquem agricolae instar agros frequentantis, <lbl expand="cum">c.</lbl> <foreign xml:lang="gez">ኀበ፡</foreign> pers.: <foreign xml:lang="gez">ውፍር፡</foreign> (v. <foreign xml:lang="gez">ፈር፡</foreign>) <foreign xml:lang="gez">ኀቤሃ፡</foreign> (<foreign xml:lang="gez">ለጥበብ፡</foreign>) <foreign xml:lang="gez">ከመ፡ ዘየሐርስ፡</foreign> <foreign xml:lang="grc">ὡς ὁ ἀροτριῶν πρόσελθε αὐτῇ</foreign> <ref cRef="Sir." loc="6,19">Sir. 6,19</ref>; <cb n="0949" xml:id="c0949"/> <foreign xml:lang="gez">በኵሉ፡ ነፍስከ፡ ውፍር፡</foreign> (v. <foreign xml:lang="gez">ፈር፡</foreign>) <foreign xml:lang="gez">ኀቤሃ፡</foreign> <ref cRef="Sir." loc="6,26">Sir. 6,26</ref>.</sense> <sense n="b" xml:id="Db"> <cit type="translation" xml:lang="la">
                                <quote> serpere </quote>
                            </cit> vel <cit type="translation" xml:lang="la">
                                <quote> diffundi per </quote>
                            </cit> <cit type="translation" xml:lang="la">
                                <quote> agros </quote>
                            </cit>: <foreign xml:lang="gez">ይወፍር፡ አዕጹቂሁ፡</foreign> <foreign xml:lang="grc">πορεύσονται οἱ κλάδοι αὐτοῦ</foreign> <ref cRef="Hos." loc="14,6">Hos. 14,6</ref> (ubi caveas, ne ex <foreign xml:lang="ar">وَفَرَ</foreign> explices locum). – <lbl expand="Vocabularium Aethiopicum, -a -a">Voc. Ae.</lbl>: <foreign xml:lang="gez">ወፈረ፡ ዘ፡ ተሰመረ።</foreign> </sense> </sense>
               
