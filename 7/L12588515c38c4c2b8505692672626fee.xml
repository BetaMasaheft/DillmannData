            <entry xml:id="L12588515c38c4c2b8505692672626fee" n="6993">
               <form>
                  <foreign xml:lang="gez">ዕራቅ</foreign>
               </form>
               <sense xml:id="Dmain" xml:lang="la" source="#dillmann"> <gramGrp> <pos expand="adjectivum">adj.</pos> </gramGrp> (<gramGrp> <pos expand="Substantivum">subst.</pos> </gramGrp>) non usurpatur nisi suffixis pronominibus <lbl expand="persona">pers.</lbl>; quae si pronomina <lbl expand="pluralis">Pl.</lbl> sunt, aut Singulari <foreign xml:lang="gez">ዕራቅ፡</foreign> aut Plurali <foreign xml:lang="gez">ዕራቃን፡</foreign> vel <foreign xml:lang="gez">ዕራቃት፡</foreign> suffiguntur. <sense n="1" xml:id="D1"> <cit type="translation" xml:lang="la"><quote> nudus </quote></cit>: <foreign xml:lang="gez">ዕራቅየ፡</foreign> <ref cRef="Gen." loc="3,10">Gen. 3,10</ref>; <ref cRef="Job" loc="1,21">Job 1,21</ref>; <ref cRef="Ruth " loc="1,21">Ruth  1,21</ref>; <foreign xml:lang="gez">ሖር፡ ዕራቀከ፡ ዘእንበለ፡ አሣእን፡</foreign> <ref cRef="Jes." loc="20,2">Jes. 20,2</ref>; <ref cRef="Prov. " loc="23,21">Prov.  23,21</ref>; <ref cRef="Gen." loc="3,11">Gen. 3,11</ref>; <foreign xml:lang="gez">የኀድጉኪ፡ ዕራቀኪ፡</foreign> <ref cRef="Hez." loc="23,29">Hez. 23,29</ref>; <foreign xml:lang="gez">ዕራቁ፡</foreign> <ref cRef="Koh." loc="5,14">Koh. 5,14</ref>; <ref cRef="Gen." loc="9,22">Gen. 9,22</ref>; <ref cRef="Marc." loc="14,51">Marc. 14,51</ref>; <foreign xml:lang="gez">ዕራቃኒሆሙ፡</foreign> <ref cRef="Jes." loc="20,4">Jes. 20,4</ref>; <ref cRef="Gen." loc="2,25">Gen. 2,25</ref>; <ref cRef="Gen." loc="3,7">Gen. 3,7</ref>; <foreign xml:lang="gez">ዕራቃቲሆሙ፡</foreign> vel <foreign xml:lang="gez">ዕራቃኒሆሙ፡</foreign> <ref cRef="Am." loc="4,3">Am. 4,3</ref>; <ref cRef="Hen." loc="32,6">Hen. 32,6</ref>; <foreign xml:lang="gez">ዕራቃቲነ፡</foreign> <ref cRef="2 Cor." loc="5,3">2 Cor. 5,3</ref>; <foreign xml:lang="grc">κενός</foreign> <ref cRef="Ps." loc="7,4">Ps. 7,4</ref>; <foreign xml:lang="gez">ያስተርኢ፡ አዕጽምቲሁ፡ ዕራቁ፡</foreign> <ref cRef="Job" loc="33,21">Job 33,21</ref>; <sense n="a" xml:id="D1a"> i.e. <cit type="translation" xml:lang="la"><quote> merus </quote></cit>, <cit type="translation" xml:lang="la"><quote> solus </quote></cit>, vel <cit type="translation" xml:lang="la"><quote> inanis </quote></cit>: <foreign xml:lang="gez">ኢትስተይ፡ ማየ፡ ዕራቆ፡</foreign> <ref cRef="1 Tim. " loc="5,23">1 Tim.  5,23</ref>; <foreign xml:lang="gez">ተጠምቀ፡ በማይ፡ ዕራቁ፡</foreign> M.M. f. 308; M.M. f. 307; <foreign xml:lang="gez">ዘዕራቃ፡ ክብር፡</foreign> <cit type="translation" xml:lang="la"><quote> gloriae </quote></cit> <cit type="translation" xml:lang="la"><quote> inanis </quote></cit> <ref cRef="Asc. Jes." loc="3,28">Asc. Jes. 3,28</ref>; <foreign xml:lang="grc">ψιλός</foreign>, ut: <foreign xml:lang="gez">ጳውሎስ፡ ሳሚሳጢ፡ ይቤ፡ እስመ፡ ክርስቶስ፡ ሰብእ፡ ዕራቁ፡</foreign> F.N. p. 7; <ref cRef="Sx. Jac." loc="1.">Sx. Jac. 1.</ref> 
</sense> <sense n="b" xml:id="D1b"> <cit type="translation" xml:lang="la"><quote> orbus </quote></cit>, <cit type="translation" xml:lang="la"><quote> destitutus </quote></cit> incolis, <ref cRef="Hen." loc="9,1">Hen. 9,1</ref>; <ref cRef="Hen." loc="67,2">Hen. 67,2</ref>.</sense>
</sense> <sense n="2" xml:id="D2"> <cit type="translation" xml:lang="la"><quote> vacuus </quote></cit>, <cit type="translation" xml:lang="la"><quote> inanis </quote></cit>, <foreign xml:lang="grc">κενός</foreign>: <foreign xml:lang="gez">ንዋይ፡ ዘዕራቁ፡</foreign> <ref cRef="Herm." loc="43">Herm. p. 43</ref>; <foreign xml:lang="gez">አሦጡ፡ ቀሣውቲሆሙ፡ ዕራቆ፡</foreign> <ref cRef="Jer." loc="14,3">Jer. 14,3</ref>; <ref cRef="4 Esr." loc="4,26">4 Esr. 4,26</ref>; <foreign xml:lang="gez">ዘዕራቁ፡ ለእለ፡ ዕራቆሙ፡</foreign> (vacuum vacuis) <ref cRef="4 Esr." loc="5,25">4 Esr. 5,25</ref>; <foreign xml:lang="gez">ነፍስ፡ ዕራቃ፡</foreign> <ref cRef="Ps." loc="106,9">Ps. 106,9</ref>. <sense n="a" xml:id="D2a"> <cit type="translation" xml:lang="la"><quote> manibus </quote></cit> <cit type="translation" xml:lang="la"><quote> vacuis </quote></cit>: <foreign xml:lang="gez">ዕራቅኪ፡</foreign> <ref cRef="Ruth" loc="3,17">Ruth 3,17</ref>; <foreign xml:lang="gez">ዕራቃኒክሙ፡</foreign> <ref cRef="Ex." loc="3,21">Ex. 3,21</ref>; <foreign xml:lang="gez">ኢትፌንዎ፡ ዕራቆ፡</foreign> <ref cRef="Deut." loc="15,13">Deut. 15,13</ref>; <foreign xml:lang="gez">እንዘ፡ ዕራቁ፡ ውእቱ፡</foreign> <foreign xml:lang="grc">κόλπῳ κενῷ</foreign> <ref cRef="Job" loc="31,34">Job 31,34</ref>; <ref cRef="Job" loc="22,9">Job 22,9</ref>; <ref cRef="Sir." loc="29,9">Sir. 29,9</ref>; <ref cRef="Sir." loc="32,6">Sir. 32,6</ref>; <ref cRef="Luc." loc="1,53">Luc. 1,53</ref>; <ref cRef="Luc." loc="20,11">Luc. 20,11</ref>; <ref cRef="Luc." loc="20,12">Luc. 20,12</ref>;
</sense> <sense n="b" xml:id="D2b"> <cit type="translation" xml:lang="la"><quote> re </quote></cit> <cit type="translation" xml:lang="la"><quote> infecta </quote></cit>: <foreign xml:lang="gez">ሐጽ፡ ዘኢይሠወጥ፡ ዕራቁ፡</foreign> <ref cRef="Jer." loc="27,9">Jer. 27,9</ref>; <foreign xml:lang="gez">ገብኡ፡ ዕራቃቲሆሙ፡</foreign> (v. <foreign xml:lang="gez">ዕራቃኒሆሙ፡</foreign>) <ref cRef="Judith" loc="1,11">Judith 1,11</ref>.
</sense> <sense n="c" xml:id="D2c"> <cit type="translation" xml:lang="la"><quote> vacuum </quote></cit>: <foreign xml:lang="gez">ሰቀላ፡ ለምድር፡ ዲበ፡ ዕራቃ፡</foreign> <foreign xml:lang="grc">κρεμάζων γῆν ἐπὶ οὐδενός</foreign> <ref cRef="Job" loc="26,7">Job 26,7</ref>.
</sense> 
</sense> </sense>
