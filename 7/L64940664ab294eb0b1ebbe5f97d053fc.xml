                    </form>
               <sense xml:lang="la" source="#dillmann" xml:id="Dmain"> <subc>I,1</subc> [<foreign xml:lang="he">פתח</foreign> <foreign xml:lang="syr">ܦܬܚ</foreign> <foreign xml:lang="ar">فتح</foreign>] <gramGrp> <pos expand="Subjunctivus">Subj.</pos> </gramGrp> <foreign xml:lang="gez">ይፍታሕ፡</foreign> <sense n="1" xml:id="D1"> <cit type="translation" xml:lang="la">
                                <quote> aperire </quote>
                            </cit>, saccum <ref cRef="Gen." loc="42,27">Gen. 42,27</ref>; <ref cRef="Gen." loc="43,21">Gen. 43,21</ref>; <foreign xml:lang="gez">ፈትሐ፡ ኪሰ፡</foreign> <ref cRef="Jsp." loc="370">Jsp. p. 370</ref>; uterum <ref cRef="Gen." loc="29,31">Gen. 29,31</ref>; <ref cRef="Ex." loc="13,2">Ex. 13,2</ref>; <ref cRef="Ex." loc="13,13">Ex. 13,13</ref>; <ref cRef="Luc." loc="2,23">Luc. 2,23</ref>; thesaurum <ref cRef="Hen." loc="11,1">Hen. 11,1</ref>; <ref cRef="Deut." loc="28,12">Deut. 28,12</ref>; os <ref cRef="4 Esr." loc="14,41">4 Esr. 14,41</ref>; <ref cRef="Hen." loc="56,8">Hen. 56,8</ref>; <ref cRef="Hen." loc="106,11">Hen. 106,11</ref>; <ref cRef="Jes." loc="52,15">Jes. 52,15</ref>; aures: <foreign xml:lang="gez">ፍታሕ፡ አፉሁ፡ ወእዘኒሁ፡</foreign> <ref cRef="Kuf." loc="12">Kuf. 12</ref>; manus: <foreign xml:lang="gez">ፈቲሐከ፡ እዴከ፡</foreign> <ref cRef="Ps." loc="103,29">Ps. 103,29</ref>; <foreign xml:lang="gez">እደዊሃ፡ ፈትሐት፡ ለምስኪናን፡</foreign> <ref cRef="Prov." loc="29,38">Prov. 29,38</ref>; <ref cRef="Deut." loc="15,8">Deut. 15,8</ref>; oculos <ref cRef="Gen." loc="21,19">Gen. 21,19</ref>; librum Asc. <ref cRef="Jes." loc="9,22">Jes. 9,22</ref>; vomicam: <foreign xml:lang="gez">ርጢናት፡ እለ፡ ይፈትሓ፡ ሕብጠተ፡</foreign> F.M. 3,3.</sense> <sense n="2" xml:id="D2"> <cit type="translation" xml:lang="la">
                                <quote> solvere </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> exsolvere </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> resolvere </quote>
                            </cit>, vincula: <foreign xml:lang="gez">መኑ፡ ፈትሐ፡ ማእሰሮ፡</foreign> <ref cRef="Job" loc="39,5">Job 39,5</ref>; <ref cRef="Jes." loc="58,6">Jes. 58,6</ref>; <ref cRef="Act." loc="22,30">Act. 22,30</ref>; cingulum: <foreign xml:lang="gez">ኢይፈትሑ፡ ቅናቶሙ፡</foreign> (<foreign xml:lang="gez">እምሐቌሆሙ፡</foreign>) <ref cRef="Jes." loc="5,27">Jes. 5,27</ref>; frenum <ref cRef="Hen." loc="46,4">Hen. 46,4</ref>; vestes: <foreign xml:lang="gez">ኢንፈትሕ፡ አልባሲነ፡</foreign> <ref cRef="2 Esr." loc="14,23">2 Esr. 14,23</ref>; calceum: <ref cRef="Ruth" loc="4,7">Ruth 4,7</ref>; sigillum <ref cRef="Apoc." loc="5,2">Apoc. 5,2</ref>; <ref cRef="Apoc." loc="6,3">Apoc. 6,3</ref>; crinem: <foreign xml:lang="gez">አንስት፡ ሶበ፡ ይጠመቃ፡ ሥዕርቶን፡ ይፍትሓ፡</foreign> <ref cRef="Kid." loc="26">Kid. f. 26</ref>; corrigiam <ref cRef="Marc." loc="1,7">Marc. 1,7</ref>; <foreign xml:lang="gez">ፍታሕ፡ አሣእኒከ፡ እምእገሪከ፡</foreign> <ref cRef="Jes." loc="20,2">Jes. 20,2</ref>; <ref cRef="Ex." loc="3,5">Ex. 3,5</ref>; <ref cRef="Jos." loc="5,15">Jos. 5,15</ref>; <ref cRef="Act." loc="7,33">Act. 7,33</ref>; anathema, fascinum <ref cRef="Matth." loc="16,19">Matth. 16,19</ref>; <ref cRef="Hen." loc="8,3">Hen. 8,3</ref>; <ref cRef="Hen." loc="95,4">Hen. 95,4</ref>; vinctum vinculis: <ref cRef="Kuf." loc="22">Kuf. 22</ref>; <ref cRef="Kuf." loc="48">Kuf. 48</ref>; <foreign xml:lang="gez">ናሁ፡ ፈታሕኩከ፡ እምነ፡ መዋቅሕቲከ፡</foreign> <ref cRef="Jer." loc="47,4">Jer. 47,4</ref>; <ref cRef="Ps." loc="104,18">Ps. 104,18</ref>; <ref cRef="Jes. " loc="14,17">Jes.  14,17</ref>; <ref cRef="Judith" loc="6,14">Judith 6,14</ref>; <ref cRef="Matth." loc="21,2">Matth. 21,2</ref>; <ref cRef="Joh." loc="11,44">Joh. 11,44</ref>; <foreign xml:lang="gez">ፈታሕኮኑ፡ እምሕማም፡</foreign> <ref cRef="Job" loc="39,2">Job 39,2</ref>. <sense n="a" xml:id="D2a"> <cit type="translation" xml:lang="la">
                                    <quote> remissum reddere </quote>
                                </cit>: <foreign xml:lang="gez">ይፈትሕ፡ እደወ፡ ሰብእ፡</foreign> <foreign xml:lang="grc">ἐκλύει</foreign> <ref cRef="Jer." loc="45,4">Jer. 45,4</ref>.</sense> <sense n="b" xml:id="D2b"> <cit type="translation" xml:lang="la">
                                    <quote> solvere </quote>
                                </cit>, <cit type="translation" xml:lang="la">
                                    <quote> dissolvere </quote>
                                </cit>, <cit type="translation" xml:lang="la">
                                    <quote> resolvere </quote>
                                </cit>, i.e. <sense n="α" xml:id="D2bα"> <cit type="translation" xml:lang="la">
                                        <quote> discindere </quote>
                                    </cit>, <cit type="translation" xml:lang="la">
                                        <quote> dirumpere </quote>
                                    </cit>, <cit type="translation" xml:lang="la">
                                        <quote> irritum facere </quote>
                                    </cit>: <foreign xml:lang="gez">ከመ፡ ይፍታሕ፡ ግብሮ፡ ለጋኔን፡</foreign> <ref cRef="1 Joh. " loc="3,8">1 Joh.  3,8</ref> <lbl expand="romanae editionis">rom.</lbl>; <foreign xml:lang="gez">ፈቲሐ፡ ፍኅረት፡</foreign> F.N. 24,3; <foreign xml:lang="gez">ብእሲት፡ ዘኀደገት፡ ምታ፡ ወትፈትሕ፡ ሰብሳበ፡ ዘሕግ፡</foreign> <ref cRef="Kedr f." loc="78">Kedr f. 78</ref>; praeceptum <ref cRef="Matth." loc="5,19">Matth. 5,19</ref>.</sense> <sense n="β" xml:id="D2bβ"> <cit type="translation" xml:lang="la">
                                        <quote> intermittere </quote>
                                    </cit>: <foreign xml:lang="gez">ፍታሕ፡ ጾመከ፡ በሰንበት፡</foreign> <ref cRef="Exp. Fid. Nic." loc="">Exp. Fid. Nic. </ref> </sense> </sense> <sense n="c" xml:id="D2c"> <cit type="translation" xml:lang="la">
                                    <quote> dimittere </quote>
                                </cit>, <sense n="α" xml:id="D2cα"> reum, captivum: <ref cRef="Matth." loc="27,26">Matth. 27,26</ref>; <ref cRef="Act." loc="16,35">Act. 16,35</ref>; <ref cRef="Act." loc="26,32">Act. 26,32</ref>.</sense> <sense n="β" xml:id="D2cβ"> populum congregatum <ref cRef="Matth." loc="14,15">Matth. 14,15</ref>; <ref cRef="Matth." loc="14,23">Matth. 14,23</ref>.</sense> </sense> <sense n="d" xml:id="D2d"> <cit type="translation" xml:lang="la">
                                    <quote> absolvere </quote>
                                </cit> aliquem in judicio vel a peccato: <foreign xml:lang="gez">ፍትሖሙ፡ ወስረይ፡ ኀጢአቶሙ፡</foreign> <ref cRef="Genz." loc="68">Genz. f. 68</ref>; Reg. Pach.; <ref cRef="Lit. " loc="160,3">Lit.  160,3</ref>.</sense> </sense> <sense n="3" xml:id="D3"> controversiam dirimere i.e. <cit type="translation" xml:lang="la">
                                <quote> jus dicere </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> judicare </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> dijudicare </quote>
                            </cit>, <foreign xml:lang="gez">ፍትሑ፡ እስኩ፡ ለሊክሙ፡</foreign> <ref cRef="Job" loc="13,8">Job 13,8</ref>; <ref cRef="Joh." loc="7,24">Joh. 7,24</ref>; <lbl expand="cum">c.</lbl> <foreign xml:lang="gez">ለ፡</foreign> litigantium: <foreign xml:lang="gez">ዘይፈትሕ፡ ለነ፡</foreign> <ref cRef="Job" loc="9,33">Job 9,33</ref> (coll. <ref cRef="Gen." loc="16,5">Gen. 16,5</ref>); <lbl expand="cum">c.</lbl> <gramGrp> <case value="accusativus">Acc.</case> </gramGrp> litigantium: <ref cRef="Ex. " loc="18,16">Ex.  18,16</ref>; <lbl expand="cum">c.</lbl> <foreign xml:lang="gez">ማእከለ፡</foreign> <ref cRef="Gen." loc="16,5">Gen. 16,5</ref>; <ref cRef="Gen." loc="31,53">Gen. 31,53</ref>; <ref cRef="Jud." loc="11,27">Jud. 11,27</ref>; <ref cRef="1 Reg." loc="24,13">1 Reg. 24,13</ref>; <ref cRef="1 Reg." loc="24,16">1 Reg. 24,16</ref>; <ref cRef="Jes." loc="5,3">Jes. 5,3</ref>; <lbl expand="cum">c.</lbl> <gramGrp> <case value="accusativus">Acc.</case> </gramGrp> sententiae: <foreign xml:lang="gez">ፍትሑ፡ ፍትሐ፡</foreign> <ref cRef="Jer." loc="21,12">Jer. 21,12</ref>; <ref cRef="Jes." loc="59,4">Jes. 59,4</ref>; 3 <ref cRef="Reg." loc="3,28">Reg. 3,28</ref>; <foreign xml:lang="gez">ይፈትሑ፡ ርቱዐ፡</foreign> <ref cRef="Prov." loc="8,15">Prov. 8,15</ref>; <ref cRef="Luc. " loc="7,43">Luc.  7,43</ref>; <ref cRef="Joh." loc="7,24">Joh. 7,24</ref>. <sense n="a" xml:id="D3a"> <cit type="translation" xml:lang="la">
                                    <quote> jus dicere </quote>
                                </cit> vel <cit type="translation" xml:lang="la">
                                    <quote> judicium facere </quote>
                                </cit> alicui i.e. ei <cit type="translation" xml:lang="la">
                                    <quote> jus reddere </quote>
                                </cit>, ejus <cit type="translation" xml:lang="la">
                                    <quote> jus tueri </quote>
                                </cit>, <lbl expand="cum">c.</lbl> <foreign xml:lang="gez">ለ፡</foreign> pers.: <foreign xml:lang="gez">አልቦ፡ ዘይፈትሕ፡ ሊተ፡</foreign> <ref cRef="Job" loc="19,7">Job 19,7</ref>; <foreign xml:lang="gez">ፈቲሕ፡ ለጻድቅ፡</foreign> <ref cRef="Job" loc="36,17">Job 36,17</ref>; <ref cRef="Sir." loc="32,22">Sir. 32,22</ref>; <ref cRef="Sir." loc="35,16">Sir. 35,16</ref>; <foreign xml:lang="gez">ፍትሑ፡ ለእጓለ፡ ማውታ፡</foreign> <ref cRef="Jes." loc="1,17">Jes. 1,17</ref>; <ref cRef="Jes." loc="49,4">Jes. 49,4</ref>; <ref cRef="Jes." loc="51,22">Jes. 51,22</ref>; <ref cRef="Jer." loc="5,28">Jer. 5,28</ref>; <ref cRef="Dan." loc="7,22">Dan. 7,22</ref>; <ref cRef="Ps." loc="7,9">Ps. 7,9</ref>; <ref cRef="Ps." loc="53,1">Ps. 53,1</ref>; <ref cRef="Ps." loc="81,3">Ps. 81,3</ref>; <foreign xml:lang="gez">ፍታሕ፡ ሊተ፡ ፍትሕየ፡</foreign> <ref cRef="Jer." loc="20,12">Jer. 20,12</ref>; vel <foreign xml:lang="gez">ፍትሐኪ፡ እፈትሕ፡</foreign> <ref cRef="Jes." loc="49,25">Jes. 49,25</ref>; <lbl expand="cum">c.</lbl> <foreign xml:lang="gez">እምነ፡</foreign> <cit type="translation" xml:lang="la">
                                    <quote> ab </quote>
                                </cit> aliquo <cit type="translation" xml:lang="la">
                                    <quote> vindicare jus </quote>
                                </cit> alicui: <foreign xml:lang="gez">ፍታሕ፡ ሊተ፡ እምኔሆሙ፡</foreign> <ref cRef="Jer. " loc="11,20">Jer.  11,20</ref>; <ref cRef="2 Reg." loc="18,19">2 Reg. 18,19</ref>; <ref cRef="2 Reg." loc="18,31">2 Reg. 18,31</ref>.</sense> <sense n="b" xml:id="D3b"> <cit type="translation" xml:lang="la">
                                    <quote> judicare </quote>
                                </cit> i.e. <cit type="translation" xml:lang="la">
                                    <quote> punire </quote>
                                </cit> sontem: <ref cRef="Kuf." loc="56.">Kuf. p. 56.</ref> </sense> <sense n="c" xml:id="D3c"> <cit type="translation" xml:lang="la">
                                    <quote> sententiam pronuntiare </quote>
                                </cit> contra aliquem, <lbl expand="cum">c.</lbl> <foreign xml:lang="gez">ላዕለ፡</foreign>, ut: <foreign xml:lang="gez">ፈትሐ፡ ላዕሌሃ፡</foreign> Lud. ex <ref cRef="Sx. Mag." loc="5 Enc.">Sx. Mag. 5 Enc.</ref>
                            </sense> <sense n="d" xml:id="D3d"> <cit type="translation" xml:lang="la">
                                    <quote> decernere </quote>
                                </cit>, <cit type="translation" xml:lang="la">
                                    <quote> sancire </quote>
                                </cit>, de legislatore: <foreign xml:lang="gez">እግዚአብሔር፡ ፈትሐ፡ በሕግ፡ ብሊት፡ ከመ፡ ይምሐል፡ ብእሲ፡ በጽድቅ፡</foreign> F.M. 22,1; nec non de lege ipsa: <foreign xml:lang="gez">ሕግ፡ ብሊት፡ ፈትሐት፡</foreign> (lege Veteris Testamenti sancitum est) <foreign xml:lang="gez">ከመ፡ ይቅትሉ፡</foreign> … <foreign xml:lang="gez">ወሕግሰ፡ ሐዳስ፡ ኢፈትሐት፡ ከመዝ፡</foreign> F.M. 4,2.</sense> </sense> </sense>
               
