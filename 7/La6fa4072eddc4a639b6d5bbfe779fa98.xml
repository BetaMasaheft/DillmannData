            <entry xml:id="La6fa4072eddc4a639b6d5bbfe779fa98" n="3693">
               <form>
                  <foreign xml:lang="gez">ቈጸረ</foreign>
               <rs type="root"/>
                    </form>
               <sense xml:lang="la" source="#dillmann" xml:id="Dmain"> vel <foreign xml:lang="gez">ቈፀረ፡</foreign> <subc>I,1</subc> [<foreign xml:lang="syr">ܩܛܰܪ</foreign> <foreign xml:lang="he">קָשַׁר</foreign>; amh. <foreign xml:lang="gez">ቋጠረ፡</foreign>] <gramGrp> <pos expand="Subjunctivus">Subj.</pos> </gramGrp> <foreign xml:lang="gez">ይቍጽር፡</foreign> <sense n="1" xml:id="D1"> <cit type="translation" xml:lang="la">
                                <quote> constringere </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> colligare </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> nodare </quote>
                            </cit>: <foreign xml:lang="gez">ነሥአ፡ ዲናረ፡ ወርቅ፡ ወቈፀረ፡ በጸርቅ፡</foreign> <ref cRef="Sx. Tachs." loc="10">Sx. Tachs. 10</ref>; <cb n="0474" xml:id="c0474"/> <foreign xml:lang="gez">ቈጸርዎ፡</foreign> (<foreign xml:lang="gez">ለአዕፅምተ፡ ቅዱስ፡</foreign>) <foreign xml:lang="gez">በልብሶሙ፡</foreign> <ref cRef="M.M." loc="f. 51">M.M. f. 51</ref>; <lbl expand="translate">transl.</lbl>: <foreign xml:lang="gez">ቈፀሩ፡ አማዑቶሙ፡</foreign> <foreign xml:lang="grc">τὸ στέαρ αὐτῶν συνέκλεισαν</foreign> <ref cRef="Ps." loc="16,10">Ps. 16,10</ref>.</sense> <sense n="2" xml:id="D2"> <cit type="translation" xml:lang="la">
                                <quote> corrugare </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> caperare </quote>
                            </cit> frontem, <cit type="translation" xml:lang="la">
                                <quote> austere contrahere </quote>
                            </cit> vultum, <cit type="translation" xml:lang="la">
                                <quote> tetricum </quote>
                            </cit> vel <cit type="translation" xml:lang="la">
                                <quote> moestum vultum gerere </quote>
                            </cit>, <foreign xml:lang="grc">σκυθρωπάζειν</foreign>, <foreign xml:lang="gez">ኵሉ፡ ዘኀለፈ፡ እንተ፡ ኀቤሃ፡ ይቈጽር፡ ወይትፋጸይ፡</foreign> <ref cRef="Jer." loc="19,8">Jer. 19,8</ref>; <foreign xml:lang="gez">ዘይቈጽሩ፡ ገጾሙ፡</foreign> <ref cRef="Chrys. Ta." loc="17 ">Chrys. Ta. 17 </ref>(coll. <ref cRef="Matth." loc="6,16">Matth. 6,16</ref>); <foreign xml:lang="gez">ኢትቍጽሩ፡ ገጸክሙ፡</foreign> <ref cRef="F.N." loc="45">F.N. 45</ref>; <foreign xml:lang="gez">እፎ፡ ከመዝ፡ ይቈጽር፡ ገጾ፡</foreign> <foreign xml:lang="grc">κατηφὴς τῇ ἰδέᾳ</foreign> Herm. p. 3; <foreign xml:lang="gez">ትካዝ፡ ወቈጺረ፡ ገጽ፡</foreign> <foreign xml:lang="grc">κατήφεια</foreign> <ref cRef="Chrys. Ta." loc="29 ">Chrys. Ta. 29 </ref>(aliâs <foreign xml:lang="gez">ደምኖ፡ ገጽ፡</foreign>).</sense> <sense n="3" xml:id="D3"> <sense n="a" xml:id="D3a"> in bonam partem: <cit type="translation" xml:lang="la">
                                    <quote> nectere </quote>
                                </cit> i.e. <cit type="translation" xml:lang="la">
                                    <quote> excogitare </quote>
                                </cit> consilium, ut: <foreign xml:lang="gez">ቈጺረከ፡ ምክረ፡ ዘትገብር፡ አውሥእ፡</foreign> (<foreign xml:lang="grc">σύνδησον χαιδείαν καὶ ἀποκρίθητι</foreign>) <ref cRef="Sir. " loc="36,4">Sir.  36,4</ref>.</sense> <sense n="b" xml:id="D3b"> crebrius in malam partem: <cit type="translation" xml:lang="la">
                                    <quote> nectere dolum </quote>
                                </cit>, <cit type="translation" xml:lang="la">
                                    <quote> insidias </quote>
                                </cit>; <cit type="translation" xml:lang="la">
                                    <quote> struere technam </quote>
                                </cit>, <cit type="translation" xml:lang="la">
                                    <quote> insidiari </quote>
                                </cit>, <cit type="translation" xml:lang="la">
                                    <quote> fraude uti </quote>
                                </cit>: <foreign xml:lang="gez">ይቈጽሩ፡ ምክረ፡ ላዕሌሁ፡</foreign> <ref cRef="Dan." loc="11,25">Dan. 11,25</ref>; <ref cRef="1 Reg." loc="24,12">1 Reg. 24,12</ref>; <ref cRef="Ex. " loc="21,14">Ex.  21,14</ref>; <ref cRef="Ex." loc="22,7">Ex. 22,7</ref>; <foreign xml:lang="gez">ለመጋቢ፡ ኢይደልዎ፡ ከመ፡ ይቍጽር፡ ለአመጋቢሁ፡ ውስተ፡ ሠይጥ፡ ወተሣይጦ፡</foreign> <ref cRef="F.N." loc="30">F.N. 30</ref>.</sense> </sense> </sense>
               
