            <entry xml:id="La69905d0f7ff4e48bcf97148742b472f" n="9397">
               <form>
                  <foreign xml:lang="gez">አስተጻንዐ</foreign>
               </form>
               <sense xml:lang="la" source="#dillmann" xml:id="Dmain"> <subc>IV,3</subc>, rarius <foreign xml:lang="gez">አስተጸንዐ፡</foreign> <subc>IV,1</subc> et <subc>IV,2</subc> <sense n="1" xml:id="D1"> <cit type="translation" xml:lang="la">
                                <quote> firmiter </quote>
                            </cit> <sense n="a" xml:id="D1a"> <cit type="translation" xml:lang="la">
                                    <quote> stabilire </quote>
                                </cit>, <cit type="translation" xml:lang="la">
                                    <quote> solidare </quote>
                                </cit>, <cit type="translation" xml:lang="la">
                                    <quote> constabilire </quote>
                                </cit>: <foreign xml:lang="gez">፩ ምስለ፡ ካልኡ፡ አስተጸንዖሙ፡</foreign> (v. <foreign xml:lang="gez">አስተጻንዖሙ፡</foreign>) <foreign xml:lang="gez">ለሠናይ፡</foreign> <ref cRef="Sir." loc="42,24">Sir. 42,24</ref>; <foreign xml:lang="gez">ቅዱሳን፡ አስተጻንዑ፡ መሠረተ፡ እምነታ፡ ለኢትዮጵያ፡</foreign> <ref cRef="Gad. Za Mich." loc="">Gad. Za Mich. </ref> ; <foreign xml:lang="gez">አቀሙ፡ ቤተ፡ ንግሥከ፡ ወአስተጻንዑ፡ ድዳ፡</foreign> <ref cRef="Sx. Teq." loc="9 Enc.">Sx. Teq. 9 Enc.</ref>; <foreign xml:lang="gez">ፈቀደ፡ ያስተጸንዕ፡ ምዕማዲሁ፡</foreign> <ref cRef="Chrys. ho." loc="3 ">Chrys. ho. 3 </ref>(<lbl expand="videas">vid.</lbl> sub <foreign xml:lang="gez">ምዕማድ፡</foreign>).</sense> <sense n="b" xml:id="D1b"> <cit type="translation" xml:lang="la">
                                    <quote> adstringere </quote>
                                </cit>, <cit type="translation" xml:lang="la">
                                    <quote> constringere </quote>
                                </cit>: <ref cRef="Ex." loc="29,5">Ex. 29,5</ref> <lbl expand="cum">c.</lbl> </sense> <sense n="c" xml:id="D1c"> <cit type="translation" xml:lang="la">
                                    <quote> communire </quote>
                                </cit>: <foreign xml:lang="gez">አስተጸንዖሙ፡</foreign> (v. <foreign xml:lang="gez">አስተጻንዖሙ፡</foreign>) <foreign xml:lang="gez">ፈድፋደ፡</foreign> 2 <ref cRef="Par." loc="11,12">Par. 11,12</ref>. <cb n="1291" xml:id="c1291"/> </sense> </sense> <sense n="2" xml:id="D2"> <cit type="translation" xml:lang="la">
                                <quote> confirmare </quote>
                            </cit> <sense n="a" xml:id="D2a"> aliquid rationibus i.e. <cit type="translation" xml:lang="la">
                                    <quote> comprobare </quote>
                                </cit>: <foreign xml:lang="gez">ወያስተጻንዕ፡ ዘንተ፡ በብሂሎቱ፡</foreign> F.N. in Prol.</sense> <sense n="b" xml:id="D2b"> <cit type="translation" xml:lang="la">
                                    <quote> ratum facere </quote>
                                </cit>: <foreign xml:lang="gez">ያስተጻንዑ፡ ፍኅረተ፡ በቅድመ፡ ፪ ካህናት፡ በአንብሮተ፡ እድ፡ ወበመስቀል፡ ወበሕልቀት፡</foreign> F.N. 24,3.</sense> <sense n="c" xml:id="D2c"> <cit type="translation" xml:lang="la">
                                    <quote> audaciam </quote>
                                </cit>, <cit type="translation" xml:lang="la">
                                    <quote> animum </quote>
                                </cit>, <cit type="translation" xml:lang="la">
                                    <quote> robur addere </quote>
                                </cit>, <cit type="translation" xml:lang="la">
                                    <quote> confortare </quote>
                                </cit>: <foreign xml:lang="gez">ዘያስተጻንዑ፡ ዐበይቶሙ፡ ወአኀዊሆሙ፡</foreign> <ref cRef="2 Esr." loc="20,29">2 Esr. 20,29</ref> <lbl expand="varia lectio">var.</lbl>; <foreign xml:lang="gez">ዘንተ፡ ይቤ፡ ከመ፡ ያስተጻንዖሙ፡</foreign> <foreign xml:lang="grc">παραθαῤῥύνειν</foreign> <ref cRef="Chrys. ho." loc="3">Chrys. ho. 3</ref>; <ref cRef="Jsp." loc="348">Jsp. p. 348</ref>; <ref cRef="Sx. Masc." loc="7 Enc.">Sx. Masc. 7 Enc.</ref>
                            </sense> </sense> <sense n="3" xml:id="D3"> <sense n="a" xml:id="D3a"> <cit type="translation" xml:lang="la">
                                    <quote> vires contendere </quote>
                                </cit> vel <cit type="translation" xml:lang="la">
                                    <quote> colligere </quote>
                                </cit>: <foreign xml:lang="gez">አላ፡ አስተጸንዐ፡</foreign> (v. <foreign xml:lang="gez">አስተጻንዐ፡</foreign>) <foreign xml:lang="gez">ወአኀዘ፡ ይትቃተሎ፡</foreign> <ref cRef="2 Par." loc="35,22">2 Par. 35,22</ref>.</sense> <sense n="b" xml:id="D3b"> <cit type="translation" xml:lang="la">
                                    <quote> robur facere </quote>
                                </cit>: <foreign xml:lang="gez">ከመ፡ ያስተጻንዕ፡</foreign> <foreign xml:lang="grc">τοῦ ἐνισχῦσαι</foreign> <ref cRef="1 Par." loc="15,21">1 Par. 15,21</ref>.</sense> <sense n="c" xml:id="D3c"> <cit type="translation" xml:lang="la">
                                    <quote> intendere </quote>
                                </cit> brachia: <foreign xml:lang="gez">አስተጻንዐ፡ መዝራዕቶ፡ ወአቈልቈለ፡ ኲናቶ፡</foreign> <ref cRef="Gad. T.H." loc="">Gad. T.H. </ref> </sense> </sense> </sense>
               
