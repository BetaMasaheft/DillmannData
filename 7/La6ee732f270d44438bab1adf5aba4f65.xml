            <entry xml:id="La6ee732f270d44438bab1adf5aba4f65" n="7550">
               <form>
                  <foreign xml:lang="gez">ዘርእ</foreign>
               </form>
               <sense xml:lang="la" source="#dillmann" xml:id="Dmain">
                  <gramGrp>
                     <pos expand="Substantivum">subst.</pos>
                  </gramGrp> (<gramGrp>
                     <gen value="masculinum">m.</gen>
                  </gramGrp>), <lbl expand="pluralis">Pl.</lbl>
                  <foreign xml:lang="gez">አዝርእት፡</foreign>
                  <sense n="1" xml:id="D1">
                     <cit type="translation" xml:lang="la">
                        <quote>satio</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>sementis</quote>
                     </cit>: <ref cRef="Ex." loc="34,21">Ex. 34,21</ref>; <foreign xml:lang="gez">በጽሐ፡ ጊዜ፡ ዘርእ፡ </foreign>
                     <ref cRef="Kuf." loc="11">Kuf. 11</ref>; <ref cRef="Gen." loc="8,22">Gen.
                        8,22</ref>; <ref cRef="Lev." loc="26,5">Lev. 26,5</ref>; <ref cRef="Am." loc="9,13">Am. 9,13</ref>; <foreign xml:lang="gez">ዘርእ፡ ወማእረር፡</foreign>
                     <ref cRef="Kuf." loc="6.">Kuf. 6.</ref>
                  </sense>
                  <sense n="2" xml:id="D2">
                     <cit type="translation" xml:lang="la">
                        <quote>semen</quote>
                     </cit> cujusvis generis: <ref cRef="Gen." loc="1,11">Gen. 1,11</ref>; <ref cRef="Gen." loc="1,12">Gen. 1,12</ref>; <ref cRef="Gen." loc="1,29">Gen.
                        1,29</ref>; <ref cRef="Gen." loc="47,19">Gen. 47,19</ref>; <ref cRef="Gen. " loc="47,23">Gen. 47,23</ref>; <ref cRef="Lev." loc="11,37">Lev. 11,37</ref>;
                        <ref cRef="Lev." loc="26,16">Lev. 26,16</ref>; <ref cRef="Ps." loc="125,7">Ps. 125,7</ref>; <ref cRef="Koh." loc="11,6">Koh. 11,6</ref>; <ref cRef="4 Esr." loc="9,35">4 Esr. 9,35</ref>; <ref cRef="Matth." loc="13,24">Matth. 13,24</ref>
                     <lbl expand="sequens">seq.</lbl>; <ref cRef="1 Cor." loc="15,38">1 Cor.
                        15,38</ref>.</sense>
                  <sense n="3" xml:id="D3"> quae ex semine oriuntur, <cit type="translation" xml:lang="la">
                        <quote>satum</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>seges</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>olus</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>viride</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>herba</quote>
                     </cit>
                     <cit type="translation" xml:lang="la">
                        <quote>sativa</quote>
                     </cit>: <foreign xml:lang="gez">ዘርአ፡ ምድር፡</foreign>
                     <foreign xml:lang="grc">σπέρματα</foreign>
                     <ref cRef="Dan." loc="1,16">Dan. 1,16</ref>; <foreign xml:lang="grc">γεννήματα</foreign>
                     <ref cRef="Cant. " loc="6,11">Cant. 6,11</ref>; <foreign xml:lang="gez">ሐመልማለ፡
                        ዘርእ፡</foreign>
                     <foreign xml:lang="grc">χλοὴ σπόρου</foreign>
                     <ref cRef="Sir." loc="40,22">Sir. 40,22</ref>; <ref cRef="Hen." loc="80,2">Hen.
                        80,2</ref>; <ref cRef="1 Reg." loc="8,15">1 Reg. 8,15</ref>; <ref cRef="Lev." loc="27,30">Lev. 27,30</ref>; <foreign xml:lang="gez">አዝርእት፡</foreign>
                     <foreign xml:lang="grc">σπέρματα </foreign>
                     <ref cRef="Matth." loc="13,32">Matth. 13,32</ref>; <foreign xml:lang="gez">ፍሬያተ፡ አዝርእት፡</foreign>
                     <foreign xml:lang="grc">καρποί</foreign>
                     <ref cRef="Sap." loc="16,22">Sap. 16,22</ref>; <foreign xml:lang="gez">ርኢ፡ ከመ፡
                        ሠርፀ፡ አዝርእቲሃ፡</foreign> (<foreign xml:lang="gez">ለገራውህ፡</foreign>) Rel.
                     Bar.</sense>
                  <sense n="4" xml:id="D4">
                     <sense n="a" xml:id="D4a">
                        <cit type="translation" xml:lang="la">
                           <quote>semen virile</quote>
                        </cit>, <cit type="translation" xml:lang="la">
                           <quote>sperma</quote>
                        </cit>, <ref cRef="Dan." loc="2,43">Dan. 2,43</ref>; <ref cRef="Lev." loc="15,3">Lev. 15,3</ref>; <ref cRef="Lev." loc="18,20">Lev.
                        18,20</ref>; <ref cRef="Lev. " loc="18,23">Lev. 18,23</ref>; <foreign xml:lang="gez">ከዐወ፡ ዘርአ፡ ውስተ፡ ምድር፡</foreign>
                        <ref cRef="Kuf." loc="41">Kuf. 41</ref>; F.M. 13.</sense>
                     <sense n="b" xml:id="D4b">
                        <cit type="translation" xml:lang="la">
                           <quote>proles</quote>
                        </cit>, <cit type="translation" xml:lang="la">
                           <quote>soboles</quote>
                        </cit>
                        <ref cRef="Kuf." loc="41">Kuf. 41</ref>; <ref cRef="Gen." loc="3,15">Gen.
                           3,15</ref>; <ref cRef="Gen." loc="4,25">Gen. 4,25</ref>; <ref cRef="Gen." loc="17,7">Gen. 17,7</ref>; <ref cRef="Gen. " loc="17,8">Gen. 17,8</ref>;
                           <ref cRef="Gen." loc="38,8">Gen. 38,8</ref>; <ref cRef="Gen." loc="38,9">Gen. 38,9</ref>; <ref cRef="Jes." loc="43,5">Jes. 43,5</ref>; <ref cRef="Jes." loc="61,9">Jes. 61,9</ref>; <ref cRef="Ps." loc="36,30">Ps.
                           36,30</ref>; <ref cRef="Ps." loc="111,2">Ps. 111,2</ref>; <foreign xml:lang="gez">አንሥእ፡ ዘርአ፡ ለእኁከ፡</foreign>
                        <ref cRef="Kuf." loc="41">Kuf. 41</ref>; <foreign xml:lang="gez">ዘርእ፡
                           ብእሲ፡</foreign>
                        <ref cRef="1 Reg." loc="1,11">1 Reg. 1,11</ref>; <lbl expand="pluralis">Pl.</lbl>
                        <foreign xml:lang="gez">አዝርእት፡ </foreign>
                        <cit type="translation" xml:lang="la">
                           <quote>posteri</quote>
                        </cit>
                        <ref cRef="Act." loc="7,6">Act. 7,6</ref>
                        <lbl expand="romanae editionis">rom.</lbl>; <ref cRef="Gal." loc="3,16">Gal.
                           3,16</ref>; F.N. 42,10; <lbl expand="Epilogus">Epil.</lbl> Evang. Joh. in
                           <lbl expand="Novum Testamentum">N.T.</lbl>
                        <lbl expand="romanae editionis">rom.</lbl>
                        <sense n="α" xml:id="D4bα">
                           <cit type="translation" xml:lang="la">
                              <quote>stirps</quote>
                           </cit>, <cit type="translation" xml:lang="la">
                              <quote>gens</quote>
                           </cit>, <cit type="translation" xml:lang="la">
                              <quote>genus</quote>
                           </cit>: <ref cRef="Ps." loc="68,42">Ps. 68,42</ref>; <foreign xml:lang="gez">እምዘርአ፡ ሜዶን፡</foreign>
                           <ref cRef="Dan." loc="9,1">Dan. 9,1</ref>; <foreign xml:lang="gez">ዘርእ፡
                              ቅዱስ፡ </foreign>
                           <ref cRef="2 Esr." loc="9,2">2 Esr. 9,2</ref>.</sense>
                        <sense n="β" xml:id="D4bβ"> proles singularum aetatum sese excipientium, <cit type="translation" xml:lang="la">
                              <quote>aetas</quote>
                           </cit>: <foreign xml:lang="gez">፲ወ፬ አዝርእት፡</foreign> (<note>14
                              Generationen</note>) M.M. f. 161.</sense>
                     </sense>
                  </sense>
               </sense>
               
            </entry>
