               <sense xml:lang="la" source="#dillmann" xml:id="Dmain"> <gramGrp> <pos expand="Substantivum">subst.</pos> </gramGrp> <sense n="1" xml:id="D1"> de loco: <cit type="translation" xml:lang="la">
                                <quote> id quod ante est </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> pars antica </quote>
                            </cit> (<cit type="translation" xml:lang="la">
                                <quote> frons </quote>
                            </cit>): <foreign xml:lang="gez">እምቅድም፡ ወእምድኅር፡</foreign> <ref cRef="Mavâs." loc=" 17">Mavâs.  17</ref>; <ref cRef="Apoc." loc="4,6">Apoc. 4,6</ref>; <cb n="0462" xml:id="c0462"/> <foreign xml:lang="gez">ውስተ፡ ቅድመ፡ ደብተራ፡</foreign> <ref cRef="Num." loc="3,38">Num. 3,38</ref>; <foreign xml:lang="gez">እመንገለ፡ ቅድም፡</foreign> <ref cRef="Ex." loc="28,14">Ex. 28,14</ref>; <foreign xml:lang="gez">እምቅድም፡</foreign> <ref cRef="2 Reg." loc="10,9">2 Reg. 10,9</ref>; <foreign xml:lang="gez">ውስተ፡ ቅድመ፡</foreign> <foreign xml:lang="grc">ἀπέναντι</foreign> <ref cRef="Num. " loc="33,7">Num.  33,7</ref>.</sense> <sense n="2" xml:id="D2"> de tempore: <sense n="a" xml:id="D2a"> <cit type="translation" xml:lang="la">
                                    <quote> initium </quote>
                                </cit>, <cit type="translation" xml:lang="la">
                                    <quote> primordium </quote>
                                </cit> <ref cRef="Gen." loc="10,10">Gen. 10,10</ref>; <ref cRef="Cant." loc="4,8">Cant. 4,8</ref>; <foreign xml:lang="gez">እምቅድመ፡ ዓለም፡</foreign> <ref cRef="Hebr." loc="4,3">Hebr. 4,3</ref> ant.; <ref cRef="Hen." loc="41,4">Hen. 41,4</ref>; <foreign xml:lang="gez">እምቅድመ፡ ዓለም፡ ወእስከ፡ ለዓለም፡</foreign> <ref cRef="1 Par." loc="29,10">1 Par. 29,10</ref>; <foreign xml:lang="gez">እምቅድም፡</foreign> <foreign xml:lang="grc">ἀπ᾽ ἀρχῆς</foreign> <ref cRef="1 Joh." loc="1,1">1 Joh. 1,1</ref> Platt; <foreign xml:lang="gez">እምቅድም፡ ወእስከ፡ ለዓለም፡</foreign> <ref cRef="Hen." loc="39,10">Hen. 39,10</ref>; <ref cRef="Hen. " loc="62,7">Hen.  62,7</ref>.</sense> <sense n="b" xml:id="D2b"> <cit type="translation" xml:lang="la">
                                    <quote> tempus praeteritum </quote>
                                </cit>, <cit type="translation" xml:lang="la">
                                    <quote> prius </quote>
                                </cit>: <foreign xml:lang="gez">እምቅድም፡</foreign> <foreign xml:lang="grc">πρότερον</foreign> <cit type="translation" xml:lang="la">
                                    <quote> prius </quote>
                                </cit>, <cit type="translation" xml:lang="la">
                                    <quote> olim </quote>
                                </cit> <ref cRef="1 Par." loc="9,2">1 Par. 9,2</ref>; <foreign xml:lang="gez">እከያቲሁ፡ ዘቅድም፡</foreign> <cit type="translation" xml:lang="la">
                                    <quote> iniquitates ejus priores </quote>
                                </cit> <ref cRef="Sx. Enc." loc="">Sx. Enc. </ref>; (corpus ejus translatum) <foreign xml:lang="gez">እመቃብሪሁ፡ ዘቅድም፡ ኀበ፡ መቃብሪሁ፡ ዘካዕብ፡</foreign> <ref cRef="Sx. Teq." loc="6 Enc.">Sx. Teq. 6 Enc.</ref> </sense> <sense n="c" xml:id="D2c"> <cit type="translation" xml:lang="la">
                                    <quote> primitiae </quote>
                                </cit>: <foreign xml:lang="gez">ቅድመ፡</foreign> (v. <foreign xml:lang="gez">ቀዳሜ፡</foreign>) <foreign xml:lang="gez">እክሎሙ፡</foreign> <ref cRef="Sir. " loc="45,20">Sir.  45,20</ref>.</sense> <sense n="d" xml:id="D2d"> <cit type="translation" xml:lang="la">
                                    <quote> prioratus </quote>
                                </cit>: <foreign xml:lang="gez">ብኵርናከ፡ ዘቅድመ፡ ልደት፡</foreign> <ref cRef="Kuf. " loc="88.">Kuf.  p. 88.</ref> Hinc</sense> </sense> </sense>
               
