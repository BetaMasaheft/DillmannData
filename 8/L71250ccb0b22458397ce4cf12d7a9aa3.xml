               <sense xml:lang="la" source="#dillmann" xml:id="Dmain">
                  <gramGrp>
                     <pos expand="Substantivum">subst.</pos>
                  </gramGrp>
                  <sense n="1" xml:id="D1">
                     <cit type="translation" xml:lang="la">
                        <quote>communio</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>societas</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>communicatio</quote>
                     </cit>: <foreign xml:lang="gez">ሱታፌ፡ መንፈስ፡ ቅዱስ፡</foreign>
                     <ref cRef="2 Cor." loc="13,14">2 Cor. 13,14</ref>; <ref cRef="Kedr f." loc="1">Kedr f. 1</ref>; <ref cRef="Philem." loc="6">Philem. 6</ref>; <ref cRef="1 Cor." loc="10,16">1 Cor. 10,16</ref>
                     <lbl expand="sequens">seq.</lbl>; <lbl expand="cum">c.</lbl>
                     <foreign xml:lang="gez">ምስለ፡</foreign>
                     <ref cRef="1 Joh." loc="1,3">1 Joh. 1,3</ref>; <ref cRef="1 Joh." loc="1,6">1
                        Joh. 1,6</ref>; <foreign xml:lang="gez">ከመ፡ ንንሣእ፡ እምሱታፌ፡ ምስጢር፡
                        ቡሩክ፡</foreign>
                     <ref cRef="Lit." loc="159,4">Lit. 159,4</ref>; societas (mercaturae et
                     negotiorum) F.N. 34.</sense>
                  <sense n="2" xml:id="D2">
                     <lbl expand="concretus, e (opp. abstracto)">concr.</lbl>, <gramGrp>
                        <gen value="masculinum">m.</gen>
                     </gramGrp> et <gramGrp>
                        <gen value="femininus">fem.</gen>
                     </gramGrp>, <lbl expand="collatio, -is vel collectivum, -e">coll.</lbl> et <lbl expand="singularis">sing.</lbl>, <cit type="translation" xml:lang="la">
                        <quote>socius</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>consors</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>sodalis</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>collega</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>particeps</quote>
                     </cit>; <ref cRef="Kuf." loc="113">Kuf. p. 113</ref>; <foreign xml:lang="gez">ዝንቱ፡ ሱታፌ፡ ለረሲዕ፡</foreign> (<foreign xml:lang="grc">κοινωνός</foreign>) <ref cRef="Prov." loc="28,24">Prov. 28,24</ref>; <foreign xml:lang="gez">ከመ፡
                        አንትሙኒ፡ ትኩኑ፡ ሱታፌ፡ ምስሌነ፡</foreign>
                     <ref cRef="1 Joh." loc="1,3">1 Joh. 1,3</ref>; <ref cRef="Apoc." loc="1,9">Apoc. 1,9</ref>; <ref cRef="1 Cor." loc="10,18">1 Cor. 10,18</ref>
                     <lbl expand="romanae editionis">rom.</lbl>; <ref cRef="2 Cor." loc="8,23">2
                        Cor. 8,23</ref>
                     <lbl expand="romanae editionis">rom.</lbl>; <foreign xml:lang="gez">አነ፡ ሱታፌከ፡
                        በኵሉ፡ ግብርከ፡</foreign>
                     <ref cRef="Jsp." loc="366">Jsp. p. 366</ref>; <foreign xml:lang="gez">አስቴር፡
                        ሱታፌ፡ መንግሥትነ፡</foreign>
                     <ref cRef="Esth." loc="8,13">Esth. 8,13</ref> apocr.; <foreign xml:lang="gez">ወይእቲ፡ ሱታፌከ፡</foreign>
                     <ref cRef="Mal." loc="2,14">Mal. 2,14</ref>; <foreign xml:lang="gez">ጽድቅ፡ ይከውን፡
                        ለክሙ፡ ሱታፌ፡</foreign>
                     <ref cRef="Hen." loc="91,4">Hen. 91,4</ref>; <foreign xml:lang="gez">ለእመ፡ ተፃብአ፡
                        ካህን፡ ምስለ፡ ካህን፡ ሱታፌሁ፡</foreign> F.N. 9; socius negotiorum <ref cRef="Sir." loc="42,3">Sir. 42,3</ref>. Ante <gramGrp>
                        <pos expand="pronomen">pron.</pos>
                     </gramGrp> suffixa forma <foreign xml:lang="gez">ሱታፍ፡</foreign> passim
                     occurrit: <foreign xml:lang="gez">ንኩን፡ ሱታፌ፡ ለጽድቅ፡</foreign>
                     <ref cRef="3 Joh." loc="8">3 Joh. 8</ref>; <ref cRef="1 Petr." loc="5,1">1
                        Petr. 5,1</ref>; <ref cRef="1 Cor." loc="9,23">1 Cor. 9,23</ref>; <foreign xml:lang="gez">ሰላም፡ ለሕፅንኪ፡ ለጥበበ፡ ሰማይ፡ ምዕራፋ። ዘኢይትረከብ፡ ሱታፋ፡</foreign> III <ref cRef="Sal. Mar." loc="22">Sal. Mar. 22</ref>; <ref cRef="Sx. Ter" loc="17 ">Sx. Ter 17 </ref>Enc.</sense>
               </sense>
               
