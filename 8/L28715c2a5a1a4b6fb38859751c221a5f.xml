               <sense xml:id="Dmain" xml:lang="la" source="#dillmann"> <subc>I,1</subc> [<foreign xml:lang="ar">رَادَ</foreign> <foreign xml:lang="he">רוּד</foreign> <cit type="translation" xml:lang="la"><quote> ultro citroque discurrit, libere vagatus est </quote></cit>, (<cit type="translation" xml:lang="la"><quote> currendo </quote></cit>) <cit type="translation" xml:lang="la"><quote> petiit </quote></cit>; vicinum est <foreign xml:lang="gez">ሮጸ፡</foreign>; <lbl expand="videas">vid.</lbl> etiam <foreign xml:lang="gez">መረደ፡</foreign>] <gramGrp> <pos expand="Subjunctivus">Subj.</pos> </gramGrp> <foreign xml:lang="gez">ይሩድ፡</foreign> <sense n="1" xml:id="D1"> <cit type="translation" xml:lang="la"><quote> incurrere </quote></cit>, <cit type="translation" xml:lang="la"><quote> incursare </quote></cit>, <cit type="translation" xml:lang="la"><quote> invadere </quote></cit>, <cit type="translation" xml:lang="la"><quote> oppugnare </quote></cit>, <cit type="translation" xml:lang="la"><quote> incursionem </quote></cit> vel <cit type="translation" xml:lang="la"><quote> impetum facere </quote></cit>, <cit type="translation" xml:lang="la"><quote> expeditione bellica petere </quote></cit>: <foreign xml:lang="gez">ሮዱ፡ ወቀተሉ፡ በይሁዳ፡</foreign> <foreign xml:lang="grc">ἐπέθεντο καὶ ἐπάταξαν</foreign> <ref cRef="2 Par." loc="28,17">2 Par. 28,17</ref>; <foreign xml:lang="gez">ይረውጹ፡ ላዕሌኪ፡ እንዘ፡ ይረውዱ፡</foreign> <ref cRef="Jer." loc="28,14">Jer. 28,14</ref> <lbl expand="varia lectio">var.</lbl>; <foreign xml:lang="gez">ወይረውዱ፡</foreign> <foreign xml:lang="grc">καὶ παρατάξονται</foreign> <ref cRef="Zach." loc="10,5">Zach. 10,5</ref>; <lbl expand="cum">c.</lbl> <gramGrp> <case value="accusativus">Acc.</case> </gramGrp>: <foreign xml:lang="gez">ኢሎፍሊ፡ ሮዱ፡ አህጉረ፡</foreign> <ref cRef="2 Par." loc="28,18">2 Par. 28,18</ref>; <foreign xml:lang="gez">ይሩዶሙ፡</foreign> <foreign xml:lang="grc">προκαταλαβέσθαι αὐτούς</foreign> <ref cRef="2 Par." loc="32,1">2 Par. 32,1</ref>; <ref cRef="Jud." loc="9,33">Jud. 9,33</ref>; <ref cRef="Kuf." loc="37">Kuf. 37</ref>; <foreign xml:lang="gez">ሮዱኒ፡</foreign> <foreign xml:lang="grc">κατέδραμον ἐπ᾽ ἐμοί</foreign> <ref cRef="Job" loc="16,10">Job 16,10</ref>; <foreign xml:lang="gez">ሮዱኒ፡ ወሮጹ፡ ላዕሌየ፡</foreign> <ref cRef="Job" loc="16,14">Job 16,14</ref>; <foreign xml:lang="gez">ሮደከ፡ ፀባኢት፡ ኀያል፡</foreign> <ref cRef="Job" loc="29,10">Job 29,10</ref>; <ref cRef="4 Esr." loc="13,10">4 Esr. 13,10</ref>; <ref cRef="4 Esr." loc="13,15">4 Esr. 13,15</ref>; <foreign xml:lang="gez">ከመ፡ ይሩዱነ፡</foreign> <ref cRef="4 Reg." loc="7,6">4 Reg. 7,6</ref>; <foreign xml:lang="gez">ሮድዎሙ፡</foreign> <foreign xml:lang="grc">μονόζωνοι ἦλθον</foreign> <ref cRef="4 Reg." loc="13,20">4 Reg. 13,20</ref>; <ref cRef="4 Reg." loc="13,21">4 Reg. 13,21</ref>; <foreign xml:lang="gez">ዕደው፡ እለ፡ ኢሮዱ፡ አጋርያን፡</foreign> <foreign xml:lang="grc">ἄνδρες οἳ ἦσαν πεζοί</foreign> (<lbl expand="oppositum, -o, -nitur">opp.</lbl> equites expediti) <ref cRef="Judith " loc="7,2">Judith  7,2</ref>. 
</sense> <sense n="2" xml:id="D2"> insurgere contra aliquem, <cit type="translation" xml:lang="la"><quote> impugnare </quote></cit>, <cit type="translation" xml:lang="la"><quote> insectari </quote></cit>, <cit type="translation" xml:lang="la"><quote> premere </quote></cit> (impugnatione): <foreign xml:lang="gez">ሮድዎ፡ ወሰሐብዎ፡</foreign> <foreign xml:lang="grc">ἐπιστάντες</foreign> <ref cRef="Act." loc="6,12">Act. 6,12</ref>; <ref cRef="Num." loc="16,19">Num. 16,19</ref>; <ref cRef="Num." loc="17,7">Num. 17,7</ref>; <ref cRef="Ps." loc="26,6">Ps. 26,6</ref>; <foreign xml:lang="gez">ይረውዱከ፡</foreign> <foreign xml:lang="grc">παροιστρήσουσι</foreign> <ref cRef="Hez." loc="2,6">Hez. 2,6</ref>; <foreign xml:lang="gez">እለ፡ ይረውድዎ፡</foreign> <foreign xml:lang="grc">θλίβοντες</foreign> <ref cRef="Thren." loc="1,17">Thren. 1,17</ref>; <ref cRef="Ps. " loc="105,12">Ps.  105,12</ref>; <ref cRef="Jes." loc="19,20">Jes. 19,20</ref>; <ref cRef="Jes." loc="29,7">Jes. 29,7</ref>. 
</sense> <sense n="3" xml:id="D3"> <cit type="translation" xml:lang="la"><quote> persequi </quote></cit>, <cit type="translation" xml:lang="la"><quote> insequi </quote></cit>, <foreign xml:lang="grc">διώκειν</foreign>: <foreign xml:lang="gez">አፍራስ፡ ዘይረውድ፡</foreign> <ref cRef="Nah." loc="3,2">Nah. 3,2</ref>; <foreign xml:lang="gez">አልቦ፡ ዘይረውድ፡</foreign> <ref cRef="Jes." loc="17,2">Jes. 17,2</ref>; <ref cRef="Ps." loc="43,18">Ps. 43,18</ref>; <lbl expand="cum">c.</lbl> <gramGrp> <case value="accusativus">Acc.</case> </gramGrp> <ref cRef="Ps." loc="7,1">Ps. 7,1</ref>; <ref cRef="Ps." loc="30,19">Ps. 30,19</ref>; <ref cRef="Ps." loc="141,9">Ps. 141,9</ref>; <ref cRef="Ps." loc="142,3">Ps. 142,3</ref>; <ref cRef="Jer." loc="15,15">Jer. 15,15</ref>; <ref cRef="Jes." loc="16,4">Jes. 16,4</ref>; <ref cRef="Thren." loc="1,6">Thren. 1,6</ref>. – <lbl expand="">Rad.</lbl> <lbl expand="d'Abbadie">Abb.</lbl>: <foreign xml:lang="gez">ሮደ፡ ዘ፡ ከበበ፡</foreign> (i.e. <cit type="translation" xml:lang="la"><quote> circumvenire </quote></cit>). 
</sense> </sense>
               
