            <entry xml:id="L6927d66e94364d42b77b937ace9594b5" n="10099">
               <form>
                  <foreign xml:lang="gez">ተፈጥረ</foreign>
               </form>
               <sense xml:lang="la" source="#dillmann" xml:id="Dmain"> <subc>III,1</subc> <cit type="translation" xml:lang="la">
                     <quote>creari</quote>
                  </cit>, <ref cRef="Kuf." loc="3">Kuf. 3</ref>; <ref cRef="Ex." loc="9,18">Ex.
                     9,18</ref>; <ref cRef="Lev." loc="18,27">Lev. 18,27</ref>; <ref cRef="Hez." loc="28,14">Hez. 28,14</ref>; <foreign xml:lang="gez">ይከውኑ፡ ከመ፡
                     ዘኢተፈጥሩ፡</foreign>
                  <ref cRef="Jes." loc="41,11">Jes. 41,11</ref>; <ref cRef="Asc. Jes." loc="4,18">Asc. Jes. 4,18</ref>; <ref cRef="Ps. " loc="32,9">Ps. 32,9</ref>; <ref cRef="Koh." loc="8,9">Koh. 8,9</ref>; <foreign xml:lang="gez">እምአመ፡ ተፈጥረ፡ ሰብእ፡
                     ዲበ፡ ምድር፡</foreign>
                  <ref cRef="Job" loc="20,4">Job 20,4</ref>; <ref cRef="Sap." loc="2,2">Sap.
                     2,2</ref>; <foreign xml:lang="gez">ዘእንበለ፡ ይትፈጠር፡ ዓለም፡</foreign>
                  <ref cRef="Sir." loc="42,21">Sir. 42,21</ref>; <ref cRef="4 Esr." loc="8,6">4 Esr.
                     8,6</ref>; <ref cRef="4             Esr." loc="9,25">4 Esr. 9,25</ref>; <ref cRef="Jac." loc="3,9">Jac. 3,9</ref>; <ref cRef="2 Petr." loc="3,5">2 Petr.
                     3,5</ref>; <ref cRef="Hebr." loc="4,13">Hebr. 4,13</ref>; <ref cRef="Hebr." loc="11,3">Hebr. 11,3</ref>; <ref cRef="Apoc." loc="4,11">Apoc. 4,11</ref>;
                     <foreign xml:lang="gez">ተፈጢርየ፡</foreign>
                  <ref cRef="Job" loc="10,19">Job 10,19</ref>; <ref cRef="4 Esr." loc="8,69">4 Esr.
                     8,69</ref>; <ref cRef="4 Esr." loc="9,21">4 Esr. 9,21</ref>; <foreign xml:lang="gez">ቦኑ፡ ቀደምከ፡ ተፈጥሮ፡</foreign>
                  <ref cRef="Job" loc="15,7">Job 15,7</ref>; <ref cRef="Koh." loc="6,8">Koh.
                     6,8</ref>; <foreign xml:lang="gez">ኢተፈጥሮ፡</foreign>
                  <ref cRef="Koh." loc="4,3">Koh. 4,3</ref>; <lbl expand="cum">c.</lbl>
                  <foreign xml:lang="gez">እምነ፡</foreign> materiae: <foreign xml:lang="gez">ኵሉ፡ እምነ፡
                     ምድር፡ ተፈጥረ፡</foreign>
                  <ref cRef="Sir." loc="41,10">Sir. 41,10</ref>. <sense n="a" xml:id="Da">
                     <cit type="translation" xml:lang="la">
                        <quote>generari</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>gigni</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>natura</quote>
                     </cit>
                     <cit type="translation" xml:lang="la">
                        <quote>fieri</quote>
                     </cit>: <foreign xml:lang="gez">ተፈጠርኩ፡ ውስተ፡ ከርሥ፡</foreign>
                     <foreign xml:lang="grc">ἐγενόμην</foreign>
                     <ref cRef="Job" loc="31,15">Job 31,15</ref>; <foreign xml:lang="gez">ዘተኀጽወ፡
                        በግብር፡ ወተፈጥረ፡ ምዉተ፡ ኀፍረተ፡ ዚአሁ፡</foreign> F.M. 33,1; <foreign xml:lang="gez">ዕፄ፡ ኢተፈጥረ፡ በላዕሌሁ፡</foreign>
                     <ref cRef="Ex." loc="16,24">Ex. 16,24</ref>.</sense>
                  <sense n="b" xml:id="Db">
                     <cit type="translation" xml:lang="la">
                        <quote>fieri</quote>
                     </cit> (inde ab initio rerum vel a creatore): <foreign xml:lang="gez">እስመ፡
                        ትእዛዘ፡ ሕግ፡ ተፈጥረ፡ ሞተ፡ ትሙት፡</foreign> (<foreign xml:lang="grc">ἀπ᾽
                        αἰῶνος</foreign>) <ref cRef="Sir." loc="14,17">Sir. 14,17</ref>; <foreign xml:lang="gez">ሰንበት፡ ተፈጥረት፡ በእንተ፡ ሰብእ፡ </foreign>
                     <ref cRef="Marc." loc="2,27">Marc. 2,27</ref>.</sense>
               </sense>
               
