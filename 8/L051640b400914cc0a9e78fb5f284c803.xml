            <entry xml:id="L051640b400914cc0a9e78fb5f284c803" n="7447">
               <form>
                  <foreign xml:lang="gez">ንዝህላል</foreign>
               </form>
               <sense xml:lang="la" source="#dillmann" xml:id="Dmain">, rarius <foreign xml:lang="gez">ነዛህላል፡</foreign>, <gramGrp> <pos expand="not found">subst.</pos> </gramGrp> <cit type="translation" xml:lang="la">
                            <quote> vita dissoluta </quote>
                        </cit>, <cit type="translation" xml:lang="la">
                            <quote> animus dissolutus </quote>
                        </cit> <sense n="a" xml:id="Da"> <cit type="translation" xml:lang="la">
                                <quote> languor </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> desidia </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> ignavia </quote>
                            </cit>: <foreign xml:lang="gez">ሀኬት፡ ወንዛህላል፡</foreign> (<foreign xml:lang="grc">ὄκνος</foreign>) <ref cRef="Chrys. Ta." loc="27">Chrys. Ta. 27</ref>; <foreign xml:lang="gez">ሀኬት፡ ወንዛህላል፡</foreign> <foreign xml:lang="grc">νωθής</foreign> <ref cRef="Chrys. ho." loc="32">Chrys. ho. 32</ref>; <foreign xml:lang="gez">ንዝህላል፡ ወትዝኅርት፡</foreign> non decent anachoretam <ref cRef="Phlx." loc="5.">Phlx. 5.</ref> </sense> <sense n="b" xml:id="Db"> <cit type="translation" xml:lang="la">
                                <quote> lascivia </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> heluatio </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> nequitia </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> animus cupiditatibus agitatus </quote>
                            </cit>: viduae ecclesiasticae <foreign xml:lang="gez">ኢይሑራ፡ ውስተ፡ ሐውዘ፡ መባልዕት፡ ወንዝህላል፡</foreign> <ref cRef="Did." loc="14">Did. 14</ref>; <foreign xml:lang="gez">ነዛህላለ፡</foreign> (v. <foreign xml:lang="gez">ንዝህላለ፡</foreign>) <foreign xml:lang="gez">ፍትወት፡</foreign> <foreign xml:lang="grc">ῥεμβασμὸς ἐπιθυμίας</foreign> <ref cRef="Sap. " loc="4,12">Sap.  4,12</ref>.</sense> <sense n="c" xml:id="Dc"> <cit type="translation" xml:lang="la">
                                <quote> animi </quote>
                            </cit> <cit type="translation" xml:lang="la">
                                <quote> levitas </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> temeritas </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> vanitas </quote>
                            </cit> (<cit type="translation" xml:lang="de">
                                <quote>Leichtsinn</quote>
                            </cit>, <cit type="translation" xml:lang="de">
                                <quote>Leichtfertigkeit</quote>
                            </cit>), spec. in loquendo: <foreign xml:lang="gez">እለ፡ ወድቁ፡ ውስተ፡ ንዝህላል፡</foreign> <foreign xml:lang="grc">εἰς τὸ ῥᾴθυμον</foreign> (<cit type="translation" xml:lang="de">
                                <quote>verfallen in leichtsinnige Reden</quote>
                            </cit>) <ref cRef="Cyr. c. Pall." loc="76">Cyr. c. Pall. f. 76</ref>; si pugnam detrectaveris <foreign xml:lang="gez">ናሁ፡ ተበየነ፡ ንዝህላልከ፡</foreign> <ref cRef="Jsp." loc="282">Jsp. p. 282</ref>; <foreign xml:lang="gez">ነዛህላል፡</foreign> (<cit type="translation" xml:lang="la">
                                <quote>„nugae scurriles“</quote>
                            </cit>) Lud. ex <ref cRef="Org." loc="5.">Org. 5.</ref> </sense> </sense>
               
