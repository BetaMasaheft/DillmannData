               <sense xml:id="Dmain" xml:lang="la" source="#dillmann"> <subc>III,3</subc> (e <bibl><ptr target="bm:Dillmann1857Gramm"/>gramm.</bibl> § 48,4 saepissime <foreign xml:lang="gez">ተመሐለ፡</foreign> exscriptum) <sense n="1" xml:id="D1"> <cit type="translation" xml:lang="la"><quote> sibi invicem jurare </quote></cit>, <cit type="translation" xml:lang="la"><quote> conjurare </quote></cit> <ref cRef="Jud." loc="21,1">Jud. 21,1</ref>; <foreign xml:lang="gez">ተማሐልነ፡ ክልኤነ፡</foreign> <ref cRef="1 Reg." loc="20,42">1 Reg. 20,42</ref>; <ref cRef="Gen." loc="31,44">Gen. 31,44</ref>; <ref cRef="Act." loc="23,12">Act. 23,12</ref>; <ref cRef="Act. " loc="23,13">Act.  23,13</ref>.
</sense> <sense n="2" xml:id="D2"> <cit type="translation" xml:lang="la"><quote> jurejurando foedus sancire </quote></cit> cum aliquo vel inter se: <foreign xml:lang="gez">ተማሒልየ፡</foreign> <ref cRef="3 Reg." loc="20,34">3 Reg. 20,34</ref>; <lbl expand="cum">c.</lbl> <foreign xml:lang="gez">በበይናቲሆሙ፡</foreign> <ref cRef="Gen." loc="21,27">Gen. 21,27</ref>; <ref cRef="Gen." loc="26,28">Gen. 26,28</ref>; <ref cRef="Gen." loc="26,31">Gen. 26,31</ref>; <lbl expand="cum">c.</lbl> <foreign xml:lang="gez">ምስለ፡</foreign> <ref cRef="Jos." loc="9,4">Jos. 9,4</ref> <lbl expand="sequens">seq.</lbl>; <foreign xml:lang="gez">ተማሐል፡ ምስሌነ፡</foreign> <ref cRef="1 Reg." loc="11,1">1 Reg. 11,1</ref>; <foreign xml:lang="gez">ተማሐሉ፡ ምስሌሁ፡</foreign> <foreign xml:lang="grc">ἔνορκοι ἦσαν αὐτῷ</foreign> <ref cRef="2 Esr." loc="16,18">2 Esr. 16,18</ref>; <foreign xml:lang="gez">ተማሐልነ፡ ምስለ፡ ሲኦል፡</foreign> <foreign xml:lang="grc">ἐποιήσαμεν διαθήκην μετὰ</foreign> <cb n="0156" xml:id="c0156"/> <foreign xml:lang="grc">τοῦ ᾅδου</foreign> <ref cRef="Jes." loc="28,15">Jes. 28,15</ref>; <lbl expand="cum">c.</lbl> <gramGrp> <case value="accusativus">Acc.</case> </gramGrp> <lbl expand="persona">pers.</lbl>: <foreign xml:lang="gez">ዕደው፡ እለ፡ ተማሐልከ፡</foreign> <foreign xml:lang="grc">οἱ ἄνδρες τῆς διαθήκης σου</foreign> <ref cRef="Ob." loc="7.">Ob. 7.</ref> 
</sense> </sense>
               
            <sense xml:id="Tmain" xml:lang="en" source="#traces"> <cit type="transcription" xml:lang="gez"><quote>tamāḥala</quote></cit> <sense n="G"> <foreign xml:lang="gez">ተማሐለ፡</foreign> <cit type="transcription" xml:lang="gez"><quote>tamāḥala</quote></cit> <subc>III,3</subc> <foreign xml:lang="gez">ይትማሐል፡</foreign> «<cit type="translation" xml:lang="fr"><quote>jurer l’un à l’autre</quote></cit>, <cit type="translation" xml:lang="fr"><quote>prêter serment réciproque</quote></cit>» ― <foreign xml:lang="am">ተማማለ፡</foreign> Ms. <ref type="BM" target="BNFabb217">BNFabb217</ref>, fol. 34r. <lb/>
 <bibl><ptr target="bm:Grebaut1952SuppLexicon"/><citedRange unit="page"> 78</citedRange></bibl> 
</sense> <sense n="L"> <foreign xml:lang="gez">ተማሐለ</foreign> <cit type="transcription" xml:lang="gez"><quote>tamāḥala</quote></cit> <cit type="translation" xml:lang="en"><quote>swear to one another</quote></cit>, <cit type="translation" xml:lang="en"><quote>conspire</quote></cit>, <cit type="translation" xml:lang="en"><quote>form a conspiracy</quote></cit>, <cit type="translation" xml:lang="en"><quote>bind one another by an oath</quote></cit>, <cit type="translation" xml:lang="en"><quote>make a covenant</quote></cit> <bibl><ptr target="bm:Leslau1987dic"/><citedRange unit="page"> 335b</citedRange></bibl> 
</sense> </sense></entry>
