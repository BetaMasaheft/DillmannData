                     <cit type="translation" xml:lang="la">
                        <quote>aperte</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>palam</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>manifesto</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>ingenue</quote>
