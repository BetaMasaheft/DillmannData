         <div>
            <entry xml:id="L7846de634b6644acb444cb098b1416b8" n="8050">
               <form>
                  <foreign xml:lang="gez">ደነስ</foreign>
               </form>
               <sense xml:lang="la" source="#dillmann" xml:id="Dmain">
                  <gramGrp>
                     <pos expand="Substantivum">subst.</pos>
                  </gramGrp>, <lbl expand="pluralis">Pl.</lbl>
                  <foreign xml:lang="gez">ደነሳት፡</foreign>
                  <sense n="1" xml:id="D1">
                     <cit type="translation" xml:lang="la">
                        <quote>spurcitia</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>sordes</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>impudicitia</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>turpitudo</quote>
                     </cit>: <foreign xml:lang="gez">አንጽሐነ፡ እምኵሉ፡ ደነስ፡ ወእምኵሉ፡ ርኵስ፡</foreign>
                     <ref cRef="Lit." loc="164,1">Lit. 164,1</ref>; <foreign xml:lang="gez">በፍትወተ፡
                        ደነስ፡</foreign> (impressum est <foreign xml:lang="gez">ደንስ፡</foreign>) <ref cRef="Lit." loc="170,4">Lit. 170,4</ref>; Lud. e Cant. Mar.; <foreign xml:lang="gez">ያነጽሕ፡ ልቦ፡ እምደነሳት፡</foreign>
                     <ref cRef="Phlx." loc="211.">Phlx. 211.</ref>
                  </sense>
                  <sense n="2" xml:id="D2">
                     <cit type="translation" xml:lang="la">
                        <quote>merces meretricia</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>quaestus meretricius</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>pretium stupri</quote>
                     </cit>: <foreign xml:lang="gez">ሀበኒ፡ ደነስየ፡</foreign>
                     <ref cRef="Kuf." loc="41">Kuf. 41</ref>; <ref cRef="Hos." loc="2,14">Hos.
                        2,14</ref>; <ref cRef="Mich." loc="1,7">Mich. 1,7</ref>; <ref cRef="Hez. " loc="16,31">Hez. 16,31</ref>; <ref cRef="Hez." loc="27,15">Hez. 27,15</ref>;
                        <foreign xml:lang="gez">ደነሰ፡ ዘማ፡</foreign>
                     <ref cRef="Deut." loc="23,19">Deut. 23,19</ref>; <foreign xml:lang="gez">ደነሰ፡
                        ዝሙት፡</foreign>
                     <ref cRef="Mich." loc="1,7">Mich. 1,7</ref>. — <cit type="translation" xml:lang="la">
                        <quote>Voc</quote>
                     </cit>. <cit type="translation" xml:lang="la">
                        <quote>Ae</quote>
                     </cit>.: <foreign xml:lang="gez">ደነስ፡ ዘ፡ አደፍ፡</foreign>; <cit type="translation" xml:lang="la">
                        <quote>aliud</quote>
                     </cit>: <foreign xml:lang="gez">ደነስ፡ ዘ፡ ዐስበ፡ ዝሙት።</foreign>
                  </sense>
               </sense>
               
