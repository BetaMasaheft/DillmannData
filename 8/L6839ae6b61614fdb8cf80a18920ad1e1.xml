            <entry xml:id="L6839ae6b61614fdb8cf80a18920ad1e1" n="5675">
               <form>
                  <foreign xml:lang="gez">አእተወ</foreign>
               </form>
               <sense xml:id="Dmain" xml:lang="la" source="#dillmann"> <subc>II,1</subc> <sense n="1" xml:id="D1"> de personis: <sense n="a" xml:id="D1a"> <cit type="translation" xml:lang="la"><quote> domum </quote></cit> (vel <cit type="translation" xml:lang="la"><quote> in </quote></cit> <cit type="translation" xml:lang="la"><quote> patriam </quote></cit>) <cit type="translation" xml:lang="la"><quote> reducere </quote></cit> vel <cit type="translation" xml:lang="la"><quote> deducere </quote></cit> aliquem; ut exercitum <ref cRef="Judith " loc="1,13">Judith  1,13</ref>; uxorem: <foreign xml:lang="gez">አእትዋ፡ ኀበ፡ አቡከ፡</foreign> <ref cRef="Tob." loc="7,12">Tob. 7,12</ref>; exsules: <foreign xml:lang="gez">ያአትዎሙ፡ ብሔሮሙ፡</foreign> <ref cRef="Jes." loc="14,2">Jes. 14,2</ref>; <foreign xml:lang="gez">ኣአትዎሙ፡ ውስተ፡ ብሔሮሙ፡</foreign> <ref cRef="Jer." loc="16,15">Jer. 16,15</ref>; <foreign xml:lang="gez">እንዘ፡ ይትፌሥሑ፡ ኣአትዎሙ፡</foreign> <ref cRef="Jer." loc="38,9">Jer. 38,9</ref>.
</sense> <sense n="b" xml:id="D1b"> <cit type="translation" xml:lang="la"><quote> sinere </quote></cit> vel <cit type="translation" xml:lang="la"><quote> jubere </quote></cit> aliquem <cit type="translation" xml:lang="la"><quote> domum </quote></cit> <cit type="translation" xml:lang="la"><quote> redire </quote></cit>: <foreign xml:lang="gez">አእተወኒ፡ ነነዌ፡</foreign> (v. <foreign xml:lang="gez">ውስተ፡ ነነዌ፡</foreign>) <ref cRef="Tob." loc="1,22">Tob. 1,22</ref>.
</sense> 
</sense><sense n="2" xml:id="D2"> de rebus: <sense n="a" xml:id="D2a"> <cit type="translation" xml:lang="la"><quote> importare </quote></cit>, <cit type="translation" xml:lang="la"><quote> invehere </quote></cit>: <foreign xml:lang="gez">ኵሎ፡ ጾረ፡ ያአትዉ፡ ኢየሩሳሌም፡</foreign> <ref cRef="2 Esr." loc="23,15">2 Esr. 23,15</ref>; <ref cRef="2 Esr." loc="20,31">2 Esr. 20,31</ref> <lbl expand="varia lectio">var.</lbl> 
</sense> <sense n="b" xml:id="D2b"> <cit type="translation" xml:lang="la"><quote> domum </quote></cit> <cit type="translation" xml:lang="la"><quote> introducere </quote></cit> aliquid, <ref cRef="Deut." loc="22,2">Deut. 22,2</ref>.
</sense> <sense n="c" xml:id="D2c"> <cit type="translation" xml:lang="la"><quote> domum </quote></cit> <cit type="translation" xml:lang="la"><quote> referre </quote></cit> vel <cit type="translation" xml:lang="la"><quote> reportare </quote></cit> aliquid, <ref cRef="Num." loc="31,53">Num. 31,53</ref>; <ref cRef="Ruth" loc="2,18">Ruth 2,18</ref>; <foreign xml:lang="gez">ኵሎ፡ ታአትዉ፡ ለአንስት፡ ወትሁብዎን፡</foreign> <ref cRef="1 Esr." loc="4,22">1 Esr. 4,22</ref>; <ref cRef="1 Esr." loc="4,24">1 Esr. 4,24</ref>; lucrum e mercatura <ref cRef="2 Par." loc="8,18">2 Par. 8,18</ref>. <sense n="α" xml:id="D2cα"> responsum referre: <foreign xml:lang="gez">እስከ፡ ያአትዉ፡ ሎሙ፡ መጽሐፈ፡</foreign> <ref cRef="1 Esr." loc="6,6">1 Esr. 6,6</ref>.
</sense> <sense n="β" xml:id="D2cβ"> <cit type="translation" xml:lang="la"><quote> invehere </quote></cit> frumenta, messem in horrea, <ref cRef="Deut." loc="14,22">Deut. 14,22</ref>; <ref cRef="2 Reg." loc="9,10">2 Reg. 9,10</ref>; <ref cRef="Hagg. " loc="1,6">Hagg.  1,6</ref>; <foreign xml:lang="gez">ዘዘርአ፡ ስድሰ፡ ያአቱ፡ ሥልሰ፡</foreign> <ref cRef="Jes." loc="5,10">Jes. 5,10</ref>; <foreign xml:lang="gez">ከመ፡ ያእቱ፡ ለከ፡ ተግባረ፡ ማእረርከ፡</foreign> <ref cRef="Job " loc="39,12">Job  39,12</ref>.
</sense> <sense n="γ" xml:id="D2cγ"> <cit type="translation" xml:lang="la"><quote> redire </quote></cit> <cit type="translation" xml:lang="la"><quote> facere </quote></cit> vel <cit type="translation" xml:lang="la"><quote> reddere </quote></cit>, ut vectigal: <foreign xml:lang="gez">ያአትዉ፡ ጸባሕተ፡ ለንጉሥ፡</foreign> <foreign xml:lang="grc">ἀναφέρουσι τοὺς φόρους</foreign> <ref cRef="1 Esr." loc="4,6">1 Esr. 4,6</ref>. 
</sense> 
</sense> 
</sense> </sense>
