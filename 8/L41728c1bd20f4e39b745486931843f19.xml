            <entry xml:id="L41728c1bd20f4e39b745486931843f19" n="6375">
               <form>
                  <foreign xml:lang="gez">ተካፈለ</foreign>
               </form>
               <sense xml:lang="la" source="#dillmann" xml:id="Dmain"> <subc>III,3</subc> <sense n="1" xml:id="D1">
                     <cit type="translation" xml:lang="la">
                        <quote>inter se dividere</quote>
                     </cit> vel <cit type="translation" xml:lang="la">
                        <quote>distribuere</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>cum aliis</quote>
                     </cit> vel <cit type="translation" xml:lang="la">
                        <quote>inter plures dividere</quote>
                     </cit>, <lbl expand="cum">c.</lbl>
                     <gramGrp>
                        <case value="accusativus">Acc.</case>
                     </gramGrp> rei: <foreign xml:lang="gez">ኵሎ፡ ተግባረከ፡ ይትካፈሉ፡ </foreign> (v.
                        <foreign xml:lang="gez">ይትከፈሉ፡</foreign>) <foreign xml:lang="gez">መዋርስቲከ፡</foreign>
                     <ref cRef="Sir." loc="14,15">Sir. 14,15</ref>; <ref cRef="Sir." loc="46,9">Sir.
                        46,9</ref>; <foreign xml:lang="gez">ተካፈሉ፡ ምድርየ፡ </foreign>
                     <ref cRef="Joel" loc="4,2">Joel 4,2</ref>; <ref cRef="Mich." loc="2,4">Mich.
                        2,4</ref>; <ref cRef="Ex." loc="21,35">Ex. 21,35</ref>; <ref cRef="Num." loc="32,18">Num. 32,18</ref>; <ref cRef="Jos." loc="14,5">Jos. 14,5</ref>;
                        <ref cRef="2 Reg." loc="19,29">2 Reg. 19,29</ref>; <ref cRef="Matth." loc="27,35">Matth. 27,35</ref>; <ref cRef="Marc." loc="15,24">Marc.
                        15,24</ref>; <foreign xml:lang="gez">እለ፡ ይትካፈሉ፡ በርበረ፡</foreign> vel <foreign xml:lang="gez">ምህርካ፡</foreign>
                     <ref cRef="Jes." loc="9,3">Jes. 9,3</ref>; <ref cRef="Judith" loc="9,4">Judith
                        9,4</ref>; <ref cRef="Prov." loc="16,19">Prov. 16,19</ref>; <ref cRef="Ex. " loc="15,9">Ex. 15,9</ref>; <ref cRef="Ps." loc="59,6">Ps. 59,6</ref>; <ref cRef="Ps." loc="67,13">Ps. 67,13</ref>; <foreign xml:lang="gez">ከመ፡
                        ይትካፈሉክሙ፡</foreign>
                     <ref cRef="Hez." loc="25,4">Hez. 25,4</ref>; <ref cRef="Jes." loc="14,2">Jes.
                        14,2</ref>; <lbl expand="translate">transl.</lbl>: <foreign xml:lang="gez">ይትካፈሉ፡ አብዳን፡ እከየ፡</foreign>
                     <ref cRef="Prov." loc="14,18">Prov. 14,18</ref>; omisso objecto: <foreign xml:lang="gez">ከመዝ፡ ትትካፈሉ፡</foreign>
                     <ref cRef="Hez." loc="48,29">Hez. 48,29</ref>; <lbl expand="cum">c.</lbl>
                     <foreign xml:lang="gez">ለ፡</foreign> pers.: <foreign xml:lang="gez">አኀዙ፡ ይትካፈሉ፡
                        ሎሙ፡</foreign> (sibi, inter se) <foreign xml:lang="gez">ምድረ፡</foreign>
                     <ref cRef="Kuf." loc="8">Kuf. 8</ref>; <ref cRef="Ps." loc="21,19">Ps.
                        21,19</ref>; <lbl expand="cum">c.</lbl>
                     <gramGrp>
                        <case value="accusativus">Acc.</case>
                     </gramGrp>
                     <lbl expand="persona">pers.</lbl> (i.e. <cit type="translation" xml:lang="la">
                        <quote>cum</quote>
                     </cit> vel <cit type="translation" xml:lang="la">
                        <quote>inter</quote>
                     </cit>) <ref cRef="Num." loc="32,19">Num. 32,19</ref>; <lbl expand="cum">c.</lbl>
                     <foreign xml:lang="gez">ምስለ፡</foreign> pers.: <foreign xml:lang="gez">ዘይትካፈል፡
                        ምስለ፡ ሰራቂ፡ </foreign>
                     <ref cRef="Prov." loc="29,24">Prov. 29,24</ref>.</sense>
                  <sense n="2" xml:id="D2">
                     <cit type="translation" xml:lang="la">
                        <quote>participare</quote>
                     </cit> aliquid i.e. <cit type="translation" xml:lang="la">
                        <quote>participem fieri</quote>
                     </cit> rei, <lbl expand="cum">c.</lbl>
                     <lbl expand="duplex">dupl.</lbl>
                     <gramGrp>
                        <case value="accusativus">Acc.</case>
                     </gramGrp>, <ref cRef="Apoc. " loc="18,4">Apoc. 18,4</ref>.</sense>
               </sense>
               
