               <sense xml:lang="la" source="#dillmann" xml:id="Dmain"> <subc>III,3</subc> <sense n="a" xml:id="Da"> <cit type="translation" xml:lang="la">
                                <quote> alterum alterum </quote>
                            </cit> vel <cit type="translation" xml:lang="la">
                                <quote> sese invicem sequi </quote>
                            </cit> vel <cit type="translation" xml:lang="la">
                                <quote> excipere </quote>
                            </cit> <ref cRef="2 Reg." loc="21,1">2 Reg. 21,1</ref>; <ref cRef="Num." loc="2,17">Num. 2,17</ref>; <foreign xml:lang="gez">እንዘ፡ ይታልዉ፡ ፩ድኅረ፡ ፩፡</foreign> <ref cRef="Sx. Tachs." loc="23.">Sx. Tachs. 23.</ref> </sense> <sense n="b" xml:id="Db"> <cit type="translation" xml:lang="la">
                                <quote> alium post alium </quote>
                            </cit> (<cit type="translation" xml:lang="la">
