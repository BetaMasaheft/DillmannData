               <sense xml:id="Dmain" xml:lang="la" source="#dillmann"> <subc>I,1</subc> [amh. <foreign xml:lang="gez">ቀና፡</foreign>; <foreign xml:lang="ar">قَنَأَ</foreign> <cit type="translation" xml:lang="la"><quote> valde rubuit </quote></cit>; <foreign xml:lang="he">קִנֵּא</foreign> <cit type="translation" xml:lang="la"><quote> zelotypus fuit </quote></cit>, <cit type="translation" xml:lang="la"><quote> invidit </quote></cit>; <foreign xml:lang="syr">ܩܶܢܶܐܬܳܐ</foreign> <cit type="translation" xml:lang="la"><quote> invidia </quote></cit>] <gramGrp> <pos expand="Subjunctivus">Subj.</pos> </gramGrp> <foreign xml:lang="gez">ይቅናእ፡</foreign> <sense n="1" xml:id="D1"> <cit type="translation" xml:lang="la"><quote> zelotypum esse </quote></cit>, <cit type="translation" xml:lang="la"><quote> invidere </quote></cit>; <lbl expand="construitur cum">c.c.</lbl> <foreign xml:lang="gez">ላዕለ፡</foreign> pers. <ref cRef="Gen." loc="26,14">Gen. 26,14</ref>; <ref cRef="Gen." loc="30,1">Gen. 30,1</ref>; <ref cRef="Gen." loc="37,11">Gen. 37,11</ref>; <ref cRef="Num." loc="5,14">Num. 5,14</ref>; <ref cRef="Kuf." loc="24">Kuf. 24</ref>; <ref cRef="Ps." loc="36,1">Ps. 36,1</ref>; <ref cRef="Ps. " loc="72,3">Ps.  72,3</ref>; <ref cRef="Prov." loc="23,17">Prov. 23,17</ref>; <ref cRef="Sir." loc="45,18">Sir. 45,18</ref>; <ref cRef="Jes." loc="11,13">Jes. 11,13</ref>; <lbl expand="cum">c.</lbl> <foreign xml:lang="gez">ለ፡</foreign> pers.: <ref cRef="Kuf." loc="103">Kuf. p. 103</ref>; <lbl expand="cum">c.</lbl> <foreign xml:lang="gez">በ፡</foreign> pers. vel rei: <ref cRef="Kuf." loc="103">Kuf. p. 103</ref>; <ref cRef="Prov." loc="23,17">Prov. 23,17</ref>; <ref cRef="Asc. Jes." loc="11,19">Asc. Jes. 11,19</ref>; <foreign xml:lang="gez">ኢትቅናእ፡ በፍናዊሆሙ፡</foreign> <ref cRef="Prov." loc="3,31">Prov. 3,31</ref>. 
</sense> <sense n="2" xml:id="D2"> <cit type="translation" xml:lang="la"><quote> zelare </quote></cit>, <cit type="translation" xml:lang="la"><quote> ardenti studio </quote></cit> vel <cit type="translation" xml:lang="la"><quote> ira flagrare </quote></cit>: <foreign xml:lang="gez">ቀንኡ፡ ቅንአተከ፡</foreign> <ref cRef="Judith" loc="9,4">Judith 9,4</ref>; <foreign xml:lang="gez">ከመ፡ ይቅንኡ፡ ተረፍ፡</foreign> <ref cRef="Jes." loc="11,11">Jes. 11,11</ref>; <ref cRef="Deut." loc="32,19">Deut. 32,19</ref>. <sense n="a" xml:id="D2a"> <lbl expand="cum">c.</lbl> <foreign xml:lang="gez">ለ፡</foreign> ejus, cujus causam quis defendit: <ref cRef="Num." loc="11,29">Num. 11,29</ref>; <ref cRef="Num." loc="25,11">Num. 25,11</ref>; <ref cRef="Num." loc="25,13">Num. 25,13</ref>; <ref cRef="3 Reg." loc="19,10">3 Reg. 19,10</ref>; <foreign xml:lang="gez">ቀናእኩ፡ ለሠናይ፡</foreign> <ref cRef="Sir." loc="51,18">Sir. 51,18</ref>; <foreign xml:lang="gez">ቀንአ፡ ለፍርሀተ፡ እግዚአብሔር፡</foreign> <ref cRef="Sir." loc="45,23">Sir. 45,23</ref>; <ref cRef="Rom." loc="10,2">Rom. 10,2</ref> Platt; <ref cRef="2 Cor." loc="11,2">2 Cor. 11,2</ref>; rarius <lbl expand="cum">c.</lbl> <foreign xml:lang="gez">ላዕለ፡</foreign>, ut <ref cRef="Rom." loc="10,2">Rom. 10,2</ref>; <foreign xml:lang="gez">ቀናእኩ፡ ላዕለ፡ ኢየሩሳሌም፡ ዐቢየ፡ ቅንአተ፡</foreign> <ref cRef="Zach." loc="1,14">Zach. 1,14</ref>; <foreign xml:lang="gez">ቀንአ፡ ላዕሌሃ፡ ንጉሥ፡ ወፈቀደ፡ ያውስባ፡</foreign> Vit. Dan.; vel <lbl expand="cum">c.</lbl> <gramGrp> <case value="accusativus">Acc.</case> </gramGrp> <lbl expand="persona">pers.</lbl>: <foreign xml:lang="gez">ከመ፡ ይቅናእ፡ ተረፈ፡</foreign> <ref cRef="Jes." loc="11,11">Jes. 11,11</ref> <lbl expand="varia lectio">var.</lbl> 
</sense> <sense n="b" xml:id="D2b"> <lbl expand="cum">c.</lbl> <foreign xml:lang="gez">ላዕለ፡</foreign> ejus, contra quem quis ira flagrat <ref cRef="Num." loc="25,11">Num. 25,11</ref>. 
</sense> <sense n="c" xml:id="D2c"> <lbl expand="sequens">seq.</lbl> <foreign xml:lang="gez">ለ፡</foreign> <lbl expand="cum">c.</lbl> Infin.: <foreign xml:lang="gez">ኢትቅንኡ፡ ለመዊት፡</foreign> <ref cRef="Sap." loc="1,12">Sap. 1,12</ref>; vel <foreign xml:lang="gez">ለ፡</foreign> <lbl expand="cum">c.</lbl> <gramGrp> <pos expand="abstractum, -e">abstr.</pos> </gramGrp>, ut: <foreign xml:lang="gez">ቅናእ፡ ለንስሓ፡</foreign> <ref cRef="Apoc." loc="3,19">Apoc. 3,19</ref>; <lbl expand="sequens">seq.</lbl> <foreign xml:lang="gez">ከመ፡</foreign> <lbl expand="cum">c.</lbl> <gramGrp> <pos expand="Subjunctivus">Subj.</pos> </gramGrp>, <ref cRef="Kuf. " loc="30.">Kuf.  30.</ref> 
</sense> 
</sense> <sense n="3" xml:id="D3"> <cit type="translation" xml:lang="la"><quote> aemulari </quote></cit>: <foreign xml:lang="gez">ትቀንኡ፡ ለገቢረ፡ ሠናይ፡</foreign> <foreign xml:lang="grc">μιμηταί</foreign> <ref cRef="1 Petr." loc="3,13">1 Petr. 3,13</ref>; <foreign xml:lang="gez">ተመየጥ፡ ኀበ፡ ቃህም፡ ወቅናእ፡ ርእየከ፡ ፍናዊሁ፡</foreign> <ref cRef="Prov." loc="6,6">Prov. 6,6</ref>; <ref cRef="Rom." loc="11,11">Rom. 11,11</ref>; <ref cRef="Gal." loc="4,18">Gal. 4,18</ref> Platt. 
</sense> </sense>
               
            <sense xml:id="Tmain" xml:lang="en" source="#traces"> <cit type="transcription" xml:lang="gez"><quote>qanʾa</quote></cit> <sense n="G" xml:id="TG"> <foreign xml:lang="gez">ቀንአ፡</foreign> <cit type="transcription" xml:lang="gez"><quote>qanʾa</quote></cit> <subc>I,1</subc> <foreign xml:lang="gez">ይቀንእ፡</foreign>, <foreign xml:lang="gez">ይቅናዕ</foreign> {sic} <cit type="translation" xml:lang="la"><quote>zelo, individia, amore ductus fuit</quote></cit>, <cit type="translation" xml:lang="fr"><quote>être poussé par la jalousie, l’envie, l’amour</quote></cit> ― <foreign xml:lang="am">ቀና፡</foreign> Ms. Roma, Biblioteca Nazionale Centrale di Roma, Orientale 134, p. 60. <lb/>
 <bibl><ptr target="bm:Grebaut1952SuppLexicon"/><citedRange unit="page"> 177</citedRange></bibl> 
</sense> <sense n="L" xml:id="TL"> <foreign xml:lang="gez">ቀንአ</foreign> <cit type="transcription" xml:lang="gez"><quote>qanʾa</quote></cit> (yǝqnāʾ) <cit type="translation" xml:lang="en"><quote>be envious</quote></cit>, <cit type="translation" xml:lang="en"><quote>be jealous</quote></cit>, <cit type="translation" xml:lang="en"><quote>be zealous</quote></cit>, <cit type="translation" xml:lang="en"><quote>be eager</quote></cit>, <cit type="translation" xml:lang="en"><quote>emulate</quote></cit>, <cit type="translation" xml:lang="en"><quote>imitate</quote></cit> <bibl><ptr target="bm:Leslau1987dic"/><citedRange unit="page"> 433b</citedRange></bibl> 
</sense> </sense></entry>
