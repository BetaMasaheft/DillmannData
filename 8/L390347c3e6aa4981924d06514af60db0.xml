            <entry xml:id="L390347c3e6aa4981924d06514af60db0" n="6767">
               <form>
                  <foreign xml:lang="gez">ወደየ</foreign>
               <rs type="root"/>
                    </form>
               <sense xml:lang="la" source="#dillmann" xml:id="Dmain"> <subc>I,1</subc> [<foreign xml:lang="he">יָדָה</foreign> et <foreign xml:lang="he">יָדַד</foreign> <cit type="translation" xml:lang="la">
                            <quote> jecit </quote>
                        </cit>; vicinum est <foreign xml:lang="gez">ወደደ፡</foreign>; <lbl expand="videas">vid.</lbl> in Ges. thes. p. 565] <gramGrp> <pos expand="Subjunctivus">Subj.</pos> </gramGrp> <foreign xml:lang="gez">ይደይ፡</foreign> (raro <foreign xml:lang="gez">ይውዲ፡</foreign> ut <ref cRef="Sx. Tachs." loc="25 ">Sx. Tachs. 25 </ref>Enc. ob rhythmum finalem), Imperat. <foreign xml:lang="gez">ደይ፡</foreign> (quamquam etiam formae <foreign xml:lang="gez">ውዲ፡</foreign> <ref cRef="Genz. " loc="157">Genz.  f. 157</ref>; <ref cRef="Sx. Ter" loc="27,">Sx. Ter 27,</ref> et <foreign xml:lang="gez">ውደይ፡</foreign> <ref cRef="Herm." loc="81 ">Herm. p. 81 </ref>occurrunt), <sense n="1" xml:id="D1"> <cit type="translation" xml:lang="la">
                                <quote> jacere </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> projicere </quote>
                            </cit>: <foreign xml:lang="gez">ኢትደዩ፡ ባሕርየክሙ፡ ቅድመ፡ አሕርው፡</foreign> <ref cRef="Matth." loc="7,6">Matth. 7,6</ref>; <foreign xml:lang="gez">ኢትደይ፡ ዕቅፍተ፡ ቅድመ፡ ዕዉር፡</foreign> <ref cRef="Lev. " loc="19,17">Lev.  19,17</ref>.</sense> <sense n="2" xml:id="D2"> <cit type="translation" xml:lang="la">
                                <quote> injicere </quote>
                            </cit>, <foreign xml:lang="grc">ἐμβάλλειν</foreign>, <foreign xml:lang="grc">ἐπιβάλλειν</foreign>, <ref cRef="Matth." loc="10,34">Matth. 10,34</ref>; <ref cRef="Hen." loc="10,4">Hen. 10,4</ref>; <ref cRef="Hen." loc="10,5">Hen. 10,5</ref>; <ref cRef="Hen. " loc="54,2">Hen.  54,2</ref>; <ref cRef="Hen." loc="54,5">Hen. 54,5</ref>; <ref cRef="Hen." loc="54,6">Hen. 54,6</ref>; <foreign xml:lang="gez">ወደይዎ፡ ውስተ፡ ግበ፡ ዐናብስት፡</foreign> <ref cRef="Dan. apocr." loc="13,31">Dan. apocr. 13,31</ref>; <ref cRef="Jer." loc="20,2">Jer. 20,2</ref>; <ref cRef="Gen." loc="40,2">Gen. 40,2</ref>; <ref cRef="Kuf." loc="39">Kuf. 39</ref>; <ref cRef="Luc." loc="12,5">Luc. 12,5</ref>; <foreign xml:lang="gez">ወደዮ፡ ቤተ፡ ሞቅሕ፡</foreign> <ref cRef="Kuf." loc="39">Kuf. 39</ref>; <foreign xml:lang="gez">ንደይ፡ ዕፀ፡ ውስተ፡ ኅብስቱ፡</foreign> <ref cRef="Jer." loc="11,19">Jer. 11,19</ref>; <foreign xml:lang="gez">ልጓመ፡</foreign> seq. <foreign xml:lang="gez">ለ፡</foreign> vel <foreign xml:lang="gez">ውስተ፡</foreign> <ref cRef="Jac." loc="3,2">Jac. 3,2</ref>; <ref cRef="Jac." loc="3,3">Jac. 3,3</ref>; manum alicui, ut: <foreign xml:lang="gez">ኢትደይ፡ እዴከ፡ ላዕለ፡ ወልድ፡</foreign> <ref cRef="Gen." loc="22,12">Gen. 22,12</ref>; oculos (<cit type="translation" xml:lang="la">
                                <quote> conjicere </quote>
                            </cit> in aliquem): <foreign xml:lang="gez">ወደየት፡ አዕይንቲሃ፡ ላዕለ፡ ዮሴፍ፡</foreign> <ref cRef="Gen." loc="39,7">Gen. 39,7</ref>; ignem: <foreign xml:lang="gez">ዘወደየ፡ እሳተ፡ ውስተ፡ ሀገር፡</foreign> F.N. 50,6; sommum alicui <lbl expand="cum">c.</lbl> <foreign xml:lang="gez">ላዕለ፡</foreign> <ref cRef="Kuf." loc="3">Kuf. 3</ref>; <foreign xml:lang="gez">መርበብተ፡ ውስተ፡ ባሕር፡</foreign> <ref cRef="Matth." loc="4,18">Matth. 4,18</ref>.</sense> <sense n="3" xml:id="D3"> <cit type="translation" xml:lang="la">
                                <quote> immittere </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> indere </quote>
                            </cit>: <foreign xml:lang="gez">ደይ፡ እዴከ፡ ውስተ፡ ሕፅንከ፡</foreign> <ref cRef="Ex." loc="4,6">Ex. 4,6</ref>; <ref cRef="Ex." loc="4,7">Ex. 4,7</ref>; <foreign xml:lang="gez">ወደዮን፡ ውስጠ፡</foreign> <ref cRef="Ex." loc="40,18">Ex. 40,18</ref>; <ref cRef="Ex." loc="25,21">Ex. 25,21</ref>; <ref cRef="Num." loc="4,10">Num. 4,10</ref>; <foreign xml:lang="gez">ውስተ፡ ሙዳይ፡ ኢትወዲ፡</foreign> <ref cRef="Deut." loc="23,26">Deut. 23,26</ref>; <ref cRef="Jer." loc="39,14">Jer. 39,14</ref>; <ref cRef="4 Esr." loc="9,37">4 Esr. 9,37</ref>; <ref cRef="Matth." loc="27,6">Matth. 27,6</ref>; <ref cRef="Marc." loc="7,33">Marc. 7,33</ref>; <ref cRef="Joh." loc="20,25">Joh. 20,25</ref>; <foreign xml:lang="gez">ወደዩ፡ እብነ፡ ውስተ፡ ንድቅ፡</foreign> <ref cRef="Herm." loc="9">Herm. p. 9</ref>; <foreign xml:lang="gez">ይወዲ፡ እዴሁ፡ ውስተ፡ ግበ፡ አርዌ፡ ምድር፡</foreign> <ref cRef="Jes." loc="11,8">Jes. 11,8</ref>; <foreign xml:lang="gez">እወዲ፡ ዝማማተ፡ ውስተ፡ አንፍከ፡</foreign> <ref cRef="Jes." loc="37,29">Jes. 37,29</ref>; <foreign xml:lang="gez">ዘይወዲ፡ ርእሶ፡ ታሕተ፡ አርዑት፡</foreign> <ref cRef="Jer." loc="34,9">Jer. 34,9</ref>; <ref cRef="Hen." loc="48,9">Hen. 48,9</ref>; <ref cRef="Hen." loc="78,13">Hen. 78,13</ref>; <ref cRef="2 Esr." loc="13,3">2 Esr. 13,3</ref> (<lbl expand="videas">vid.</lbl> <foreign xml:lang="gez">ወደደ፡</foreign>). <sense n="a" xml:id="D3a"> de liquidis: <cit type="translation" xml:lang="la">
                                    <quote> infundere </quote>
                                </cit>, <cit type="translation" xml:lang="la">
                                    <quote> inspergere </quote>
                                </cit>, ut: <foreign xml:lang="gez">ውዲ፡ ማየ፡ ጸሎት፡ ዲበ፡ መቃብሩ፡</foreign> <ref cRef="Genz." loc="157">Genz. f. 157</ref>; <foreign xml:lang="gez">ወደዮ፡</foreign> (<foreign xml:lang="gez">ለወይን፡</foreign>) <foreign xml:lang="gez">ውስተ፡ ንዋይ፡</foreign> <ref cRef="Kuf." loc="7">Kuf. 7</ref>; <ref cRef="Matth." loc="9,17">Matth. 9,17</ref>; <foreign xml:lang="gez">ወይነ፡ ኂሩትከ፡ ውዲ፡ ወቶስሕ፡ ውስቴቱ፡</foreign> <ref cRef="Sx. Ter" loc="27">Sx. Ter 27</ref>; <foreign xml:lang="gez">ወዲየ፡ ወይን፡ ውስተ፡ ጽዋዐ፡ ማህው፡</foreign> <ref cRef="Org." loc="2.">Org. 2.</ref> </sense> <sense n="b" xml:id="D3b"> <lbl expand="translate">transl.</lbl>: <sense n="α" xml:id="D3bα"> aliquid in animum alicujus, ut: <ref cRef="Ps." loc="4,8">Ps. 4,8</ref>; <foreign xml:lang="gez">ወደየ፡ ውስተ፡ ልቦሙ፡ ፍርሀተ፡</foreign> <ref cRef="Sir." loc="17,8">Sir. 17,8</ref>; <foreign xml:lang="gez">እወዲ፡ ቃልየ፡ ውስተ፡ አፉከ፡</foreign> <ref cRef="Jes." loc="51,16">Jes. 51,16</ref>; <ref cRef="Jes. " loc="59,21">Jes.  59,21</ref>; <ref cRef="Jer." loc="1,9">Jer. 1,9</ref>; <foreign xml:lang="gez">እወዲ፡ ሕግየ፡ ውስተ፡ ልቦሙ፡</foreign> <ref cRef="Jer. " loc="38,33">Jer.  38,33</ref>.</sense> <sense n="β" xml:id="D3bβ"> <cit type="translation" xml:lang="la">
                                        <quote> subjicere </quote>
                                    </cit>, <cit type="translation" xml:lang="la">
                                        <quote> suggerere </quote>
                                    </cit> alicui aliquid: <foreign xml:lang="gez">ወደየ፡ እግዚአብሔር፡ ውስተ፡ ልብየ፡</foreign> <ref cRef="2 Esr." loc="17,5">2 Esr. 17,5</ref>; <foreign xml:lang="gez">ወደየ፡ ሰይጣን፡ ውስተ፡ ልቡ፡ ወይቤ፡</foreign> <ref cRef="Sx. Jac." loc="5">Sx. Jac. 5</ref>; <foreign xml:lang="gez">ወደየ፡ ውስተ፡ አፉሁ፡ ንባበ፡</foreign> <ref cRef="Kuf." loc="8.">Kuf. 8.</ref> </sense> <sense n="γ" xml:id="D3bγ"> immittere aliquid in animum suum i.e. <cit type="translation" xml:lang="la">
                                        <quote> mandare animo </quote>
                                    </cit>, <cit type="translation" xml:lang="la">
                                        <quote> menti </quote>
                                    </cit>: <foreign xml:lang="gez">ደይ፡ ቃሎ፡ ውስተ፡ ልብከ፡</foreign> <ref cRef="Job " loc="22,22">Job  22,22</ref>; <foreign xml:lang="gez">ኢትደዮ፡ ውስተ፡ ልብከ፡</foreign> <ref cRef="Sir." loc="31,6">Sir. 31,6</ref>; <ref cRef="Prov. " loc="6,21">Prov.  6,21</ref>.</sense> </sense> <sense n="c" xml:id="D3c"> radices i.e. <cit type="translation" xml:lang="la">
                                    <quote> agere </quote>
                                </cit>, <cb n="0934" xml:id="c0934"/> <foreign xml:lang="gez">ወደየ፡ ሥርወ፡</foreign> <ref cRef="4 Esr." loc="8,47">4 Esr. 8,47</ref>.</sense> </sense> <sense n="4" xml:id="D4"> <cit type="translation" xml:lang="la">
                                <quote> imponere </quote>
                            </cit> aliquid rei, <lbl expand="sequens">seq.</lbl> <gramGrp> <case value="accusativus">Acc.</case> </gramGrp> et <foreign xml:lang="gez">ዲበ፡</foreign> vel <foreign xml:lang="gez">ላዕለ፡</foreign> vel <foreign xml:lang="gez">ውስተ፡</foreign> rei, ut: <foreign xml:lang="gez">ወይወዲ፡ ዲቤሁ፡ ስኂነ፡</foreign> <ref cRef="Lev." loc="2,1">Lev. 2,1</ref>; <ref cRef="Lev." loc="2,15">Lev. 2,15</ref>; <ref cRef="Kuf." loc="6">Kuf. 6</ref>; <foreign xml:lang="gez">ወይወድዩ፡ እሳተ፡ ላዕለ፡ ምሥዋዕ፡</foreign> <ref cRef="Lev." loc="1,7">Lev. 1,7</ref>; <ref cRef="Num." loc="16,18">Num. 16,18</ref>; <ref cRef="Lev." loc="8,7">Lev. 8,7</ref>; <foreign xml:lang="gez">እወዲ፡ እዴየ፡ ዲበ፡ አፉየ፡</foreign> <ref cRef="Job" loc="39,34">Job 39,34</ref>; <ref cRef="Marc. " loc="5,23">Marc.  5,23</ref>; <ref cRef="Hen." loc="67,2">Hen. 67,2</ref>; <foreign xml:lang="gez">እመ፡ ወደይከ፡ ማዕጾ፡ ወመንሥገ፡ ውስተ፡</foreign> (v. <foreign xml:lang="gez">ላዕለ፡</foreign>) <foreign xml:lang="gez">አፉከ፡</foreign> <ref cRef="Sir." loc="28,25">Sir. 28,25</ref>; vel <lbl expand="cum">c.</lbl> <foreign xml:lang="gez">ለ፡</foreign>, ut: <foreign xml:lang="gez">ወደየ፡ ለከናፍሪሁ፡ ማዕጾ፡ እብን፡</foreign> <ref cRef="Sx. Masc." loc="16.">Sx. Masc. 16.</ref> <sense n="a" xml:id="D4a"> de rebus, quae sparguntur et funduntur: <cit type="translation" xml:lang="la">
                                    <quote> spargere in </quote>
                                </cit> vel <cit type="translation" xml:lang="la">
                                    <quote> super </quote>
                                </cit> aliquid, <cit type="translation" xml:lang="la">
                                    <quote> allinere </quote>
                                </cit>: <foreign xml:lang="gez">ወደየት፡ ሐመደ፡ ውስተ፡ ርእሳ፡</foreign> <ref cRef="Judith" loc="9,1">Judith 9,1</ref>; <ref cRef="Jer." loc="6,26">Jer. 6,26</ref>, vel <foreign xml:lang="gez">ዲበ፡ ርእስ፡</foreign> <ref cRef="Esth." loc="4,1">Esth. 4,1</ref>; <ref cRef="Job" loc="2,12">Job 2,12</ref>; <foreign xml:lang="gez">ትወዲ፡ ደመ፡ ውስተ፡ አቅርንት፡</foreign> <ref cRef="Ex." loc="29,12">Ex. 29,12</ref>; <ref cRef="Lev." loc="4,7">Lev. 4,7</ref>; <ref cRef="Lev. " loc="4,18">Lev.  4,18</ref>; <ref cRef="Lev." loc="14,14">Lev. 14,14</ref>.</sense> <sense n="b" xml:id="D4b"> de tegumentis capitis, annulis et similibus rebus: <cit type="translation" xml:lang="la">
                                    <quote> imponere </quote>
                                </cit>, <cit type="translation" xml:lang="la">
                                    <quote> inducere </quote>
                                </cit>, <cit type="translation" xml:lang="la">
                                    <quote> induere </quote>
                                </cit> (<note>aufsezen</note>, <note>anlegen</note>): <foreign xml:lang="gez">ሕልቀተ፡ ውስተ፡ እድ፡</foreign> <ref cRef="Gen." loc="41,42">Gen. 41,42</ref>; <ref cRef="Hez." loc="23,42">Hez. 23,42</ref>; <foreign xml:lang="gez">ወደየ፡ ጌራ፡ መድኀኒት፡ ውስተ፡ ርእሱ፡</foreign> <ref cRef="Jes." loc="59,17">Jes. 59,17</ref>; <ref cRef="1 Reg." loc="17,38">1 Reg. 17,38</ref>; <foreign xml:lang="gez">ደዩ፡ ሰበነ፡ ንጹሐ፡ ዲበ፡ ርእሱ፡</foreign> <ref cRef="Zach. " loc="3,5">Zach.  3,5</ref>; <ref cRef="Ex." loc="29,6">Ex. 29,6</ref>. – <lbl expand="Vocabularium Aethiopicum, -a -a">Voc. Ae.</lbl>: <foreign xml:lang="gez">ወደየ፡ ዘ፡ ዶለ፡</foreign> </sense> </sense> </sense>
               
            </entry>
         </div>
