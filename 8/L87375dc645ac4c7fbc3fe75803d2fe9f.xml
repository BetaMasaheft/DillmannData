            <entry xml:id="L87375dc645ac4c7fbc3fe75803d2fe9f" n="6934">
               <form>
                  <foreign xml:lang="gez">ማዕሌት</foreign>
               </form>
               <sense xml:lang="la" source="#dillmann" xml:id="Dmain">
                  <gramGrp>
                     <pos expand="Substantivum">subst.</pos>
                  </gramGrp>, <gramGrp>
                     <gen value="femininus">fem.</gen>
                  </gramGrp> et <gramGrp>
                     <gen value="masculinum">m.</gen>
                  </gramGrp>, <sense n="1" xml:id="D1">
                     <cit type="translation" xml:lang="la">
                        <quote>defectio</quote>
                     </cit> a Deo vel lege ejus: <foreign xml:lang="gez">ዐለዉ፡ ማዕሌተ፡ እኩየ፡ </foreign>
                     <ref cRef="Jer." loc="8,5">Jer. 8,5</ref>; <foreign xml:lang="grc">ἀθέτημα</foreign>
                     <ref cRef="Dan." loc="12,1">Dan. 12,1</ref>; <foreign xml:lang="gez">በማዕሌቶሙ፡
                        ዘዐለዉከ፡</foreign>
                     <foreign xml:lang="grc">ἀθεσία </foreign>
                     <ref cRef="Dan." loc="9,7">Dan. 9,7</ref>.</sense>
                  <sense n="2" xml:id="D2">
                     <cit type="translation" xml:lang="la">
                        <quote>rei publicae conversio</quote>
                     </cit>,<cit type="translation" xml:lang="la">
                        <quote> rebellio</quote>
                     </cit>,<cit type="translation" xml:lang="la">
                        <quote> seditio</quote>
                     </cit>,<cit type="translation" xml:lang="la">
                        <quote> conspiratio</quote>
                     </cit>: <foreign xml:lang="gez">ማዕሌተ፡ ሀገር፡</foreign>
                     <foreign xml:lang="grc">διαβολὴ πόλεως</foreign>
                     <ref cRef="Sir." loc="26,5">Sir. 26,5</ref>; <foreign xml:lang="gez">ሀገረ፡ ማዕሌት፡ </foreign>
                     <foreign xml:lang="grc">πόλις ἀποστάτις</foreign>
                     <cb n="0955" xml:id="c0955"/>
                     <ref cRef="2 Esr. " loc="4,15">2 Esr. 4,15</ref>; <foreign xml:lang="gez">መልአከ፡
                        ማዕሌት፡</foreign>
                     <foreign xml:lang="grc">συστρέμματος</foreign>
                     <ref cRef="3 Reg." loc="11,14">3 Reg. 11,14</ref>; <ref cRef="Num." loc="32,14">Num. 32,14</ref>; <foreign xml:lang="grc">συστροφή</foreign>
                     <ref cRef="Ps." loc="63,2">Ps. 63,2</ref>; <ref cRef="4 Reg." loc="15,15">4
                        Reg. 15,15</ref>; <ref cRef="Act." loc="23,12">Act. 23,12</ref>
                     <lbl expand="romanae editionis">rom.</lbl>; <foreign xml:lang="grc">σύνδεσμος</foreign>
                     <ref cRef="4 Reg." loc="11,14">4 Reg. 11,14</ref>; <ref cRef="Jer." loc="11,9">Jer. 11,9</ref>
                     <lbl expand="versio altera">vers. alt.</lbl>; <foreign xml:lang="gez">ከመ፡ ይኅበር፡
                        ማዕሌቶሙ፡</foreign>
                     <ref cRef="Hos." loc="7,2">Hos. 7,2</ref>; <ref cRef="1 Reg." loc="22,13">1
                        Reg. 22,13</ref>; <foreign xml:lang="gez">ማዕሌተ፡ ይገብር፡ ላዕሌከ፡ አሞጽ፡</foreign>
                     <ref cRef="Am." loc="7,11">Am. 7,11</ref>; <ref cRef="Hez." loc="17,15">Hez.
                        17,15</ref>; <ref cRef="2 Esr. " loc="16,6">2 Esr. 16,6</ref>; <ref cRef="2 Par." loc="25,27">2 Par. 25,27</ref>. – <lbl expand="Vocabularium Aethiopicum, -a -a.">Voc. Ae.</lbl>: <foreign xml:lang="gez">ማዕሌት፡ ዘ፡ መንጂያ።</foreign>
                  </sense>
               </sense>
               
