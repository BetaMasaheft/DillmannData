               <sense xml:id="Dmain" xml:lang="la" source="#dillmann"> <subc>II,2</subc> <cit type="translation" xml:lang="la"><quote> recedere facere </quote></cit>, <cit type="translation" xml:lang="la"><quote> removere </quote></cit>, <cit type="translation" xml:lang="la"><quote> amovere </quote></cit>, <cit type="translation" xml:lang="la"><quote> amoliri </quote></cit> (<cit type="translation" xml:lang="la"><quote> auferre </quote></cit>, <cit type="translation" xml:lang="la"><quote> exuere </quote></cit>, <cit type="translation" xml:lang="la"><quote> deponere </quote></cit>, <cit type="translation" xml:lang="la"><quote> avertere </quote></cit>, <cit type="translation" xml:lang="la"><quote> expellere </quote></cit>), <lbl expand="cum">c.</lbl> <gramGrp> <case value="accusativus">Acc.</case> </gramGrp> et <foreign xml:lang="gez">እምነ፡</foreign>; <foreign xml:lang="gez">ይቤ፡ ያሰስሉ፡ ኵሎ፡ ሰብአ፡ እምቅድሜየ፡</foreign> <ref cRef="Gen." loc="45,1">Gen. 45,1</ref>; lapidem a puteo <ref cRef="Gen. " loc="29,3">Gen.  29,3</ref>; corticem virgae 30,37; adipem victimae <ref cRef="Lev." loc="4,8">Lev. 4,8</ref>; <ref cRef="Lev." loc="4,19">Lev. 4,19</ref>; cineres <ref cRef="Lev." loc="6,3">Lev. 6,3</ref>; idola <ref cRef="1 Reg." loc="7,3">1 Reg. 7,3</ref>; 1 reg. 7,4; ornamenta <ref cRef="Ex." loc="33,5">Ex. 33,5</ref>; <ref cRef="Ex." loc="33,6">Ex. 33,6</ref>; vestes ab aliquo <ref cRef="1 Reg. " loc="17,39">1 Reg.  17,39</ref>; <ref cRef="Deut." loc="21,13">Deut. 21,13</ref>; vestes a se (deponendo vel exuendo) <ref cRef="1 Reg." loc="19,24">1 Reg. 19,24</ref>; <cb n="0350" xml:id="c0350"/> <ref cRef="Lev. " loc="6,4">Lev.  6,4</ref>; <foreign xml:lang="gez">አሰሰለት፡ ሠቀ፡</foreign> <ref cRef="Judith" loc="10,3">Judith 10,3</ref>; altare <ref cRef="4 Reg." loc="16,14">4 Reg. 16,14</ref>; <foreign xml:lang="gez">ኢያሰሰልከ፡ ዐምደ፡ ደመና፡ እምኔሆሙ፡</foreign> <ref cRef="2 Esr." loc="19,19">2 Esr. 19,19</ref>; tristitiam a se <ref cRef="4 Esr." loc="10,20">4 Esr. 10,20</ref>; <ref cRef="4 Esr." loc="10,37">4 Esr. 10,37</ref>; concupiscentiam ab aliquo: <foreign xml:lang="gez">አሰስል፡ ፍትወተ፡ እምኔየ፡</foreign> <ref cRef="Sir." loc="23,5">Sir. 23,5</ref>; morbos: <foreign xml:lang="gez">ያሴስል፡ ኵሎ፡ ሕማሞሙ፡</foreign> <ref cRef="Sir." loc="38,7">Sir. 38,7</ref>; peccata (<foreign xml:lang="grc">αἵρειν</foreign>) <ref cRef="1 Joh. " loc="3,5">1 Joh.  3,5</ref>; <ref cRef="Joh." loc="1,29">Joh. 1,29</ref>; lacrymas <ref cRef="Apoc." loc="7,17">Apoc. 7,17</ref>; <ref cRef="Apoc." loc="21,4">Apoc. 21,4</ref>; verba e libro <ref cRef="Apoc." loc="22,19">Apoc. 22,19</ref>; iram a se: <foreign xml:lang="gez">አሰስላ፡ ለመዐት፡</foreign> <ref cRef="Sir." loc="28,7">Sir. 28,7</ref>; malitiam a se (<foreign xml:lang="grc">ἀποθέμενοι</foreign>) <ref cRef="1 Petr." loc="2,1">1 Petr. 2,1</ref>; jugum (excutere): <foreign xml:lang="gez">እመ፡ አሰሰልከ፡ አርዑቶ፡ እምክሣድከ፡</foreign> <ref cRef="Kuf." loc="26">Kuf. 26</ref>; homines, populos (depellendo, expellendo): <foreign xml:lang="gez">አሰሰሎሙ፡</foreign> <foreign xml:lang="grc">ἐξῆρεν αὐτούς</foreign> <ref cRef="Sir." loc="10,17">Sir. 10,17</ref>; <foreign xml:lang="gez">አሴስሎሙ፡ እማእከለ፡ ምድር፡</foreign> <ref cRef="Kuf." loc="1">Kuf. 1</ref>; <foreign xml:lang="gez">ከመ፡ ያሰስሎሙ፡ እምታሕተ፡ ምድር፡</foreign> <ref cRef="Kuf." loc="5">Kuf. 5</ref>; <foreign xml:lang="gez">ለአሰስሎ፡</foreign> <ref cRef="Hen." loc="1,1">Hen. 1,1</ref>. </sense>
               
