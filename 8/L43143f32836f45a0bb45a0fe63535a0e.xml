            <entry xml:id="L43143f32836f45a0bb45a0fe63535a0e" n="7159">
               <form>
                  <foreign xml:lang="gez">አዕበየ</foreign>
               </form>
               <sense xml:id="Dmain" xml:lang="la" source="#dillmann"> <subc>II,1</subc> <sense n="1" xml:id="D1"> <cit type="translation" xml:lang="la"><quote> magnum facere </quote></cit>, <cit type="translation" xml:lang="la"><quote> augere </quote></cit>, <ref cRef="Matth." loc="23,5">Matth. 23,5</ref>; <ref cRef="Luc." loc="1,58">Luc. 1,58</ref>; <ref cRef="Gen." loc="12,2">Gen. 12,2</ref>; <ref cRef="Gen." loc="19,19">Gen. 19,19</ref>; <foreign xml:lang="gez">አዕበይኩ፡ ጥበበ፡</foreign> <ref cRef="Koh." loc="1,16">Koh. 1,16</ref>; <foreign xml:lang="gez">ረሲዓን፡ ያዐብዩ፡ ኀሳረ፡</foreign> <ref cRef="Prov." loc="3,35">Prov. 3,35</ref>; <foreign xml:lang="gez">ያዐብያ፡ ለመዐቱ፡</foreign> <ref cRef="Sir." loc="28,10">Sir. 28,10</ref>; <ref cRef="Ps." loc="17,54">Ps. 17,54</ref>; <ref cRef="2 Tim." loc="2,16">2 Tim. 2,16</ref>. <sense n="a" xml:id="D1a"> <lbl expand="sequens">seq.</lbl> <foreign xml:lang="gez">ገቢረ፡</foreign> <cit type="translation" xml:lang="la"><quote> magna facinora facere </quote></cit> <ref cRef="Ps." loc="125,3">Ps. 125,3</ref>; <ref cRef="Ps. " loc="125,4">Ps.  125,4</ref>; vel <foreign xml:lang="gez">አዕበየ፡ ከመ፡ ይግበር፡</foreign> <ref cRef="Joel" loc="2,21">Joel 2,21</ref>; vel <gramGrp> <pos expand="absolute">absol.</pos> </gramGrp>: <ref cRef="1 Reg." loc="12,24">1 Reg. 12,24</ref> vel <foreign xml:lang="gez">አዕበየ፡ ምግባረ፡</foreign> <cit type="translation" xml:lang="la"><quote> magna opera facere </quote></cit> <cb n="0986" xml:id="c0986"/> <ref cRef="Koh." loc="2,4">Koh. 2,4</ref>; etiam omisso <foreign xml:lang="gez">ምግባረ፡</foreign> <ref cRef="Koh." loc="2,9">Koh. 2,9</ref>.
</sense> <sense n="b" xml:id="D1b"> magnum i.e. <cit type="translation" xml:lang="la"><quote> opulentum </quote></cit>, <cit type="translation" xml:lang="la"><quote> potentem reddere </quote></cit>: <ref cRef="Gen." loc="24,35">Gen. 24,35</ref>; <ref cRef="Gen." loc="28,3">Gen. 28,3</ref>; <foreign xml:lang="gez">ያዐብዮ፡ ወያከብሮ፡</foreign> <ref cRef="Sir." loc="11,13">Sir. 11,13</ref>.
</sense> <sense n="c" xml:id="D1c"> <cit type="translation" xml:lang="la"><quote> magnificare </quote></cit>, <cit type="translation" xml:lang="la"><quote> amplificare </quote></cit>: <ref cRef="Jos." loc="3,7">Jos. 3,7</ref>; <ref cRef="Jos." loc="4,14">Jos. 4,14</ref>; <ref cRef="Ps." loc="36,36">Ps. 36,36</ref>; <foreign xml:lang="gez">ወአዕበዮ፡ እግዚአብሔር፡ ለሰሎሞን፡ ላዕለ፡ ኵሉ፡</foreign> <ref cRef="1 Par." loc="29,25">1 Par. 29,25</ref>; <ref cRef="Jer. " loc="38,14">Jer.  38,14</ref> (<lbl expand="videas">vid.</lbl> n. 2,a).
</sense> <sense n="d" xml:id="D1d"> <cit type="translation" xml:lang="la"><quote> magnifacere </quote></cit>: <ref cRef="Job" loc="7,17">Job 7,17</ref>; <ref cRef="Act." loc="5,13">Act. 5,13</ref>.
</sense> <sense n="e" xml:id="D1e"> vocem, clamorem <cit type="translation" xml:lang="la"><quote> vehementem facere </quote></cit>: <foreign xml:lang="gez">አዕበዩ፡ ጸሪኀ፡</foreign> <ref cRef="Matth." loc="20,31">Matth. 20,31</ref>.
</sense> <sense n="f" xml:id="D1f"> <cit type="translation" xml:lang="la"><quote> natu majorem facere </quote></cit> vel <cit type="translation" xml:lang="la"><quote> declarare </quote></cit>: <foreign xml:lang="gez">ኢይክል፡ አዕብዮቶ፡</foreign> <ref cRef="Deut." loc="21,16">Deut. 21,16</ref>.
</sense> <sense n="g" xml:id="D1g"> <foreign xml:lang="gez">አፈ፡</foreign> <cit type="translation" xml:lang="la"><quote> os </quote></cit> i.e. <cit type="translation" xml:lang="la"><quote> magna loqui </quote></cit>(<cit type="translation" xml:lang="la"><quote> insolenter jactare </quote></cit>), <foreign xml:lang="grc">μεγαλοῤῥημονεῖν</foreign>: <ref cRef="Ps." loc="37,17">Ps. 37,17</ref>; <ref cRef="Ps." loc="54,13">Ps. 54,13</ref>; <foreign xml:lang="gez">መጠነ፡ አዕበየ፡ አፉሁ፡</foreign> <ref cRef="Judith" loc="6,17">Judith 6,17</ref>; <foreign xml:lang="gez">እመ፡ አዕቤከ፡ አፉከ፡ ላዕሌሁ፡</foreign> <ref cRef="Sir." loc="22,22">Sir. 22,22</ref>; <ref cRef="Sir." loc="20,15">Sir. 20,15</ref>, vel <lbl expand="sequens">seq.</lbl> <foreign xml:lang="gez">ነቢበ፡</foreign> <ref cRef="Ps." loc="11,3">Ps. 11,3</ref>; <ref cRef="Ps." loc="11,4">Ps. 11,4</ref>.
</sense> <sense n="h" xml:id="D1h"> <cit type="translation" xml:lang="la"><quote> crescere facere </quote></cit>plantam, Lud. e Ssal. Req.
</sense> 
</sense> <sense n="2" xml:id="D2"> <cit type="translation" xml:lang="la"><quote> tollere </quote></cit>, <cit type="translation" xml:lang="la"><quote> extollere </quote></cit>, <cit type="translation" xml:lang="la"><quote> exaltare </quote></cit>: <foreign xml:lang="gez">ያዐብያ፡ ለክምረ፡ እክሉ፡</foreign> <ref cRef="Sir." loc="20,28">Sir. 20,28</ref>; <foreign xml:lang="gez">ታዐቢ፡ እደዊከ፡ ኀቤሁ፡</foreign> <ref cRef="Job" loc="11,13">Job 11,13</ref>. <sense n="a" xml:id="D2a"> laudibus (<cit type="translation" xml:lang="la"><quote> magnificare </quote></cit>): <foreign xml:lang="gez">አዕብይዎ፡ ለስሙ፡</foreign> <ref cRef="Sir." loc="39,15">Sir. 39,15</ref>; <ref cRef="Sir." loc="43,30">Sir. 43,30</ref>; <ref cRef="Ps." loc="33,4">Ps. 33,4</ref>; <ref cRef="Ps." loc="68,35">Ps. 68,35</ref>; <ref cRef="Luc." loc="1,46">Luc. 1,46</ref>; <ref cRef="Act. " loc="19,17">Act.  19,17</ref>.
</sense> <sense n="b" xml:id="D2b"> ad dignitatem, potentiam, felicitatem <cit type="translation" xml:lang="la"><quote> evehere </quote></cit>: <foreign xml:lang="gez">አዕበዮ፡ ንጉሥ፡ ለሐማ፡</foreign> <ref cRef="Esth." loc="3,1">Esth. 3,1</ref>; <foreign xml:lang="gez">ጥበብ፡ አዕበየቶሙ፡ ለደቂቃ፡</foreign> <ref cRef="Sir." loc="4,11">Sir. 4,11</ref>; <foreign xml:lang="gez">ታዐብዮ፡ እምነ፡ ቢጹ፡</foreign> <ref cRef="Sir." loc="15,5">Sir. 15,5</ref>; <foreign xml:lang="gez">ያዐብያ፡ ለነፍስከ፡</foreign> <ref cRef="Sir. " loc="31,20">Sir.  31,20</ref>; <ref cRef="Phil." loc="2,9">Phil. 2,9</ref>; <foreign xml:lang="gez">እምውስተ፡ መዋዕል፡ ቦ፡ ዘቀደሰ፡ ወአዕበየ፡</foreign> <ref cRef="Phil." loc="36,9">Phil. 36,9</ref>.
</sense> <sense n="c" xml:id="D2c"> <foreign xml:lang="gez">ርእሶ፡</foreign> <cit type="translation" xml:lang="la"><quote> se efferre </quote></cit>: <foreign xml:lang="gez">ኢታዕቢ፡ ርእሰከ፡</foreign> <ref cRef="Sir." loc="1,30">Sir. 1,30</ref>; <ref cRef="Sir." loc="4,7">Sir. 4,7</ref>; <ref cRef="Sir." loc="6,2">Sir. 6,2</ref>; <ref cRef="Sir. " loc="11,4">Sir.  11,4</ref>; <ref cRef="Luc." loc="18,14">Luc. 18,14</ref>; <ref cRef="Matth." loc="23,12">Matth. 23,12</ref>.
</sense> <sense n="d" xml:id="D2d"> <foreign xml:lang="gez">ልቦ፡</foreign> <cit type="translation" xml:lang="la"><quote> magnos sibi sumere spiritus </quote></cit>, <cit type="translation" xml:lang="la"><quote> superbire </quote></cit>: <foreign xml:lang="gez">ዘያዐቢ፡ ልቦ፡</foreign> <ref cRef="Jes." loc="10,12">Jes. 10,12</ref>; <ref cRef="Jer." loc="31,29">Jer. 31,29</ref>; <foreign xml:lang="gez">አዕብዮ፡ ልብ፡</foreign> <cit type="translation" xml:lang="la"><quote> fastus </quote></cit> <ref cRef="Kuf. " loc="40.">Kuf.  40.</ref> 
</sense> <sense n="e" xml:id="D2e"> <foreign xml:lang="gez">ምግባሮ፡</foreign> <cit type="translation" xml:lang="la"><quote> fastuose agere </quote></cit> <ref cRef="Joel" loc="2,20">Joel 2,20</ref>.
</sense> <sense n="f" xml:id="D2f"> <cit type="translation" xml:lang="la"><quote> vocem tollere </quote></cit> seu clamorem: <foreign xml:lang="gez">ያዐቢ፡ ቃሎ፡</foreign> <ref cRef="Sir." loc="21,20">Sir. 21,20</ref>; <foreign xml:lang="gez">ከመ፡ ያዕቢ፡ ቃለ፡ በውውዓ፡</foreign> <ref cRef="Hez. " loc="21,22">Hez.  21,22</ref> <lbl expand="versio nova">vers. nov.</lbl> 
</sense> 
</sense> </sense>
