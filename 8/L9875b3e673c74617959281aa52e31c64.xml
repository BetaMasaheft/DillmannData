         <div>
            <entry xml:id="L9875b3e673c74617959281aa52e31c64" n="8714">
               <form>
                  <foreign xml:lang="gez">ጕጉእ</foreign>
               </form>
               <sense xml:lang="la" source="#dillmann" xml:id="Dmain">
                  <gramGrp>
                     <pos expand="participium">part.</pos>
                  </gramGrp>
                  <sense n="1" xml:id="D1">
                     <cit type="translation" xml:lang="la">
                        <quote>properus</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>festinatus</quote>
                     </cit>: <foreign xml:lang="gez">ጕጉእ፡ ግብሩ፡</foreign>
                     <ref cRef="1                 Esr." loc="6,9">1 Esr. 6,9</ref>. <sense n="a" xml:id="D1a">
                        <foreign xml:lang="gez">ጕጉአ፡</foreign> vel <foreign xml:lang="gez">በጕጉእ፡ </foreign>
                        <gramGrp>
                           <pos expand="adverbium, adverbialiter">adv.</pos>
                        </gramGrp>
                        <cit type="translation" xml:lang="la">
                           <quote>propere</quote>
                        </cit>, <cit type="translation" xml:lang="la">
                           <quote>festinanter</quote>
                        </cit>, <cit type="translation" xml:lang="la">
                           <quote>citatim</quote>
                        </cit>: <foreign xml:lang="grc">ταχέως</foreign>
                        <ref cRef="Joel" loc="4,4">Joel 4,4</ref>; <foreign xml:lang="grc">κατὰ
                           σπουδήν</foreign>
                        <ref cRef="1 Esr." loc="2,25">1 Esr. 2,25</ref>; <foreign xml:lang="gez">በጕጉእ፡ </foreign>
                        <foreign xml:lang="grc">ἐν σπουδῇ</foreign>
                        <ref cRef="Deut." loc="16,3">Deut. 16,3</ref>.</sense>
                     <sense n="b" xml:id="D1b"> in malam partem: <cit type="translation" xml:lang="la">
                           <quote>praeproperus</quote>
                        </cit>, <cit type="translation" xml:lang="la">
                           <quote>praeceps</quote>
                        </cit>, <cit type="translation" xml:lang="la">
                           <quote>temerarius</quote>
                        </cit>: <foreign xml:lang="gez">ሰማዕት፡ ጕጉእ፡</foreign>
                        <foreign xml:lang="grc">μάρτυς ταχύς</foreign> Prov.12,19; <foreign xml:lang="gez">እመ፡ ርኢከ፡ ብእሴ፡ ጕጉአ፡ በቃል፡</foreign>
                        <ref cRef="Prov." loc="29,20">Prov. 29,20</ref>; <foreign xml:lang="gez">ጕጉእ፡ በከናፍሪሁ፡</foreign>
                        <foreign xml:lang="grc">προπετής</foreign>
                        <ref cRef="Prov." loc="13,3">Prov. 13,3</ref>; <foreign xml:lang="gez">አፍ፡
                           ጕጉእ፡</foreign>
                        <ref cRef="Prov." loc="10,14">Prov. 10,14</ref>; <foreign xml:lang="grc">θρασύς</foreign>
                        <ref cRef="Prov." loc="18,6">Prov. 18,6</ref>; <ref cRef="Prov." loc="21,24">Prov. 21,24</ref>.</sense>
                     <sense n="2" xml:id="D12">
                        <cit type="translation" xml:lang="la">
                           <quote>summo studio agens</quote>
                        </cit>, <cit type="translation" xml:lang="la">
                           <quote>sedulus</quote>
                        </cit>, Lud. ex. Org.; <gramGrp>
                           <pos expand="adverbium, adverbialiter">adv.</pos>
                        </gramGrp>
                        <foreign xml:lang="gez">ጕጉአ፡</foreign>
                        <cit type="translation" xml:lang="la">
                           <quote>summo studio</quote>
                        </cit>, <cit type="translation" xml:lang="la">
                           <quote>acriter</quote>
                        </cit>
                        <ref cRef="Sx. Genb. " loc="19 ">Sx. Genb. 19 </ref>Enc.</sense>
                  </sense>
               </sense>
               
