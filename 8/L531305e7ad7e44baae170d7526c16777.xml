               <sense xml:id="Dmain" xml:lang="la" source="#dillmann"> <subc>I,1</subc> [amh. <lbl expand="idem">id.</lbl>; <foreign xml:lang="he">כָּפַל</foreign> <cit type="translation" xml:lang="la"><quote> duplicare </quote></cit>, <cit type="translation" xml:lang="la"><quote> bis complicare </quote></cit>: <foreign xml:lang="ar">كِفْلٌ</foreign> <cit type="translation" xml:lang="la"><quote> duplum </quote></cit>, <lbl expand="videas">vid.</lbl> <foreign xml:lang="gez">ቀፈለ፡</foreign>] <gramGrp> <pos expand="Subjunctivus">Subj.</pos> </gramGrp> <foreign xml:lang="gez">ይክፍል፡</foreign> <sense n="1" xml:id="D1"> <cit type="translation" xml:lang="la"><quote> dividere </quote></cit>, <cit type="translation" xml:lang="la"><quote> partiri </quote></cit>, <lbl expand="cum">c.</lbl> <gramGrp> <case value="accusativus">Acc.</case> </gramGrp> rei et <gramGrp> <case value="accusativus">Acc.</case> </gramGrp> partium, <foreign xml:lang="gez">ትከፍል፡ ደወለ፡ መሥልስተ፡</foreign> <foreign xml:lang="grc">τριμεριεῖς</foreign> <ref cRef="Deut." loc="19,3">Deut. 19,3</ref>; <foreign xml:lang="gez">ክፍልዋ፡ ለምድር፡ ሰብዐተ፡ ክፍለ፡</foreign> <ref cRef="Jos. " loc="18,6">Jos.  18,6</ref>; <foreign xml:lang="gez">ከፈልዋ፡ ለምድር፡ ፫ መክፈልተ፡</foreign> <ref cRef="Kuf." loc="8">Kuf. 8</ref>; <foreign xml:lang="gez">ከፈለ፡ ሰራዊቶ፡ ፬ ክፍለ፡</foreign> <ref cRef="Jsp." loc="364">Jsp. p. 364</ref>; <lbl expand="cum">c.</lbl> <foreign xml:lang="gez">ለ፡</foreign> partium <ref cRef="Jud." loc="7,16">Jud. 7,16</ref>; <ref cRef="Jud." loc="9,43">Jud. 9,43</ref>; vel <lbl expand="cum">c.</lbl> <foreign xml:lang="gez">ኀበ፡</foreign>, ut: <foreign xml:lang="gez">ይክፍልዋ፡ ለመዓልት፡ ኀበ፡ ፫ መክፈልት፡</foreign> F.N. 10,6; <foreign xml:lang="gez">ትከፍሎ፡ ለዝንቱ፡ ኀበ፡ ፷፡</foreign> Abush., vel <lbl expand="cum">c.</lbl> <foreign xml:lang="gez">በበ፡</foreign>, Abush. (crebro). <sense n="a" xml:id="D1a"> <cit type="translation" xml:lang="la"><quote> separare </quote></cit>, <cit type="translation" xml:lang="la"><quote> arcere </quote></cit>: <foreign xml:lang="gez">ትክፍል፡ ሕዝበ፡ ይዑድዋ፡</foreign> <foreign xml:lang="grc">ἀφοριεῖς τὸν λαὸν κύκλῳ</foreign> <ref cRef="Ex. " loc="19,12">Ex.  19,12</ref>.
</sense> <sense n="b" xml:id="D1b"> <cit type="translation" xml:lang="la"><quote> distinguere </quote></cit>: <foreign xml:lang="gez">ከመ፡ ይክፍል፡ ማእከለ፡ ቅዱስ፡ ወዘኢኮነ፡ ቅዱሰ፡</foreign> <foreign xml:lang="grc">διαστέλλειν</foreign> <ref cRef="Hez." loc="42,20">Hez. 42,20</ref>.
</sense> <sense n="c" xml:id="D1c"> <cit type="translation" xml:lang="la"><quote> dissecare </quote></cit>: <foreign xml:lang="gez">አበሳኬ፡ ውእቱ፡ ከፊል፡ ለ፪ እስመ፡ ፩ ውእቱ፡ ኢየሱስ፡</foreign> <foreign xml:lang="grc">διατέμνειν</foreign> Cyr. ad <ref cRef="Theod." loc="23">Theod. f. 23</ref>; <foreign xml:lang="gez">እኂዞ፡ መጥባሕተ፡ ለዕድው፡ እንዘ፡ ይከፍሎ፡</foreign> (adversarium dissecans) <ref cRef="Sx. Ter" loc="19 ">Sx. Ter 19 </ref>Enc. <cb n="0877" xml:id="c0877"/> </sense>
</sense> <sense n="2" xml:id="D2"> <cit type="translation" xml:lang="la"><quote> dis </quote></cit> <cit type="translation" xml:lang="la"><quote> t </quote></cit> <cit type="translation" xml:lang="la"><quote> ribuere </quote></cit>, <cit type="translation" xml:lang="la"><quote> dispertiri </quote></cit>, <foreign xml:lang="grc">διαμερίζειν</foreign> <ref cRef="Deut." loc="32,8">Deut. 32,8</ref>; <ref cRef="Hez." loc="45,1">Hez. 45,1</ref>; <ref cRef="Hen." loc="41,8">Hen. 41,8</ref>; <foreign xml:lang="gez">ይከፍል፡ ምድረ፡ በሤጥ፡</foreign> <ref cRef="Dan." loc="11,39">Dan. 11,39</ref>; <foreign xml:lang="gez">ይከፍሎ፡ ለማይ፡ ዘከመ፡ ፈቀደ፡</foreign> <ref cRef="Job" loc="37,9">Job 37,9</ref>; <lbl expand="cum">c.</lbl> <foreign xml:lang="gez">ውስተ፡</foreign> loci: <foreign xml:lang="gez">አንበሮሙ፡ ወከፈሎሙ፡ ውስተ፡ አህጉር፡</foreign> 2 <ref cRef="Par." loc="9,25">Par. 9,25</ref>; <ref cRef="Gen." loc="49,7">Gen. 49,7</ref>; <lbl expand="cum">c.</lbl> <foreign xml:lang="gez">ማእከለ፡</foreign> pers.: <foreign xml:lang="gez">ይክፍሉ፡ ማእከለ፡ ክልኤሆሙ፡ ዕሩየ፡</foreign> F.N. 41,3; <ref cRef="Hen." loc="60,15">Hen. 60,15</ref>; <ref cRef="Kuf. " loc="9">Kuf.  9</ref>; <ref cRef="Num." loc="31,27">Num. 31,27</ref>; <lbl expand="cum">c.</lbl> <foreign xml:lang="gez">ለ፡</foreign> pers.: <foreign xml:lang="gez">ከፈለ፡ ኵሎ፡ ዘቦ፡ ጥሪቶ፡ ለክልኤሆሙ፡</foreign> <ref cRef="Kuf." loc="36">Kuf. 36</ref>; <ref cRef="Judith" loc="16,24">Judith 16,24</ref>; <ref cRef="Prov." loc="19,14">Prov. 19,14</ref>; <lbl expand="cum">c.</lbl> <gramGrp> <case value="accusativus">Acc.</case> </gramGrp> <lbl expand="persona">pers.</lbl>: <ref cRef="Jos." loc="1,6">Jos. 1,6</ref>; <ref cRef="Luc." loc="12,13">Luc. 12,13</ref>; <ref cRef="Marc." loc="6,41">Marc. 6,41</ref>; vel <lbl expand="cum">c.</lbl> <foreign xml:lang="gez">ላዕለ፡</foreign> pers.: <foreign xml:lang="gez">ፈቀድኩ፡ ከመ፡ እክፍላ፡</foreign> (<foreign xml:lang="gez">ለመንግሥትየ፡</foreign>) <foreign xml:lang="gez">ላዕለ፡ ፫ ውሉድየ፡ ዕሩየ፡</foreign> <ref cRef="Jsp." loc="364">Jsp. p. 364</ref>; <ref cRef="Genz. " loc="156.">Genz.  f. 156.</ref> <sense n="a" xml:id="D2a"> <cit type="translation" xml:lang="la"><quote> tribuere </quote></cit>, <cit type="translation" xml:lang="la"><quote> attribuere </quote></cit>, <cit type="translation" xml:lang="la"><quote> assignare </quote></cit> alicui aliquid, <lbl expand="cum">c.</lbl> <gramGrp> <case value="accusativus">Acc.</case> </gramGrp> <lbl expand="persona">pers.</lbl>: <foreign xml:lang="gez">በከመ፡ ከፈሎ፡</foreign> <ref cRef="Rom." loc="12,3">Rom. 12,3</ref>; <ref cRef="1 Cor." loc="7,17">1 Cor. 7,17</ref>; <ref cRef="Hen. " loc="27,4">Hen.  27,4</ref>; <foreign xml:lang="gez">መኑ፡ እምከፈለኒ፡ ከመ፡ እኩን፡</foreign> <ref cRef="Org." loc="1">Org. 1</ref>; <lbl expand="cum">c.</lbl> <lbl expand="duplex">dupl.</lbl> <gramGrp> <case value="accusativus">Acc.</case> </gramGrp>: <foreign xml:lang="gez">ርስትየ፡ ዘከፈልክዎ፡ ለሕዝብየ፡</foreign> <ref cRef="Jer." loc="12,14">Jer. 12,14</ref>; <ref cRef="Hebr. " loc="7,2">Hebr.  7,2</ref>; <ref cRef="2 Petr." loc="1,1">2 Petr. 1,1</ref>.
</sense> <sense n="b" xml:id="D2b"> <cit type="translation" xml:lang="la"><quote> donare </quote></cit>, <cit type="translation" xml:lang="la"><quote> largiri </quote></cit>, <lbl expand="cum">c.</lbl> <gramGrp> <case value="accusativus">Acc.</case> </gramGrp> rei: <ref cRef="Act." loc="2,45">Act. 2,45</ref>; <lbl expand="cum">c.</lbl> <lbl expand="duplex">dupl.</lbl> <gramGrp> <case value="accusativus">Acc.</case> </gramGrp>: <foreign xml:lang="gez">ከመ፡ እክፍሎሙ፡ ጥሪተ፡ ለእለ፡ ያአምሩኒ፡</foreign> <ref cRef="Prov." loc="8,21">Prov. 8,21</ref>; <foreign xml:lang="gez">ክፍሎ፡ መንፈሰ፡ ጸጋ፡</foreign> <ref cRef="Const. Ap." loc="22.">Const. Ap. 22.</ref> 
</sense> <sense n="c" xml:id="D2c"> <cit type="translation" xml:lang="la"><quote> participem facere </quote></cit> aliquem alicujus, <lbl expand="cum">c.</lbl> <gramGrp> <case value="accusativus">Acc.</case> </gramGrp> <lbl expand="persona">pers.</lbl> et <foreign xml:lang="gez">ምስለ፡</foreign> <ref cRef="2 Cor." loc="6,15">2 Cor. 6,15</ref>.
</sense> 
</sense><sense n="3" xml:id="D3"> ut videtur, intransit.: <cit type="translation" xml:lang="la"><quote> competere </quote></cit>, <cit type="translation" xml:lang="la"><quote> contingere </quote></cit> alicui, <lbl expand="cum">c.</lbl> <gramGrp> <case value="accusativus">Acc.</case> </gramGrp> <lbl expand="persona">pers.</lbl>: <ref cRef="Matth." loc="22,8">Matth. 22,8</ref>; <foreign xml:lang="gez">ትረስዮሙ፡ ከመ፡ ይክፍሎሙ፡ አክሊሎሙ፡</foreign> Lud. ex Lit. Joh.; fortasse etiam <ref cRef="Hen." loc="27,4">Hen. 27,4</ref>.
</sense> </sense>
               
