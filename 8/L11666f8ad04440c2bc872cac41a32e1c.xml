            <entry xml:id="L11666f8ad04440c2bc872cac41a32e1c" n="6697">
               <form>
                  <foreign xml:lang="gez">አውከፈ</foreign>
               </form>
               <sense xml:lang="la" source="#dillmann" xml:id="Dmain"> <subc>II,1</subc> <sense n="1" xml:id="D1"> <cit type="translation" xml:lang="la">
                                <quote> excipere </quote>
                            </cit> (manu) aliquid: <foreign xml:lang="gez">አውከፈት፡ አንብዐ፡ ወለታ፡</foreign> <foreign xml:lang="grc">ἀπεδέξατο τὰ δάκρυα τῆς θυγατρός</foreign> <ref cRef="Tob." loc="7,16">Tob. 7,16</ref> (v. <foreign xml:lang="gez">አከፈት፡</foreign> cum glossa marginali <foreign xml:lang="gez">ማሰሰት፡</foreign> <cit type="translation" xml:lang="la">
                                <quote> abstersit </quote>
                            </cit>?); <cb n="0922" xml:id="c0922"/> <foreign xml:lang="gez">ገልዕ፡ በዘ፡ ያወክፉ፡</foreign> (v. <foreign xml:lang="gez">ያዌክፉ፡</foreign>, <lbl expand="vel">v.</lbl> <foreign xml:lang="gez">ያከውፉ</foreign>) <foreign xml:lang="gez">ማየ፡ ኅዳጠ፡</foreign> <ref cRef="Jes." loc="30,14">Jes. 30,14</ref> (<lbl expand="videas">vid.</lbl> <foreign xml:lang="gez">አክወፈ፡</foreign>).</sense> <sense n="2" xml:id="D2"> <cit type="translation" xml:lang="la">
                                <quote> in se recipere </quote>
                            </cit> aliquid, ut mandatum exsequendum (<cit type="translation" xml:lang="de">
                                <quote>auf sich nehmen</quote>
                            </cit>, <cit type="translation" xml:lang="de">
                                <quote>sich anheischig machen</quote>
                            </cit>), <lbl expand="sequens">seq.</lbl> <foreign xml:lang="gez">ከመ፡</foreign> <lbl expand="cum">c.</lbl> <gramGrp> <pos expand="Subjunctivus">Subj.</pos> </gramGrp>, ut: <foreign xml:lang="gez">አውከፉ፡ አይሁድ፡ ላዕሌሆሙ፡ ወለዘርኦሙ፡ ከመ፡ ኢይፍልሱ፡</foreign> <ref cRef="Esth. " loc="9,27">Esth.  9,27</ref>.</sense> </sense>
               
