               <sense xml:lang="la" source="#dillmann" xml:id="Dmain"> vel <foreign xml:lang="gez">ጤአ፡</foreign> <subc>I,1</subc> [<foreign xml:lang="he">טוּחַ</foreign> <foreign xml:lang="ar">طاخ</foreign> (<foreign xml:lang="ar">طَيْخٌ</foreign>)] <gramGrp> <pos expand="Subjunctivus">Subj.</pos> </gramGrp> <foreign xml:lang="gez">ይጢዕ፡</foreign> <sense n="1" xml:id="D1"> <cit type="translation" xml:lang="la">
                                <quote> linere </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> oblinere </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> collinere </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> illinere </quote>
                            </cit>: <foreign xml:lang="gez">ንሣእ፡ ዐጽቀ፡ በለስ፡ ወጢዕ፡</foreign> (v. <foreign xml:lang="gez">ወደይ፡</foreign>) <foreign xml:lang="gez">ቦቱ፡ ኀበ፡ ተሐምም፡</foreign> <ref cRef="4 Reg." loc="20,7">4 Reg. 20,7</ref>; <foreign xml:lang="gez">አልቦ፡ ከመ፡ ዘይጠይዕዎ፡ ሥራየ፡</foreign> <foreign xml:lang="grc">οὐκ ἔστι μάλαγμα ἐπιθεῖναι</foreign> <ref cRef="Jes." loc="1,6">Jes. 1,6</ref>.</sense> <sense n="2" xml:id="D2"> <cit type="translation" xml:lang="la">
                                <quote> linendo opplere </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> obturare </quote>
                            </cit>: <foreign xml:lang="gez">ወይጠይዑ፡ እዘኒሆሙ፡</foreign> <ref cRef="Mich." loc="7,16">Mich. 7,16</ref>; <foreign xml:lang="gez">ጤዑ፡</foreign> (v. pro <foreign xml:lang="gez">አጽመሙ፡</foreign>) <foreign xml:lang="gez">እዘኒሆሙ፡ ከመ፡ ኢይስምዑ፡</foreign> <ref cRef="Jes." loc="6,10">Jes. 6,10</ref>. – <lbl expand="Radices Abbadianae (collectio radicum Geez linguae Mss.).">Rad. Abb.</lbl>: <foreign xml:lang="gez">ጤዐ፡ ዘ፡ መረገ፡</foreign> </sense> </sense>
               
