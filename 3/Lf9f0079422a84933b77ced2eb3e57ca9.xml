            <entry xml:id="Lf9f0079422a84933b77ced2eb3e57ca9" n="9972">
               <form>
                  <foreign xml:lang="gez">ተፈትሐ</foreign>
               </form>
               <sense xml:id="Dmain" xml:lang="la" source="#dillmann"> <subc>III,1</subc> <cit type="translation" xml:lang="la"><quote> aperiri </quote></cit>, de oculis: <ref cRef="Gen." loc="3,5">Gen. 3,5</ref>; <ref cRef="Gen." loc="3,7">Gen. 3,7</ref>; <ref cRef="Hen." loc="32,6">Hen. 32,6</ref>; <ref cRef="Hen. " loc="90,10">Hen.  90,10</ref>; <ref cRef="Marc." loc="7,34">Marc. 7,34</ref> <lbl expand="romanae editionis">rom.</lbl>; <foreign xml:lang="gez">መዓምቃት፡ ተፈትሑ፡</foreign> <cb n="1365" xml:id="c1365"/> <ref cRef="Hen. " loc="89,7">Hen.  89,7</ref>. <sense n="2" xml:id="D2"> <cit type="translation" xml:lang="la"><quote> solvi </quote></cit>, <cit type="translation" xml:lang="la"><quote> exsolvi </quote></cit>, <cit type="translation" xml:lang="la"><quote> resolvi </quote></cit>, <ref cRef="Sir." loc="22,16">Sir. 22,16</ref>; <ref cRef="Job" loc="41,8">Job 41,8</ref> <lbl expand="varia lectio">var.</lbl>; de vinculis: <ref cRef="Marc." loc="7,35">Marc. 7,35</ref>; <foreign xml:lang="gez">ተፈትሐ፡ ማኅሜሁ፡ እምነ፡ መዝራዕቱ፡</foreign> <ref cRef="Jud. " loc="15,14">Jud.  15,14</ref>; de ligatis vel vinctis <foreign xml:lang="gez">ከመ፡ ጣዕዋ፡ ዘተፈትሐ፡ እማእሰሩ፡</foreign> <ref cRef="Mal." loc="3,20">Mal. 3,20</ref>; <ref cRef="Luc." loc="13,16">Luc. 13,16</ref>; <foreign xml:lang="gez">ሕቡስ፡ ለእመ፡ ተፈትሐ፡ እሞቅሑ፡</foreign> <ref cRef="Jsp." loc="381">Jsp. p. 381</ref>; <ref cRef="Apoc." loc="20,3">Apoc. 20,3</ref>; <ref cRef="Apoc." loc="20,7">Apoc. 20,7</ref>; de aqua, quae glacie solvitur: <foreign xml:lang="gez">ወተፈትሐ፡ ማይ፡</foreign> (<foreign xml:lang="gez">ዘረግዐ፡</foreign>) <ref cRef="Sx. Mij." loc="25">Sx. Mij. 25</ref>; <lbl expand="translate">transl.</lbl>: <foreign xml:lang="gez">ወይትፈታሕ፡ ማእሰረ፡ ተዋስቦ፡</foreign> F.M. 12,5. <sense n="a" xml:id="D2a"> <cit type="translation" xml:lang="la"><quote> dissolvi </quote></cit>: <foreign xml:lang="gez">ተፈትሐ፡ መሌሊተ፡ ሐቌሁ፡</foreign> <ref cRef="Dan." loc="5,6">Dan. 5,6</ref>; <ref cRef="Hen." loc="60,3">Hen. 60,3</ref>.
</sense> <sense n="b" xml:id="D2b"> <cit type="translation" xml:lang="la"><quote> remitti </quote></cit>: <foreign xml:lang="gez">ተፈትሐት፡ ኀጢአታ፡</foreign> <ref cRef="Jes. " loc="40,2">Jes.  40,2</ref>.
</sense> <sense n="c" xml:id="D2c"> <cit type="translation" xml:lang="la"><quote> dimitti </quote></cit>, <cit type="translation" xml:lang="la"><quote> absolvi </quote></cit>: <foreign xml:lang="gez">ተፈቲሖሙ፡</foreign> <ref cRef="Act. " loc="4,23">Act.  4,23</ref>.</sense>
</sense> <sense n="3" xml:id="D3"> <cit type="translation" xml:lang="la"><quote> judicari </quote></cit>, de personis: <foreign xml:lang="gez">ይትፈትሑ፡</foreign> <ref cRef="Can. Ant. " loc="13.">Can. Ant.  13.</ref> <sense n="a" xml:id="D3a"> <cit type="translation" xml:lang="la"><quote> dici </quote></cit> vel <cit type="translation" xml:lang="la"><quote> pronuntiari </quote></cit>, de sententia judicis: <foreign xml:lang="gez">ተፈትሐ፡ ላዕሌሁ፡ ፍትሐ፡ ሞት፡</foreign> F.N. 34.
</sense> <sense n="b" xml:id="D3b"> <cit type="translation" xml:lang="la"><quote> decerni </quote></cit>, <cit type="translation" xml:lang="la"><quote> sanciri </quote></cit>: <foreign xml:lang="gez">ተወሰነ፡ ወተፈትሐ፡ ላዕለ፡ ኵሎሙ፡ ምእመናን፡</foreign> seq. <foreign xml:lang="gez">ከመ፡</foreign> <lbl expand="cum">c.</lbl> <gramGrp> <pos expand="Subjunctivus">Subj.</pos> </gramGrp>, F.M. 10,3; F.M. 25,2.
</sense> 
</sense> </sense>
               
