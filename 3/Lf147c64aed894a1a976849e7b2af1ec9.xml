 <bibl><ptr target="bm:Grebaut1952SuppLexicon"/><citedRange unit="page"> 134</citedRange></bibl> 
</sense> <sense n="L" xml:id="TL"><foreign xml:lang="gez">ሥዕል</foreign> <cit type="transcription" xml:lang="gez"><quote>śǝʿl</quote></cit> <cit type="translation" xml:lang="en"><quote>picture</quote></cit>, <cit type="translation" xml:lang="en"><quote>painting</quote></cit>, <cit type="translation" xml:lang="en"><quote>image</quote></cit>, <cit type="translation" xml:lang="en"><quote>icon</quote></cit>, <cit type="translation" xml:lang="en"><quote>plan</quote></cit>, <cit type="translation" xml:lang="en"><quote>model</quote></cit> <lb/>
