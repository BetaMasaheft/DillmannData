            <entry xml:id="Lf5de899d550045c4a2d69ed0563846ed" n="941">
               <form>
                  <foreign xml:lang="gez">አሕየወ</foreign>
               </form>
               <sense xml:id="Dmain" xml:lang="la" source="#dillmann"> <subc>II,1</subc> <sense n="1" xml:id="D1"> <cit type="translation" xml:lang="la"><quote> vivificare </quote></cit>, <cit type="translation" xml:lang="la"><quote> vivere jubere </quote></cit> <ref cRef="Koh." loc="7,12">Koh. 7,12</ref>; <ref cRef="4 Esr." loc="8,16">4 Esr. 8,16</ref>; <foreign xml:lang="gez">አውረሶሙ፡ ሕገ፡ ዘያሐይዎሙ፡</foreign> <foreign xml:lang="grc">νόμον ζωῆς</foreign> <ref cRef="Sir." loc="17,11">Sir. 17,11</ref>; <foreign xml:lang="grc">ζωοποιεῖν</foreign> <ref cRef="Rom." loc="8,11">Rom. 8,11</ref>. 
</sense> <sense n="2" xml:id="D2"> <cit type="translation" xml:lang="la"><quote> vivum servare </quote></cit>: <foreign xml:lang="gez">አሕየውኪ፡ ነፍሰኪ፡</foreign> <ref cRef="Judith" loc="10,15">Judith 10,15</ref>; <foreign xml:lang="gez">አትረፈነ፡ ወአሕየወነ፡</foreign> <ref cRef="2 Esr." loc="9,8">2 Esr. 9,8</ref>. <sense n="a" xml:id="D2a"> <cit type="translation" xml:lang="la"><quote> alimento sustentare </quote></cit> (ne pereat) <ref cRef="Gen." loc="47,25">Gen. 47,25</ref>; <foreign xml:lang="gez">እመቦ፡ ከመ፡ ንርከብ፡ ሣዕረ፡ ወናሐዩ፡ አፍራሲነ፡</foreign> <ref cRef="3 Reg." loc="18,5">3 Reg. 18,5</ref>. 
</sense> <sense n="b" xml:id="D2b"> <cit type="translation" xml:lang="la"><quote> vitam concedere </quote></cit> alicui, <cit type="translation" xml:lang="la"><quote> parcere vitae </quote></cit> alicujus <ref cRef="Judith" loc="11,9">Judith 11,9</ref>; <ref cRef="1 Reg. " loc="15,9">1 Reg.  15,9</ref> <lbl expand="sequens">seq.</lbl>; <ref cRef="1 Reg." loc="27,9">1 Reg. 27,9</ref>; <ref cRef="Gen." loc="12,12">Gen. 12,12</ref>; <ref cRef="Ex." loc="1,17">Ex. 1,17</ref>; <ref cRef="Num." loc="31,15">Num. 31,15</ref>; <ref cRef="Jos." loc="9,19">Jos. 9,19</ref>. 
</sense> <sense n="c" xml:id="D2c"> reum <cit type="translation" xml:lang="la"><quote> absolvere </quote></cit> in judicio, <cit type="translation" xml:lang="la"><quote> liberum dimittere </quote></cit> <foreign xml:lang="grc">ἀπολύειν</foreign> <ref cRef="Dan. apocr." loc="1,53">Dan. apocr. 1,53</ref>; <ref cRef="Matth." loc="27,15">Matth. 27,15</ref>; <ref cRef="Marc. " loc="15,6">Marc.  15,6</ref>. 
</sense> 
</sense> <sense n="3" xml:id="D3"> <cit type="translation" xml:lang="la"><quote> revivificare </quote></cit>, <cit type="translation" xml:lang="la"><quote> vitae restituere </quote></cit>, <cit type="translation" xml:lang="la"><quote> in vitam revocare </quote></cit> <sense n="a" xml:id="D3a"> languidum <cit type="translation" xml:lang="la"><quote> refocillare </quote></cit>, <cit type="translation" xml:lang="la"><quote> recreare </quote></cit>: <ref cRef="Ps." loc="70,22">Ps. 70,22</ref>; <ref cRef="Ps." loc="118,37">Ps. 118,37</ref>; <ref cRef="Ps." loc="118,40">Ps. 118,40</ref>. 
</sense> <sense n="b" xml:id="D3b"> mortuos <ref cRef="Deut." loc="32,39">Deut. 32,39</ref>; <ref cRef="1 Sam." loc="2,6">1 Sam. 2,6</ref>. 
</sense> <sense n="c" xml:id="D3c"> aegrotos: <cit type="translation" xml:lang="la"><quote> valetudini reddere </quote></cit>, <cit type="translation" xml:lang="la"><quote> sanare </quote></cit> <ref cRef="Ps." loc="106,20">Ps. 106,20</ref>; <ref cRef="Job " loc="42,10">Job  42,10</ref>; <cb n="0127" xml:id="c0127"/> <ref cRef="Hos." loc="6,2">Hos. 6,2</ref>; <ref cRef="Matth." loc="8,7">Matth. 8,7</ref> <lbl expand="romanae editionis">rom.</lbl>; <foreign xml:lang="gez">ያሐይዎሙ፡ ሕይወተ፡</foreign> <ref cRef="Jes." loc="19,22">Jes. 19,22</ref>. 
</sense> <sense n="d" xml:id="D3d"> res fractas, scissas <cit type="translation" xml:lang="la"><quote> reparare </quote></cit>, <cit type="translation" xml:lang="la"><quote> reficere </quote></cit>: <foreign xml:lang="gez">ያሐይዉ፡ አረፋተ፡</foreign> <ref cRef="1 Esr." loc="2,17">1 Esr. 2,17</ref>; <foreign xml:lang="gez">አሕየወ፡ ምሥዋዐ፡ ዘመዝበረ፡</foreign> <ref cRef="3 Reg." loc="18,32">3 Reg. 18,32</ref>. 
</sense> 
</sense> <sense n="4" xml:id="D4"> sensu theologico: <cit type="translation" xml:lang="la"><quote> salvum reddere </quote></cit>, <cit type="translation" xml:lang="la"><quote> salvare </quote></cit> <ref cRef="Rom." loc="1,16">Rom. 1,16</ref>; <ref cRef="Joh." loc="12,47">Joh. 12,47</ref> <lbl expand="romanae editionis">rom.</lbl>; <ref cRef="2 Tim." loc="4,18">2 Tim. 4,18</ref>. 
</sense> </sense>
               
            <sense xml:id="Tmain" xml:lang="en" source="#traces"> <cit type="transcription" xml:lang="gez"><quote>ʾaḥyawa</quote></cit> <sense n="G" xml:id="TG"> <foreign xml:lang="gez">አሕየወ፡</foreign> <cit type="transcription" xml:lang="gez"><quote>ʾaḥyawa</quote></cit> <subc>II,1</subc> <foreign xml:lang="gez">ያሐዩ፡</foreign> «<cit type="translation" xml:lang="fr"><quote>faire vivre</quote></cit>, <cit type="translation" xml:lang="fr"><quote>guérir</quote></cit> (actif), <cit type="translation" xml:lang="fr"><quote>rétablir en santé</quote></cit>» ― <foreign xml:lang="am">አዳነ፡</foreign> Ms. <ref type="BM" target="BNFabb217">BNFabb217</ref>, fol. 28v. <lb/>
 <bibl><ptr target="bm:Grebaut1952SuppLexicon"/><citedRange unit="page"> 62</citedRange></bibl> 
</sense> <sense n="L" xml:id="TL"> <foreign xml:lang="gez">አሕየወ</foreign> <cit type="transcription" xml:lang="gez"><quote>ʾaḥyawa</quote></cit> <cit type="translation" xml:lang="en"><quote>keep alive</quote></cit>, <cit type="translation" xml:lang="en"><quote>make to live</quote></cit>, <cit type="translation" xml:lang="en"><quote>let live</quote></cit>, <cit type="translation" xml:lang="en"><quote>enliven</quote></cit>, <cit type="translation" xml:lang="en"><quote>vivify</quote></cit>, <cit type="translation" xml:lang="en"><quote>raise up alive</quote></cit>, <cit type="translation" xml:lang="en"><quote>restore to life</quote></cit>, <cit type="translation" xml:lang="en"><quote>cure</quote></cit>, <cit type="translation" xml:lang="en"><quote>heal</quote></cit> <bibl><ptr target="bm:Leslau1987dic"/><citedRange unit="page"> 252a</citedRange></bibl> 
</sense> </sense></entry>
