            <entry xml:id="Lfa39e98b2cd8469cae9744a7494fd8fc" n="8913">
               <form>
                  <foreign xml:lang="gez">አጥብዐ</foreign>
               </form>
               <sense xml:id="Dmain" xml:lang="la" source="#dillmann"> <subc>II,1</subc> <sense n="A" xml:id="DA"> idem fere quod <subc>I,1</subc>, quamquam crebrius usitatum: <sense n="1" xml:id="DA1"> <cit type="translation" xml:lang="la"><t:quote xmlns:t="http://www.tei-c.org/ns/1.0"> promtum esse </t:quote></cit> animo, <cit type="translation" xml:lang="la"><t:quote xmlns:t="http://www.tei-c.org/ns/1.0"> lubentem se praestare </t:quote></cit>: <foreign xml:lang="gez">መኑ፡ እንከ፡ ዘያጠብዕ፡ ይፈጽም፡ እደዊሁ፡</foreign> <foreign xml:lang="grc">προθυμούμενος πληρῶσαι</foreign> <ref cRef="1 Par." loc="29,5">1 Par. 29,5</ref>; <ref cRef="Matth." loc="16,24">Matth. 16,24</ref> Platt; <ref cRef="Marc." loc="8,34">Marc. 8,34</ref>; <ref cRef="Luc." loc="9,23">Luc. 9,23</ref>; mulier <foreign xml:lang="gez">አጥብዐት፡ ለተኰንኖ፡</foreign> <ref cRef="Sx. Teq." loc="11 ">Sx. Teq. 11 </ref>Enc.; Sx. <ref cRef="Jac." loc="29 ">Jac. 29 </ref>Enc.; <foreign xml:lang="gez">ለእመ፡ አጥባዕከ፡ እምነፍስከ፡</foreign> <foreign xml:lang="grc">ἐὰν δῷς τὴν ψυχήν σου</foreign> <ref cRef="Sir." loc="6,32">Sir. 6,32</ref>. <sense n="a" xml:id="DA1a"> <cit type="translation" xml:lang="la"><t:quote xmlns:t="http://www.tei-c.org/ns/1.0"> lubenter sustinere </t:quote></cit> aliquid, <lbl expand="cum">c.</lbl> <foreign xml:lang="gez">ለ፡</foreign> rei: <foreign xml:lang="gez">ኀለዩ፡ በልቦሙ፡ ወአጥብዑ፡ ለመቅሠፍተ፡ ኀጢአቶሙ፡</foreign> <ref cRef="Hez." loc="14,3">Hez. 14,3</ref>; <ref cRef="Hez." loc="14,4">Hez. 14,4</ref>; <ref cRef="Hez." loc="14,7">Hez. 14,7</ref>; <lbl expand="sequens">seq.</lbl> <gramGrp> <case value="accusativus">Acc.</case> </gramGrp> rei: <foreign xml:lang="gez">አጥባዕኮ፡ ለሞት፡</foreign> <ref cRef="Sx. Nah." loc="27 ">Sx. Nah. 27 </ref>Enc. (Lud.).
</sense> <sense n="b" xml:id="DA1b"> <cit type="translation" xml:lang="la"><t:quote xmlns:t="http://www.tei-c.org/ns/1.0"> se parare </t:quote></cit> ad aggrediendum aliquid: <foreign xml:lang="gez">አጥብዒ፡ ከመ፡ ትቅትሊ፡</foreign> <foreign xml:lang="grc">ἑτοίμη εἰς</foreign> <ref cRef="Hez." loc="21,10">Hez. 21,10</ref>; <foreign xml:lang="gez">አጥብዕ፡ ወተደሎ፡</foreign> <foreign xml:lang="grc">ἑτοιμάσθητι ἑτοίμασον σεαυτόν</foreign> <ref cRef="Hez." loc="38,7">Hez. 38,7</ref>; <foreign xml:lang="gez">አጥባዕኩ፡ ወኢናፈቁ፡</foreign> <foreign xml:lang="grc">ἡτοιμάσθην</foreign> <ref cRef="Ps." loc="118,60">Ps. 118,60</ref>; <lbl expand="sequens">seq.</lbl> <foreign xml:lang="gez">ኀበ፡</foreign> rei: <ref cRef="Jsp." loc="382 ">Jsp. p. 382 </ref>(<lbl expand="videas">vid. sub</lbl> <foreign xml:lang="gez">ጠብዐ፡</foreign>).</sense>
</sense> <sense n="2" xml:id="DA2"> <cit type="translation" xml:lang="la"><t:quote xmlns:t="http://www.tei-c.org/ns/1.0"> audere </t:quote></cit>, <lbl expand="sequens">seq.</lbl> <gramGrp> <case value="accusativus">Acc.</case> </gramGrp> Infin.: <ref cRef="Matth." loc="22,46">Matth. 22,46</ref> <lbl expand="romanae editionis">rom.</lbl>; <lbl expand="sequens">seq.</lbl> <gramGrp> <pos expand="Subjunctivus">Subj.</pos> </gramGrp>: <ref cRef="Rom. " loc="5,7">Rom.  5,7</ref>.
</sense> <sense n="3" xml:id="DA3"> <cit type="translation" xml:lang="la"><t:quote xmlns:t="http://www.tei-c.org/ns/1.0"> velle </t:quote></cit> i.e. <cit type="translation" xml:lang="la"><t:quote xmlns:t="http://www.tei-c.org/ns/1.0"> sibi proponere </t:quote></cit>, <cit type="translation" xml:lang="la"><t:quote xmlns:t="http://www.tei-c.org/ns/1.0"> animum inducere </t:quote></cit>, <cit type="translation" xml:lang="la"><t:quote xmlns:t="http://www.tei-c.org/ns/1.0"> cum animo statuere </t:quote></cit> vel <cit type="translation" xml:lang="la"><t:quote xmlns:t="http://www.tei-c.org/ns/1.0"> statutum habere </t:quote></cit>, <lbl expand="sequens">seq.</lbl> <foreign xml:lang="gez">ከመ፡</foreign> <lbl expand="cum">c.</lbl> <gramGrp> <pos expand="Subjunctivus">Subj.</pos> </gramGrp>: <foreign xml:lang="gez">ወአጥብዐ፡ ዳንኤል፡ በልቡ፡ ከመ፡ ኢይብላዕ፡ እማእደ፡ ንጉሥ፡</foreign> <foreign xml:lang="grc">ἔθετο εἰς τὴν καρδίαν</foreign> <ref cRef="Dan." loc="1,8">Dan. 1,8</ref>; <ref cRef="Act." loc="20,16">Act. 20,16</ref>; <lbl expand="sequens">seq.</lbl> <gramGrp> <pos expand="Subjunctivus">Subj.</pos> </gramGrp> (rarius Imperf.) verbi: <foreign xml:lang="grc">κρίνειν</foreign> <ref cRef="Tit." loc="3,12">Tit. 3,12</ref>; <ref cRef="2 Cor. " loc="2,1">2 Cor.  2,1</ref> <lbl expand="romanae editionis">rom.</lbl>; <ref cRef="Act." loc="15,34">Act. 15,34</ref>; <foreign xml:lang="gez">አጥባዕኩ፡ እፈንዎ፡</foreign> (<lbl expand="romanae editionis">rom.</lbl> <foreign xml:lang="gez">እፌንዎ፡</foreign>) <ref cRef="Act." loc="25,25">Act. 25,25</ref>; <foreign xml:lang="gez">አጥባዕኩ፡ ኢይግባእ፡</foreign> (Laur.: <foreign xml:lang="gez">ኢይገብእ፡</foreign>) <ref cRef="4 Esr." loc="10,6">4 Esr. 10,6</ref>.
</sense> <sense n="4" xml:id="DA4"> <cit type="translation" xml:lang="la"><t:quote xmlns:t="http://www.tei-c.org/ns/1.0"> perseverare in re proposita </t:quote></cit>, <cit type="translation" xml:lang="la"><t:quote xmlns:t="http://www.tei-c.org/ns/1.0"> stare animo </t:quote></cit>, <cit type="translation" xml:lang="la"><t:quote xmlns:t="http://www.tei-c.org/ns/1.0"> constantem esse </t:quote></cit>: <foreign xml:lang="gez">መሐልኩ፡ ወአጥባዕኩ፡</foreign> <ref cRef="Ps." loc="118,106">Ps. 118,106</ref>; <ref cRef="Genz." loc="48">Genz. f. 48</ref>; <foreign xml:lang="gez">አጥባዕኩ፡ አንሰ፡</foreign> <foreign xml:lang="grc">ἀνδριοῦμαι</foreign> <ref cRef="Jer." loc="2,25">Jer. 2,25</ref>; <foreign xml:lang="gez">ወይቤሉ፡ አጥባዕነ፡ ንትሉ፡ በዕልወትነ፡</foreign> <cb n="1232" xml:id="c1232"/> <ref cRef="Jer." loc="18,12">Jer. 18,12</ref>; <ref cRef="Luc." loc="8,15">Luc. 8,15</ref>; <foreign xml:lang="gez">አጥብዑ፡ ሰማዕት፡</foreign> (martyres) <ref cRef="Clem." loc="153">Clem. f. 153</ref>; <lbl expand="sequens">seq.</lbl> <gramGrp> <case value="accusativus">Acc.</case> </gramGrp> Infin. <foreign xml:lang="gez">ናጥብዕ፡ ቀዊመ፡ በሃይማኖት፡</foreign> Mavâs. 30; <lbl expand="sequens">seq.</lbl> <foreign xml:lang="gez">በ፡</foreign> rei: <foreign xml:lang="gez">እመ፡ አጥባዕከ፡ በኑፋቄከ፡</foreign> M.M. f. 11; vel <foreign xml:lang="gez">ውስተ፡</foreign> <ref cRef="Phil." loc="3,16">Phil. 3,16</ref>; <foreign xml:lang="gez">ኢያጥብዖት፡</foreign> <cit type="translation" xml:lang="la"><t:quote xmlns:t="http://www.tei-c.org/ns/1.0"> inconstantia </t:quote></cit> <ref cRef="1 Cor." loc="8,7">1 Cor. 8,7</ref>; <foreign xml:lang="gez">አጥብዖተ፡ እግዚአብሔር፡</foreign> <foreign xml:lang="grc">ἀποτομία</foreign> <cit type="translation" xml:lang="la"><t:quote xmlns:t="http://www.tei-c.org/ns/1.0"> constantia in severitate </t:quote></cit> (<lbl expand="oppositum, -o, -nitur">opp.</lbl> <foreign xml:lang="gez">ምሕረት፡</foreign>) <ref cRef="Rom." loc="11,22">Rom. 11,22</ref>.
</sense> <sense n="5" xml:id="DA5"> <cit type="translation" xml:lang="la"><t:quote xmlns:t="http://www.tei-c.org/ns/1.0"> confidere </t:quote></cit>, <cit type="translation" xml:lang="la"><t:quote xmlns:t="http://www.tei-c.org/ns/1.0"> animo bono esse </t:quote></cit>, <cit type="translation" xml:lang="la"><t:quote xmlns:t="http://www.tei-c.org/ns/1.0"> fortem </t:quote></cit>, <cit type="translation" xml:lang="la"><t:quote xmlns:t="http://www.tei-c.org/ns/1.0"> firmum et imperterritum esse </t:quote></cit> animo: <foreign xml:lang="gez">አጥቢዖሙ፡</foreign> <foreign xml:lang="grc">ἀφόβους</foreign> <ref cRef="Phil." loc="1,14">Phil. 1,14</ref>; <foreign xml:lang="gez">አጥብዒ፡ ወኢይድከማ፡ እደዊኪ፡</foreign> <foreign xml:lang="grc">θάρσει</foreign> <ref cRef="Zeph." loc="3,16">Zeph. 3,16</ref>; <foreign xml:lang="gez">አጥብዑ፡ ወአጽንዑ፡ እደዊክሙ፡</foreign> <ref cRef="Zach." loc="8,13">Zach. 8,13</ref>; <foreign xml:lang="gez">አጥብዕ፡ በኀይልከ፡</foreign> <foreign xml:lang="grc">ἄνδρισαι</foreign> <ref cRef="Nah." loc="2,2">Nah. 2,2</ref>; <foreign xml:lang="gez">ወአጥብዐ፡ ሕዝብ፡ በቃለ፡ ሕዝቅያስ፡</foreign> <foreign xml:lang="grc">κατεθάρσησε</foreign> <ref cRef="2 Par." loc="32,8">2 Par. 32,8</ref>.</sense>
</sense> <sense n="B" xml:id="DB"> <sense n="1" xml:id="DB1"> <cit type="translation" xml:lang="la"><t:quote xmlns:t="http://www.tei-c.org/ns/1.0"> promtum facere </t:quote></cit> vel <cit type="translation" xml:lang="la"><t:quote xmlns:t="http://www.tei-c.org/ns/1.0"> parare animum </t:quote></cit>, <lbl expand="cum">c.</lbl> <foreign xml:lang="gez">ኀበ፡</foreign> alicui vel erga aliquem: <foreign xml:lang="gez">ኢታጠብዑ፡ ል</foreign>(<foreign xml:lang="gez">በ</foreign>)<foreign xml:lang="gez">ክሙ፡ ኀበ፡ እግዚአብሔር፡</foreign> <ref cRef="Herm." loc="18.">Herm. p. 18.</ref> 
</sense> <sense n="2" xml:id="DB2"> <cit type="translation" xml:lang="la"><t:quote xmlns:t="http://www.tei-c.org/ns/1.0"> animos facere </t:quote></cit> alicui, <cit type="translation" xml:lang="la"><t:quote xmlns:t="http://www.tei-c.org/ns/1.0"> animum </t:quote></cit> alicujus <cit type="translation" xml:lang="la"><t:quote xmlns:t="http://www.tei-c.org/ns/1.0"> confirmare </t:quote></cit>, <cit type="translation" xml:lang="la"><t:quote xmlns:t="http://www.tei-c.org/ns/1.0"> fortem </t:quote></cit>, <cit type="translation" xml:lang="la"><t:quote xmlns:t="http://www.tei-c.org/ns/1.0"> firmum et constantem reddere </t:quote></cit> aliquem: <foreign xml:lang="gez">አጥብዐ፡ አልባቢሆሙ፡</foreign> <ref cRef="2 Par." loc="32,6">2 Par. 32,6</ref>; <ref cRef="Jsp." loc="358 ">Jsp. p. 358 </ref>(<lbl expand="videas">vid.</lbl> sub <foreign xml:lang="gez">ጠብዐ፡</foreign>); <foreign xml:lang="gez">ኢያዕረፈ፡ አንደፊር፡</foreign> (Antipater) <foreign xml:lang="gez">ያጥብዖ፡</foreign> <ref cRef="Jsp." loc="307">Jsp. p. 307</ref>; virtutes quas dixit <foreign xml:lang="gez">ያጠብዕዎ፡ ለብእሲ፡ ለመዊት፡</foreign> <ref cRef="Jsp." loc="351">Jsp. p. 351</ref>; <foreign xml:lang="gez">መንፈስ፡ ቅዱስ፡ አጥብዓ፡</foreign> (sc. mulierem) <foreign xml:lang="gez">ከመ፡ ትኩን፡ ስምዐ፡</foreign> <ref cRef="Sx. Masc." loc="6 ">Sx. Masc. 6 </ref>Enc.; <foreign xml:lang="gez">ቀድሳ፡ ወአጥብዓ፡</foreign> <ref cRef="Kid." loc="21">Kid. f. 21</ref>; <foreign xml:lang="gez">አጥብዖሙ፡ ለመዋእያን፡ ሰማዕት፡</foreign> <ref cRef="Org." loc="1">Org. 1</ref>; etiam animum suum: <foreign xml:lang="gez">አጥብዑ፡ ልቦሙ፡</foreign> <ref cRef="Clem." loc="175">Clem. f. 175</ref>; Lud. ex Enc. Mar.
</sense> <sense n="3" xml:id="DB3"> in malam partem: <cit type="translation" xml:lang="la"><t:quote xmlns:t="http://www.tei-c.org/ns/1.0"> obfirmare </t:quote></cit> i.e. <cit type="translation" xml:lang="la"><t:quote xmlns:t="http://www.tei-c.org/ns/1.0"> obstinatum reddere </t:quote></cit> aliquem, Lud. ex <ref cRef="Org." loc="2.">Org. 2.</ref> 
</sense> 
</sense> </sense>

