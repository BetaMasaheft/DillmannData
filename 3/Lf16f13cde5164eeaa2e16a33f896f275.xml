               <sense xml:id="Dmain" xml:lang="la" source="#dillmann"> <gramGrp> <pos expand="Substantivum">subst.</pos> </gramGrp> (a <lbl expand="radix">rad.</lbl> <ref cRef="Nr." loc="2">Nr. 2</ref>; primitus significat <cit type="translation" xml:lang="la"><quote> nobiles </quote></cit>, <cit type="translation" xml:lang="la"><quote> liberos </quote></cit>, quorum praecipuum munus erat cum rege in expeditionem proficisci, <lbl expand="videas">vid.</lbl> Lud. in <lbl expand="lexicon">lex.</lbl> amh. p. 5: <foreign xml:lang="gez">ሐራ፡</foreign> <cit type="translation" xml:lang="la"><quote> liber homo </quote></cit>, <cit type="translation" xml:lang="la"><quote> ingenuus </quote></cit>) <sense n="1" xml:id="D1"> <lbl expand="collatio, -is vel collectivum,&#xA;                            -e">coll.</lbl> <cit type="translation" xml:lang="la"><quote> exercitus </quote></cit>, <cit type="translation" xml:lang="la"><quote> copiae </quote></cit>: <foreign xml:lang="gez">ኵሉ፡ ሐራሁ፡</foreign> <foreign xml:lang="grc">στρατιά</foreign> <ref cRef="Ex. " loc="14,4">Ex.  14,4</ref>; <ref cRef="Judith" loc="5,24">Judith 5,24</ref>; <ref cRef="Judith" loc="6,6">Judith 6,6</ref>; <ref cRef="Judith" loc="7,1">Judith 7,1</ref>; <foreign xml:lang="gez">፰፻፡ ብእሲ፡ ሐራ፡</foreign> <ref cRef="2 Reg." loc="23,8">2 Reg. 23,8</ref>; <ref cRef="3 Reg." loc="22,34">3 Reg. 22,34</ref>; <foreign xml:lang="grc">στρατιῶται</foreign>, <foreign xml:lang="grc">στρατευόμενοι</foreign> <ref cRef="Matth." loc="8,9">Matth. 8,9</ref>; <ref cRef="Marc." loc="15,16">Marc. 15,16</ref>; <ref cRef="Luc. " loc="3,14">Luc.  3,14</ref>; <foreign xml:lang="gez">ሊቀ፡ ሐራ፡</foreign> <ref cRef="Act." loc="28,16">Act. 28,16</ref> <lbl expand="romanae editionis">rom.</lbl>; copiae coelestes: <foreign xml:lang="gez">ሐራ፡ ሰማይ፡</foreign> <ref cRef="Luc." loc="2,13">Luc. 2,13</ref>; <ref cRef="Apoc." loc="19,14">Apoc. 19,14</ref>; <ref cRef="Hen. " loc="104,6">Hen.  104,6</ref>. 
</sense> <sense n="2" xml:id="D2"> <lbl expand="collatio, -is vel collectivum,&#xA;                            -e">coll.</lbl> <cit type="translation" xml:lang="la"><quote> tribuni </quote></cit>, <cit type="translation" xml:lang="la"><quote> centuriones </quote></cit>, <cit type="translation" xml:lang="la"><quote> praefecti militum </quote></cit>: <foreign xml:lang="gez">ይሬስዮሙ፡ ሐራሁ፡ ወወዓሊሁ፡</foreign> <foreign xml:lang="grc">ἑκατοντάρχους καὶ χιλιάρχους</foreign> <ref cRef="1 Reg." loc="8,12">1 Reg. 8,12</ref>; <foreign xml:lang="gez">ወዓሊሁ፡ ወሐራሁ፡</foreign> <foreign xml:lang="grc">παρατρέχοντες</foreign> (<lbl expand="videas">vid.</lbl> 11,6) <foreign xml:lang="grc">καὶ τριστάται</foreign> <ref cRef="4 Reg." loc="10,25">4 Reg. 10,25</ref>. 
</sense> <sense n="3" xml:id="D3"> <lbl expand="">sing.</lbl> <cit type="translation" xml:lang="la"><quote> miles </quote></cit>, <cit type="translation" xml:lang="la"><quote> vir militaris </quote></cit> <ref cRef="Act." loc="10,7">Act. 10,7</ref> (Platt); <foreign xml:lang="gez">አነ፡ ሐራ፡ ለክርስቶስ፡</foreign> (ait Minas) Mavâs. 18; item in Deg. 
</sense> </sense>
               
            <sense xml:id="Tmain" xml:lang="en" source="#traces"><cit type="transcription" xml:lang="gez"><quote>ḥarā</quote></cit> <sense n="C" xml:id="TC"> <sense n="a" xml:id="TCa"> for <foreign xml:lang="gez">ሊቀ፡ ሐራ፡</foreign> <lbl expand="videas">vid.</lbl> <foreign xml:lang="gez">ሊቅ፡</foreign> (Grébaut, Leslau) 
</sense> <sense n="b" xml:id="TCb"> for <foreign xml:lang="gez">ሊቀ፡ ሐራ፡ ለአፍራስ፡</foreign> <lbl expand="videas">vid.</lbl> <foreign xml:lang="gez">ሊቅ፡</foreign> (Grébaut) 
</sense> <sense n="c" xml:id="TCc"> for <foreign xml:lang="gez">ሊቀ፡ ሐራ፡ ለአጋር፡</foreign> <lbl expand="videas">vid.</lbl> <foreign xml:lang="gez">ሊቅ፡</foreign> (Grébaut) 
</sense> <sense n="d" xml:id="TCd"> for <foreign xml:lang="gez">መላህቅተ፡ ሐራ፡</foreign> <lbl expand="videas">vid.</lbl> <foreign xml:lang="gez">መላህቅት፡</foreign> (Grébaut) 
</sense> <sense n="e" xml:id="TCe"> for <foreign xml:lang="gez">መስተራትዕ፡ ሐራ፡</foreign> <lbl expand="videas">vid.</lbl> <foreign xml:lang="gez">መስተራትዕ፡</foreign> (Leslau) 
</sense> 
