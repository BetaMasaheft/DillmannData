            <entry xml:id="Lfc20cbef2c3149c0835ec3a0c3958ed4" n="4973">
               <form>
                  <foreign xml:lang="gez">አንተገ</foreign>
               </form>
               <sense xml:lang="la" source="#dillmann" xml:id="Dmain"> <subc>II,1</subc> <sense n="1" xml:id="D1"> <sense n="a" xml:id="D1a"> <cit type="translation" xml:lang="la">
                                    <quote> imminuere </quote>
                                </cit>, <cit type="translation" xml:lang="la">
                                    <quote> deminuere </quote>
                                </cit>, <lbl expand="cum">c.</lbl> <gramGrp> <case value="accusativus">Acc.</case> </gramGrp> ejus, quod minuitur, <foreign xml:lang="gez">ዘኢያነትግ፡ ትእዛዘ፡ እግዚአብሔር፡</foreign> <ref cRef="Clem." loc="86.">Clem. f. 86.</ref> </sense> <sense n="b" xml:id="D1b"> <cit type="translation" xml:lang="la">
                                    <quote> demere </quote>
                                </cit>, <cit type="translation" xml:lang="la">
                                    <quote> detrahere </quote>
                                </cit>, <cit type="translation" xml:lang="la">
                                    <quote> omittere </quote>
                                </cit>: <foreign xml:lang="gez">አልቦ፡ ከመ፡ ያንትጉ፡</foreign> <foreign xml:lang="grc">οὐκ ἔστιν ἐλαττῶσαι</foreign> (<lbl expand="oppositum, -o, -nitur">opp.</lbl> <foreign xml:lang="grc">προσθεῖναι</foreign>) <ref cRef="Sir." loc="18,6">Sir. 18,6</ref>; <foreign xml:lang="gez">ይሜህሩ፡ ካልአ፡ ወያነትጉ፡ ወይዌስኩ፡</foreign> <ref cRef="Kid." loc="37">Kid. f. 37</ref>; <lbl expand="cum">c.</lbl> <foreign xml:lang="gez">እምነ፡</foreign> ejus, quod minuitur: <foreign xml:lang="gez">ኢታንትግ፡ እምኔሁ፡ ቃለ፡</foreign> <ref cRef="Jer." loc="33,2">Jer. 33,2</ref>; <ref cRef="Deut." loc="4,2">Deut. 4,2</ref> <lbl expand="varia lectio">var.</lbl>; <ref cRef="Ex. " loc="30,15">Ex.  30,15</ref>; <foreign xml:lang="gez">ዘወሰክነ፡ ወዘአንተግነ፡</foreign> <lbl expand="Novum Testamentum">N.T.</lbl> <lbl expand="romanae editionis">rom.</lbl> f. 157,2.</sense> </sense> <sense n="2" xml:id="D2"> <cit type="translation" xml:lang="la">
                                <quote> intermittere </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> cessare </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> desinere </quote>
                            </cit>, <ref cRef="Kuf." loc="24">Kuf. p. 24</ref>; <foreign xml:lang="gez">ወታንትግ፡ ዛቲ፡ መቅሠፍት፡ እምሕዝብየ፡</foreign> <foreign xml:lang="grc">παύσεται</foreign> <ref cRef="1 Par." loc="21,22">1 Par. 21,22</ref>; <lbl expand="construitur cum">c.c.</lbl> <gramGrp> <case value="accusativus">Acc.</case> </gramGrp> rei vel Infin.: <foreign xml:lang="gez">ኢታንትግ፡ እከየ፡ ላዕለ፡ ሰብእ፡</foreign> (sic diabolus homini suadet) <ref cRef="Clem." loc="226">Clem. f. 226</ref>; <foreign xml:lang="gez">አንተጉ፡ እንከ፡ ሐኒጸ፡ ሀገር፡</foreign> <ref cRef="Kuf." loc="10">Kuf. 10</ref>; <ref cRef="Jer." loc="17,8">Jer. 17,8</ref> <lbl expand="varia lectio">var.</lbl>; <ref cRef="Luc." loc="7,45">Luc. 7,45</ref>; <ref cRef="Eph." loc="1,16">Eph. 1,16</ref>; <lbl expand="cum">c.</lbl> <foreign xml:lang="gez">እንዘ፡</foreign>, ut: <foreign xml:lang="gez">እለ፡ ኢያነትጉ፡ እንዘ፡ ይሴብሑ፡</foreign> <ref cRef="Kid." loc="4">Kid. f. 4</ref>; <lbl expand="cum">c.</lbl> <foreign xml:lang="gez">እምነ፡</foreign> rei <ref cRef="Hen." loc="23,2">Hen. 23,2</ref> <lbl expand="varia lectio">var.</lbl> </sense> </sense>
               
