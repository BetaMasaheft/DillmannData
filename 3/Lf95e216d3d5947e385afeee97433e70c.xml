         <div>
            <entry xml:id="Lf95e216d3d5947e385afeee97433e70c" n="9845">
               <form>
                  <foreign xml:lang="gez">ተፈልጠ</foreign>
               </form>
               <sense xml:lang="la" source="#dillmann" xml:id="Dmain"> <subc>III,1</subc> <sense n="1" xml:id="D1"> <sense n="a" xml:id="D1a"> <gramGrp> <pos expand="reflexivum">refl.</pos> </gramGrp> <cit type="translation" xml:lang="la">
                                    <quote> se sejungere </quote>
                                </cit> vel <cit type="translation" xml:lang="la">
                                    <quote> secernere </quote>
                                </cit> ab aliquo: <foreign xml:lang="gez">ተፈለጥ፡ እምአሕዛብ፡</foreign> <ref cRef="Kuf." loc="22">Kuf. 22</ref>; <ref cRef="2 Esr." loc="9,1">2 Esr. 9,1</ref>; <ref cRef="2 Esr." loc="10,11">2 Esr. 10,11</ref>; <foreign xml:lang="gez">ተፈልጡ፡ እምኵሉ፡ ሕዝብ፡ ነኪር፡</foreign> <ref cRef="2 Esr." loc="19,2">2 Esr. 19,2</ref>; <ref cRef="Jud." loc="4,11">Jud. 4,11</ref>; <foreign xml:lang="gez">ኢይትፈለጥ፡ እምኔሁ፡</foreign> <ref cRef="Prov." loc="8,30">Prov. 8,30</ref>; <foreign xml:lang="gez">ዘይፈቅድ፡ ይትፈለጥ፡ እምአዕርክት፡</foreign> <ref cRef="Prov." loc="18,1">Prov. 18,1</ref>; <ref cRef="Luc." loc="12,53">Luc. 12,53</ref>; <ref cRef="2 Cor." loc="6,17">2 Cor. 6,17</ref>; <gramGrp> <pos expand="absolute">abs.</pos> </gramGrp>: <foreign xml:lang="gez">ተፈለጡ፡</foreign> <foreign xml:lang="grc">ἀφορίσθητε</foreign> <ref cRef="Jes." loc="52,11">Jes. 52,11</ref>; <foreign xml:lang="gez">ተፈልጡ፡ በበነገዶሙ፡</foreign> <ref cRef="Jos." loc="7,17">Jos. 7,17</ref>; <ref cRef="Jos." loc="7,18">Jos. 7,18</ref>. <sense n="α" xml:id="D1aα"> <cit type="translation" xml:lang="la">
                                        <quote> secedere </quote>
                                    </cit>, <cit type="translation" xml:lang="la">
                                        <quote> discedere </quote>
                                    </cit>: <foreign xml:lang="gez">ተፈልጠ፡ እምአቡሁ፡</foreign> <ref cRef="Kuf." loc="7">Kuf. 7</ref>; <ref cRef="Kuf." loc="11">Kuf. 11</ref>; in malam partem: <foreign xml:lang="gez">ተግሕሠ፡ ወተፈልጠ፡ እምእግዚኡ፡</foreign> <foreign xml:lang="grc">ἀπέστη</foreign> <ref cRef="2 Par. " loc="13,6">2 Par.  13,6</ref>.</sense> <sense n="β" xml:id="D1aβ"> <cit type="translation" xml:lang="la">
                                        <quote> se dividere </quote>
                                    </cit>, <ref cRef="Gen." loc="2,10">Gen. 2,10</ref>.</sense> <sense n="γ" xml:id="D1aγ"> <cit type="translation" xml:lang="la">
                                        <quote> se dispergere </quote>
                                    </cit> vel <cit type="translation" xml:lang="la">
                                        <quote> diffundere </quote>
                                    </cit>: <ref cRef="Gen." loc="10,5">Gen. 10,5</ref>; <foreign xml:lang="gez">ተፈልጡ፡ አርድእት፡ እስከ፡ አጽናፈ፡ ዓለም፡ ከመ፡ ይስብኩ፡ ዜና፡ ወንጌል፡</foreign> <ref cRef="Clem. " loc="1.">Clem.  f. 1.</ref> </sense> <sense n="δ" xml:id="D1aδ"> <cit type="translation" xml:lang="la">
                                        <quote> se abstinere </quote>
                                    </cit>: <foreign xml:lang="gez">በሌሊት፡ ዘባቲ፡ ይጠመቅ፡ ወልዶሙ፡ ይደሉ፡ ላዕሌሆሙ፡</foreign> (parentibus) <foreign xml:lang="gez">ተፈልጦ፡</foreign> F.M. 10,2.</sense> <sense n="ε" xml:id="D1aε"> <cit type="translation" xml:lang="la">
                                        <quote> divortium facere </quote>
                                    </cit> cum aliquo vel aliqua: <foreign xml:lang="gez">እስከ፡ ይትፈለጡ፡</foreign> F.M. 5,2; <foreign xml:lang="gez">እመ፡ ፈቀደት፡ ከመ፡ ትትፈለጥ፡ እምታ፡</foreign> F.M. 5,5; F.M. 12,1; F.M. 12,2.</sense> </sense> <sense n="b" xml:id="D1b"> <gramGrp> <pos expand="passivum">pass.</pos> </gramGrp> <cit type="translation" xml:lang="la">
                                    <quote> separari </quote>
                                </cit>, <cit type="translation" xml:lang="la">
                                    <quote> sejungi </quote>
                                </cit>, <cit type="translation" xml:lang="la">
                                    <quote> dirimi </quote>
                                </cit>: <foreign xml:lang="gez">ነፍስ፡ እመ፡ ተፈልጠት፡ እምሥጋ፡</foreign> <ref cRef="Jsp." loc="381">Jsp. p. 381</ref>; <foreign xml:lang="gez">ብእሲ፡ ይትፈለጥ፡ እምእኁሁ፡</foreign> <ref cRef="Jer." loc="13,14">Jer. 13,14</ref>; <ref cRef="Hen." loc="22,8">Hen. 22,8</ref>; <ref cRef="Hen. " loc="22,9">Hen.  22,9</ref>; <ref cRef="Hen." loc="22,11">Hen. 22,11</ref>. <sense n="α" xml:id="D1bα"> <foreign xml:lang="gez">ተፈልጠ፡</foreign> <cit type="translation" xml:lang="la">
                                        <quote> intervallum interpositum est </quote>
                                    </cit> <ref cRef="Hen." loc="22,12">Hen. 22,12</ref>.</sense> <sense n="β" xml:id="D1bβ"> <cit type="translation" xml:lang="la">
                                        <quote> dividi </quote>
                                    </cit>: <foreign xml:lang="gez">በትእዛዘ፡ እግዚአብሔር፡ ይትፈለጥ፡ ኵሉ፡ መዋዕል፡</foreign> <ref cRef="Sir." loc="36,8">Sir. 36,8</ref>.</sense> <sense n="γ" xml:id="D1bγ"> <cit type="translation" xml:lang="la">
                                        <quote> privari </quote>
                                    </cit> <ref cRef="Gen." loc="48,11">Gen. 48,11</ref>.</sense> <sense n="δ" xml:id="D1bδ"> <cit type="translation" xml:lang="la">
                                        <quote> dispergi </quote>
                                    </cit>: <foreign xml:lang="gez">አባግዒሁ፡ ዘተፈልጠ፡</foreign> <ref cRef="Hez. " loc="34,12">Hez.  34,12</ref>.</sense> <sense n="ε" xml:id="D1bε"> de conjugibus: <foreign xml:lang="gez">ተፈልጠት፡ እምኔሁ፡</foreign> (<foreign xml:lang="gez">ለምታ፡</foreign>) <foreign xml:lang="gez">በሞት፡</foreign> <ref cRef="Did." loc="14.">Did. 14.</ref> </sense> </sense> </sense> <sense n="2" xml:id="D2"> <cit type="translation" xml:lang="la">
                                <quote> segregari </quote>
                            </cit>, <sense n="a" xml:id="D2a"> <cit type="translation" xml:lang="la">
                                    <quote> secludi </quote>
                                </cit>: <ref cRef="2 Thess." loc="3,14">2 Thess. 3,14</ref> Platt; <foreign xml:lang="gez">ለይትፈለጥ፡</foreign> <foreign xml:lang="grc">ἀφοριζέσθω</foreign> Syn., <lbl expand="exempli gratia">ex. gr.</lbl> <ref cRef="Can. Ap." loc="5.">Can. Ap. 5.</ref> </sense> <sense n="b" xml:id="D2b"> <cit type="translation" xml:lang="la">
                                    <quote> seligi </quote>
                                </cit>, <ref cRef="Rom." loc="1,1">Rom. 1,1</ref>.</sense> </sense> <sense n="3" xml:id="D3"> <cit type="translation" xml:lang="la">
                                <quote> secerni </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> distingui </quote>
                            </cit>: <foreign xml:lang="gez">ባቲ፡</foreign> (<foreign xml:lang="gez">ለወርኅ፡</foreign>) <foreign xml:lang="gez">ይትፈለጥ፡ መዋዕል፡</foreign> <ref cRef="Sir." loc="43,7">Sir. 43,7</ref>; <lbl expand="cum">c.</lbl> <lbl expand="duplex">dupl.</lbl> <foreign xml:lang="gez">ማእከለ፡</foreign> <ref cRef="Lev." loc="10,10">Lev. 10,10</ref>; <ref cRef="Sx. Sen." loc="26 Enc.">Sx. Sen. 26 Enc.</ref>
                        </sense> </sense>
               
