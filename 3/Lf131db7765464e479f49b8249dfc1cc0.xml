               <sense xml:id="Dmain" xml:lang="la" source="#dillmann"> <gramGrp> <pos expand="Substantivum">subst.</pos> </gramGrp> (sine dubio pro <foreign xml:lang="gez">መሪት፡</foreign> vel <foreign xml:lang="gez">መራት፡</foreign>), [amh. <lbl expand="idem">id.</lbl>], <gramGrp> <gen value="not found">m.</gen> </gramGrp> et <gramGrp> <gen value="femininus">fem.</gen> </gramGrp>: „terra omnis generis, quae artefacta in pulverem et madefacta in lutum seu coenum resolvitur“ (Lud.), <sense n="1" xml:id="D1"> <cit type="translation" xml:lang="la"><quote> pulvis terrae </quote></cit>, <cit type="translation" xml:lang="la"><quote> humus </quote></cit>, <foreign xml:lang="grc">γῆ</foreign> <ref cRef="Gen." loc="3,19">Gen. 3,19</ref>; <foreign xml:lang="gez">አንተ፡ መሬት፡</foreign> <ref cRef="4 Esr." loc="5,15">4 Esr. 5,15</ref>; <foreign xml:lang="gez">ታገብአኒ፡ ውስተ፡ መሬት፡</foreign> <ref cRef="Job" loc="10,9">Job 10,9</ref>; <ref cRef="Job" loc="34,15">Job 34,15</ref>; <foreign xml:lang="gez">ከመ፡ እኩን፡ መሬተ፡</foreign> <ref cRef="Tob." loc="3,6">Tob. 3,6</ref>; <foreign xml:lang="gez">መሬት፡ ወሐመድ፡</foreign> <ref cRef="Job " loc="42,6">Job  42,6</ref>; <ref cRef="Sir." loc="10,9">Sir. 10,9</ref>; <foreign xml:lang="gez">ኰኵሕ፡ ኀበ፡ አልቦ፡ መሬተ፡</foreign> <ref cRef="Matth." loc="13,5">Matth. 13,5</ref>; <ref cRef="Jes." loc="47,1">Jes. 47,1</ref>; <ref cRef="Gen." loc="26,15">Gen. 26,15</ref>; <ref cRef="Kuf." loc="24">Kuf. 24</ref>; <ref cRef="4 Esr." loc="1,5">4 Esr. 1,5</ref>; <ref cRef="4 Esr." loc="5,33">4 Esr. 5,33</ref>; <ref cRef="4 Esr." loc="8,4">4 Esr. 8,4</ref>; <foreign xml:lang="grc">χοῦς</foreign> <ref cRef="Koh." loc="3,20">Koh. 3,20</ref>; <ref cRef="Koh." loc="12,7">Koh. 12,7</ref>; <ref cRef="Ps." loc="1,5">Ps. 1,5</ref>; <ref cRef="Deut." loc="28,24">Deut. 28,24</ref>; <foreign xml:lang="gez">ወደዩ፡ መሬተ፡ ውስተ፡ ርእሶሙ፡</foreign> <ref cRef="Thren." loc="2,10">Thren. 2,10</ref>; <ref cRef="Hez." loc="26,4">Hez. 26,4</ref>; <ref cRef="Hez." loc="26,12">Hez. 26,12</ref>; <foreign xml:lang="gez">ከመ፡ መሬት፡ ንሕነ፡</foreign> <ref cRef="Ps." loc="102,14">Ps. 102,14</ref>; <ref cRef="Act." loc="18,19">Act. 18,19</ref>; <foreign xml:lang="grc">χῶμα</foreign> <foreign xml:lang="gez">መሬተ፡ ምድር፡</foreign> <ref cRef="Ex." loc="8,13">Ex. 8,13</ref>; <foreign xml:lang="gez">ረሰይኮን፡ ለአህጉር፡ መሬተ፡</foreign> <ref cRef="Jes." loc="25,2">Jes. 25,2</ref>; <foreign xml:lang="gez">እለ፡ ይነውሙ፡ ውስተ፡ መሬተ፡ ምድር፡</foreign> <ref cRef="Dan." loc="12,2">Dan. 12,2</ref>. – Est unum e quatuor elementis <lbl expand="videas">vid.</lbl> <foreign xml:lang="gez">ጠባይዕ፡</foreign> 
</sense> <sense n="2" xml:id="D2"> <cit type="translation" xml:lang="la"><quote> lutum </quote></cit>, <cit type="translation" xml:lang="la"><quote> argilla </quote></cit>, <cit type="translation" xml:lang="la"><quote> creta figularis </quote></cit>, <cit type="translation" xml:lang="la"><quote> calx praeparata </quote></cit>, (<cit type="translation" xml:lang="la"><quote> mortarium </quote></cit>) <foreign xml:lang="grc">πηλός</foreign>: <foreign xml:lang="gez">አብያተ፡ መሬት፡</foreign> <ref cRef="Job" loc="4,19">Job 4,19</ref>; <foreign xml:lang="gez">ሥጋክሙ፡ መሬት፡</foreign> <ref cRef="Job" loc="13,12">Job 13,12</ref> (alias <foreign xml:lang="gez">ጽቡር፡</foreign>); <foreign xml:lang="grc">κονία</foreign> <foreign xml:lang="gez">መሬት፡ ጸዐዳ፡</foreign> <ref cRef="Deut. " loc="27,2">Deut.  27,2</ref>; <ref cRef="Deut." loc="27,4">Deut. 27,4</ref>; <foreign xml:lang="grc">χοῦς</foreign>, qui in aedificiis exstruendis adhibetur <ref cRef="Lev." loc="14,42">Lev. 14,42</ref>; <ref cRef="Gad. Lal." loc="">Gad. Lal. </ref> (saepius). 
</sense> <sense n="3" xml:id="D3"> <cit type="translation" xml:lang="la"><quote> ramenta </quote></cit>, <cit type="translation" xml:lang="la"><quote> rudus </quote></cit>, ut lapidum caesorum <ref cRef="Gad. Lal." loc="">Gad. Lal. </ref> (<lbl expand="videas">vid.</lbl> <foreign xml:lang="gez">ምጽራብ፡</foreign>); aedificiorum dirutorum <ref cRef="Jes." loc="25,2">Jes. 25,2</ref>. 
</sense> <sense n="4" xml:id="D4"> <cit type="translation" xml:lang="la"><quote> coenum </quote></cit>, <cit type="translation" xml:lang="la"><quote> stercus </quote></cit>, <foreign xml:lang="grc">κοπρία</foreign> <ref cRef="Luc." loc="13,8">Luc. 13,8</ref>; 4 <ref cRef="Reg." loc="9,35">Reg. 9,35</ref>; <ref cRef="Job" loc="2,8">Job 2,8</ref>; <ref cRef="Ps." loc="82,9">Ps. 82,9</ref>; <ref cRef="Jes." loc="5,25">Jes. 5,25</ref>; <ref cRef="Thren." loc="4,5">Thren. 4,5</ref>. 
</sense> </sense>
               
            <sense xml:id="Tmain" xml:lang="en" source="#traces"><cit type="transcription" xml:lang="gez"><quote>maret</quote></cit>, <lbl expand="pluralis">Pl.</lbl> <foreign xml:lang="gez">መሬታት፡</foreign> <cit type="transcription" xml:lang="gez"><quote>maretāt</quote></cit> <sense n="G" xml:id="TG"> <foreign xml:lang="gez">መሬት፡</foreign> <cit type="transcription" xml:lang="gez"><quote>maret</quote></cit>, <lbl expand="pluralis">Pl.</lbl> <foreign xml:lang="gez">መሬታት፡</foreign> <cit type="transcription" xml:lang="gez"><quote>maretāt</quote></cit> «<cit type="translation" xml:lang="fr"><quote>terrain</quote></cit>, <cit type="translation" xml:lang="fr"><quote>champ</quote></cit>, <cit type="translation" xml:lang="fr"><quote>poussière</quote></cit>, <cit type="translation" xml:lang="fr"><quote>terre</quote></cit>, <cit type="translation" xml:lang="fr"><quote>terreau</quote></cit>» ― <foreign xml:lang="am">መሬት፡</foreign> Ms. <ref type="BM" target="BNFabb217">BNFabb217</ref>, fol. 36v. <lb/>
 <bibl><ptr target="bm:Grebaut1952SuppLexicon"/><citedRange unit="page"> 85</citedRange></bibl> 
</sense> <sense n="L" xml:id="TL"> <foreign xml:lang="gez">መሬት፡</foreign> <cit type="transcription" xml:lang="gez"><quote>maret</quote></cit> <cit type="translation" xml:lang="en"><quote>earth</quote></cit>, <cit type="translation" xml:lang="en"><quote>dust of the ground</quote></cit>, <cit type="translation" xml:lang="en"><quote>dirt</quote></cit>, <cit type="translation" xml:lang="en"><quote>soil</quote></cit>, <cit type="translation" xml:lang="en"><quote>clay</quote></cit>, <cit type="translation" xml:lang="en"><quote>plaster</quote></cit>, <cit type="translation" xml:lang="en"><quote>dung</quote></cit> <lb/>
 <foreign xml:lang="gez">መሬት፡ ጸዓዳ</foreign> <cit type="transcription" xml:lang="gez"><quote>maret ṣaʿādā</quote></cit> <cit type="translation" xml:lang="en"><quote>plaster</quote></cit>, <cit type="translation" xml:lang="en"><quote>clay</quote></cit> <bibl><ptr target="bm:Leslau1987dic"/><citedRange unit="page"> 361a</citedRange></bibl> 
</sense> </sense></entry>
