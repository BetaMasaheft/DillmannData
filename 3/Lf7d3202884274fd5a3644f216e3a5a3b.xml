            <entry xml:id="Lf7d3202884274fd5a3644f216e3a5a3b" n="6756">
               <form>
                  <foreign xml:lang="gez">አውደቀ</foreign>
               </form>
               <sense xml:lang="la" source="#dillmann" xml:id="Dmain"> <subc>II,1</subc> <sense n="1" xml:id="D1"> <sense n="a" xml:id="D1a"> <cit type="translation" xml:lang="la">
                                    <quote> cadere sinere </quote>
                                </cit> <ref cRef="Marc." loc="7,28">Marc. 7,28</ref>; <ref cRef="1 Cor." loc="13,8">1 Cor. 13,8</ref>.</sense> <sense n="b" xml:id="D1b"> <cit type="translation" xml:lang="la">
                                    <quote> excidere sinere </quote>
                                </cit>, <ref cRef="Num." loc="35,17">Num. 35,17</ref>; <ref cRef="Num." loc="35,18">Num. 35,18</ref>.</sense> <sense n="c" xml:id="D1c"> decidere sinere i.e. <cit type="translation" xml:lang="la">
                                    <quote> demittere </quote>
                                </cit> faciem <ref cRef="1 Reg." loc="1,19">1 Reg. 1,19</ref>; animum: <foreign xml:lang="gez">ኢታውድቁ፡ ልበክሙ፡</foreign> <ref cRef="2 Esr." loc="18,10">2 Esr. 18,10</ref>.</sense> </sense> <sense n="2" xml:id="D2"> <cit type="translation" xml:lang="la">
                                <quote> cadere facere </quote>
                            </cit> vel <cit type="translation" xml:lang="la">
                                <quote> jubere </quote>
                            </cit>: <foreign xml:lang="gez">ያወድቅ፡ ርእሶ፡</foreign> <ref cRef="Prov." loc="13,3">Prov. 13,3</ref>; <foreign xml:lang="gez">ለብዙኃን፡ አውደቆሙ፡ ትዕቢተ፡ ልቦሙ፡</foreign> <ref cRef="Sir." loc="3,24">Sir. 3,24</ref>; <foreign xml:lang="gez">ኢታውድቀ፡</foreign> (ne te cadere faciat) <ref cRef="Sir." loc="28,26">Sir. 28,26</ref>; <foreign xml:lang="gez">ጽዋዐ፡ መዐቱ፡ ዘያወድቀኪ፡</foreign> <ref cRef="Jes. " loc="51,17">Jes.  51,17</ref>; <foreign xml:lang="gez">አውደቀነ፡</foreign> <foreign xml:lang="grc">ἔπαισας</foreign> <ref cRef="Jer." loc="14,19">Jer. 14,19</ref>. <sense n="a" xml:id="D2a"> <cit type="translation" xml:lang="la">
                                    <quote> jacĕre </quote>
                                </cit>, <cit type="translation" xml:lang="la">
                                    <quote> dejicere </quote>
                                </cit>, <cit type="translation" xml:lang="la">
                                    <quote> projicere </quote>
                                </cit>, <ref cRef="Num." loc="35,20">Num. 35,20</ref>; <foreign xml:lang="gez">አውደቀት፡ በድኖ፡ እምነ፡ ምስካቡ፡</foreign> <ref cRef="Judith" loc="13,9">Judith 13,9</ref>; <ref cRef="Hen." loc="90,9">Hen. 90,9</ref>; <ref cRef="Hen. " loc="99,16">Hen.  99,16</ref>; <foreign xml:lang="gez">አውደቆ፡ ውስተ፡ ማዕምቅ፡</foreign> <foreign xml:lang="grc">ἔβαλεν αὐτὸν εἰς</foreign> <ref cRef="Apoc." loc="20,3">Apoc. 20,3</ref>; muros,ut: <foreign xml:lang="gez">አውደቀ፡ አረፋቲሃ፡ ለጌት፡</foreign> <ref cRef="2 Par." loc="26,6">2 Par. 26,6</ref>; <ref cRef="2 Reg." loc="20,15">2 Reg. 20,15</ref>; <foreign xml:lang="gez">አብያቲሆሙ፡ ለዐበይት፡ አውደቀት፡</foreign> <ref cRef="Sir." loc="28,14">Sir. 28,14</ref>; <foreign xml:lang="gez">አውደቀ፡ አህጉረ፡ ጽኑዓተ፡</foreign> <ref cRef="Jes." loc="25,2">Jes. 25,2</ref>.</sense> <sense n="b" xml:id="D2b"> <cit type="translation" xml:lang="la">
                                    <quote> caedere </quote>
                                </cit>arbores: <foreign xml:lang="gez">ከመ፡ ታውድቂ፡ ኵሎ፡ ዕፀ፡</foreign> <ref cRef="Hez. " loc="21,10">Hez.  21,10</ref>.</sense> <sense n="c" xml:id="D2c"> <cit type="translation" xml:lang="la">
                                    <quote> prosternere </quote>
                                </cit>, <cit type="translation" xml:lang="la">
                                    <quote> occidere </quote>
                                </cit>, <cit type="translation" xml:lang="la">
                                    <quote> caedere </quote>
                                </cit> gladio, <cit type="translation" xml:lang="la">
                                    <quote> in fugam convertere </quote>
                                </cit>: <ref cRef="Dan." loc="8,7">Dan. 8,7</ref>; <ref cRef="1 Reg." loc="4,3">1 Reg. 4,3</ref>; <foreign xml:lang="gez">አውደቅዎ፡ በሰይፍ፡</foreign> <ref cRef="2 Par." loc="32,21">2 Par. 32,21</ref>; <ref cRef="Sir." loc="26,28">Sir. 26,28</ref>; <foreign xml:lang="gez">ቀተሎሙ፡ ለኢሎፍሊ፡ ወአውደቆሙ፡</foreign> <ref cRef="1 Par." loc="18,1">1 Par. 18,1</ref>; <ref cRef="1 Par." loc="19,18">1 Par. 19,18</ref>.</sense> <sense n="d" xml:id="D2d"> <cit type="translation" xml:lang="la">
                                    <quote> incidere facere </quote>
                                </cit>: <foreign xml:lang="gez">ይፈቅድ፡ ያውድቀ፡ ውስተ፡ እዴሆሙ፡</foreign> <ref cRef="Kuf." loc="48">Kuf. 48</ref>; <foreign xml:lang="gez">ይመክር፡ ውስተ፡ ግብ፡ ያውድቀ፡</foreign> <ref cRef="Sir. " loc="12,16">Sir.  12,16</ref>.</sense> <sense n="e" xml:id="D2e"> <cit type="translation" xml:lang="la">
                                    <quote> concidere facere </quote>
                                </cit>, <ref cRef="Num." loc="5,21">Num. 5,21</ref>.</sense> <sense n="f" xml:id="D2f"> <cit type="translation" xml:lang="la">
                                    <quote> deprimere </quote>
                                </cit>: <foreign xml:lang="gez">አውደቆ፡ ልቦ፡</foreign> <ref cRef="1 Reg." loc="24,6">1 Reg. 24,6</ref>; <foreign xml:lang="gez">ፈቀደ፡ ከመ፡ ያውድቅ፡ ተመክሖቶሙ፡ ወትዕቢቶሙ፡</foreign> <ref cRef="Chrys. ho. " loc="12.">Chrys. ho.  12.</ref> </sense> <sense n="g" xml:id="D2g"> <cit type="translation" xml:lang="la">
                                    <quote> cadere </quote>
                                </cit> vel <cit type="translation" xml:lang="la">
                                    <quote> labi facere </quote>
                                </cit> in peccatum vel peccato: <foreign xml:lang="gez">ሰይጣን፡ አውደቃ፡</foreign> (eam) <foreign xml:lang="gez">በፍትወተ፡ ዝሙት፡</foreign> <ref cRef="Clem." loc="221">Clem. f. 221</ref>; <foreign xml:lang="gez">ያወድቅዎ፡ በዝሙት፡</foreign> Lud. e Vit. Baam.</sense> <sense n="h" xml:id="D2h"> <cit type="translation" xml:lang="la">
                                    <quote> excidere facere </quote>
                                </cit> vel <cit type="translation" xml:lang="la">
                                    <quote> jubere </quote>
                                </cit> aliquem a munere (<cit type="translation" xml:lang="la">
                                    <quote> amovere </quote>
                                </cit>): <foreign xml:lang="gez">በእንተ፡ ምክንያት፡ እንተ፡ ታወድቆ፡ ለቀሲስ፡ እመዓርጊሁ፡</foreign> F.N. 6,5; <foreign xml:lang="gez">ሕገ፡ ክርስቶስ፡ ታወድቆ፡ እመዓርገ፡ ካህናት፡</foreign> F.M. 20,4.</sense> </sense> </sense>
               
            </entry>
