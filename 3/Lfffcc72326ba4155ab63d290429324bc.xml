                     <ref cRef="2 Par." loc="1,9">2 Par. 1,9</ref> (<foreign xml:lang="grc">πιστωθήτω</foreign>); <ref cRef="3 Reg." loc="8,26">3 Reg.
                     8,26</ref>.</sense>
                  <sense n="2" xml:id="D2">
                     <cit type="translation" xml:lang="la">
                        <quote>verus</quote>
                     </cit>, <foreign xml:lang="grc">ἀληθής</foreign>, <foreign xml:lang="grc">ἀληθινός</foreign>, <ref cRef="Gen." loc="41,32">Gen. 41,32</ref>; <ref cRef="Job" loc="6,3">Job 6,3</ref>; <ref cRef="Job" loc="6,25">Job
                        6,25</ref>; <ref cRef="Job" loc="36,4">Job 36,4</ref>; <ref cRef="Jes." loc="41,26">Jes. 41,26</ref>; <foreign xml:lang="gez">እሙን፡ ሕልሙ፡</foreign>
                     <ref cRef="Dan." loc="2,45">Dan. 2,45</ref>; <ref cRef="Dan." loc="6,12">Dan.
                        6,12</ref>; <ref cRef="Or. Az." loc="3 ">Or. Az. 3 </ref> (<ref cRef="Dan." loc="3,3">Dan. 3,3</ref>); <foreign xml:lang="gez">እሙን፡ ቃል፡ ዘሰማዕኩ፡</foreign>
                     <ref cRef="2 Par." loc="9,5">2 Par. 9,5</ref>; <ref cRef="1 Joh. " loc="2,8">1
                        Joh. 2,8</ref>; <ref cRef="1 Joh." loc="2,27">1 Joh. 2,27</ref>; <ref cRef="Joh." loc="4,37">Joh. 4,37</ref>; <foreign xml:lang="gez">እሙነ፡
                        ኮነ፡</foreign>
                     <cit type="translation" xml:lang="la">
                        <quote>verum fuit</quote>
                     </cit> vel <cit type="translation" xml:lang="la">
                        <quote>inventum fuit</quote>
                     </cit>: <foreign xml:lang="gez">እሙነ፡ ኮነ፡ ምክረ፡ ትካት፡ </foreign>
                     <ref cRef="Jes." loc="25,1">Jes. 25,1</ref>; <ref cRef="Deut." loc="17,4">Deut.
                        17,4</ref>; <ref cRef="Deut." loc="22,20">Deut. 22,20</ref>; <ref cRef="2 Cor." loc="7,14">2 Cor. 7,14</ref>.</sense>
                  <sense n="3" xml:id="D3">
                     <cit type="translation" xml:lang="la">
                        <quote>confisus</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>securus</quote>
                     </cit>: <foreign xml:lang="gez">የሐውር፡ እሙነ፡ </foreign>
                     <foreign xml:lang="grc">πεποιθώς</foreign>
                     <ref cRef="Prov." loc="10,9">Prov. 10,9</ref>.</sense>
                  <sense n="4" xml:id="D4">
                     <cit type="translation" xml:lang="la">
                        <quote>fidei concretum habens</quote>
                     </cit>: <foreign xml:lang="gez">እሙን፡ ዲበ፡ ንዋየ፡ ግብር፡ </foreign>
                     <ref cRef="1 Par." loc="9,31">1 Par. 9,31</ref>.</sense>
                  <sense n="5" xml:id="D5"> Adv. <foreign xml:lang="gez">እሙነ፡</foreign>
                     <sense n="a" xml:id="D5a">
                        <cit type="translation" xml:lang="la">
                           <quote>vere</quote>
                        </cit>, <cit type="translation" xml:lang="la">
                           <quote>reverâ</quote>
                        </cit>
                        <ref cRef="1 Reg." loc="22,7">1 Reg. 22,7</ref>; <ref cRef="Deut." loc="17,4">Deut. 17,4</ref>; <ref cRef="Deut." loc="22,20">Deut.
                           22,20</ref>; <foreign xml:lang="gez">አማን፡ ተወልደ፡ እግዚእነ፡ ኢየሲስ፤ እሙነ፡ ልህቀ፡
                           ወእሙነ፡ በልዐ፡ ወእሙነ፡ ሐመ፡ </foreign>
                        <ref cRef="Haim. Ab." loc="6">Haim. Ab. 6</ref>; etiam <foreign xml:lang="gez">እሙን፡</foreign>, ut: <foreign xml:lang="gez">ካልእ፡ እሙን፡ ውእቱ፡
                           ዘዚአክሙ፡ ንጉሥ፡</foreign> Lud. e V.S.</sense>
                     <sense n="b" xml:id="D5b">
                        <cit type="translation" xml:lang="la">
                           <quote>secure</quote>
                        </cit>
                        <ref cRef="Prov." loc="10,9">Prov. 10,9</ref>.</sense>
                  </sense>
               </sense>
               
