               </form>


               <sense xml:id="Tmain" xml:lang="en" source="#traces">
                  <cit type="transcription" xml:lang="gez">
                     <quote> žān </quote>
                  </cit>, <foreign xml:lang="gez">ዠን፡</foreign>
                  <cit type="transcription" xml:lang="gez">
                     <quote> žan </quote>
                  </cit>, <foreign xml:lang="gez">ዛን፡</foreign>
                  <cit type="transcription" xml:lang="gez">
                     <quote>zān</quote>
                  </cit>, <foreign xml:lang="gez">ጃን፡</foreign>
                  <cit type="transcription" xml:lang="gez">
                     <quote>ǧān</quote>
                  </cit>
                  <gramGrp><pos expand="Substantivum">subst.</pos></gramGrp>
                  <sense n="1" xml:id="T1"> the first component of a title, <cit type="translation" xml:lang="en">
                        <quote>court officer</quote>
                     </cit>
                  </sense>
                  <sense n="X" xml:id="TX">
                     <sense n="a" xml:id="TXa">
                        <foreign xml:lang="gez">ዣን፡ ሐጸና፡</foreign>
                        <cit type="transcription" xml:lang="gez">
                           <quote> žān ḥaṣanā</quote>
                        </cit>, <foreign xml:lang="gez">ዠን፡ ሐጸና፡</foreign>
                        <cit type="transcription" xml:lang="gez">
                           <quote> žan ḥaṣanā</quote>
                        </cit>, <foreign xml:lang="gez">ዠን፡ ሐፀና፡</foreign>
                        <cit type="transcription" xml:lang="gez">
                           <quote> žan ḥaḍanā</quote>
                        </cit>, <foreign xml:lang="gez">ዛን፡ ሐጸና፡</foreign>
                        <cit type="transcription" xml:lang="gez">
                           <quote>zān ḥaṣanā</quote>
                        </cit>, <foreign xml:lang="gez">ዛንሐጸና፡</foreign>
                        <cit type="transcription" xml:lang="gez">
                           <quote>zānḥaṣanā</quote>
                        </cit> (title) officer of the royal palace <bibl><ptr target="bm:Kane1990DictAmhVol2"/><citedRange unit="page">
                              1867a</citedRange></bibl>
                        <foreign xml:lang="gez">ቀሚሰሂ፡ ፀዓዳ፡ ኢይለብሱ፡ ዘእንበለ፡ ቀላሚ፡ ወኳፍሬ፡ ወሾቍጤ። እመሂ፡
                           አዛዝያን፡ ወእመሂ፡ ጌቶች፡ እመሂ፡ ሊቀ፡ መጣኒ፡ ምስለ፡ እሊአሁ፡ ዣን፡ ማስ{sic}ሮች፡ ወኢቃቄታች፡ ምስለ፡
                           እሊአሁ፡ ዣን፡ ሐጸና።</foreign> ‘<cit type="translation" xml:lang="fr">
                           <quote>Ils ne portaient pas la
                              tunique blanche, mais le Qalâmi, le Kuâfrê et le Shôquetê, et cette
                              tenue était obligatoire aussi bien pour les Azâzĕyân que pour les
                              Gêtôtj, le Liqa Matâni et ses gens, les Jân Mâsrôtj, le Iqâqêtâtj et
                              ses gens et le Jân Haçanâ*</quote>
                        </cit>’ <bibl><ptr target="bm:Perruchon1893Zarayaeqob"/><citedRange unit="page"> 33 l. 4–7 (ed.), 33 l. 4–8 (tr.)</citedRange></bibl>;
                           <foreign xml:lang="gez">ዛን፡ ሐጸና፡ በኃይሉ፡</foreign> ‘<cit type="translation" xml:lang="fr">
                           <quote>le zān ḥaṣanā,
                              Baḫāylu</quote>
                        </cit>’ <bibl><ptr target="bm:ContiRossini1909LiberT"/><citedRange unit="page"> 27 l. 12 (ed.)</citedRange></bibl>, <bibl><ptr target="bm:ContiRossini1910LiberV"/><citedRange unit="page"> 31 l. 7
                              (tr.)</citedRange></bibl>; <bibl><ptr target="bm:ContiRossini1909LiberT"/><citedRange unit="page"> 28 l. 5
                              (ed.)</citedRange></bibl>, <bibl><ptr target="bm:ContiRossini1910LiberV"/><citedRange unit="page"> 32 l.
                              9–10 (tr.)</citedRange></bibl>; <foreign xml:lang="gez">ወዣን፡ ሐፀና፡
                           ጊዮርጊስ፤</foreign> ‘<cit type="translation" xml:lang="fr">
                           <quote>et le žān ḥaṣanā,
                              Giyorgis</quote>
                        </cit>’ <bibl><ptr target="bm:ContiRossini1909LiberT"/><citedRange unit="page"> 31 l. 32, 32 l. 1 (ed.)</citedRange></bibl>, <bibl><ptr target="bm:ContiRossini1910LiberV"/><citedRange unit="page"> 37 l. 7
                              (tr.)</citedRange></bibl>, <foreign xml:lang="gez">ዛንሐጸና፡
                           ሃኖ፡</foreign> ‘<cit type="translation" xml:lang="fr">
                           <quote>le za{sic}nḥaṣanā, Hāno
                           </quote>
                        </cit>’ <bibl><ptr target="bm:ContiRossini1909LiberT"/><citedRange unit="page"> 33 l. 14–15 (ed.)</citedRange></bibl>, <bibl><ptr target="bm:ContiRossini1910LiberV"/><citedRange unit="page"> 38 l. 30
                              (tr.)</citedRange></bibl>, <lbl expand="videas">vid.</lbl>
                        <foreign xml:lang="gez">ሐጸና፡</foreign>
                     </sense>
                     <sense n="b" xml:id="TXb">
                        <foreign xml:lang="gez">ዣን፡ መአሰሬ፡</foreign>
                        <cit type="transcription" xml:lang="gez">
                           <quote> žān maʾasare</quote>
                        </cit>, <foreign xml:lang="gez">ዠን፡ መአሰሬ፡</foreign>
                        <cit type="transcription" xml:lang="gez">
                           <quote> žan maʾasare</quote>
                        </cit>, <foreign xml:lang="gez">ዛን፡ መአሰሬ፡</foreign>
                        <cit type="transcription" xml:lang="gez">
                           <quote>zān maʾasare</quote>
                        </cit>, <foreign xml:lang="gez">ዣን፡ ማሰሬ፡</foreign>
                        <cit type="transcription" xml:lang="gez">
                           <quote> žān māsare</quote>
                        </cit>
                        <lbl expand="pluralis">Pl.</lbl>
                        <foreign xml:lang="gez">ዣን፡ ማሰሮች፡</foreign>
                        <cit type="transcription" xml:lang="gez">
                           <quote> žān māsaročč</quote>
                        </cit> (title) chief of protocol in the Gondarine monarchy who introduced
                        foreigners of status to the monarch <bibl><ptr target="bm:Kane1990DictAmhVol2"/><citedRange unit="page">
                              1867a</citedRange></bibl>
                        <foreign xml:lang="gez">ወለሐናፅያኒሂ፡ አዘዝዎሙ፡ ለእመ፡ ጸነ፡ ከመ፡ ያርትዑ፡ ራቅ፡ ማሰሮችሰ፡ ዘየማን፡
                           ወዘፀጋም፡ ኢይልበሱ፡ ቀማይሲሆሙ፡ እስከ፡ ይትፌጸም፡ ውእቱ፡ ገብረ፡ ጃጐል፡ ወዣን፡ ማሰሬሂ፡ ከመ{sic}ሆሙ፡
                           ኢይልበሱ፡ እለ፡ ይሰርሑ፡ ጃጐለ፡</foreign> ‘<cit type="translation" xml:lang="fr">
                           <quote>Il recommanda aux
                              constructeurs de veiller à ce qu’il fût bien aligné, sans aucune
                              déviation, et aux Râq Mâsarôtj de droite et de gauche, ainsi qu’aux
                              Jân Mâsarê qui étaient employés à la construction du Djagual, de ne
                              pas prendre leurs tuniques jusqu'à ce que ce travail fût
                              achevé</quote>
                        </cit>’ <bibl><ptr target="bm:Perruchon1893Zarayaeqob"/><citedRange unit="page"> 23 l. 9–24 l. 4 (ed.), 24 l. 1–5
                           (tr.)</citedRange></bibl>; <foreign xml:lang="gez">ቀሚሰሂ፡ ፀዓዳ፡ ኢይለብሱ፡
                           ዘእንበለ፡ ቀላሚ፡ ወኳፍሬ፡ ወሾቍጤ። እመሂ፡ አዛዝያን፡ ወእመሂ፡ ጌቶች፡ እመሂ፡ ሊቀ፡ መጣኒ፡ ምስለ፡ እሊአሁ፡
                           ዣን፡ ማስ{sic}ሮች፡ ወኢቃቄታች፡ ምስለ፡ እሊአሁ፡ ዣን፡ ሐጸና።</foreign> ‘<cit type="translation" xml:lang="fr">
                           <quote>Ils ne portaient pas la
                              tunique blanche, mais le Qalâmi, le Kuâfrê et le Shôquetê, et cette
                              tenue était obligatoire aussi bien pour les Azâzĕyân que pour les
                              Gêtôtj, le Liqa Matâni et ses gens, les Jân Mâsrôtj, le Iqâqêtâtj et
                              ses gens et le Jân Haçanâ*</quote>
                        </cit>’ <bibl><ptr target="bm:Perruchon1893Zarayaeqob"/><citedRange unit="page"> 33 l. 4–7 (ed.), 33 l. 4–8 (tr.)</citedRange></bibl>;
                           <foreign xml:lang="gez">ወበሳኒታሁ፡ አዘዘ፡ ንጉሥ፡ ከመ፡ ያስተጋብዕዎሙ፡ ለኵሎሙ፡ ዣን፡ ማሰሮች፡
                           ወያብዕዎሙ፡ ውስተ፡ ጃጐል፡፡</foreign> ‘<cit type="translation" xml:lang="fr">
                           <quote>Le lendemain, Ba’eda
                              Mâryâm donna l’ordre de réunir tous les Jân Mâsarôtj et de les amener
                              dans le Djagual</quote>
                        </cit>’ <bibl><ptr target="bm:Perruchon1893Zarayaeqob"/><citedRange unit="page"> 162 l. 7–9 (ed.), 162 l. 8–10 (tr.)</citedRange></bibl>;
                           <foreign xml:lang="gez">ዛን፡ መአሰሬ፡ ገብረ፡ ማርያም፡</foreign> ‘<cit type="translation" xml:lang="fr">
                           <quote>le zān maʾasarē, Gabra
                              Māryām</quote>
                        </cit>’ <bibl><ptr target="bm:ContiRossini1909LiberT"/><citedRange unit="page"> 27 l. 12–13 (ed.)</citedRange></bibl>, <bibl><ptr target="bm:ContiRossini1910LiberV"/><citedRange unit="page"> 31 l. 7–8
                              (tr.)</citedRange></bibl>; <bibl><ptr target="bm:ContiRossini1909LiberT"/><citedRange unit="page"> 31 l. 32
                              (ed.)</citedRange></bibl>, <bibl><ptr target="bm:ContiRossini1910LiberV"/><citedRange unit="page"> 37 l. 6
                              (tr.)</citedRange></bibl>; <foreign xml:lang="gez">ዣን፡ መአሰሬ፡ ሰመረ፡
                           ጽዮን፡</foreign> ‘<cit type="translation" xml:lang="fr">
                           <quote>et le žān maʾasarē, Samara
                              Ṣyon</quote>
                        </cit>’ <bibl><ptr target="bm:ContiRossini1909LiberT"/><citedRange unit="page"> 37 l. 18 (ed.)</citedRange></bibl>, <bibl><ptr target="bm:ContiRossini1910LiberV"/><citedRange unit="page"> 44 l. 6–7
                              (tr.)</citedRange></bibl>, <lbl expand="videas">vid.</lbl>
                        <foreign xml:lang="gez">መአሰሬ፡</foreign>
                     </sense>
                     <sense n="c" xml:id="TXc">
                        <foreign xml:lang="gez">ዣን፡ ተከል፡</foreign>
                        <cit type="transcription" xml:lang="gez">
                           <quote>žān takal</quote>
                        </cit> one of the junior officers of the royal household <bibl><ptr target="bm:Abbadie1881Dict"/><citedRange unit="page">
                              733a</citedRange></bibl>
                        <foreign xml:lang="gez">ዣን፡ ተከል፡ አፈ፡ ወርቅ፡</foreign> ‘<cit type="translation" xml:lang="fr">
                           <quote>et le žān takal, Afa
                              Warq</quote>
                        </cit>’ <bibl><ptr target="bm:ContiRossini1909LiberT"/><citedRange unit="page"> 43 l. 32 (ed.)</citedRange></bibl>, <bibl><ptr target="bm:ContiRossini1910LiberV"/><citedRange unit="page"> 52 l.
                              14–15 (tr.)</citedRange></bibl>, <lbl expand="videas">vid.</lbl>
                        <foreign xml:lang="gez">ተከል፡</foreign>
                     </sense>
                     <sense n="d" xml:id="TXd">
                        <foreign xml:lang="gez">ዣን፡ ደረባ፡</foreign>
                        <cit type="transcription" xml:lang="gez">
                           <quote>žān darabā</quote>
                        </cit>, <foreign xml:lang="gez">ዣንደረባ፡</foreign>
                        <cit type="transcription" xml:lang="gez">
                           <quote>žāndarabā</quote>
                        </cit>, <foreign xml:lang="gez">ጃንደረባ፡</foreign>
                        <cit type="transcription" xml:lang="gez">
                           <quote>ǧāndarabā</quote>
                        </cit>
                        <lbl expand="pluralis">Pl.</lbl>
                        <foreign xml:lang="gez">ዣንደረቦች፡</foreign>
                        <cit type="transcription" xml:lang="gez">
                           <quote>žāndarabočč</quote>
                        </cit> ‘<cit type="translation" xml:lang="en">
