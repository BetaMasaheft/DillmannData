               <sense xml:id="Tmain" xml:lang="en" source="#traces">
                  <cit type="transcription" xml:lang="gez">
                     <t:quote xmlns:t="http://www.tei-c.org/ns/1.0"> žān </t:quote>
                  </cit>, <foreign xml:lang="gez">ዠን፡</foreign>
                  <cit type="transcription" xml:lang="gez">
                     <t:quote xmlns:t="http://www.tei-c.org/ns/1.0"> žan </t:quote>
                  </cit>, <foreign xml:lang="gez">ዛን፡</foreign>
                  <cit type="transcription" xml:lang="gez">
                     <t:quote xmlns:t="http://www.tei-c.org/ns/1.0">zān</t:quote>
                  </cit>, <foreign xml:lang="gez">ጃን፡</foreign>
                  <cit type="transcription" xml:lang="gez">
                     <t:quote xmlns:t="http://www.tei-c.org/ns/1.0">ǧān</t:quote>
                  </cit>
                  <gramGrp><pos expand="Substantivum">subst.</pos></gramGrp>
                  <sense n="1" xml:id="T1"> the first component of a title, <cit type="translation" xml:lang="en">
                        <t:quote xmlns:t="http://www.tei-c.org/ns/1.0">court officer</t:quote>
                     </cit>
                  </sense>
                  <sense n="X" xml:id="TX">
                     <sense n="a" xml:id="TXa">
                        <foreign xml:lang="gez">ዣን፡ ሐጸና፡</foreign>
                        <cit type="transcription" xml:lang="gez">
                           <t:quote xmlns:t="http://www.tei-c.org/ns/1.0"> žān ḥaṣanā</t:quote>
                        </cit>, <foreign xml:lang="gez">ዠን፡ ሐጸና፡</foreign>
                        <cit type="transcription" xml:lang="gez">
                           <t:quote xmlns:t="http://www.tei-c.org/ns/1.0"> žan ḥaṣanā</t:quote>
                        </cit>, <foreign xml:lang="gez">ዠን፡ ሐፀና፡</foreign>
                        <cit type="transcription" xml:lang="gez">
                           <t:quote xmlns:t="http://www.tei-c.org/ns/1.0"> žan ḥaḍanā</t:quote>
                        </cit>, <foreign xml:lang="gez">ዛን፡ ሐጸና፡</foreign>
                        <cit type="transcription" xml:lang="gez">
                           <t:quote xmlns:t="http://www.tei-c.org/ns/1.0">zān ḥaṣanā</t:quote>
                        </cit>, <foreign xml:lang="gez">ዛንሐጸና፡</foreign>
                        <cit type="transcription" xml:lang="gez">
                           <t:quote xmlns:t="http://www.tei-c.org/ns/1.0">zānḥaṣanā</t:quote>
                        </cit> (title) officer of the royal palace <bibl><ptr target="bm:Kane1990DictAmhVol2"/><citedRange unit="page">
                              1867a</citedRange></bibl>
                        <foreign xml:lang="gez">ቀሚሰሂ፡ ፀዓዳ፡ ኢይለብሱ፡ ዘእንበለ፡ ቀላሚ፡ ወኳፍሬ፡ ወሾቍጤ። እመሂ፡
                           አዛዝያን፡ ወእመሂ፡ ጌቶች፡ እመሂ፡ ሊቀ፡ መጣኒ፡ ምስለ፡ እሊአሁ፡ ዣን፡ ማስ{sic}ሮች፡ ወኢቃቄታች፡ ምስለ፡
                           እሊአሁ፡ ዣን፡ ሐጸና።</foreign> ‘<cit type="translation" xml:lang="fr">
                           <t:quote xmlns:t="http://www.tei-c.org/ns/1.0">Ils ne portaient pas la
                              tunique blanche, mais le Qalâmi, le Kuâfrê et le Shôquetê, et cette
                              tenue était obligatoire aussi bien pour les Azâzĕyân que pour les
                              Gêtôtj, le Liqa Matâni et ses gens, les Jân Mâsrôtj, le Iqâqêtâtj et
                              ses gens et le Jân Haçanâ*</t:quote>
                        </cit>’ <bibl><ptr target="bm:Perruchon1893Zarayaeqob"/><citedRange unit="page"> 33 l. 4–7 (ed.), 33 l. 4–8 (tr.)</citedRange></bibl>;
                           <foreign xml:lang="gez">ዛን፡ ሐጸና፡ በኃይሉ፡</foreign> ‘<cit type="translation" xml:lang="fr">
                           <t:quote xmlns:t="http://www.tei-c.org/ns/1.0">le zān ḥaṣanā,
                              Baḫāylu</t:quote>
                        </cit>’ <bibl><ptr target="bm:ContiRossini1909LiberT"/><citedRange unit="page"> 27 l. 12 (ed.)</citedRange></bibl>, <bibl><ptr target="bm:ContiRossini1910LiberV"/><citedRange unit="page"> 31 l. 7
                              (tr.)</citedRange></bibl>; <bibl><ptr target="bm:ContiRossini1909LiberT"/><citedRange unit="page"> 28 l. 5
                              (ed.)</citedRange></bibl>, <bibl><ptr target="bm:ContiRossini1910LiberV"/><citedRange unit="page"> 32 l.
                              9–10 (tr.)</citedRange></bibl>; <foreign xml:lang="gez">ወዣን፡ ሐፀና፡
                           ጊዮርጊስ፤</foreign> ‘<cit type="translation" xml:lang="fr">
                           <t:quote xmlns:t="http://www.tei-c.org/ns/1.0">et le žān ḥaṣanā,
                              Giyorgis</t:quote>
                        </cit>’ <bibl><ptr target="bm:ContiRossini1909LiberT"/><citedRange unit="page"> 31 l. 32, 32 l. 1 (ed.)</citedRange></bibl>, <bibl><ptr target="bm:ContiRossini1910LiberV"/><citedRange unit="page"> 37 l. 7
                              (tr.)</citedRange></bibl>, <foreign xml:lang="gez">ዛንሐጸና፡
                           ሃኖ፡</foreign> ‘<cit type="translation" xml:lang="fr">
                           <t:quote xmlns:t="http://www.tei-c.org/ns/1.0">le za{sic}nḥaṣanā, Hāno
                           </t:quote>
                        </cit>’ <bibl><ptr target="bm:ContiRossini1909LiberT"/><citedRange unit="page"> 33 l. 14–15 (ed.)</citedRange></bibl>, <bibl><ptr target="bm:ContiRossini1910LiberV"/><citedRange unit="page"> 38 l. 30
                              (tr.)</citedRange></bibl>, <lbl expand="videas">vid.</lbl>
                        <foreign xml:lang="gez">ሐጸና፡</foreign>
                     </sense>
                     <sense n="b" xml:id="TXb">
                        <foreign xml:lang="gez">ዣን፡ መአሰሬ፡</foreign>
                        <cit type="transcription" xml:lang="gez">
                           <t:quote xmlns:t="http://www.tei-c.org/ns/1.0"> žān maʾasare</t:quote>
                        </cit>, <foreign xml:lang="gez">ዠን፡ መአሰሬ፡</foreign>
                        <cit type="transcription" xml:lang="gez">
                           <t:quote xmlns:t="http://www.tei-c.org/ns/1.0"> žan maʾasare</t:quote>
                        </cit>, <foreign xml:lang="gez">ዛን፡ መአሰሬ፡</foreign>
                        <cit type="transcription" xml:lang="gez">
