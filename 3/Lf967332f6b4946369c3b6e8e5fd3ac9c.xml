               <sense xml:id="Dmain" xml:lang="la" source="#dillmann"> <subc>II,1</subc> <sense n="1" xml:id="D1"> <cit type="translation" xml:lang="la"><quote> assuefacere </quote></cit>, aliquem rei vel re, <cb n="0036" xml:id="c0036"/> <lbl expand="construitur cum">c.c.</lbl> <lbl expand="duplex">dupl.</lbl> <gramGrp> <case value="accusativus">Acc.</case> </gramGrp>: <foreign xml:lang="gez">ኢታለምዶ፡ መሐላ፡ ለአፉከ፡</foreign> <ref cRef="Sir." loc="23,9">Sir. 23,9</ref>; <foreign xml:lang="gez">አልምዳ፡ ለነፍስከ፡ ትዕግሥተ፡ ላዕለ፡ ኵሉ፡ መከራት፡</foreign> <ref cRef="Fal." loc="27.">Fal. f. 27.</ref> 
</sense> <sense n="2" xml:id="D2"> <cit type="translation" xml:lang="la"><quote> consuetudinem exercere </quote></cit> (<cit type="translation" xml:lang="la"><quote> colere </quote></cit>) i.e. <cit type="translation" xml:lang="la"><quote> in usu vel consuetudine habere </quote></cit> aliquid, <cit type="translation" xml:lang="la"><quote> facere solitum esse </quote></cit>: <lbl expand="construitur cum">c.c.</lbl> <gramGrp> <case value="accusativus">Acc.</case> </gramGrp> <foreign xml:lang="gez">እለ፡ ያለምዱ፡ ዝሙተ፡</foreign> F.M. 7,2; <ref cRef="Matth." loc="27,15">Matth. 27,15</ref>; vel <gramGrp> <pos expand="absolute">absol.</pos> </gramGrp>: <foreign xml:lang="gez">ዘከመ፡ ያለምድ፡</foreign> <ref cRef="Num." loc="24,1">Num. 24,1</ref>; <ref cRef="Judith" loc="13,10">Judith 13,10</ref>; Dan. apocr. 13,15; <ref cRef="Marc." loc="10,1">Marc. 10,1</ref>; <ref cRef="Luc." loc="2,42">Luc. 2,42</ref>.
</sense> </sense>
               
