            <entry xml:id="Lfc88a4e0f6b94f7293ac2ee2aaa2ba3e" n="5704">
               <form>
                  <foreign xml:lang="gez">መአኀዝ</foreign>
               </form>
               <sense xml:lang="la" source="#dillmann" xml:id="Dmain"> vel <foreign xml:lang="gez">መአኀዝት፡</foreign> (<ref cRef="Hez." loc="48,21">Hez. 48,21</ref>; sine dubio pro <foreign xml:lang="gez">ማእኀዝ፡</foreign> collato Subjunctivo stirpis <subc>I,1</subc> <foreign xml:lang="gez">ይእኀዝ፡</foreign> et <foreign xml:lang="gez">የአኀዝ፡</foreign>), <gramGrp> <pos expand="Substantivum">subst.</pos> </gramGrp>, <lbl expand="pluralis">Pl.</lbl> <foreign xml:lang="gez">መኣኅዝት፡</foreign> et <foreign xml:lang="gez">መኣኅዝ፡</foreign>, nec non <foreign xml:lang="gez">መአኀዛት፡</foreign> (et <foreign xml:lang="gez">መአኅዛት፡</foreign>) <sense n="1" xml:id="D1"> <cit type="translation" xml:lang="la">
                                <quote> ansa </quote>
                            </cit> vel <cit type="translation" xml:lang="la">
                                <quote> uncus </quote>
                            </cit>, quibus quid apprehenditur, quum tollitur, <foreign xml:lang="grc">εὐρεῖς</foreign> <foreign xml:lang="gez">መኣኅዝት፡</foreign> <ref cRef="Ex. " loc="38,4">Ex.  38,4</ref>; <ref cRef="Ex." loc="38,10">Ex. 38,10</ref>; <ref cRef="Ex." loc="38,24">Ex. 38,24</ref>.</sense> <sense n="2" xml:id="D2"> <cit type="translation" xml:lang="la">
                                <quote> junctura </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> commissura </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> id quo </quote>
                            </cit> duae pluresve res <cit type="translation" xml:lang="la">
                                <quote> continentur </quote>
                            </cit> sive <cit type="translation" xml:lang="la">
                                <quote> coagmentantur </quote>
                            </cit>, <ref cRef="3 Reg." loc="7,15">3 Reg. 7,15</ref> (<foreign xml:lang="grc">τὰ ἐξεχόμενα</foreign>); <ref cRef="3 Reg." loc="7,16">3 Reg. 7,16</ref> (<foreign xml:lang="grc">τὰ προςέχοντα</foreign>); ecclesiae Lalibelae exstructae sunt sine <foreign xml:lang="gez">ጽቡር፡</foreign> (maltha) <foreign xml:lang="gez">ለመኣኅዝቲሆን፡</foreign>, <ref cRef="Gad. Lal." loc="">Gad. Lal. </ref> <cb n="0770" xml:id="c0770"/> </sense> <sense n="3" xml:id="D3"> <cit type="translation" xml:lang="la">
                                <quote> ornamenta </quote>
                            </cit> illa, calicis speciem referentia, aliaque, quibus scapi vel brachia candelabri sacri quasi inclusa sunt (Einfassungen): <foreign xml:lang="gez">መኣኅዝ፡</foreign> <foreign xml:lang="grc">κρατῆρες</foreign> <ref cRef="Ex." loc="25,31">Ex. 25,31</ref>; <ref cRef="Ex." loc="25,33">Ex. 25,33</ref>; <ref cRef="Ex. " loc="25,34">Ex.  25,34</ref> (v. <foreign xml:lang="gez">መአኀዝ፡</foreign>); <foreign xml:lang="gez">መኣኅዝት፡</foreign> (v. <foreign xml:lang="gez">መአኀዛት፡</foreign>) <foreign xml:lang="grc">ἐνθέμια</foreign> <ref cRef="Ex." loc="38,16">Ex. 38,16</ref>; <foreign xml:lang="he">גְּבִיעִים</foreign> <ref cRef="Ex. " loc="37,17">Ex.  37,17</ref>; <ref cRef="Ex." loc="37,19">Ex. 37,19</ref> <lbl expand="secundum">sec.</lbl> text. Hebr. in Cod. C (Oct. P. <ref cRef="II." loc="92.">II. p. 92.</ref>); item <cit type="translation" xml:lang="la">
                                <quote> funda </quote>
                            </cit> seu <cit type="translation" xml:lang="la">
                                <quote> pala </quote>
                            </cit> gemmarum (Einfassung) <ref cRef="Ex." loc="28,11">Ex. 28,11</ref> <lbl expand="cum">c.</lbl> </sense> <sense n="4" xml:id="D4"> <cit type="translation" xml:lang="la">
                                <quote> fundus occupatus </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> possessio </quote>
                            </cit>: <foreign xml:lang="gez">መአኀዘ፡ ሀገር፡</foreign> <foreign xml:lang="grc">κατάσχεσις πόλεως</foreign> <ref cRef="Hez." loc="45,6">Hez. 45,6</ref>; <ref cRef="Hez. " loc="45,7">Hez.  45,7</ref>; <foreign xml:lang="gez">መአኀዝተ፡ ሀገር፡</foreign> <ref cRef="Hez." loc="48,21">Hez. 48,21</ref>.</sense> <sense n="5" xml:id="D5"> i.q. <foreign xml:lang="gez">ምእኃዝ፡</foreign> q.v.</sense> </sense>
               
