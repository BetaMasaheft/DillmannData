               <sense xml:id="Dmain" xml:lang="la" source="#dillmann"> <gramGrp> <pos expand="Substantivum">subst.</pos> </gramGrp> (<gramGrp> <gen value="not found">m.</gen> </gramGrp>), <lbl expand="pluralis">Pl.</lbl> <foreign xml:lang="gez">አውታር፡</foreign>, <sense n="1" xml:id="D1"> <cit type="translation" xml:lang="la"><t:quote xmlns:t="http://www.tei-c.org/ns/1.0"> nervus </t:quote></cit>, <cit type="translation" xml:lang="la"><t:quote xmlns:t="http://www.tei-c.org/ns/1.0"> chorda </t:quote></cit>: <foreign xml:lang="gez">አውታር፡ ሐደስት፡</foreign> <foreign xml:lang="grc">νευραί</foreign> <ref cRef="Jud." loc="16,7">Jud. 16,7</ref> <lbl expand="sequens">seq.</lbl>; <foreign xml:lang="grc">χορδή</foreign> <ref cRef="Nah." loc="3,8">Nah. 3,8</ref>; <foreign xml:lang="gez">ቃለ፡ አውታረ፡ መሰንቆ፡</foreign> <ref cRef="Hez." loc="23,42">Hez. 23,42</ref>; <ref cRef="Clem." loc="16">Clem. f. 16</ref>; <ref cRef="Fal." loc="54">Fal. f. 54</ref>; <foreign xml:lang="gez">መዝሙር፡ ዘዐሠርቱ፡ አውታሪሁ፡</foreign> <ref cRef="Ps. " loc="32,2">Ps.  32,2</ref>; <ref cRef="Ps." loc="91,3">Ps. 91,3</ref>; <ref cRef="Ps." loc="143,10">Ps. 143,10</ref>; <ref cRef="Ps." loc="150,4">Ps. 150,4</ref>.</sense> <sense n="2" xml:id="D2"> <cit type="translation" xml:lang="la"><t:quote xmlns:t="http://www.tei-c.org/ns/1.0"> funiculus </t:quote></cit>, <cit type="translation" xml:lang="la"><t:quote xmlns:t="http://www.tei-c.org/ns/1.0"> filum </t:quote></cit>: <ref cRef="Ex." loc="35,17">Ex. 35,17</ref> in <lbl expand="cum">c.</lbl> (<foreign xml:lang="he">מֵיתָרִים</foreign>); <foreign xml:lang="gez">አውታረ፡ ሣሬት፡</foreign> <foreign xml:lang="grc">ἱστὸς ἀράχνης</foreign> <ref cRef="Jes." loc="59,5">Jes. 59,5</ref> <lbl expand="varia lectio">var.</lbl> – <lbl expand="Vocabularium Aethiopicum, -a -a">Voc. Ae.</lbl>: <foreign xml:lang="gez">አውታሪሁ፡</foreign> (sc. <foreign xml:lang="gez">ለመሰንቆ፡</foreign>) <foreign xml:lang="gez">ዘ፡ ዥማቶች።</foreign> </sense> </sense>
               
