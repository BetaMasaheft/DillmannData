            <entry xml:id="Lf372da2ed34d4358ba7da580ff327e27" n="10142">
               <form>
                  <foreign xml:lang="gez">ተፈፅመ</foreign>
               </form>
               <sense xml:lang="la" source="#dillmann" xml:id="Dmain"> <subc>III,1</subc> et <foreign xml:lang="gez">ተፈፀመ፡</foreign> <subc>III,2</subc> <sense n="1" xml:id="D1">
                     <cit type="translation" xml:lang="la">
                        <quote>obstrui</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>occludi</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>obturari</quote>
                     </cit>: <foreign xml:lang="gez">ተፈፀመ፡ አንቅዕተ፡ ቀላይ፡</foreign>
                     <ref cRef="Kuf." loc="23">Kuf. p. 23</ref>; <foreign xml:lang="gez">ተፈፀመ፡ አፈ፡
                        መዓምቅት፡ </foreign>
                     <ref cRef="Kuf." loc="27">Kuf. p. 27</ref>; <foreign xml:lang="gez">ይትፌፀም፡</foreign>
                     <foreign xml:lang="grc">ἐμφραχθήσεται</foreign>
                     <ref cRef="Zach." loc="14,5">Zach. 14,5</ref>; <foreign xml:lang="gez">ለትትፈፀም፡
                        አፈ፡ ዐናብስት፡</foreign>
                     <ref cRef="Genz." loc="107">Genz. f. 107</ref>; <foreign xml:lang="gez">ይትፌፀም፡
                        አፉሁ፡ ለዐማፂ፡</foreign>
                     <ref cRef="Job" loc="5,16">Job 5,16</ref>; <ref cRef="Ps." loc="62,10">Ps.
                        62,10</ref>; <ref cRef="Ps." loc="106,42">Ps. 106,42</ref>; <foreign xml:lang="gez">ተፈፀመ፡ አፈ፡ ኵሉ፡ አራዊት፡ እምነቢብ፡</foreign>
                     <ref cRef="Kuf." loc="14.">Kuf. p. 14.</ref>
                  </sense>
                  <sense n="2" xml:id="D2">
                     <lbl expand="translate">transl.</lbl>: <sense n="a" xml:id="D2a">
                        <cit type="translation" xml:lang="la">
                           <quote>obmutescere</quote>
                        </cit>, <cit type="translation" xml:lang="la">
                           <quote>conticescere</quote>
                        </cit>, <ref cRef="Matth." loc="22,12">Matth. 22,12</ref>; <ref cRef="Marc." loc="1,25">Marc. 1,25</ref>; <ref cRef="Marc." loc="4,39">Marc.
                           4,39</ref>; <foreign xml:lang="gez">ኀያላን፡ ይትፌፀሙ፡ ኵሎሙ፡ ወኢይነብቡ፡</foreign>
                        <foreign xml:lang="grc">ἐπαύσαντο λαλοῦντες</foreign>
                        <ref cRef="Job" loc="29,9">Job 29,9</ref>.</sense>
                     <sense n="b" xml:id="D2b">
                        <cit type="translation" xml:lang="la">
                           <quote>obsurdescere</quote>
                        </cit> (ut videtur): <foreign xml:lang="gez">ኢትትፈፀሙ፡ </foreign>
                        <foreign xml:lang="grc">μὴ ἀποφραγῆτε</foreign> (quamquam lectio <lbl expand="varia lectio">var.</lbl> praebet: <foreign xml:lang="gez">ኢትፍፅሙ፡
                           አፈከሙ፡</foreign>) <ref cRef="Prov." loc="8,33">Prov. 8,33</ref>. – <lbl expand="Vocabularium Aethiopicum, -a -a.">Voc. Ae.</lbl>: <foreign xml:lang="gez">ተፈፅመ፡ ዘ፡ ተዘጋ።</foreign>
                     </sense>
                  </sense>
               </sense>
               
