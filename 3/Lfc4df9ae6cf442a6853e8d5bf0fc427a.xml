               <form>
                  <foreign xml:lang="gez">አስተበውሐ</foreign>
               </form>
               <sense xml:lang="la" source="#dillmann" xml:id="Dmain"> (et <foreign xml:lang="gez">አስተብሐ፡</foreign>
                  <ref cRef="Lud. e" loc="1 ">Lud. e 1 </ref>
                  <ref cRef="Reg." loc="20,28">Reg. 20,28</ref>) <subc>IV,1</subc> et <foreign xml:lang="gez">አስተባውሐ፡</foreign> <subc>IV,3</subc> <sense n="1" xml:id="D1">
                     <cit type="translation" xml:lang="la">
                        <quote>veniam petere</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>petere ut liceat</quote>
                     </cit> vel <cit type="translation" xml:lang="la">
                        <quote>permittatur</quote>
                     </cit>, <lbl expand="cum">c.</lbl>
                     <gramGrp>
                        <case value="accusativus">Acc.</case>
                     </gramGrp>
                     <lbl expand="persona">pers.</lbl>
                     <ref cRef="2 Esr." loc="23,6">2 Esr. 23,6</ref>; <lbl expand="sequens">seq.</lbl>
                     <foreign xml:lang="gez">ከመ፡</foreign>
                     <lbl expand="cum">c.</lbl>
                     <gramGrp>
                        <pos expand="Subjunctivus">Subj.</pos>
                     </gramGrp>
                     <ref cRef="1 Reg." loc="20,6">1 Reg. 20,6</ref>; <ref cRef="1 Reg. " loc="20,28">1 Reg. 20,28</ref>; <lbl expand="sequens">seq.</lbl>
                     <gramGrp>
                        <pos expand="Subjunctivus">Subj.</pos>
                     </gramGrp>
                     <ref cRef="Act." loc="13,28">Act. 13,28</ref>
                     <lbl expand="romanae editionis">rom.</lbl>
                  </sense>
                  <sense n="2" xml:id="D2">
                     <cit type="translation" xml:lang="la">
                        <quote>veniam nancisci</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>concessam licentiam habere</quote>
                     </cit>, <lbl expand="cum">c.</lbl>
                     <foreign xml:lang="gez">ኀበ፡</foreign> vel <foreign xml:lang="gez">በኀበ፡</foreign> pers.: <foreign xml:lang="gez">አስተባዊሖ፡ ኀበ፡ ሊቀ፡ ካህናት፡
                        መጽአ፡</foreign>
                     <ref cRef="Sx. Haml." loc="5">Sx. Haml. 5</ref>; <ref cRef="Act. " loc="9,14">Act. 9,14</ref> Platt; <ref cRef="Act." loc="26,10">Act. 26,10</ref>
                     Platt.</sense>
                  <sense n="3" xml:id="D3">
                     <cit type="translation" xml:lang="la">
                        <quote>veniam concedere</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>permittere</quote>
                     </cit>: Satanas <foreign xml:lang="gez">አስተባውሓ፡ </foreign> (Evam) <foreign xml:lang="gez">ለበሊዐ፡ ዕፀ፡ ዕልወት፡</foreign>
                     <ref cRef="M.M." loc="f. 37">M.M. f. 37</ref>; <foreign xml:lang="gez">አስተበውሖቶሙ፡ ለበሊዐ፡ ንዋየ፡ ውስጥ፡</foreign>
                     <ref cRef="Sap." loc="12,5">Sap. 12,5</ref>.</sense>
               </sense>
               
