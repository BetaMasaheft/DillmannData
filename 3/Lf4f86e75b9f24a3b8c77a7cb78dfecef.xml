         <div>
            <entry xml:id="Lf4f86e75b9f24a3b8c77a7cb78dfecef" n="5552">
               <form>
                  <foreign xml:lang="gez">ተአስረ</foreign>
               </form>
               <sense xml:lang="la" source="#dillmann" xml:id="Dmain"> <subc>III,1</subc> <sense n="1" xml:id="D1">
                     <cit type="translation" xml:lang="la">
                        <quote>ligari</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>alligari</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>colligari</quote>
                     </cit>: <ref cRef="Lit." loc="172,1">Lit. 172,1</ref>; <foreign xml:lang="gez">ተአስረት፡ ከርሥየ፡ ከመ፡ ዝቅ፡</foreign>
                     <ref cRef="Job" loc="32,19">Job 32,19</ref>; <lbl expand="cum">c.</lbl>
                     <foreign xml:lang="gez">በ፡</foreign>, ut: <foreign xml:lang="gez">ተአስረት፡ ነፍስ፡
                        በሥጋ፡ </foreign>
                     <ref cRef="Jsp." loc="381">Jsp. p. 381</ref>; <foreign xml:lang="gez">ነፍሰ፡
                        ዮናታን፡ ትአስረት፡ በነፍሰ፡ ዳዊት፡ </foreign> (<cit type="translation" xml:lang="la">
                        <quote>affixa erat</quote>
                     </cit>) <ref cRef="1 Reg." loc="18,1">1 Reg. 18,1</ref>. <sense n="a" xml:id="D1a">
                        <cit type="translation" xml:lang="la">
                           <quote>coërceri</quote>
                        </cit>
                        <ref cRef="2 Tim." loc="2,9">2 Tim. 2,9</ref>.</sense>
                     <sense n="b" xml:id="D1b">
                        <cit type="translation" xml:lang="la">
                           <quote>obligari</quote>
                        </cit>, <cit type="translation" xml:lang="la">
                           <quote>obstringi</quote>
                        </cit>, <foreign xml:lang="gez">ተአሰርኩ፡ በመንፈስየ፡ </foreign>
                        <ref cRef="Act." loc="20,22">Act. 20,22</ref>.</sense>
                     <sense n="2" xml:id="D12">
                        <cit type="translation" xml:lang="la">
                           <quote>vinciri</quote>
                        </cit>, <cit type="translation" xml:lang="la">
                           <quote>vinculis adstringi</quote>
                        </cit>, <cit type="translation" xml:lang="la">
                           <quote>captivum fieri</quote>
                        </cit>: <ref cRef="Hen." loc="10,14">Hen. 10,14</ref>; <ref cRef="Hen." loc="21,4">Hen. 21,4</ref>; <ref cRef="Kuf." loc="21">Kuf. p. 21</ref>;
                           <ref cRef="Sap." loc="17,2">Sap. 17,2</ref>; <foreign xml:lang="gez">ከመ፡
                           ከልብ፡ ዘየሐውር፡ ለተአስሮ፡ </foreign>
                        <ref cRef="Prov." loc="7,22">Prov. 7,22</ref>; <ref cRef="Judae" loc="6">Judae 6</ref>; <ref cRef="2 Reg." loc="3,34">2 Reg. 3,34</ref>; <lbl expand="cum">c.</lbl>
                        <foreign xml:lang="gez">በ፡ </foreign> vinculi <ref cRef="Hen." loc="69,28">Hen. 69,28</ref>; <ref cRef="Job" loc="18,8">Job 18,8</ref>
                        <lbl expand="varia lectio">var.</lbl>; <foreign xml:lang="gez">ይትአሰሩ፡
                           በመዋቅሕት፡</foreign>
                        <ref cRef="Jes." loc="43,14">Jes. 43,14</ref>; <foreign xml:lang="gez">ይጼወኑ፡
                           ወይትአሰሩ፡ </foreign>
                        <ref cRef="Jes." loc="24,22">Jes. 24,22</ref>.</sense>
                     <sense n="3" xml:id="D13">
                        <cit type="translation" xml:lang="la">
                           <quote>se ipsum ligare</quote>
                        </cit> vel <cit type="translation" xml:lang="la">
                           <quote>vincire</quote>
                        </cit>, vel <cit type="translation" xml:lang="la">
                           <quote>se vinciri sinere</quote>
                        </cit>: <foreign xml:lang="gez">ተአስረ፡ ለሊሁ፡ እደዊሁ፡ ወእገሪሁ፡</foreign>
                        <ref cRef="Act." loc="21,11">Act. 21,11</ref>; <foreign xml:lang="gez">ተአሰሩ፡</foreign> (Imperat.) <ref cRef="Jsp." loc="378.">Jsp. p.
                           378.</ref>
                        <cb n="0748" xml:id="c0748"/>
                     </sense>
                  </sense>
               </sense>
               
            </entry>
