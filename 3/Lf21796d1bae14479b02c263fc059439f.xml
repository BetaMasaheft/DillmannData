               <sense xml:lang="la" source="#dillmann" xml:id="Dmain"> et <foreign xml:lang="gez">አጸረ፡</foreign>
                  <subc>II,1</subc> <cit type="translation" xml:lang="la">
                     <quote>portare jubere</quote>
                  </cit>, <cit type="translation" xml:lang="la">
                     <quote>ad portandum</quote>
                  </cit> vel <cit type="translation" xml:lang="la">
                     <quote>gestandum adigere</quote>
                  </cit>, <cit type="translation" xml:lang="la">
                     <quote>portandum</quote>
                  </cit> vel <cit type="translation" xml:lang="la">
                     <quote>ferendum imponere</quote>
                  </cit>, <lbl expand="cum">c.</lbl>
                  <gramGrp>
                     <case value="accusativus">Acc.</case>
                  </gramGrp> portantis et <gramGrp>
                     <case value="accusativus">Acc.</case>
                  </gramGrp> oneris: <ref cRef="Ruth" loc="3,15">Ruth 3,15</ref>; <foreign xml:lang="gez">አጾረታ፡</foreign>
                  <ref cRef="Judith" loc="10,5">Judith 10,5</ref>; <ref cRef="1 Reg." loc="20,40">1
                     Reg. 20,40</ref>; <ref cRef="Gen." loc="22,6">Gen. 22,6</ref>; <foreign xml:lang="gez">ዘአጸርከኒ፡</foreign> (v. <foreign xml:lang="gez">ዘአጾርከኒ፡</foreign>)
                     <foreign xml:lang="gez">እጸውር፡</foreign>
                  <ref cRef="4 Reg." loc="18,14">4 Reg. 18,14</ref>; <ref cRef="Luc." loc="11,46">Luc. 11,46</ref>; <ref cRef="Luc." loc="23,26">Luc. 23,26</ref>; Lit. Orth.;
                     <foreign xml:lang="gez">ያጹርዎ፡ ቀኖና፡ ዘማውያን፡</foreign> F.M. 5,8; F.M. 23,9;
                     <foreign xml:lang="gez">ታጸውርዎሙ፡ ለሰብእ፡ አርዑተ፡ ክቡደ፡</foreign>
                  <ref cRef="Chrys. ho." loc="14">Chrys. ho. 14</ref>; F.N. 36 (<lbl expand="videas">vid.</lbl>
                  <foreign xml:lang="gez">ጥጥ፡</foreign>; <lbl expand="cum">c.</lbl>
                  <gramGrp>
                     <case value="accusativus">Acc.</case>
                  </gramGrp> rei et <foreign xml:lang="gez">መልዕልተ፡ </foreign>
                  <cit type="translation" xml:lang="la">
                     <quote>jumenti</quote>
                  </cit>: <foreign xml:lang="gez">ወአጸርዎ፡</foreign> (v. <foreign xml:lang="gez">ወአጾርዎ፡</foreign>) <foreign xml:lang="gez">መልዕልተ፡ አፍራስ፡</foreign> 2 <ref cRef="Par." loc="25,28">Par. 25,28</ref>; <lbl expand="cum">c.</lbl>
                  <foreign xml:lang="gez">ለ፡</foreign> rei et <gramGrp>
                     <case value="accusativus">Acc.</case>
                  </gramGrp>
                  <lbl expand="persona">pers.</lbl>: <foreign xml:lang="gez">አጾረ፡</foreign> (v.
                     <foreign xml:lang="gez">አጸረ፡</foreign>) <foreign xml:lang="gez">ሎቱ፡ ፪ እምውስተ፡
                     ደቁ፡</foreign>
                  <ref cRef="4 Reg." loc="5,23">4 Reg. 5,23</ref>. </sense>
               
