               <sense xml:lang="la" source="#dillmann" xml:id="Dmain"> vel <foreign xml:lang="gez">ዐሥራት፡</foreign> <gramGrp> <pos expand="Substantivum">subst.</pos> </gramGrp>, <gramGrp> <gen value="femininus">fem.</gen> </gramGrp> et <gramGrp> <gen value="not found">m.</gen> </gramGrp>, <cit type="translation" xml:lang="la">
                            <quote> decima pars </quote>
                        </cit> cujuscumque rei, <ref cRef="Kuf." loc="42">Kuf. p. 42</ref>; <foreign xml:lang="gez">ዓሥራተ፡ እደ፡ ዘርእክሙ፡</foreign> <ref cRef="1 Reg." loc="8,15">1 Reg. 8,15</ref>; <foreign xml:lang="gez">ወሀቦ፡ ዓሥራተ፡ እድ፡</foreign> <ref cRef="Gen. " loc="14,20">Gen.  14,20</ref>; <ref cRef="Gen." loc="28,22">Gen. 28,22</ref>; <foreign xml:lang="gez">ዓሥራተ፡ እዴሃ፡ ለመስፈርተ፡ ኢፍ፡</foreign> <ref cRef="Num." loc="15,4">Num. 15,4</ref>; <foreign xml:lang="gez">ዓሥራታ፡ ለኢን፡</foreign> <ref cRef="Ex." loc="29,40">Ex. 29,40</ref>; <ref cRef="Hez." loc="45,11">Hez. 45,11</ref>; <foreign xml:lang="gez">ዓሥራቲሁ፡ ለባት፡</foreign> <ref cRef="Hez." loc="45,14">Hez. 45,14</ref>; <ref cRef="Matth." loc="23,23">Matth. 23,23</ref>. <sense n="a" xml:id="Da"> <cit type="translation" xml:lang="la">
                                <quote> nomen mensurae </quote>
                            </cit> (<foreign xml:lang="grc">δέκατον</foreign> <foreign xml:lang="he">עִשָּׂרוֹן</foreign>), quae decimam Ephae partem continet: <ref cRef="Lev. " loc="23,17">Lev.  23,17</ref>; <ref cRef="Lev." loc="24,5">Lev. 24,5</ref>; <ref cRef="Num." loc="15,9">Num. 15,9</ref>; <ref cRef="Num." loc="28 ">Num. 28 </ref>et <ref cRef="Num." loc="29.">Num. 29.</ref> </sense> <sense n="b" xml:id="Db"> <cit type="translation" xml:lang="la">
                                <quote> decimae </quote>
                            </cit> (frugum pecorisve), deinde <cit type="translation" xml:lang="la">
                                <quote> tributum </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> munus </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> oblatio </quote>
                            </cit> in genere: <foreign xml:lang="grc">τὰ ἐπιδέκατα</foreign> <ref cRef="Deut. " loc="14,23">Deut.  14,23</ref>; <ref cRef="Lev." loc="27,30">Lev. 27,30</ref> <lbl expand="sequens">seq.</lbl>; <ref cRef="Num." loc="18,21">Num. 18,21</ref> <lbl expand="sequens">seq.</lbl>; <foreign xml:lang="gez">ዐሥራቱ፡ ለዐሥራት፡</foreign> <ref cRef="Num." loc="18,26">Num. 18,26</ref>; <ref cRef="Kuf." loc="13">Kuf. 13</ref>; <foreign xml:lang="gez">ደገመ፡ ዓሥራተ፡ ለእግዚአብሔር፡</foreign> <ref cRef="Kuf." loc="32">Kuf. 32</ref>; <ref cRef="Mal." loc="3,8">Mal. 3,8</ref>; <ref cRef="Am." loc="4,4">Am. 4,4</ref>; <ref cRef="1 Reg." loc="1,21">1 Reg. 1,21</ref>; <ref cRef="2 Esr." loc="20,37">2 Esr. 20,37</ref>; <ref cRef="2 Esr." loc="20,38">2 Esr. 20,38</ref>; 2 <ref cRef="Par." loc="31,5">Par. 31,5</ref>; <ref cRef="2 Par." loc="31,6">2 Par. 31,6</ref>; <ref cRef="Sir." loc="32,11">Sir. 32,11</ref>; <foreign xml:lang="gez">ቀዳሚ፡ ዓሥራት፡</foreign> et <foreign xml:lang="gez">ዳግም፡ ዓሥራት፡</foreign> <ref cRef="Tob." loc="1,7">Tob. 1,7</ref>; <foreign xml:lang="gez">ሣልስ፡ ዓሥራት፡</foreign> <ref cRef="Tob." loc="1,8">Tob. 1,8</ref>; <foreign xml:lang="gez">ዓሥራተ፡ እክልነ፡</foreign> <ref cRef="Tob." loc="5,14">Tob. 5,14</ref>; <ref cRef="Hebr." loc="7,2">Hebr. 7,2</ref> <lbl expand="sequens">seq.</lbl>; <ref cRef="Lit." loc="160,1">Lit. 160,1</ref>; <foreign xml:lang="grc">ἀπαρχή</foreign> <ref cRef="Ex. " loc="36,6">Ex.  36,6</ref>; <ref cRef="Ex. " loc="39,1">Ex.  39,1</ref>; <ref cRef="Lev." loc="2,12">Lev. 2,12</ref>; <ref cRef="Lev." loc="22,12">Lev. 22,12</ref>; <foreign xml:lang="grc">μερίς</foreign> <ref cRef="2 Par." loc="31,4">2 Par. 31,4</ref>; <foreign xml:lang="grc">τὰ ἐπίλεκτα</foreign> <ref cRef="Joel" loc="4,5">Joel 4,5</ref>. – <lbl expand="Vocabularium Aethiopicum, -a -a">Voc. Ae.</lbl>: <foreign xml:lang="gez">ዓሥራት፡ ዘ፡ ቀረጽ፡</foreign> </sense> </sense>
               
