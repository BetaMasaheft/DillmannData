               <form>
                  <foreign xml:lang="gez">ተሰልጠ</foreign>
               </form>
               <sense xml:id="Dmain" xml:lang="la" source="#dillmann"> <subc>III,1</subc> et <foreign xml:lang="gez">ተሰለጠ፡</foreign> <subc>III,2</subc> <sense n="1" xml:id="D1"> <cit type="translation" xml:lang="la"><quote> compleri </quote></cit>, <cit type="translation" xml:lang="la"><quote> impleri </quote></cit>, <cit type="translation" xml:lang="la"><quote> perfectum fieri </quote></cit>, <cit type="translation" xml:lang="la"><quote> consummari </quote></cit>, <cit type="translation" xml:lang="la"><quote> absolvi </quote></cit>, de vaticinio <ref cRef="Luc." loc="4,21">Luc. 4,21</ref>; <foreign xml:lang="gez">በዘ፡ ይሴለጥ፡ ሙስናሃ፡ ለኢየሩሳሌም፡</foreign> <ref cRef="Dan." loc="9,2">Dan. 9,2</ref>; <foreign xml:lang="gez">ከመ፡ ትሰለጥ፡ ኀጢአት፡</foreign> <ref cRef="Dan." loc="9,24">Dan. 9,24</ref>; <foreign xml:lang="gez">ተሰልጠ፡ መዐትየ፡</foreign> <ref cRef="Hez." loc="6,12">Hez. 6,12</ref> <lbl expand="varia lectio">var.</lbl>; <foreign xml:lang="gez">ከመ፡ ኢይሰለጡ፡</foreign> <foreign xml:lang="grc">ἵνα μὴ τελειωθῶσι</foreign> <ref cRef="Hebr." loc="11,40">Hebr. 11,40</ref>; <foreign xml:lang="gez">በኀቤክሙ፡ ተሰልጠት፡ ጥበብ፡</foreign> <ref cRef="Job" loc="12,2">Job 12,2</ref> (v. <foreign xml:lang="gez">ጠፍአት፡</foreign>); <foreign xml:lang="gez">ወተሰልጠ፡ ሕማምየ፡ ውስተ፡ መሬት፡</foreign> <ref cRef="Job" loc="16,15">Job 16,15</ref>; <ref cRef="Joh." loc="3,30">Joh. 3,30</ref> <foreign xml:lang="grc">ἐλαττοῦσθαι</foreign>. 
</sense> <sense n="2" xml:id="D2"> <cit type="translation" xml:lang="la"><quote> efficacem fieri </quote></cit>, <cit type="translation" xml:lang="la"><quote> proficere </quote></cit>, <cit type="translation" xml:lang="la"><quote> peragere </quote></cit>: <foreign xml:lang="gez">ተገብረ፡ ወተሰልጠ፡ እሳት፡ ውስቴቱ፡</foreign> (<foreign xml:lang="gez">ለማይ፡</foreign>) <foreign xml:lang="grc">ἐνήργει</foreign> <ref cRef="Sap." loc="16,17">Sap. 16,17</ref>; <foreign xml:lang="gez">ኮነ፡ ወተሰልጠ፡</foreign> <foreign xml:lang="grc">καὶ κατευωδώθη αὐτῷ</foreign> <ref cRef="Dan. " loc="8,11">Dan.  8,11</ref>; <foreign xml:lang="gez">ገብረ፡ ወተሰልጠ፡</foreign> <ref cRef="Dan." loc="8,12">Dan. 8,12</ref>. – <lbl expand="Vocabularium Aethiopicum, -a -a">Voc. Ae.</lbl>: <foreign xml:lang="gez">በጽሐ፡ ተሰልጠ፡ ዘ፡ ደረሰ፡</foreign> 
</sense> </sense>
               
            <sense xml:id="Tmain" xml:lang="en" source="#traces"> <cit type="transcription" xml:lang="gez"><quote>tasalṭa</quote></cit>, <cit type="transcription" xml:lang="gez"><quote>tasallaṭa</quote></cit> <sense n="L" xml:id="TL"> <foreign xml:lang="gez">ተሰልጠ</foreign> <cit type="transcription" xml:lang="gez"><quote>tasalṭa</quote></cit>, <foreign xml:lang="gez">ተሰለጠ</foreign> <cit type="transcription" xml:lang="gez"><quote>tasallaṭa</quote></cit> <cit type="translation" xml:lang="en"><quote>be finished</quote></cit>, <cit type="translation" xml:lang="en"><quote>be accomplished</quote></cit>, <cit type="translation" xml:lang="en"><quote>be consummated</quote></cit>, <cit type="translation" xml:lang="en"><quote>be fulfilled</quote></cit>, <cit type="translation" xml:lang="en"><quote>be made perfect</quote></cit>, <cit type="translation" xml:lang="en"><quote>be efficient</quote></cit>, <cit type="translation" xml:lang="en"><quote>prosper</quote></cit> <bibl><ptr target="bm:Leslau1987dic"/><citedRange unit="page"> 501a</citedRange></bibl> 
