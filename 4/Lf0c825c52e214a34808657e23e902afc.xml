               <sense xml:id="Dmain" xml:lang="la" source="#dillmann"> <gramGrp> <pos expand="Substantivum">subst.</pos> </gramGrp>, <gramGrp> <gen value="not found">m.</gen> </gramGrp> et <gramGrp> <gen value="femininus">fem.</gen> </gramGrp>, <lbl expand="pluralis">Pl.</lbl> <foreign xml:lang="gez">ሢመታት፡</foreign> positio <sense n="1" xml:id="D1"> locus ubi quis positus est: <cit type="translation" xml:lang="la"><quote> statio </quote></cit>, <cit type="translation" xml:lang="la"><quote> sedes </quote></cit>: <foreign xml:lang="gez">አቀምክዎሙ፡ በበሢመቶሙ፡</foreign> <ref cRef="2 Esr." loc="23,11">2 Esr. 23,11</ref> (<foreign xml:lang="grc">στάσις</foreign>); <foreign xml:lang="gez">ከመ፡ ኢይንሣእ፡ ሢመተከ፡</foreign> <ref cRef="Sir." loc="12,12">Sir. 12,12</ref> (<foreign xml:lang="grc">καθέδρα</foreign>). 
</sense> <sense n="2" xml:id="D2"> <cit type="translation" xml:lang="la"><quote> impositio </quote></cit> manus: <foreign xml:lang="grc">ἐπίθεσιν χειρῶν</foreign> <foreign xml:lang="gez">ሢመተ፡ እድ፡</foreign> <ref cRef="1 Tim." loc="4,14">1 Tim. 4,14</ref>; <ref cRef="2 Tim." loc="1,6">2 Tim. 1,6</ref>; <ref cRef="Hebr." loc="6,2">Hebr. 6,2</ref>; etiam <foreign xml:lang="gez">እድ፡</foreign> (<cit type="translation" xml:lang="la"><quote> ordinatio </quote></cit>), ut: <foreign xml:lang="gez">ጸሎተ፡ ሢመት፡</foreign> <ref cRef="Kid." loc="6">Kid. f. 6</ref>; <ref cRef="Kid." loc="14">Kid. f. 14</ref>; <ref cRef="Const. Ap." loc="13">Const. Ap. 13</ref>-15; <ref cRef="Const. Ap." loc="21">Const. Ap. 21</ref>-23. 
</sense> <sense n="3" xml:id="D3"> <cit type="translation" xml:lang="la"><quote> munus </quote></cit>, <cit type="translation" xml:lang="la"><quote> officium </quote></cit>, <cit type="translation" xml:lang="la"><quote> functio </quote></cit>, <cit type="translation" xml:lang="la"><quote> dignitas </quote></cit>, <cit type="translation" xml:lang="la"><quote> administratio </quote></cit> (civilis, militaris, ecclesiastica): <ref cRef="Job" loc="24,24">Job 24,24</ref>; <ref cRef="Ps." loc="108,7">Ps. 108,7</ref>; <ref cRef="Ps." loc="109,5">Ps. 109,5</ref>; <ref cRef="Gen." loc="40,13">Gen. 40,13</ref>; <ref cRef="Sir." loc="38,24">Sir. 38,24</ref>; <foreign xml:lang="gez">እለ፡ ያፈቅሩ፡ ሢመተ፡</foreign> <ref cRef="Asc. Jes." loc="3,23">Asc. Jes. 3,23</ref>; Asc. <ref cRef="Jes." loc="7,2">Jes. 7,2</ref>; <foreign xml:lang="grc">πολιτεία</foreign> <ref cRef="Act." loc="22,28">Act. 22,28</ref>; <foreign xml:lang="grc">ἐπισκοπή</foreign> <ref cRef="Act." loc="1,20">Act. 1,20</ref>; <foreign xml:lang="grc">τάξις</foreign> <ref cRef="Hebr." loc="5,6">Hebr. 5,6</ref>; <ref cRef="Hebr." loc="6,20">Hebr. 6,20</ref>; <ref cRef="1 Tim." loc="3,13">1 Tim. 3,13</ref>; <foreign xml:lang="gez">ሢመተ፡ ክህነት፡</foreign> <ref cRef="Lit." loc="163,2">Lit. 163,2</ref>; F.M. 25; <foreign xml:lang="gez">አፍቅሮ፡ ሢመት፡</foreign> <ref cRef="Clem. " loc="191">Clem.  f. 191</ref>; <foreign xml:lang="gez">እመ፡ ዘሢመት፡</foreign> (clericus) <foreign xml:lang="gez">ውእቱ፡ ይሰዐር፡ እምሢመቱ፡</foreign> <cb n="0263" xml:id="c0263"/> <ref cRef="Clem." loc="204">Clem. f. 204</ref>; <foreign xml:lang="gez">በመዋዕለ፡ ሢመቱ፡</foreign> <cit type="translation" xml:lang="la"><quote> tempore administrationis ejus </quote></cit> <ref cRef="Sx. Genb. " loc="12.">Sx. Genb.  12.</ref> 
</sense> </sense>
               
