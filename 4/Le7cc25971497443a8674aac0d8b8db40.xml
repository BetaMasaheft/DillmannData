               <sense xml:id="Dmain" xml:lang="la" source="#dillmann"> <gramGrp> <pos expand="participium">part.</pos> </gramGrp> <sense n="1" xml:id="D1"> <cit type="translation" xml:lang="la"><quote> abscisus </quote></cit>, <cit type="translation" xml:lang="la"><quote> abscissus </quote></cit>, <cit type="translation" xml:lang="la"><quote> amputatus </quote></cit>, <cit type="translation" xml:lang="la"><quote> truncatus </quote></cit>, <cit type="translation" xml:lang="la"><quote> exsectus </quote></cit>, <cit type="translation" xml:lang="la"><quote> praecisus </quote></cit>: <ref cRef="Jud." loc="1,7">Jud. 1,7</ref>; <ref cRef="1 Reg." loc="5,4">1 Reg. 5,4</ref>; <foreign xml:lang="gez">ምቱረ፡ እድ፡</foreign> <foreign xml:lang="grc">κυλλός</foreign> <ref cRef="Marc." loc="9,43">Marc. 9,43</ref>; <foreign xml:lang="gez">ምቱረ፡ እዝን፡</foreign> <foreign xml:lang="grc">ὠτότμητος</foreign> <ref cRef="Lev." loc="21,18">Lev. 21,18</ref>; <ref cRef="Lev." loc="21,22">Lev. 21,22</ref>; <ref cRef="Lev." loc="21,23">Lev. 21,23</ref>; <foreign xml:lang="gez">ምቱረ፡ ልሳን፡</foreign> <foreign xml:lang="grc">γλωσσότμητος</foreign> <ref cRef="Lev." loc="22,22">Lev. 22,22</ref>; <foreign xml:lang="gez">ምቱረ፡ ዘነብ፡</foreign> <ref cRef="Lev." loc="22,23">Lev. 22,23</ref>; <foreign xml:lang="gez">ምቱራን፡</foreign> (<foreign xml:lang="grc">ἡ κατατομή</foreign>) <ref cRef="Phil." loc="3,2">Phil. 3,2</ref>; <foreign xml:lang="grc">ἐκτομίας</foreign> et <foreign xml:lang="grc">ἀποκεκομμένος</foreign> <ref cRef="Lev." loc="22,24">Lev. 22,24</ref>; <ref cRef="Deut." loc="23,2">Deut. 23,2</ref>; <cit type="translation" xml:lang="la"><quote> detruncatus </quote></cit>: <foreign xml:lang="gez">ምቱራን፡ በሰይፍ፡ አምሳለ፡ ዕፀው፡</foreign> <ref cRef="Sx. Teq." loc="22 ">Sx. Teq. 22 </ref>Enc. 
</sense> <sense n="2" xml:id="D2"> Transl. <sense n="a" xml:id="D2a"> <cit type="translation" xml:lang="la"><quote> remotus </quote></cit> ab officio, <cit type="translation" xml:lang="la"><quote> depositus </quote></cit>: <foreign xml:lang="gez">ምቱር፡ እመዓርጊሁ፡</foreign> <ref cRef="Clem." loc="128">Clem. f. 128</ref>; al.; vel <cit type="translation" xml:lang="la"><quote> excommunicatus </quote></cit> (Lud.). 
</sense> <sense n="b" xml:id="D2b"> <cit type="translation" xml:lang="la"><quote> decisus </quote></cit> i.e. <cit type="translation" xml:lang="la"><quote> decretus </quote></cit>, <cit type="translation" xml:lang="la"><quote> definitus </quote></cit> (<ref cRef="Jes." loc="8,6">Jes. 8,6</ref>); <ref cRef="Jes." loc="10,22">Jes. 10,22</ref>; <ref cRef="Jes." loc="28,22">Jes. 28,22</ref>; <ref cRef="Rom." loc="9,28">Rom. 9,28</ref>; <ref cRef="Jer." loc="18,9">Jer. 18,9</ref> <lbl expand="varia lectio">var.</lbl>; <foreign xml:lang="gez">ምቱርኑ፡ ዝነገር፡</foreign> <ref cRef="Kid." loc="41">Kid. f. 41</ref>; <ref cRef="Gad. Ad." loc="14.">Gad. Ad. f. 14.</ref> 
</sense> <sense n="c" xml:id="D2c"> <cit type="translation" xml:lang="la"><quote> decisus </quote></cit>, <cit type="translation" xml:lang="la"><quote> praecisus </quote></cit> <foreign xml:lang="grc">ἀπότομος</foreign> i.e. <sense n="α" xml:id="D2cα"> <cit type="translation" xml:lang="la"><quote> praeruptus </quote></cit>, <cit type="translation" xml:lang="la"><quote> severus </quote></cit>: <foreign xml:lang="gez">እስመ፡ ኵነኔ፡ ምቱር፡ ለመኳንንት፡ ይከውን፡</foreign> <ref cRef="Sap." loc="6,6">Sap. 6,6</ref>; <ref cRef="2 Cor." loc="13,10">2 Cor. 13,10</ref>; <gramGrp> <pos expand="adverbium, adverbialiter">adv.</pos> </gramGrp> <ref cRef="Tit." loc="1,13">Tit. 1,13</ref>. 
</sense> <sense n="β" xml:id="D2cβ"> <gramGrp> <pos expand="adverbium, adverbialiter">adv.</pos> </gramGrp> <cit type="translation" xml:lang="la"><quote> praecise </quote></cit>, <cit type="translation" xml:lang="la"><quote> penitus </quote></cit>, <cit type="translation" xml:lang="la"><quote> omnino </quote></cit>: <foreign xml:lang="gez">ይደልዎ፡ ይትፈለጥ፡ ምቱረ፡</foreign> (<foreign xml:lang="gez">እምብእሲቱ፡</foreign>) F.M. 12,5. 
</sense> 
</sense> 
