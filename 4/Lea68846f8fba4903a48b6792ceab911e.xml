         <div>
            <entry xml:id="Lea68846f8fba4903a48b6792ceab911e" n="5669">
               <form>
                  <foreign xml:lang="gez">ተአተተ</foreign>
               </form>
               <sense xml:lang="la" source="#dillmann" xml:id="Dmain"> <subc>III,1</subc> et <subc>III,2</subc> <sense n="1" xml:id="D1">
                     <gramGrp>
                        <pos expand="reflexivum">refl.</pos>
                     </gramGrp>
                     <cit type="translation" xml:lang="la">
                        <quote>se</quote>
                     </cit>
                     <cit type="translation" xml:lang="la">
                        <quote>amovere</quote>
                     </cit> (sich entfernen) i.e. <cit type="translation" xml:lang="la">
                        <quote>procul</quote>
                     </cit>
                     <cit type="translation" xml:lang="la">
                        <quote>ire</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>discedere</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>abscedere</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>recedere</quote>
                     </cit>, <foreign xml:lang="gez">ኢትትኤተቱ፡ ወርሑቀ፡ ኢተሐውሩ፡</foreign>
                     <ref cRef="Ex." loc="8,24">Ex. 8,24</ref>; <foreign xml:lang="gez">ተአተተ፡ ሕቀ፡
                        እምህየ፡</foreign>
                     <ref cRef="Matth." loc="26,39">Matth. 26,39</ref>; <ref cRef="Marc." loc="14,35">Marc. 14,35</ref>; <foreign xml:lang="gez">ኢይትአተቱ፡
                        እምኔሁ፡</foreign>
                     <ref cRef="Hen." loc="14,23">Hen. 14,23</ref>; Stephanus <foreign xml:lang="gez">ኢተአተተ፡ ወኢተዐደወ፡ እምነ፡ ዚአሁ፡ ዐቅም፡</foreign> (<foreign xml:lang="gez">አላ፡ ሥርዐተ፡ ዲያቆን፡ ዐቀበ፡</foreign>) Mavâs. 12; etiam <cit type="translation" xml:lang="la">
                        <quote>desistere</quote>
                     </cit> vel <cit type="translation" xml:lang="la">
                        <quote>absistere</quote>
                     </cit> incepto, <lbl expand="sequens">seq.</lbl>
                     <foreign xml:lang="gez">እምነ፡</foreign>
                     <ref cRef="Jsp." loc="272.">Jsp. p. 272.</ref>
                  </sense>
                  <sense n="2" xml:id="D2">
                     <gramGrp>
                        <pos expand="passivum">pass.</pos>
                     </gramGrp>: <cit type="translation" xml:lang="la">
                        <quote>amoveri</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>removeri</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>submoveri</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>auferri</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>tolli</quote>
                     </cit>: <foreign xml:lang="gez">ተአተተ፡ እምኔየ፡ በትር፡</foreign>
                     <ref cRef="Job" loc="9,34">Job 9,34</ref>; <ref cRef="Jer. " loc="4,1">Jer.
                        4,1</ref>
                     <lbl expand="varia lectio">var.</lbl>; <foreign xml:lang="gez">ሶበ፡ ጥቀ፡ ይትሌዐል፡
                        ሰማይ፡ ወይትአተት፡</foreign>
                     <ref cRef="Jer." loc="38,37">Jer. 38,37</ref>; <ref cRef="Jer." loc="39,40">Jer. 39,40</ref>
                     <lbl expand="varia lectio">var.</lbl>; <foreign xml:lang="gez">ይትአተት፡ በትረ፡
                        ግብጽ፡</foreign>
                     <ref cRef="Zach." loc="10,11">Zach. 10,11</ref>; <ref cRef="Act." loc="8,33">Act. 8,33</ref>. – <lbl expand="Vocabularium Aethiopicum, -a -a.">Voc. Ae.</lbl>: <foreign xml:lang="gez">ተአተተ፡ ዘ፡ ረሐቀ።</foreign>
                  </sense>
               </sense>
               
