               <sense xml:id="Dmain" xml:lang="la" source="#dillmann"> <subc>I,1</subc> [amh. <lbl expand="idem">id.</lbl>; hebr. <foreign xml:lang="he">לָמַד</foreign> <cit type="translation" xml:lang="la"><quote> didicit </quote></cit>; de <foreign xml:lang="ar">لَمَدَ</foreign> et <foreign xml:lang="syr">ܥܡܰܕ</foreign> <lbl expand="videas">vid.</lbl> Ges. thes. p. 756]. <gramGrp> <pos expand="Subjunctivus">Subj.</pos> </gramGrp> <foreign xml:lang="gez">ይልመድ፡</foreign> <cit type="translation" xml:lang="la"><quote> assuetum </quote></cit>, <cit type="translation" xml:lang="la"><quote> consuetum </quote></cit>, <cit type="translation" xml:lang="la"><quote> solitum </quote></cit>, <cit type="translation" xml:lang="la"><quote> familiarem esse </quote></cit>: <gramGrp> <pos expand="absolute">absol.</pos> </gramGrp> <ref cRef="Hebr." loc="10,25">Hebr. 10,25</ref>; <ref cRef="1 Cor." loc="11,16">1 Cor. 11,16</ref> Platt (quamquam hoc sensu <subc>II,1</subc> magis usitatum est); <lbl expand="construitur cum">c.c.</lbl> <gramGrp> <case value="accusativus">Acc.</case> </gramGrp>: <foreign xml:lang="gez">ዘኢትለምዱ፡ ይበጽሐክሙ፡</foreign> <ref cRef="1 Petr." loc="4,12">1 Petr. 4,12</ref> <lbl expand="romanae editionis">rom.</lbl>; <foreign xml:lang="gez">ደም፡ ዘይለምዳ፡ አንስት፡</foreign> (menstrua) <ref cRef="Did." loc="33">Did. 33</ref>; {<ref target="#c1426"/> <foreign xml:lang="gez">ለመደ፡ ብሕትውና፡</foreign> assuevit vitae solitariae <ref cRef="Sx. Haml." loc="10">Sx. Haml. 10</ref>}; <foreign xml:lang="gez">እሙንቱ፡ ይለምዱ፡ ቤቶ፡</foreign> <cit type="translation" xml:lang="la"><quote> frequentabant domum ejus </quote></cit> (<cit type="translation" xml:lang="la"><quote> familiares ejus erant </quote></cit>) <ref cRef="Sx. Masc." loc="28">Sx. Masc. 28</ref>; <ref cRef="Sx. Tachs." loc="7 ">Sx. Tachs. 7 </ref>Enc.; maxime <lbl expand="cum">c.</lbl> <gramGrp> <case value="accusativus">Acc.</case> </gramGrp> <ref cRef="Infin. Phil." loc="4,12">Infin. Phil. 4,12</ref>; <foreign xml:lang="gez">ልሳኖሙ፡ ለመደ፡ ተናግሮ፡ ሐሰተ፡</foreign> <ref cRef="Jer." loc="9,5">Jer. 9,5</ref>; <foreign xml:lang="gez">ቀቲለ፡ ለመዱ፡</foreign> <ref cRef="Prov." loc="1,18">Prov. 1,18</ref>; <foreign xml:lang="gez">ከመ፡ ኢይልመዱ፡</foreign> (dediscant) <foreign xml:lang="gez">ዐሪፈ፡</foreign> <ref cRef="1 Tim." loc="1,20">1 Tim. 1,20</ref>; <ref cRef="Herm." loc="22">Herm. p. 22</ref>; <foreign xml:lang="gez">እስመ፡ ኢለመዱ፡ አፍራሲሁ፡ ተፃብኦ፡ ምስለ፡ መስተጽእናነ፡ ነጌያት፡</foreign> <ref cRef="Jsp." loc="283">Jsp. p. 283</ref>; et <lbl expand="cum">c.</lbl> <foreign xml:lang="gez">ከመ፡</foreign> seq. <gramGrp> <pos expand="Subjunctivus">Subj.</pos> </gramGrp>: <foreign xml:lang="gez">ኢለመደ፡ ከመ፡ ይፃእ፡ እምበአቱ፡</foreign> (e spelunca sua) <ref cRef="Phlx." loc="115.">Phlx. 115.</ref> </sense>
               
            <sense xml:id="Tmain" xml:lang="en" source="#traces"> <cit type="transcription" xml:lang="gez"><quote>lamada</quote></cit> <sense n="G" xml:id="TG"> <foreign xml:lang="gez">ለመደ፡</foreign> <cit type="transcription" xml:lang="gez"><quote>lamada</quote></cit> <subc>I,1</subc> <foreign xml:lang="gez">ይለምድ፡</foreign>, <foreign xml:lang="gez">ይልምድ፡</foreign> «<cit type="translation" xml:lang="fr"><quote>s’accoutumer</quote></cit>, <cit type="translation" xml:lang="fr"><quote>s’habituer</quote></cit>, <cit type="translation" xml:lang="fr"><quote>être apprivoisé</quote></cit>» ― <foreign xml:lang="am">ለመደ፡</foreign> Ms. <ref type="BM" target="BNFabb217">BNFabb217</ref>, fol. 13v. Ms. Roma, Biblioteca Nazionale Centrale di Roma, Orientale 134, p. 4. <lb/>
