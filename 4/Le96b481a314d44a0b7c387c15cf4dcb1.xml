               <sense xml:lang="la" source="#dillmann" xml:id="Dmain">
                  <gramGrp>
                     <pos expand="adjectivum">adj.</pos>
                     <pos expand="relativum">rel.</pos>
                  </gramGrp> et <gramGrp>
                     <pos expand="Substantivum">subst.</pos>
                  </gramGrp>
                  <sense n="1" xml:id="D1">
                     <cit type="translation" xml:lang="la">
                        <quote>liber</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>nobilis</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>ingenuus</quote>
                     </cit>, <cb n="0086" xml:id="c0086"/>
                     <foreign xml:lang="grc">ἐλεύθερος</foreign> (<lbl expand="oppositum, -o, -nitur">opp.</lbl>
                     <foreign xml:lang="gez">ገብር፡</foreign>) <ref cRef="Chrys. Ta. " loc="15.">Chrys. Ta. 15.</ref>
                  </sense>
                  <sense n="2" xml:id="D2"> qui ad exercitum pertinet, i.e. <sense n="a" xml:id="D2a">
                        <cit type="translation" xml:lang="la">
                           <quote>centurio</quote>
                        </cit>, <cit type="translation" xml:lang="la">
                           <quote>tribunus</quote>
                        </cit>
                        <ref cRef="Marc." loc="15,44">Marc. 15,44</ref>; <ref cRef="Marc." loc="15,45">Marc. 15,45</ref>.</sense>
                     <sense n="b" xml:id="D2b">
                        <cit type="translation" xml:lang="la">
                           <quote>miles</quote>
                        </cit>, <cit type="translation" xml:lang="la">
                           <quote>miles gregarius</quote>
                        </cit>
                        <foreign xml:lang="grc">δημότης</foreign> (<lbl expand="oppositum, -o, -nitur">opp.</lbl>
                        <foreign xml:lang="gez">ንጉሥ፡</foreign>) <ref cRef="Sap." loc="18,11">Sap.
                           18,11</ref>; <foreign xml:lang="grc">στρατιώτης </foreign>
                        <ref cRef="Joh." loc="19,23">Joh. 19,23</ref>; <foreign xml:lang="gez">ሐራዌ፡
                           መኰንን፡</foreign>
                        <cit type="translation" xml:lang="la">
                           <quote>miles proconsulis</quote>
                        </cit>
                        <ref cRef="Const. Ap." loc="27">Const. Ap. 27</ref>; <foreign xml:lang="gez">ሐራዊ፡ ዘክርስቶስ፡ </foreign>
                        <cit type="translation" xml:lang="la">
                           <quote>miles Christi</quote>
                        </cit>
                        <ref cRef="Phlx." loc="192.">Phlx. 192.</ref>
                     </sense>
                  </sense>
