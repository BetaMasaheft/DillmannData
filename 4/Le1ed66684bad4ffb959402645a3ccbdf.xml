               <sense xml:lang="la" source="#dillmann" xml:id="Dmain"> <lbl expand="radix">rad.</lbl>, quam <bibl>
                            <ptr target="bm:Bochart1794HierozII"/>
                            <citedRange unit="page"> 489</citedRange>
                        </bibl> cum <foreign xml:lang="he">עָדַן</foreign> composuit, alii (ut Ges. in thes. p. 24), cum e <cit type="translation" xml:lang="la">
                            <quote> rubro </quote>
                        </cit> in <cit type="translation" xml:lang="la">
                            <quote> pulchrum </quote>
                        </cit> facilis sit transitus, ad <foreign xml:lang="he">אָדַם</foreign> <cit type="translation" xml:lang="la">
                            <quote> rubuit </quote>
                        </cit> referendam censent, (sive quia <cit type="translation" xml:lang="la">
                            <quote> rubrum </quote>
                        </cit> est <cit type="translation" xml:lang="la">
                            <quote> pulchrum </quote>
                        </cit>, sive quia rubor faciei vitalis <cit type="translation" xml:lang="la">
                            <quote> gaudentis </quote>
                        </cit>, <cit type="translation" xml:lang="la">
                            <quote> amantis </quote>
                        </cit>, <cit type="translation" xml:lang="la">
                            <quote> arridentis </quote>
                        </cit>, <cit type="translation" xml:lang="la">
                            <quote> voluptate affecti </quote>
                        </cit> est sicut pallor solliciti, desiderantis) <lbl expand="collatio, -is vel collectivum,&#xA;                            -e">coll.</lbl> amh. <foreign xml:lang="gez">ደምግቡ፡</foreign> </sense>
               
