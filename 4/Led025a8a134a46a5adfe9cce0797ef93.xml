         <div>
            <entry xml:id="Led025a8a134a46a5adfe9cce0797ef93" n="4856">
               <form>
                  <foreign xml:lang="gez">አንቅሀ</foreign>
               </form>
               <sense xml:lang="la" source="#dillmann" xml:id="Dmain"> <subc>II,1</subc> <cit type="translation" xml:lang="la">
                     <quote>expergefacere</quote>
                  </cit>, <cit type="translation" xml:lang="la">
                     <quote>excitare</quote>
                  </cit>, <cit type="translation" xml:lang="la">
                     <quote>suscitare</quote>
                  </cit>, dormientem: <ref cRef="Matth." loc="8,25">Matth. 8,25</ref>; <ref cRef="Joh." loc="11,11">Joh. 11,11</ref>; <foreign xml:lang="gez">አንቅሆ፡ ለእግዚእነ፡ </foreign>
                  <ref cRef="Judith" loc="14,13">Judith 14,13</ref>; <ref cRef="Jes." loc="50,4">Jes. 50,4</ref>
                  <lbl expand="varia lectio">var.</lbl>; <foreign xml:lang="gez">አንቅሆ፡ እምዐቢይ፡ ንዋም፡
                     ለዘይነውም፡</foreign>
                  <ref cRef="Sir." loc="22,7">Sir. 22,7</ref>; <ref cRef="Kuf." loc="3">Kuf.
                  3</ref>; <foreign xml:lang="gez">አንቃህከነ፡ እምነ፡ ንዋም፡ </foreign> Deg. (in hymno
                  matutino); feram cubantem <ref cRef="Gen." loc="49,9">Gen. 49,9</ref>; <ref cRef="Num." loc="24,9">Num. 24,9</ref>. <sense n="a" xml:id="Da">
                     <lbl expand="translate">transl.</lbl> de animo: <ref cRef="Ps." loc="76,6">Ps.
                        76,6</ref>; <ref cRef="2 Petr." loc="1,13">2 Petr. 1,13</ref>; <foreign xml:lang="gez">ከመ፡ ያንቅሆሙ፡ </foreign> vel <foreign xml:lang="gez">ከመ፡ ያንቅህ፡
                        ልቦሙ፡</foreign> Chrys. (crebro).</sense>
                  <sense n="b" xml:id="Db">
                     <cit type="translation" xml:lang="la">
                        <quote>resuscitare</quote>
                     </cit> mortuos: <ref cRef="Clem." loc="252">Clem. f. 252</ref>; Rel. Bar.;
                        <foreign xml:lang="gez">አንቅሀ፡ በድነ፡</foreign> Sx. Enc.</sense>
                  <sense n="c" xml:id="Dc"> de necromantia: <cit type="translation" xml:lang="la">
                        <quote>evocare</quote>
                     </cit> vel <cit type="translation" xml:lang="la">
                        <quote>suscitare</quote>
                     </cit> mortuum vel pythonem 1 <ref cRef="Reg." loc="28,3">Reg. 28,3</ref>; <ref cRef="1 Reg." loc="28,7">1 Reg. 28,7</ref>; <ref cRef="1 Reg." loc="28,11">1
                        Reg. 28,11</ref>; <ref cRef="Deut." loc="18,11">Deut. 18,11</ref>; <foreign xml:lang="gez">እለ፡ ያነቅሁ፡ </foreign> (<foreign xml:lang="gez">ያነቅሃ፡</foreign>) <foreign xml:lang="gez">በፃውዐ፡</foreign>
                     <ref cRef="Jes." loc="44,25">Jes. 44,25</ref>.</sense>
               </sense>
               
