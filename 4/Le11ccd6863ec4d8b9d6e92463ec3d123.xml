            <entry xml:id="Le11ccd6863ec4d8b9d6e92463ec3d123" n="1149">
               <form>
                  <foreign xml:lang="gez">አስተምሐረ</foreign>
               </form>
               <sense xml:id="Dmain" xml:lang="la" source="#dillmann"> <subc>IV,1</subc> <sense n="1" xml:id="D1"> <cit type="translation" xml:lang="la"><quote> facere ut aliquis misericordiam inveniat </quote></cit> i.e. <sense n="a" xml:id="D1a"> <cit type="translation" xml:lang="la"><quote> misericordiam praestare </quote></cit>, <cit type="translation" xml:lang="la"><quote> impertire </quote></cit>, <cit type="translation" xml:lang="la"><quote> se misericordem praebere </quote></cit> alicui: <gramGrp> <pos expand="absolute">abs.</pos> </gramGrp>, ut: <ref cRef="1 Cor." loc="13,4">1 Cor. 13,4</ref> <lbl expand="romanae editionis">rom.</lbl>; <lbl expand="cum">c.</lbl> <foreign xml:lang="gez">ለ፡</foreign> pers. <ref cRef="Herm." loc="58">Herm. p. 58</ref>; plerumque <lbl expand="cum">c.</lbl> <gramGrp> <case value="accusativus">Acc.</case> </gramGrp>: <foreign xml:lang="gez">እንዘ፡ ኢታስተምሕር፡ ሰብአ፡ ዘከማከ፡</foreign> <ref cRef="Sir." loc="28,4">Sir. 28,4</ref>; <foreign xml:lang="gez">ኢያስተምሕር፡</foreign> (v. <foreign xml:lang="gez">ይምሕር፡</foreign>) <foreign xml:lang="gez">ብእሲ፡</foreign> {<ref target="#c1427"/> <foreign xml:lang="gez">እኁሁ፡</foreign>} <ref cRef="Jes." loc="9,19">Jes. 9,19</ref>; <ref cRef="Deut." loc="28,68">Deut. 28,68</ref>; Herm. p. 28; <ref cRef="Herm." loc="37">Herm. p. 37</ref>; <ref cRef="Judith" loc="6,19">Judith 6,19</ref>; <foreign xml:lang="gez">አስተምሕረኒ፡</foreign> (Imperat.) <ref cRef="Tob." loc="3,15">Tob. 3,15</ref>; etiam <lbl expand="cum">c.</lbl> <foreign xml:lang="gez">ዲበ፡</foreign>, ut: <foreign xml:lang="gez">አስተምሐረ፡ እግዚእ፡ ዲበ፡ አንብዕነ፡</foreign> Rel. Bar. 
</sense> <sense n="b" xml:id="D1b"> <cit type="translation" xml:lang="la"><quote> misericordiam </quote></cit> alicujus <cit type="translation" xml:lang="la"><quote> implorare </quote></cit>, <cit type="translation" xml:lang="la"><quote> commiserationem petere </quote></cit> ab aliquo aut sibi aut aliis (<cit type="translation" xml:lang="la"><quote> intercedere </quote></cit>): <ref cRef="Clem." loc="68">Clem. f. 68</ref>; <ref cRef="Marc." loc="7,34">Marc. 7,34</ref>; <lbl expand="cum">c.</lbl> <foreign xml:lang="gez">ለ፡</foreign> pers.: <foreign xml:lang="gez">ኣስተምሕር፡ ሎሙ፡</foreign> (var.) <ref cRef="Job" loc="19,17">Job 19,17</ref>; <lbl expand="cum">c.</lbl> <gramGrp> <case value="accusativus">Acc.</case> </gramGrp> <ref cRef="Gen." loc="42,21">Gen. 42,21</ref>; <lbl expand="cum">c.</lbl> <foreign xml:lang="gez">ኀበ፡</foreign> <ref cRef="Hen." loc="38,6">Hen. 38,6</ref>; <ref cRef="Lit." loc="170,2">Lit. 170,2</ref>; etiam <lbl expand="cum">c.</lbl> <foreign xml:lang="gez">ለ፡</foreign> ejus, pro quo interceditur, ut: <foreign xml:lang="gez">አስተምሕር፡ ለነ፡ ሚካኤል፡</foreign> Mavâs. 16, et alias saepissime in precibus et litaniis. 
</sense> 
</sense> <sense n="2" xml:id="D2"> <cit type="translation" xml:lang="la"><quote> ad misericordiam adducere </quote></cit> <ref cRef="1 Cor." loc="13,7">1 Cor. 13,7</ref> <lbl expand="romanae editionis">rom.</lbl> 
</sense> </sense>
               
            <sense xml:id="Tmain" xml:lang="en" source="#traces"> <cit type="transcription" xml:lang="gez"><quote>ʾastamḥara</quote></cit> <sense n="G" xml:id="TG"> <foreign xml:lang="gez">አስተምሐረ፡</foreign> <cit type="transcription" xml:lang="gez"><quote>ʾastamḥara</quote></cit> <subc>IV,1</subc> <foreign xml:lang="gez">ያስተምሕር፡</foreign> «<cit type="translation" xml:lang="fr"><quote>être enclin à la miséricorde</quote></cit>, <cit type="translation" xml:lang="fr"><quote> intercéder pour un autre</quote></cit>, <cit type="translation" xml:lang="fr"><quote>obtenir des grâces à un autre</quote></cit>» ― <foreign xml:lang="am">አማለደ፡</foreign> Ms. <ref type="BM" target="BNFabb217">BNFabb217</ref>, fol. 34v. <lb/>
 <bibl><ptr target="bm:Grebaut1952SuppLexicon"/><citedRange unit="page"> 78</citedRange></bibl> 
</sense> <sense n="L" xml:id="TL"> <foreign xml:lang="gez">አስተምሐረ</foreign> <cit type="transcription" xml:lang="gez"><quote>ʾastamḥara</quote></cit> <cit type="translation" xml:lang="en"><quote>implore pity</quote></cit>, <cit type="translation" xml:lang="en"><quote>crave mercy</quote></cit>, <cit type="translation" xml:lang="en"><quote>seek mercy</quote></cit>, <cit type="translation" xml:lang="en"><quote>ask for mercy</quote></cit>, <cit type="translation" xml:lang="en"><quote>be prone to pity</quote></cit>, <cit type="translation" xml:lang="en"><quote>have pity</quote></cit>, <cit type="translation" xml:lang="en"><quote>intercede</quote></cit>, <cit type="translation" xml:lang="en"><quote>be merciful</quote></cit>, <cit type="translation" xml:lang="en"><quote>be compassionate</quote></cit>, <cit type="translation" xml:lang="en"><quote>obtain pardon</quote></cit> <bibl><ptr target="bm:Leslau1987dic"/><citedRange unit="page"> 336b</citedRange></bibl> 
</sense> </sense></entry>
