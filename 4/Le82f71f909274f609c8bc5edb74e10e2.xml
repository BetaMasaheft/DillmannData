                  <foreign xml:lang="gez">አስሐትያ</foreign>
               </form>
               <sense xml:lang="la" source="#dillmann" xml:id="Dmain"> et <foreign xml:lang="gez">አስሐቲያ፡</foreign>
                  <gramGrp>
                     <pos expand="Substantivum">subst.</pos>
                  </gramGrp>, <gramGrp>
                     <gen value="masculinum">m.</gen>
                  </gramGrp> et <gramGrp>
                     <gen value="femininus">fem.</gen>
                  </gramGrp> [num ab <foreign xml:lang="gez">አስሐተ፡</foreign> i.e. <foreign xml:lang="he">הִשְׁחִית</foreign> vel <foreign xml:lang="gez">ሰሐጠ፡</foreign>
                  repetendum et <cit type="translation" xml:lang="la">
                     <quote>id quod perniciosum</quote>
                  </cit> vel <cit type="translation" xml:lang="la">
                     <quote>noxium est</quote>
                  </cit> exponendum? an <foreign xml:lang="ar">سَقِيطٌ</foreign> comparandum
                  videtaur?] <cit type="translation" xml:lang="la">
                     <quote>nix</quote>
                  </cit>, <cit type="translation" xml:lang="la">
                     <quote>pruina</quote>
                  </cit>, <cit type="translation" xml:lang="la">
                     <quote>grando</quote>
                  </cit>, <cit type="translation" xml:lang="la">
                     <quote>gelicidium</quote>
                  </cit>, <cit type="translation" xml:lang="la">
                     <quote>gelu</quote>
                  </cit>, <cit type="translation" xml:lang="la">
                     <quote>glacies</quote>
                  </cit>: <foreign xml:lang="grc">χιών </foreign>
                  <ref cRef="Prov." loc="25,13">Prov. 25,13</ref>; <ref cRef="Jer." loc="18,14">Jer.
                     18,14</ref>; <foreign xml:lang="grc">πάχνη</foreign>
                  <foreign xml:lang="gez">ይዘርዎ፡ ውስተ፡ ምድር፡ ለጊሜሁ፡ ወእምከመ፡ ረግዐ፡ አስሐትያ፡ ይትፈለጽ፡ ከመ፡ ስባራተ፡
                     ማህው፡ </foreign>
                  <ref cRef="Sir." loc="43,19">Sir. 43,19</ref>; <ref cRef="Ps." loc="77,52">Ps.
                     77,52</ref>; <ref cRef="Ps." loc="118,83">Ps. 118,83</ref>; <foreign xml:lang="gez">አስሐትያ፡ ቀጢን፡ </foreign> (v. <foreign xml:lang="gez">ቀጣን፡</foreign>) <ref cRef="Sap." loc="5,15">Sap. 5,15</ref>; <foreign xml:lang="gez">አስሐትያ፡ ክረምት፡</foreign>
                  <ref cRef="Sap." loc="16,29">Sap. 16,29</ref>; <foreign xml:lang="grc">πάγος</foreign>
                  <ref cRef="Job " loc="37,9">Job 37,9</ref>; <ref cRef="Ex." loc="16,14">Ex.
                     16,14</ref>; <foreign xml:lang="gez">ቍር፡ ወአስሐትያ፡</foreign>
                  <foreign xml:lang="grc">πάγος καὶ ψῦχος</foreign>
                  <ref cRef="Dan. apocr." loc="3,48">Dan. apocr. 3,48</ref>; <foreign xml:lang="gez">አስሐትያ፡ ይረግዕ፡ ዲበ፡ ማይ፡</foreign>
                  <ref cRef="Sir." loc="43,20">Sir. 43,20</ref>; <ref cRef="Job" loc="15,7">Job
                     15,7</ref>; <foreign xml:lang="grc">χάλαζα</foreign>
                  <ref cRef="Job " loc="38,22">Job 38,22</ref>; <foreign xml:lang="gez">እብነ፡
                     አስሐትያ፡</foreign>
                  <ref cRef="Hen." loc="71,5">Hen. 71,5</ref>; <foreign xml:lang="grc">κρύσταλλος</foreign>
                  <ref cRef="Apoc." loc="21,11">Apoc. 21,11</ref> Platt; praeterea in locis aliis,
                  ubi quae voc Graeca respondeat haud satis liquet: <ref cRef="Ps." loc="148,8">Ps.
                     148,8</ref>; <ref cRef="Job" loc="6,16">Job 6,16</ref>; <ref cRef="Sap." loc="16,22">Sap. 16,22</ref>; <ref cRef="4 Esr." loc="6,10">4 Esr. 6,10</ref>;
                     <ref cRef="Hen. " loc="14,18">Hen. 14,18</ref>; <ref cRef="Hen." loc="60,17">Hen. 60,17</ref>; <ref cRef="Hen." loc="69,23">Hen. 69,23</ref>; <ref cRef="Kuf." loc="2">Kuf. 2</ref>; <ref cRef="Kuf." loc="23">Kuf. 23</ref>;
                     <foreign xml:lang="gez">ዝናም፡ ወአስሐትያ፡ አጥፍኡ፡ እሳተ፡</foreign>
                  <ref cRef="Sx. Mij." loc="30 ">Sx. Mij. 30 </ref>Enc.; <foreign xml:lang="gez">ከመ፡
                     አስሐትያ፡ ዘተሰደ፡ በነፋስ፡ ቀጢን፡</foreign>
                  <ref cRef="Sx. Tachs." loc="12 ">Sx. Tachs. 12 </ref>Enc.; <foreign xml:lang="gez">ማይ፡ ርግዕት፡ እምአስሐትያ፡ </foreign> (gelu) <ref cRef="Sx. Mag." loc="13.">Sx. Mag.
                     13.</ref> – <lbl expand="Vocabularium Aethiopicum, -a -a.">Voc. Ae.</lbl>: <foreign xml:lang="gez">አስሐትያ፡ ዘ፡ ውርጭ።</foreign>
               </sense>
               
