                     <ref cRef="Sx. Mag." loc="8 ">Sx. Mag. 8 </ref>Enc.</sense>
                  <sense n="2" xml:id="D2">
                     <cit type="translation" xml:lang="la">
                        <quote>rebellis</quote>
                     </cit>, <cb n="0954" xml:id="c0954"/>
                     <cit type="translation" xml:lang="la">
                        <quote>refractarius</quote>
                     </cit>: <foreign xml:lang="gez">ዕሉት፡ </foreign> (<foreign xml:lang="he">מְשֻׁבָה</foreign>) <ref cRef="Jer." loc="3,6">Jer. 3,6</ref>; <ref cRef="Jer." loc="3,11">Jer. 3,11</ref> vers. nov.; in specie <cit type="translation" xml:lang="la">
                        <quote>haereticus</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>apostata</quote>
                     </cit>: <foreign xml:lang="gez">ዕልዋን፡ </foreign>
                     <foreign xml:lang="grc">αἱρετικοί</foreign>
