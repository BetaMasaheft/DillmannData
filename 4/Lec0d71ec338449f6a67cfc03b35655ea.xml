         <div>
            <entry xml:id="Lec0d71ec338449f6a67cfc03b35655ea" n="5235">
               <form>
                  <foreign xml:lang="gez">አንገፈ</foreign>
               </form>
               <sense xml:lang="la" source="#dillmann" xml:id="Dmain"> <subc>II,1</subc> <sense n="1" xml:id="D1">
                     <cit type="translation" xml:lang="la">
                        <quote>facere ut</quote>
                     </cit> aliquid <cit type="translation" xml:lang="la">
                        <quote>excutiatur</quote>
                     </cit> vel <cit type="translation" xml:lang="la">
                        <quote>decutiatur</quote>
                     </cit> vel <cit type="translation" xml:lang="la">
                        <quote>decidat</quote>
                     </cit> e manu alicujus, vel ut <cit type="translation" xml:lang="la">
                        <quote>abjiciat</quote>
                     </cit> aliquid, <lbl expand="cum">c.</lbl>
                     <lbl expand="duplex">dupl.</lbl>
                     <gramGrp>
                        <case value="accusativus">Acc.</case>
                     </gramGrp>: <foreign xml:lang="gez">ኣነግፎ፡ መጥባሕቶ፡ እምእዴሁ፡</foreign>
                     <ref cRef="Hez." loc="30,22">Hez. 30,22</ref>; <foreign xml:lang="gez">ያነግፎሙ፡
                        ውስተ፡ ምድር፡ ኰያንዊሆሙ፡</foreign> (v. <foreign xml:lang="gez">ያገድፎሙ፡</foreign>)
                        <ref cRef="Jes." loc="41,2">Jes. 41,2</ref>; <foreign xml:lang="gez">ናሁ፡ አነ፡
                        ኣነግፈክሙ፡ ንዋየ፡ ሐቅልክሙ፡ </foreign>
                     <ref cRef="Jer." loc="21,4">Jer. 21,4</ref>.</sense>
                  <sense n="2" xml:id="D2">
                     <cit type="translation" xml:lang="la">
                        <quote>facere ut</quote>
                     </cit> alicujus <cit type="translation" xml:lang="la">
                        <quote>dimittat</quote>
                     </cit> vel <cit type="translation" xml:lang="la">
                        <quote>reddat</quote>
                     </cit> rem vi raptam i.e. <cit type="translation" xml:lang="la">
                        <quote>eripere</quote>
                     </cit> vel <cit type="translation" xml:lang="la">
                        <quote>excutere</quote>
                     </cit> alicui aliquid, <cit type="translation" xml:lang="la">
                        <quote>liberare</quote>
                     </cit> aliquem, <lbl expand="cum">c.</lbl>
                     <lbl expand="duplex">dupl.</lbl>
                     <gramGrp>
                        <case value="accusativus">Acc.</case>
                     </gramGrp>: <foreign xml:lang="gez">አንገፍክዎሙ፡ እማእከለ፡ ጥረሲሆሙ፡ ዘመሠጡ፡</foreign>
                     <ref cRef="Job " loc="29,17">Job 29,17</ref>; <ref cRef="1 Reg." loc="30,18">1
                        Reg. 30,18</ref>; <lbl expand="cum">c.</lbl>
                     <gramGrp>
                        <case value="accusativus">Acc.</case>
                     </gramGrp>
                     <lbl expand="persona">pers.</lbl> vel rei raptae: <foreign xml:lang="gez">እምእደ፡
                        ኵናት፡ ያነግፈከ፡</foreign> (<foreign xml:lang="grc">λύσει σε</foreign>) <ref cRef="Job" loc="5,20">Job 5,20</ref>; <ref cRef="Job " loc="6,23">Job
                        6,23</ref> (<foreign xml:lang="grc">ῥύσασθαι</foreign>); <ref cRef="Jes." loc="42,22">Jes. 42,22</ref> (<foreign xml:lang="grc">ἐξαιρεῖσθαι</foreign>); <foreign xml:lang="gez">በከመ፡ ያነግፍ፡ ኖላዊ፡ እምአፈ፡ ዐንበሳ፡
                        እግረ፡</foreign> (<foreign xml:lang="grc">ἐκσπάσῃ</foreign>) <ref cRef="Am." loc="3,12">Am. 3,12</ref>; <cb n="0696" xml:id="c0696"/>
                     <foreign xml:lang="gez">ያነግፈኪ፡ እምእደ፡ ፀርኪ፡</foreign>
                     <ref cRef="Mich." loc="4,10">Mich. 4,10</ref>; <ref cRef="Dan. " loc="3,17">Dan. 3,17</ref>; <ref cRef="Jes." loc="60,16">Jes. 60,16</ref>; <ref cRef="Ps." loc="30,1">Ps. 30,1</ref>; <ref cRef="Ps." loc="36,42">Ps.
                        36,42</ref>; <ref cRef="Ps." loc="58,1">Ps. 58,1</ref>; <ref cRef="Ps." loc="68,18">Ps. 68,18</ref>; <ref cRef="Luc." loc="4,19">Luc. 4,19</ref>;
                        <ref cRef="Clem." loc="160">Clem. f. 160</ref>; <ref cRef="Clem." loc="236">Clem. f. 236</ref>; omisso <gramGrp>
                        <case value="accusativus">Acc.</case>
                     </gramGrp>: <foreign xml:lang="gez">አንግፉ፡ እምእደ፡ ዘመሠጠ፡</foreign>
                     <ref cRef="Jer. " loc="21,12">Jer. 21,12</ref>.</sense>
                  <sense n="3" xml:id="D3">
                     <cit type="translation" xml:lang="la">
                        <quote>facere ut</quote>
                     </cit> alicujus <cit type="translation" xml:lang="la">
                        <quote>deponat luctum</quote>
                     </cit> i.e. <cit type="translation" xml:lang="la">
                        <quote>consolari</quote>
                     </cit> aliquem, vel <lbl expand="sequens">seq.</lbl>
                     <foreign xml:lang="gez">ላሐ፡</foreign>, ut: <foreign xml:lang="gez">አንገፍዎ፡ ላሐ፡ </foreign>
                     <ref cRef="Gen." loc="24,67">Gen. 24,67</ref>; <ref cRef="Gen." loc="37,35">Gen. 37,35</ref>; <ref cRef="2 Reg." loc="10,2">2 Reg. 10,2</ref>, vel
                     omisso <foreign xml:lang="gez">ላሐ፡</foreign> ut <ref cRef="2 Reg." loc="13,39">2 Reg. 13,39</ref>.</sense>
                  <sense n="4" xml:id="D4"> i.q. <subc>I,1</subc> <cit type="translation" xml:lang="la">
                        <quote>excutere</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>decutere</quote>
                     </cit>, <ref cRef="Act." loc="28,5">Act. 28,5</ref>
                     <lbl expand="romanae editionis">rom.</lbl> (si quidem lectio sana est).</sense>
               </sense>
               
            </entry>
