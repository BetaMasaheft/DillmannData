            <entry xml:id="Lf0d29f14f2c048d9ab7ba2d23a59731a" n="6188">
               <form>
                  <foreign xml:lang="gez">አክበበ</foreign>
               </form>
               <sense xml:lang="la" source="#dillmann" xml:id="Dmain"> <subc>II,1</subc>, <cit type="translation" xml:lang="la">
                     <quote>integrum</quote>
                  </cit> vel <cit type="translation" xml:lang="la">
                     <quote>totum colligere</quote>
                  </cit> (<lbl expand="videas">vid.</lbl>
                  <foreign xml:lang="gez">ክበብ፡</foreign>, <foreign xml:lang="gez">መክብብ፡</foreign>;
                  si non <cit type="translation" xml:lang="la">
                     <quote>convertere</quote>
                  </cit> i.e. <cit type="translation" xml:lang="la">
                     <quote>retro vertere</quote>
                  </cit>: <lbl expand="collatio, -is vel collectivum, -e">coll.</lbl>
                  <foreign xml:lang="he">סב</foreign>
                  <lbl expand="cum">c.</lbl> deriv.): <foreign xml:lang="gez">ከመ፡ ያግብእ፡ ወያግርር፡
                     ወያክብብ፡ </foreign> (v. <foreign xml:lang="gez">ወይክብብ፡</foreign>) <foreign xml:lang="gez">መንግሥቶ፡ </foreign>
                  <foreign xml:lang="grc">τοῦ ἐπιστρέψαι τὴν βασιλείαν</foreign>
                  <ref cRef="2 Par. " loc="11,1">2 Par. 11,1</ref>. <cb n="0848" xml:id="c0848"/>
               </sense>
               
