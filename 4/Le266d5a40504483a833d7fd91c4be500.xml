            <entry xml:id="Le266d5a40504483a833d7fd91c4be500" n="6207">
               <form>
                  <foreign xml:lang="gez">አክበደ</foreign>
               </form>
               <sense xml:id="Dmain" xml:lang="la" source="#dillmann"> <subc>II,1</subc> <sense n="1" xml:id="D1"> <cit type="translation" xml:lang="la"><quote> gravem facere </quote></cit>: <foreign xml:lang="gez">አክበድኪ፡ አርዑቶሙ፡ ፈድፋደ፡</foreign> <ref cRef="Jes. " loc="47,6">Jes.  47,6</ref>; <foreign xml:lang="gez">አክብዶ፡</foreign> <cit type="translation" xml:lang="la"><quote> (ei) </quote></cit> <foreign xml:lang="gez">ገንዶራቲሁ፡</foreign> <ref cRef="Sir." loc="30,38">Sir. 30,38</ref>. <sense n="a" xml:id="D1a"> <lbl expand="cum">c.</lbl> <gramGrp> <case value="accusativus">Acc.</case> </gramGrp> rei et <foreign xml:lang="gez">ላዕለ፡</foreign> vel <foreign xml:lang="gez">ዲበ፡</foreign> pers.: <cit type="translation" xml:lang="la"><quote> grave </quote></cit> quid vel <cit type="translation" xml:lang="la"><quote> nimis grave imponere </quote></cit> alicui: <foreign xml:lang="gez">እዴየ፡ ኢያከብድ፡ ላዕሌከ፡</foreign> <ref cRef="Job" loc="33,7">Job 33,7</ref>; <ref cRef="Job" loc="23,2">Job 23,2</ref>; <ref cRef="2 Esr." loc="15,16">2 Esr. 15,16</ref>; <ref cRef="Act." loc="15,19">Act. 15,19</ref>; <foreign xml:lang="gez">ኢታክብድ፡ ላዕሌሁ፡ ጾረ፡ ክቡደ፡</foreign> <ref cRef="Clem." loc="180">Clem. f. 180</ref>; <foreign xml:lang="gez">ይደሉ፡ ከመ፡ ያክብዱ፡ ቀኖና፡ ላዕሌሁ፡</foreign> F.M. 16,4; <foreign xml:lang="gez">እለ፡ ያከብዱ፡ ዲበ፡ አግብርቲሆሙ፡ ቅኔ፡ እኩየ፡</foreign> <ref cRef="Did." loc="21.">Did. 21.</ref> 
</sense> <sense n="b" xml:id="D1b"> <cit type="translation" xml:lang="la"><quote> molestum facere </quote></cit>: <foreign xml:lang="gez">አክበድክሙ፡ ላዕሌየ፡ ነገረክሙ፡</foreign> <ref cRef="Mal." loc="3,13">Mal. 3,13</ref>.
</sense> <sense n="c" xml:id="D1c"> <cit type="translation" xml:lang="la"><quote> cumulare (in molem) </quote></cit>: <foreign xml:lang="gez">በኢያእምሮ፡ ያከብድ፡ ነገሮ፡</foreign> <ref cRef="Job" loc="35,16">Job 35,16</ref>.
</sense> <sense n="d" xml:id="D1d"> <cit type="translation" xml:lang="la"><quote> vehementem facere </quote></cit>: <foreign xml:lang="gez">አክበደ፡ ተክዞ፡ ሙሴ፡ ጥቀ፡</foreign> <ref cRef="Num." loc="16,15">Num. 16,15</ref>.
</sense> <sense n="e" xml:id="D1e"> <cit type="translation" xml:lang="la"><quote> hebetare </quote></cit>, <cit type="translation" xml:lang="la"><quote> indurare </quote></cit>, cor suum <ref cRef="Ps." loc="4,3">Ps. 4,3</ref>; <ref cRef="1 Reg." loc="6,6">1 Reg. 6,6</ref>; <ref cRef="Ex." loc="8,28">Ex. 8,28</ref>; aliorum <ref cRef="Ex." loc="10,1">Ex. 10,1</ref>; <foreign xml:lang="gez">አክብዶ፡ ኅሊና፡</foreign> <cit type="translation" xml:lang="la"><quote> tarditas animi </quote></cit> (ad preces) Org.
</sense> 
</sense><sense n="2" xml:id="D2"> <sense n="a" xml:id="D2a"> <cit type="translation" xml:lang="la"><quote> gravare </quote></cit>, <cit type="translation" xml:lang="la"><quote> onerare </quote></cit> aliquem: <foreign xml:lang="gez">አክበደ፡ ብረኪየ፡</foreign> <ref cRef="Thren." loc="3,7">Thren. 3,7</ref>; (<foreign xml:lang="gez">ሥጋ፡</foreign>) <foreign xml:lang="gez">ያከብዳ፡ ለነፍስ፡</foreign> <ref cRef="Sap." loc="9,15">Sap. 9,15</ref>; <foreign xml:lang="gez">ኢታክብዱ፡ ልበክሙ፡ በስታይ፡</foreign> <ref cRef="Luc." loc="21,34">Luc. 21,34</ref>; <ref cRef="Clem." loc="254.">Clem. f. 254.</ref> 
</sense> <sense n="b" xml:id="D2b"> <cit type="translation" xml:lang="la"><quote> molestiam exhibere </quote></cit>, <cit type="translation" xml:lang="la"><quote> oneri esse </quote></cit> alicui, <foreign xml:lang="gez">እንዘ፡ ንክል፡ አክብዶ፡ ከመ፡ ሐዋርያተ፡ ክርስቶስ፡</foreign> <foreign xml:lang="grc">ἐν βάρει εἶναι</foreign> <ref cRef="1 Thess." loc="2,6">1 Thess. 2,6</ref>; <lbl expand="cum">c.</lbl> <foreign xml:lang="gez">ላዕለ፡</foreign> pers. <ref cRef="1 Thess." loc="2,9">1 Thess. 2,9</ref>; <ref cRef="2 Cor." loc="12,16">2 Cor. 12,16</ref>; <ref cRef="1 Tim." loc="5,16">1 Tim. 5,16</ref>.
</sense> 
</sense> </sense>
               
