                            et sub <foreign xml:lang="gez">ወይ፡</foreign> (<bibl>
                                <ptr target="bm:Dillmann1857Gramm"/>gramm.</bibl> p. 319).
                            Significat <cit type="translation" xml:lang="la">
                                <quote> motum </quote>
                            </cit>vel <cit type="translation" xml:lang="la">
                                <quote> directionem ad </quote>
                            </cit>aliquid: <sense n="1" xml:id="DI1">
                                <cit type="translation" xml:lang="la">
                                    <quote> versus </quote>
                                </cit>, <cit type="translation" xml:lang="la">
                                    <quote> ad </quote>
                                </cit>, <cit type="translation" xml:lang="la">
                                    <quote> in </quote>
                                </cit>, (de loco, re persona) <foreign xml:lang="gez">ምድር፡ እንተ፡ ላቲ፡
                                    ተሐውር፡ ህየ፡</foreign>
                                <ref cRef="Deut." loc="7,1">Deut. 7,1</ref>; <ref cRef="Deut."
                                    loc="11,29">Deut. 11,29</ref>; <foreign xml:lang="gez">ኢተግሕሣ፡
                                    ኢለየማን፡ ወኢለፀጋም፡</foreign>
                                <ref cRef="1 Reg." loc="6,12">1 Reg. 6,12</ref>; <foreign
                                    xml:lang="gez">ይትፈለጥ፡ ለ፬ መኣዝነ፡ ዓለም፡</foreign> 2,10; <foreign
                                    xml:lang="gez">አሐዱ፡ ለካልኡ፡ ይኔጽር፡</foreign>
                                <ref cRef="Hen." loc="41,7">Hen. 41,7</ref>; <foreign xml:lang="gez"
                                    >ለበጺሐ፡ እፍራታ፡</foreign>
                                <ref cRef="Gen." loc="35,16">Gen. 35,16</ref>; <ref cRef="Gen."
                                    loc="48,7">Gen. 48,7</ref>; <foreign xml:lang="gez">ወሰደ፡
                                    ለ</foreign>
