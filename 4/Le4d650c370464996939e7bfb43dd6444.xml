                    <sense xml:lang="la" source="#dillmann" xml:id="Dmain">
                        <sense n="I" xml:id="DI">
                            <cit type="translation" xml:lang="la">
                                <quote> praepositio inseparabilis </quote>
                            </cit>(eadem quae <foreign xml:lang="he">לְ</foreign>, <foreign
                                xml:lang="syr">ܠ</foreign>, <foreign xml:lang="ar">لِ</foreign>,
                                <lbl expand="videas">vid.</lbl>
                            <bibl>
                                <ptr target="bm:Dillmann1857Gramm"/>gramm.</bibl> § 164,2); <lbl
                                expand="cum">c.</lbl>
                            <gramGrp>
                                <pos expand="pronomen suffixum">pron. suff.</pos>
                            </gramGrp>
                            <foreign xml:lang="gez">ሊተ፡</foreign>, <foreign xml:lang="gez"
                                >ለከ፡</foreign>, <foreign xml:lang="gez">ለኪ፡</foreign>, <foreign
                                xml:lang="gez">ሎቱ፡</foreign>, <foreign xml:lang="gez">ላቲ፡</foreign>;
