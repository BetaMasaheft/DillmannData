               <sense xml:lang="la" source="#dillmann" xml:id="Dmain">
                  <gramGrp>
                     <pos expand="Substantivum">subst.</pos>
                     <gen value="masculinum">m.</gen>
                  </gramGrp>, Voc. crebro <foreign xml:lang="gez">እግዚኦ፡</foreign> (ut <ref cRef="Ps. " loc="3,1">Ps. 3,1</ref>; <ref cRef="Ps." loc="8,1">Ps. 8,1</ref>;
