                     <ref cRef="Dan." loc="2,47">Dan. 2,47</ref>; <ref cRef="Ps." loc="135,3">Ps.
                        135,3</ref>; <foreign xml:lang="gez">እግዚአ፡ መናፍስት፡</foreign>
                     <ref cRef="Hen." loc="38,2">Hen. 38,2</ref>
                     <lbl expand="sequens">seq.</lbl>; <foreign xml:lang="gez">እግዚኡ፡ ለኖላዊ፡</foreign>
                     <foreign xml:lang="grc">ἀρχιποιμήν</foreign>
                     <ref cRef="1 Petr." loc="5,4">1 Petr. 5,4</ref>; <foreign xml:lang="gez">እግዚኣ፡
                        ለሰንበት፡</foreign>
                     <ref cRef="Marc." loc="2,28">Marc. 2,28</ref>; sive praepositione <foreign xml:lang="gez">ለ፡</foreign>, ut: <foreign xml:lang="gez">እግዚእ፡ ለኵሉ፡
                        ምድር፡</foreign>
                     <ref cRef="Judith" loc="6,4">Judith 6,4</ref>; <ref cRef="Gen." loc="45,8">Gen.
                        45,8</ref>; <ref cRef="Gen." loc="45,9">Gen. 45,9</ref>; <ref cRef="Ps." loc="104,19">Ps. 104,19</ref>; <ref cRef="Matth." loc="12,8">Matth.
                        12,8</ref>; <ref cRef="Gal. " loc="4,1">Gal. 4,1</ref>.</sense>
                  <sense n="c" xml:id="Dc"> in specie de Deo et Christo, <ref cRef="Ex." loc="6,6">Ex.
                     6,6</ref>; <ref cRef="Ex." loc="6,7">Ex. 6,7</ref>; <ref cRef="Ex." loc="23,17">Ex. 23,17</ref>; (<lbl expand="videas">vid.</lbl>
                     <ref cRef="Ex." loc="3,4">Ex. 3,4</ref>
                     <lbl expand="annotatio">ann.</lbl>); <ref cRef="Ps." loc="109,1">Ps.
                        109,1</ref>; <ref cRef="Luc." loc="7,13">Luc. 7,13</ref>; <ref cRef="Luc." loc="10,1">Luc. 10,1</ref>; <ref cRef="Luc." loc="18,11">Luc. 18,11</ref>;
                        <ref cRef="Luc." loc="18,13">Luc. 18,13</ref>; <ref cRef="Joh." loc="4,1">Joh. 4,1</ref>; <ref cRef="Act." loc="1,21">Act. 1,21</ref> al. creberrime
                     (quamquam de Deo <foreign xml:lang="gez">እግዚአብሔር፡</foreign> magis usitatum
                     est).</sense>
                  <sense n="d" xml:id="Dd"> creberrime honoris causa in compellandis aliis, in specie
                     nobilioribus, ut: <ref cRef="Gen." loc="18,3">Gen. 18,3</ref>; <ref cRef="Gen." loc="23,11">Gen. 23,11</ref>; <ref cRef="Gen." loc="24,18">Gen. 24,18</ref>;
                        <ref cRef="Gen." loc="32,19">Gen. 32,19</ref>; <ref cRef="Joh." loc="12,21">Joh. 12,21</ref>; <ref cRef="Joh." loc="20,15">Joh. 20,15</ref> al. <cb n="1192" xml:id="c1192"/>
                  </sense>
                  <sense n="e" xml:id="De">
                     <foreign xml:lang="gez">አጋእዝት፡</foreign>
                     <foreign xml:lang="grc">κυριότητες </foreign>
                     <cit type="translation" xml:lang="la">
                        <quote>domini</quote>
                     </cit> est nomen unius e novem ordinibus angelorum, <ref cRef="Eph." loc="1,21">Eph. 1,21</ref>; <ref cRef="Col." loc="1,16">Col. 1,16</ref>; <ref cRef="Clem." loc="61">Clem. f. 61</ref>; <ref cRef="Clem." loc="102">Clem.
                        f. 102</ref>; <foreign xml:lang="gez">መላእክተ፡ አጋእዝት፡ </foreign>
                     <ref cRef="Hen." loc="61,10">Hen. 61,10</ref>.</sense>
                  <sense n="f" xml:id="Df">
                     <foreign xml:lang="gez">እግዚአብሔር፡</foreign> vid. infra.</sense>
               </sense>
               
