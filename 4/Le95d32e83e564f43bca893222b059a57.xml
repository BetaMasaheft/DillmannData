               <form>
                  <foreign xml:lang="gez">ነግህ</foreign>
               </form>
               <sense xml:lang="la" source="#dillmann" xml:id="Dmain"> {<ref target="#c1431"/> Huic radici cfr. etiam <foreign xml:lang="gez">ገሃህ፡</foreign>} <gramGrp> <pos expand="not found">subst.</pos> </gramGrp> <cit type="translation" xml:lang="la">
                            <quote> prima lux </quote>
                        </cit>, <cit type="translation" xml:lang="la">
                            <quote> diluculum </quote>
                        </cit>, <cit type="translation" xml:lang="la">
                            <quote> multum mane </quote>
                        </cit>, <cit type="translation" xml:lang="la">
                            <quote> tempus matutinum </quote>
                        </cit>: <foreign xml:lang="gez">ማእዜ፡ ነግሀ፡ ይከውን፡</foreign> (<foreign xml:lang="grc">ὄρθρος</foreign>) <ref cRef="Prov." loc="23,35">Prov. 23,35</ref>; <foreign xml:lang="gez">ነግህ፡ ብሔር፡</foreign> <ref cRef="Act." loc="2,15">Act. 2,15</ref> Platt. <sense n="a" xml:id="Da"> <foreign xml:lang="gez">እምነግህ፡</foreign> <cit type="translation" xml:lang="la">
                                <quote> a mane </quote>
                            </cit>: <foreign xml:lang="gez">እምነግህ፡ እስከ፡ ሰርክ፡</foreign> <ref cRef="Ex." loc="18,13">Ex. 18,13</ref>; <ref cRef="Ruth" loc="2,7">Ruth 2,7</ref>; <ref cRef="Sir." loc="18,26">Sir. 18,26</ref>; <ref cRef="Jes." loc="38,13">Jes. 38,13</ref>.</sense> <sense n="b" xml:id="Db"> <foreign xml:lang="gez">ለነግህ፡</foreign> <cit type="translation" xml:lang="la">
                                <quote> ad mane </quote>
                            </cit> <ref cRef="Ex." loc="12,10">Ex. 12,10</ref>.</sense> <sense n="c" xml:id="Dc"> <foreign xml:lang="gez">በነግህ፡</foreign> <cit type="translation" xml:lang="la">
                                <quote> mane </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> bene mane </quote>
                            </cit>: <ref cRef="Matth." loc="20,1">Matth. 20,1</ref>; <ref cRef="Marc." loc="11,20">Marc. 11,20</ref>; <ref cRef="Koh." loc="10,16">Koh. 10,16</ref>; <ref cRef="Koh." loc="11,6">Koh. 11,6</ref>; <ref cRef="Kuf." loc="31.">Kuf. 31.</ref> </sense> <sense n="d" xml:id="Dd"> <foreign xml:lang="gez">ነግሀ፡</foreign> <cit type="translation" xml:lang="la">
                                <quote> mane </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> bene mane </quote>
                            </cit>: <ref cRef="Ex." loc="16,7">Ex. 16,7</ref>; <ref cRef="Ps. " loc="54,19">Ps.  54,19</ref>; Kuf. crebro; <foreign xml:lang="gez">ነግሀ፡ በጽባሕ፡</foreign> <ref cRef="Kuf." loc="42.">Kuf. 42.</ref> </sense> <sense n="e" xml:id="De"> distributive: <cit type="translation" xml:lang="la">
                                <quote> singulis matutinis </quote>
                            </cit> (<cit type="translation" xml:lang="la">
                                <quote> quovis die </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> in dies </quote>
                            </cit>): <foreign xml:lang="gez">በበ፡ ነግህ፡</foreign> <ref cRef="Ex." loc="16,21">Ex. 16,21</ref>; <ref cRef="2 Par." loc="13,11">2 Par. 13,11</ref>, vel <foreign xml:lang="gez">ነግሀ፡ ነግህ፡</foreign> <ref cRef="Ex." loc="36,3">Ex. 36,3</ref>; <ref cRef="2 Reg." loc="13,4">2 Reg. 13,4</ref>; <foreign xml:lang="gez">ይኩነኒ፡ ለነግሀ፡ ነግህ፡</foreign> (v. <foreign xml:lang="gez">ለነግህ፡ ነግሀ፡</foreign>) <ref cRef="4 Reg." loc="16,15">4 Reg. 16,15</ref>.</sense> </sense>
               
