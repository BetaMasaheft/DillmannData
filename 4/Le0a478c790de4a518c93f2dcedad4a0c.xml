               <sense xml:lang="la" source="#dillmann" xml:id="Dmain"> <subc>I,1</subc> [<foreign xml:lang="ar">جَنَأَ</foreign> <cit type="translation" xml:lang="la">
                            <quote> pronum se inclinavit </quote>
                        </cit>, <foreign xml:lang="syr">ܓܢܳܐ</foreign> <cit type="translation" xml:lang="la">
                            <quote> decubuit </quote>
                        </cit>, <lbl expand="vicinus">vic.</lbl> <foreign xml:lang="ar">قنع</foreign>, <foreign xml:lang="he">כנע</foreign>, <foreign xml:lang="he">צנע</foreign>] <gramGrp> <pos expand="Subjunctivus">Subj.</pos> </gramGrp> <foreign xml:lang="gez">ይግነይ፡</foreign> <sense n="1" xml:id="D1"> <cit type="translation" xml:lang="la">
                                <quote> pronum se inclinare </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> procumbere </quote>
                            </cit>: <foreign xml:lang="gez">ግነዩ፡ ወስግዱ፡</foreign> <foreign xml:lang="grc">πίπτοντες προσκυνεῖτε</foreign> <ref cRef="Dan. " loc="3,5">Dan.  3,5</ref>.</sense> <sense n="2" xml:id="D2"> <cit type="translation" xml:lang="la">
                                <quote> se submittere </quote>
                            </cit> vel <cit type="translation" xml:lang="la">
                                <quote> subjicere </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> obsequium praestare </quote>
                            </cit>, (<cit type="translation" xml:lang="la">
                                <quote> servire </quote>
                            </cit>) alicui, <lbl expand="cum">c.</lbl> <foreign xml:lang="gez">ለ፡</foreign> pers.: <foreign xml:lang="grc">ὑποταγῆναι</foreign> <ref cRef="Ps." loc="61,5">Ps. 61,5</ref>; <ref cRef="Hebr." loc="12,9">Hebr. 12,9</ref>; <ref cRef="1 Cor." loc="15,28">1 Cor. 15,28</ref>; <ref cRef="Luc. " loc="10,20">Luc.  10,20</ref>; <cb n="1179" xml:id="c1179"/> <foreign xml:lang="gez">ለጽድቀ፡ እግዚአብሔር፡ ስእኑ፡ ገኒየ፡</foreign> <ref cRef="Rom. " loc="10,3">Rom.  10,3</ref>; <foreign xml:lang="gez">ለከ፡ ይገንዩ፡ ኵሉ፡ ተግባርከ፡</foreign> <foreign xml:lang="grc">δουλευσάτω</foreign> <ref cRef="Judith" loc="16,14">Judith 16,14</ref>; <foreign xml:lang="gez">እፎ፡ ሠመርክሙ፡ ከመ፡ ትግነዩ፡ ለአቢባል፡</foreign> (Hannibali) <ref cRef="Jsp." loc="295">Jsp. p. 295</ref>; <ref cRef="Jsp." loc="350">Jsp. p. 350</ref>; <ref cRef="Jsp." loc="381">Jsp. p. 381</ref>; ruri vixit <foreign xml:lang="gez">እንዘ፡ ለሊቁ፡ ይገኒ፡</foreign> <ref cRef="Sx. Tachs." loc="10 ">Sx. Tachs. 10 </ref>Enc.; Org.</sense> <sense n="3" xml:id="D3"> <cit type="translation" xml:lang="la">
                                <quote> demittere animum </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> submisse se gerere </quote>
                            </cit>, <ref cRef="Hen." loc="5,8">Hen. 5,8</ref>. <sense n="a" xml:id="D3a"> <cit type="translation" xml:lang="la">
                                    <quote> demisso animo pati </quote>
                                </cit>: <foreign xml:lang="gez">ግነዩ፡</foreign> <foreign xml:lang="grc">ταλαιπωρῆσατε</foreign> <ref cRef="Jac." loc="4,9">Jac. 4,9</ref>.</sense> <sense n="b" xml:id="D3b"> <cit type="translation" xml:lang="la">
                                    <quote> demisso animo accipere </quote>
                                </cit> vel <cit type="translation" xml:lang="la">
                                    <quote> admittere </quote>
                                </cit> aliquid, <lbl expand="cum">c.</lbl> <foreign xml:lang="gez">ለ፡</foreign>, ut: <foreign xml:lang="gez">ዘይፈርሆ፡ ለእግዚአብሔር፡ ይገኒ፡ ለተግሣጹ፡</foreign> <foreign xml:lang="grc">ἐκδέξεται</foreign> <ref cRef="Sir. " loc="35,14">Sir.  35,14</ref>.</sense> <sense n="c" xml:id="D3c"> <cit type="translation" xml:lang="la">
                                    <quote> demisso animo confidere </quote>
                                </cit>, <lbl expand="cum">c.</lbl> <foreign xml:lang="gez">በ፡</foreign>, ut: <foreign xml:lang="gez">ብእሲ፡ ጠቢብ፡ ይትአመን፡ ወይገኒ፡ በሕጉ፡ ለእግዚአብሔር፡</foreign> <foreign xml:lang="grc">ἐμπιστεύσει</foreign> <ref cRef="Sir." loc="36,3">Sir. 36,3</ref>.</sense> <sense n="d" xml:id="D3d"> <cit type="translation" xml:lang="la">
                                    <quote> humiliter adorare </quote>
                                </cit>, <lbl expand="cum">c.</lbl> <foreign xml:lang="gez">ለ፡</foreign>, <foreign xml:lang="grc">προσκυνεῖν</foreign> <ref cRef="Sir." loc="50,17">Sir. 50,17</ref>.</sense> <sense n="e" xml:id="D3e"> <cit type="translation" xml:lang="la">
                                    <quote> humiliter rogare </quote>
                                </cit>, <cit type="translation" xml:lang="la">
                                    <quote> supplicare </quote>
                                </cit>: <foreign xml:lang="gez">ውእቱ፡ ይገኒ፡</foreign> <foreign xml:lang="grc">προσδεηθήσεται</foreign> <ref cRef="Sir." loc="13,3">Sir. 13,3</ref>; <lbl expand="cum">c.</lbl> <foreign xml:lang="gez">ለ፡</foreign> pers.: <foreign xml:lang="gez">ገነይኩ፡ ለከ፡</foreign> <foreign xml:lang="grc">δέομαι</foreign> <ref cRef="Jos." loc="7,7">Jos. 7,7</ref>; <foreign xml:lang="gez">ይገንዩ፡ ለከ፡</foreign> <ref cRef="Job" loc="11,19">Job 11,19</ref>.</sense> <sense n="f" xml:id="D3f"> <cit type="translation" xml:lang="la">
                                    <quote> humiles gratias agere </quote>
                                </cit>, <cit type="translation" xml:lang="la">
                                    <quote> humili et grato animo celebrare </quote>
                                </cit> vel <cit type="translation" xml:lang="la">
                                    <quote> laudare </quote>
                                </cit> Deum: <foreign xml:lang="grc">ἐξομολογεῖσθαι</foreign>, <ref cRef="Dan. " loc="9,4">Dan.  9,4</ref>; <lbl expand="cum">c.</lbl> <foreign xml:lang="gez">ለ፡</foreign> <ref cRef="Kuf." loc="16">Kuf. 16</ref>; <foreign xml:lang="gez">እግነይ፡ ለከ፡ አምላኪየ፡</foreign> <ref cRef="Kuf." loc="22">Kuf. 22</ref>; <ref cRef="Sir." loc="39,6">Sir. 39,6</ref>; <ref cRef="Sir." loc="51,1">Sir. 51,1</ref>; <ref cRef="Tob." loc="11,16">Tob. 11,16</ref>; <foreign xml:lang="gez">ሎቱ፡ ግነዩ፡</foreign> <ref cRef="Tob." loc="12,6">Tob. 12,6</ref>; <foreign xml:lang="gez">ኢትትሀከዩ፡ ገኒየ፡ ሎቱ፡</foreign> <ref cRef="Tob." loc="12,6">Tob. 12,6</ref>; <ref cRef="Tob." loc="13,3">Tob. 13,3</ref>; <ref cRef="Dan." loc="2,23">Dan. 2,23</ref>; <ref cRef="Ps." loc="7,18">Ps. 7,18</ref>; <ref cRef="Ps." loc="9,1">Ps. 9,1</ref>; <ref cRef="Ps." loc="74,1">Ps. 74,1</ref>; <foreign xml:lang="gez">ኵሉ፡ ልሳን፡ ይገኒ፡ ለእግዚአብሔር፡</foreign> <foreign xml:lang="grc">ὀμεῖται τὸν θεὸν</foreign> <ref cRef="Jes." loc="45,23">Jes. 45,23</ref>; <ref cRef="Rom." loc="14,11">Rom. 14,11</ref>; <ref cRef="Luc." loc="2,38">Luc. 2,38</ref>.</sense> <sense n="g" xml:id="D3g"> <cit type="translation" xml:lang="la">
                                    <quote> humili animo confiteri </quote>
                                </cit>, <lbl expand="sequens">seq.</lbl> <foreign xml:lang="gez">ከመ፡</foreign> <ref cRef="Phil. " loc="2,11">Phil.  2,11</ref>; <lbl expand="sequens">seq.</lbl> <foreign xml:lang="gez">ለ፡</foreign> peccati, <ref cRef="Lev." loc="26,41">Lev. 26,41</ref>. – <lbl expand="Vocabularium Aethiopicum, -a -a">Voc. Ae.</lbl>: <foreign xml:lang="gez">ገረረ፡ ገነየ፡ ተኰነነ፡ ተቀንየ፡ ብ፡ ተገዛ፡ ገነየሰ፡ አመነሂ፡</foreign>; <cit type="translation" xml:lang="la">
                                    <quote> aliud </quote>
                                </cit>: <foreign xml:lang="gez">ገነየ፡ በ፪ ለመነ፡ ተለማመጠ።</foreign> </sense> </sense> </sense>
               
