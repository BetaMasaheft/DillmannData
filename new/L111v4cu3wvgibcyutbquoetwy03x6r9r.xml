            <entry xml:id="L111v4cu3wvgibcyutbquoetwy03x6r9r" n="13062">
               <form><foreign xml:lang="gez">ሐራሚ</foreign></form>
               <sense xml:id="Tmain" xml:lang="en" source="#traces"><cit type="transcription" xml:lang="gez">
                     <t:quote xmlns:t="http://www.tei-c.org/ns/1.0">ḥarāmi</t:quote>
                  </cit>
                  <sense n="G" xml:id="TG">
                     <foreign xml:lang="gez">ሐራሚ፡</foreign>
                     <cit type="transcription" xml:lang="gez">
                        <t:quote xmlns:t="http://www.tei-c.org/ns/1.0">ḥarāmi</t:quote>
                     </cit> «<cit type="translation" xml:lang="fr">
                        <t:quote xmlns:t="http://www.tei-c.org/ns/1.0">abstinent</t:quote>
                     </cit>, <cit type="translation" xml:lang="fr">
                        <t:quote xmlns:t="http://www.tei-c.org/ns/1.0">pénitent</t:quote>
                     </cit>» <lb/>
                     <foreign xml:lang="gez">ሰላም፡ ለክሙ፡ ፻ወ፵ቅዱሳን።</foreign>
                     <lb/>
                     <foreign xml:lang="gez">ማኅበራነ፡ ሰማዕታት፡ ፯ቱ፡ ኃራምያን።</foreign>
                     <lb/>
                     <foreign xml:lang="gez">ዘአስተዳለዉ፡ ለክሙ፡ ቅድመ፡ መኰንን።</foreign>
                     <lb/>
                     <foreign xml:lang="gez">ኢያፍርሃክሙ፡ እንዘ፡ ትኔጽሩ፡ በዓይን።</foreign>
                     <bibl><ptr target="bm:Guidi1906SynaxSane"/><citedRange unit="page">
                           175</citedRange></bibl>
                     <lb/>
                     <bibl><ptr target="bm:Grebaut1952SuppLexicon"/><citedRange unit="page">
                           46</citedRange></bibl>
                  </sense>
                  <sense n="L" xml:id="TL">
                     <foreign xml:lang="gez">ሐራሚ</foreign>
                     <cit type="transcription" xml:lang="gez">
                        <t:quote xmlns:t="http://www.tei-c.org/ns/1.0">ḥarāmi</t:quote>
                     </cit> (<bibl><ptr target="bm:KidaneWaldGeezGramDict"/></bibl>, <bibl><ptr target="bm:Grebaut1952SuppLexicon"/><citedRange unit="page">
                           46</citedRange></bibl>) act. part. of <lbl expand="videas">vid.</lbl>
                     <foreign xml:lang="gez">ሐረመ፡</foreign>
                     <cit type="transcription" xml:lang="gez">
                        <t:quote xmlns:t="http://www.tei-c.org/ns/1.0">ḥarama</t:quote>
                     </cit>; also <cit type="translation" xml:lang="en">
                        <t:quote xmlns:t="http://www.tei-c.org/ns/1.0">abstinent</t:quote>
                     </cit>, <cit type="translation" xml:lang="en">
                        <t:quote xmlns:t="http://www.tei-c.org/ns/1.0">penitent</t:quote>
                     </cit>
                     <bibl><ptr target="bm:Leslau1987dic"/><citedRange unit="page">
                           242b</citedRange></bibl>
                  </sense>
                  <nd/>
               </sense>
