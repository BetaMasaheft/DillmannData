                  <sense n="G" xml:id="TG">
                     <foreign xml:lang="gez">አስተምረረ፡</foreign>
                     <cit type="transcription" xml:lang="gez">
                        <t:quote xmlns:t="http://www.tei-c.org/ns/1.0">ʾastamrara</t:quote>
                     </cit>
                     <subc>IV,1</subc>
                     <cit type="translation" xml:lang="fr">
