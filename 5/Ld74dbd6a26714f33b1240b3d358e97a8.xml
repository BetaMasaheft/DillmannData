               <sense xml:lang="la" source="#dillmann" xml:id="Dmain"> vel <foreign xml:lang="gez">ስብሓት፡</foreign> <gramGrp> <pos expand="not found">subst.</pos> </gramGrp>, <gramGrp> <gen value="not found">m.</gen> </gramGrp> et <gramGrp> <gen value="femininus">fem.</gen> </gramGrp>, <lbl expand="pluralis">Pl.</lbl> <foreign xml:lang="gez">ስብሐታት፡</foreign> <sense n="1" xml:id="D1"> <cit type="translation" xml:lang="la">
                                <quote> laudatio </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> cantatio laudis </quote>
                            </cit>, <foreign xml:lang="grc">αἴνεσις</foreign>: <foreign xml:lang="gez">አዕበይዎ፡</foreign> (<foreign xml:lang="gez">ለእግዚአብሔር፡</foreign>) <foreign xml:lang="gez">በኵሉ፡ በረከት፡ ወስብሐት፡</foreign> <ref cRef="2 Esr. " loc="19,5">2 Esr.  19,5</ref>; <ref cRef="Jes." loc="51,11">Jes. 51,11</ref>; <foreign xml:lang="gez">ቃለ፡ ስብሐት፡</foreign> <ref cRef="Jes." loc="51,3">Jes. 51,3</ref>; <foreign xml:lang="gez">መሥዋዕተ፡ ስብሐት፡</foreign> <ref cRef="2 Par." loc="29,31">2 Par. 29,31</ref>; <ref cRef="Sir." loc="32,4">Sir. 32,4</ref>; <foreign xml:lang="gez">ስብሐት፡ ወዘምሮ፡</foreign> <ref cRef="Clem." loc="12">Clem. f. 12</ref>; <foreign xml:lang="gez">ኢያረሙ፡ እምስብሐቲነ፡</foreign> (ait Deus Trinus) <ref cRef="Clem." loc="49">Clem. f. 49</ref>; <foreign xml:lang="gez">ጸሎተ፡ ዕንባቆም፡ ምስለ፡ ስብሐተ፡ እግዚአብሔር፡</foreign> <foreign xml:lang="grc">μετ᾽ ᾠδῆς</foreign> <ref cRef="Hab." loc="3 ">Hab. 3 </ref>in tit.; <foreign xml:lang="gez">በማሕሌት፡ በስብሐተ፡ መዝሙር፡</foreign> 2 <ref cRef="Par." loc="7,6">Par. 7,6</ref>; <foreign xml:lang="gez">መላእክተ፡ ስብሐታት፡</foreign> <ref cRef="Asc. Jes." loc="6,13">Asc. Jes. 6,13</ref>; <foreign xml:lang="gez">ስብሐተ፡ ነግህ፡</foreign> vel <foreign xml:lang="gez">ጽባሕ፡</foreign> appellantur preces matutinae in ecclesia celebratae <ref cRef="Clem." loc="175">Clem. f. 175</ref>; <ref cRef="Kid. " loc="11.">Kid.  f. 11.</ref> </sense> <sense n="2" xml:id="D2"> <cit type="translation" xml:lang="la">
                                <quote> laus </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> laudes </quote>
                            </cit>: <foreign xml:lang="gez">ሀቡ፡ ስብሐተ፡ ለእግዚአብሔር፡</foreign> <ref cRef="Jer." loc="13,16">Jer. 13,16</ref>; <ref cRef="Asc. Jes." loc="6,9">Asc. Jes. 6,9</ref>; <ref cRef="Ps." loc="28,2">Ps. 28,2</ref>; <ref cRef="Ps." loc="95,7">Ps. 95,7</ref>; <ref cRef="Joh." loc="9,24">Joh. 9,24</ref>; <ref cRef="Luc." loc="2,14">Luc. 2,14</ref> formula doxologica: <foreign xml:lang="gez">ሎቱ፡ ስብሐት፡</foreign> <ref cRef="Rom." loc="11,36">Rom. 11,36</ref>; <ref cRef="Judae " loc="25">Judae  25</ref>; <ref cRef="Sx." loc="">Sx. </ref>; <ref cRef="Abush." loc="">Abush. </ref>, al.; et in malam <cb n="0358" xml:id="c0358"/> partem: <cit type="translation" xml:lang="la">
                                <quote> gloriae cupiditas </quote>
                            </cit>, ut: <foreign xml:lang="gez">ሰይጣነ፡ ስብሓት፡</foreign> <ref cRef="Phlx." loc="56.">Phlx. 56.</ref> </sense> <sense n="3" xml:id="D3"> <cit type="translation" xml:lang="la">
                                <quote> hymnus </quote>
