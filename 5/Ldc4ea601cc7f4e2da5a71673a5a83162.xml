               <sense xml:id="Dmain" xml:lang="la" source="#dillmann"> vel <foreign xml:lang="gez">ሰሌዳ፡</foreign> <gramGrp> <pos expand="not found">n. peregr.</pos> </gramGrp>, <lbl expand="scilicet">sc.</lbl> <foreign xml:lang="grc">σελίς</foreign>, <gramGrp> <case value="accusativus">Acc.</case> </gramGrp> <foreign xml:lang="grc">σελίδα</foreign>, <gramGrp> <gen value="not found">m.</gen> </gramGrp> et <gramGrp> <gen value="femininus">fem.</gen> </gramGrp>, <lbl expand="pluralis">Pl.</lbl> <foreign xml:lang="gez">ሰሊዳት፡</foreign> vel <foreign xml:lang="gez">ሰሌዳት፡</foreign> et <foreign xml:lang="gez">ሰላድው፡</foreign> (<ref cRef="Act." loc="27,44">Act. 27,44</ref> Platt) <sense n="1" xml:id="D1"> <cit type="translation" xml:lang="la"><quote> tabula scriptoria </quote></cit> vel <cit type="translation" xml:lang="la"><quote> pictoria </quote></cit>, <cit type="translation" xml:lang="la"><quote> tabella </quote></cit>; nec non earum <cit type="translation" xml:lang="la"><quote> pagina </quote></cit> vel <cit type="translation" xml:lang="la"><quote> columna </quote></cit>: <foreign xml:lang="grc">πυξίον</foreign> <ref cRef="Ex. " loc="24,12">Ex.  24,12</ref>; <ref cRef="Jes." loc="30,8">Jes. 30,8</ref>; <ref cRef="Hab." loc="2,2">Hab. 2,2</ref>; <ref cRef="Cant." loc="5,14">Cant. 5,14</ref>; <foreign xml:lang="grc">σελίδες</foreign> <foreign xml:lang="gez">ሶበ፡ አንበበ፡ ሠለስተ፡ ሰሌዳ፡</foreign> (v. <foreign xml:lang="gez">ሰሌዳተ፡</foreign>) <foreign xml:lang="gez">ወአርባዕተ፡</foreign> <ref cRef="Jer. " loc="43,23">Jer.  43,23</ref>; <foreign xml:lang="gez">ሰሌዳ፡ እብን፡</foreign> <ref cRef="Jer." loc="17,1">Jer. 17,1</ref> <lbl expand="versio nova">vers. nov.</lbl>; <ref cRef="4 Esr. " loc="14,26">4 Esr.  14,26</ref>; <ref cRef="Kuf." loc="32">Kuf. 32</ref>; <foreign xml:lang="gez">ሰሌዳተ፡ እብን፡ ዘሕግ፡</foreign> Org.; <foreign xml:lang="gez">ሰሌዳ፡ ሥዕል፡</foreign> <ref cRef="Sx. Masc." loc="20.">Sx. Masc. 20.</ref> 
</sense> <sense n="2" xml:id="D2"> <cit type="translation" xml:lang="la"><quote> lamina </quote></cit>, <cit type="translation" xml:lang="la"><quote> bractea </quote></cit>, <cit type="translation" xml:lang="la"><quote> tabula </quote></cit>, <cit type="translation" xml:lang="la"><quote> assis </quote></cit>, <cit type="translation" xml:lang="la"><quote> tessera strati </quote></cit>: <foreign xml:lang="grc">λεπίδες</foreign> <ref cRef="Num." loc="17,3">Num. 17,3</ref>; <foreign xml:lang="grc">σανίς</foreign> <ref cRef="4 Reg." loc="12,9">4 Reg. 12,9</ref>; <ref cRef="3 Reg." loc="6,33">3 Reg. 6,33</ref>; <ref cRef="Hez." loc="27,5">Hez. 27,5</ref>; <ref cRef="Act." loc="27,44">Act. 27,44</ref>; <foreign xml:lang="gez">ፍሉገ፡ ይኩን፡ ሰሊዳሁ፡</foreign> <foreign xml:lang="grc">κοῖλον σανιδωτόν</foreign> <ref cRef="Ex." loc="27,8">Ex. 27,8</ref>; <foreign xml:lang="gez">ሰሌዳ፡ መጽሔት፡</foreign> <ref cRef="Chr. L. Atq." loc="30">Chr. L. Atq. f. 30</ref>; <foreign xml:lang="gez">ሰሌዳተ፡ ኀጺን፡</foreign> <ref cRef="Sx. Haml." loc="5">Sx. Haml. 5</ref>; <foreign xml:lang="gez">ሰላድወ፡ ኀጺን፡</foreign> <ref cRef="Sx. Nah." loc="10">Sx. Nah. 10</ref>; <foreign xml:lang="gez">ጸፍጸፈ፡ ሰሌዳ፡</foreign> <ref cRef="Hen." loc="14,10">Hen. 14,10</ref>; <foreign xml:lang="gez">ሰሌዳ፡ በረድ፡ ጸፍጸፈ፡ ዐውዱ፡</foreign> Lit. Epiph.; <foreign xml:lang="gez">ቀብዖ፡ በሰሌዳ፡</foreign> (ubi Graecus <foreign xml:lang="grc">κεχρισμένα ἐν μίλτῳ</foreign> habet) <ref cRef="Jer." loc="22,14">Jer. 22,14</ref>. – <lbl expand="Vocabularium Aethiopicum, -a -a">Voc. Ae.</lbl>: <foreign xml:lang="gez">ሰሌዳ፡ ዘ፡ ልዝብ፡ ፀርብ፡ ልውሕሂ፡ ይ፡</foreign> 
</sense> </sense>
               
