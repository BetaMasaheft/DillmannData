                  <ref cRef="Ps." loc="50,1">Ps. 50,1</ref>; <ref cRef="Num." loc="14,19">Num.
                     14,19</ref>; <cb n="0987" xml:id="c0987"/>
                  <foreign xml:lang="gez">ዕበየ፡ ኀይሉ፡ </foreign>
                  <ref cRef="Eph." loc="1,19">Eph. 1,19</ref>; <ref cRef="Ps." loc="78,12">Ps.
                     78,12</ref>; <foreign xml:lang="gez">ዕበየ፡ ስብሐት፡</foreign>
                  <ref cRef="Ps." loc="8,2">Ps. 8,2</ref>; <ref cRef="Ps." loc="95,6">Ps.
                  95,6</ref>; <ref cRef="Ps." loc="103,2">Ps. 103,2</ref>; <ref cRef="Sir." loc="17,13">Sir. 17,13</ref>. <sense n="a" xml:id="Da">
                     <cit type="translation" xml:lang="la">
                        <quote>pars major </quote>
                     </cit>vel <cit type="translation" xml:lang="la">
                        <quote>potior</quote>
                     </cit>
                     <ref cRef="Kuf." loc="131 ">Kuf. p. 131 </ref>lin. 5 et 7.</sense>
                  <sense n="b" xml:id="Db"> magnitudo dignitatis, honoris, potentiae, <cit type="translation" xml:lang="la">
                        <quote>amplitudo</quote>
                     </cit>: <foreign xml:lang="gez">በአምጣነ፡ ዕበይከ፡</foreign>
                     <ref cRef="Sir." loc="3,18">Sir. 3,18</ref>; <ref cRef="Hebr." loc="7,4">Hebr.
                        7,4</ref>.</sense>
                  <sense n="c" xml:id="Dc">
                     <cit type="translation" xml:lang="la">
                        <quote>magnificentia</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>majestas</quote>
                     </cit>, Dei, <foreign xml:lang="gez">በከመ፡ ዕበይሁ፡ </foreign> (v. <foreign xml:lang="gez">ዕበዪሁ፡</foreign>) <foreign xml:lang="gez">ከማሁ፡ ዕበየ፡
                        ምሕረቱ፡</foreign>
                     <ref cRef="Sir." loc="2,18">Sir. 2,18</ref>; <ref cRef="Sir." loc="43,29">Sir.
                        43,29</ref>; <ref cRef="Hen." loc="84,2">Hen. 84,2</ref>; <ref cRef="Tob." loc="13,6">Tob. 13,6</ref>; <ref cRef="Tob." loc="13,7">Tob. 13,7</ref>;
                        <ref cRef="Ps. " loc="144,3">Ps. 144,3</ref>; <ref cRef="Judae" loc="25">Judae 25</ref>; <ref cRef="Hebr." loc="1,3">Hebr. 1,3</ref>; <ref cRef="Dianae Act." loc="19,27">Dianae Act. 19,27</ref>; regis <ref cRef="Dan." loc="5,18">Dan. 5,18</ref>; <foreign xml:lang="gez">በዕበይ፡
                        ወበክብር፡</foreign>
                     <ref cRef="Sx. Haml." loc="4 ">Sx. Haml. 4 </ref>Enc.</sense>
                  <sense n="d" xml:id="Dd">
                     <cit type="translation" xml:lang="la">
                        <quote>magnalia</quote>
                     </cit>, <foreign xml:lang="gez">ዕበያቲሁ፡ ለእግዚአብሔር፡</foreign>
                     <ref cRef="Herm." loc="94">Herm. p. 94</ref>; <ref cRef="Herm." loc="107">Herm.
                        p. 107</ref>; <foreign xml:lang="gez">ዕበየ፡ ግብሩ፡</foreign>
                     <foreign xml:lang="grc">τὰ μεγαλεῖα τῶν ἔργων αὐτοῦ</foreign>
                     <ref cRef="Sir." loc="17,9">Sir. 17,9</ref>; <foreign xml:lang="gez">ገባሬ፡ዕበይ፡ </foreign>
                     <ref cRef="Sx. Tachs." loc="29 ">Sx. Tachs. 29 </ref>Enc.</sense>
                  <sense n="e" xml:id="De">
                     <cit type="translation" xml:lang="la">
                        <quote>elatio</quote>
                     </cit> et <cit type="translation" xml:lang="la">
                        <quote>causa elationis </quote>
                     </cit>vel <cit type="translation" xml:lang="la">
                        <quote>gloriationis</quote>
                     </cit>: <foreign xml:lang="gez">ወይከውን፡ ፍሥሓ፡ ወክብረ፡ ወዕበየ፡ ለኵሉ፡ አሕዛብ፡</foreign>
                     <ref cRef="Jer." loc="40,9">Jer. 40,9</ref>; <foreign xml:lang="gez">ለዕበይ፡
                        ወለክብር፡</foreign>
                     <ref cRef="Kuf." loc="4">Kuf. 4</ref>; <foreign xml:lang="gez">አንቲ፡ ይእቲ፡ ዕበዮሙ፡
                        ለእስራኤል፡</foreign>
                     <ref cRef="Judith" loc="15,9">Judith 15,9</ref>.</sense>
               </sense>
               
