            <entry xml:id="Lca2a94619c4c456d8f09e4513d3b8294" n="2265">
               <form>
                  <foreign xml:lang="gez">ተራከበ</foreign>
               </form>
               <sense xml:id="Dmain" xml:lang="la" source="#dillmann"> <subc>III,3</subc> se invicem assequi vel invenire i.e. <cit type="translation" xml:lang="la"><quote> sibi invicem occurrere </quote></cit> vel <cit type="translation" xml:lang="la"><quote> obviam venire </quote></cit>; <cit type="translation" xml:lang="la"><quote> convenire </quote></cit>, <cit type="translation" xml:lang="la"><quote> concurrere </quote></cit>, <cit type="translation" xml:lang="la"><quote> congredi </quote></cit>, de personis: <foreign xml:lang="gez">ይትራከቡ፡ በህየ፡ አጋንንት፡ ወጸደናት፡</foreign> <foreign xml:lang="grc">συναντήσουσι</foreign> <ref cRef="Jes." loc="34,14">Jes. 34,14</ref>; <ref cRef="Jes." loc="34,15">Jes. 34,15</ref>; <foreign xml:lang="gez">ባዕል፡ ወነዳይ፡ ይትራከቡ፡ ኅቡረ፡</foreign> <ref cRef="Prov." loc="22,2">Prov. 22,2</ref>; <ref cRef="Dan. apocr. " loc="1,14">Dan. apocr.  1,14</ref>; <foreign xml:lang="gez">ተራኪቦሙ፡</foreign> <foreign xml:lang="grc">ἀλλήλοις συνελθόντων</foreign> <ref cRef="Prov." loc="29,13">Prov. 29,13</ref>; de rebus <ref cRef="Ps." loc="84,11">Ps. 84,11</ref>; <ref cRef="Am." loc="9,13">Am. 9,13</ref>; <lbl expand="cum">c.</lbl> <foreign xml:lang="gez">ምስለ፡</foreign>, ut: <foreign xml:lang="gez">ተራከበ፡ ምስሌሁ፡</foreign> Sx. <ref cRef="Jac." loc="5">Jac. 5</ref>; <ref cRef="Ex." loc="19,17">Ex. 19,17</ref>; <cb n="0304" xml:id="c0304"/> <ref cRef="Lev." loc="26,5">Lev. 26,5</ref> (etiam cum <foreign xml:lang="gez">ለ፡</foreign>, ibidem); <foreign xml:lang="gez">ይትራከብ፡ ዘርእ፡ ምስለ፡ ሠርጽ፡</foreign> <ref cRef="Am." loc="9,13">Am. 9,13</ref>; <foreign xml:lang="gez">ወይትራከብ፡ ቃላቲሆሙ፡ ምስለ፡ ጽራኀ፡ ብካየ፡ ጸላዕት፡</foreign> <ref cRef="Sap." loc="18,10">Sap. 18,10</ref>; nec non <lbl expand="cum">c.</lbl> <gramGrp> <case value="accusativus">Acc.</case> </gramGrp> <ref cRef="Gen." loc="49,6">Gen. 49,6</ref>; <ref cRef="Ex." loc="4,27">Ex. 4,27</ref>; <ref cRef="Ex." loc="5,20">Ex. 5,20</ref>; <ref cRef="1 Reg." loc="25,20">1 Reg. 25,20</ref>; <ref cRef="2 Reg." loc="18,9">2 Reg. 18,9</ref>; <ref cRef="Ps." loc="76,4">Ps. 76,4</ref>; <ref cRef="Matth." loc="28,9">Matth. 28,9</ref>; <ref cRef="Hebr." loc="7,1">Hebr. 7,1</ref>. <sense n="a" xml:id="Da"> in re bellica: <cit type="translation" xml:lang="la"><quote> congredi </quote></cit>, <cit type="translation" xml:lang="la"><quote> concurrere </quote></cit>, <cit type="translation" xml:lang="la"><quote> signa conferre </quote></cit>: <foreign xml:lang="gez">ተራከቡ፡ ለቀትል፡</foreign> <ref cRef="1 Par." loc="19,9">1 Par. 19,9</ref>; <foreign xml:lang="gez">ወተራከቦ፡ ድምትርያስ፡ ወፀብኦ፡</foreign> <ref cRef="Jsp." loc="304">Jsp. p. 304</ref>; <ref cRef="Jsp." loc="325">Jsp. p. 325</ref>; <ref cRef="Jsp." loc="356">Jsp. p. 356</ref>; <lbl expand="cum">c.</lbl> <foreign xml:lang="gez">ምስለ፡</foreign> <ref cRef="Jsp." loc="371.">Jsp. p. 371.</ref> 
</sense> <sense n="b" xml:id="Db"> <cit type="translation" xml:lang="la"><quote> coire </quote></cit>, <cit type="translation" xml:lang="la"><quote> concumbere </quote></cit> <lbl expand="cum">c.</lbl> muliere: <foreign xml:lang="gez">በጊዜ፡ ተራክቦቱ፡ ምስለ፡ ብእሲቱ፡</foreign> F.M. 11,2; <ref cRef="Lit." loc="171,1">Lit. 171,1</ref>; <foreign xml:lang="gez">ተራክቦ፡</foreign> <cit type="translation" xml:lang="la"><quote> concubitus </quote></cit> F.N. 24. 
</sense> <sense n="c" xml:id="Dc"> de eventis et casibus: <cit type="translation" xml:lang="la"><quote> accidere </quote></cit>, <cit type="translation" xml:lang="la"><quote> obvenire </quote></cit>, <cit type="translation" xml:lang="la"><quote> contingere </quote></cit> alicui, <cit type="translation" xml:lang="la"><quote> assequi </quote></cit> aliquem: <foreign xml:lang="gez">ይትራከቦሙ፡ ጽልመት፡</foreign> <ref cRef="Job " loc="5,14">Job  5,14</ref>; <foreign xml:lang="gez">ተራከበኒ፡ መዋዕል፡ እኩያት፡</foreign> <ref cRef="Job" loc="30,26">Job 30,26</ref>; <ref cRef="Ps." loc="20,8">Ps. 20,8</ref>; <ref cRef="Ps. " loc="39,16">Ps.  39,16</ref>. 
</sense> <sense n="d" xml:id="Dd"> passim i.q. <foreign xml:lang="gez">ተረክበ፡</foreign> sc. <cit type="translation" xml:lang="la"><quote> se alicui assequendum </quote></cit> vel <cit type="translation" xml:lang="la"><quote> inveniendum praebere </quote></cit> <ref cRef="Kuf." loc="3">Kuf. p. 3</ref>; <ref cRef="Jer." loc="36,13">Jer. 36,13</ref> <lbl expand="varia lectio">var.</lbl> 
</sense> </sense>
               
            <sense xml:id="Tmain" xml:lang="en" source="#traces"><cit type="transcription" xml:lang="gez"><quote>tarākaba</quote></cit> <sense n="G" xml:id="TG"> <foreign xml:lang="gez">ተራከበ፡</foreign> <cit type="transcription" xml:lang="gez"><quote>tarākaba</quote></cit> <subc>III,3</subc> <foreign xml:lang="gez">ይትራከብ፡</foreign> «<cit type="translation" xml:lang="fr"><quote>se retrouver ensemble</quote></cit>, <cit type="translation" xml:lang="fr"><quote>se retrouver l’un l’autre</quote></cit>, <cit type="translation" xml:lang="fr"><quote>coïter</quote></cit>» ― <foreign xml:lang="am">ተገናኘ፡</foreign> Ms. <ref type="BM" target="BNFabb217">BNFabb217</ref>, fol. 60v. <lb/>
 <bibl><ptr target="bm:Grebaut1952SuppLexicon"/><citedRange unit="page"> 155</citedRange></bibl> 
</sense> <sense n="L" xml:id="TL"> <foreign xml:lang="gez">ተራከበ</foreign> <cit type="transcription" xml:lang="gez"><quote>tarākaba</quote></cit> <cit type="translation" xml:lang="en"><quote>meet one another</quote></cit>, <cit type="translation" xml:lang="en"><quote>hold a meeting</quote></cit>, <cit type="translation" xml:lang="en"><quote>have an interview</quote></cit>, <cit type="translation" xml:lang="en"><quote>go to meet someone</quote></cit>, <cit type="translation" xml:lang="en"><quote>agree with one another</quote></cit>, <cit type="translation" xml:lang="en"><quote>be together</quote></cit>, <cit type="translation" xml:lang="en"><quote>come together</quote></cit>, <cit type="translation" xml:lang="en"><quote>congregate</quote></cit>, <cit type="translation" xml:lang="en"><quote>have intercourse</quote></cit>; with indirect object suffixes <cit type="translation" xml:lang="en"><quote>come upon</quote></cit>, <cit type="translation" xml:lang="en"><quote>happen</quote></cit>, <cit type="translation" xml:lang="en"><quote>occur</quote></cit> <bibl><ptr target="bm:Leslau1987dic"/><citedRange unit="page"> 469a</citedRange></bibl> 
</sense> </sense></entry>
