               <sense xml:lang="la" source="#dillmann" xml:id="Dmain"> <subc>III,1</subc> <sense n="1" xml:id="D1"> <cit type="translation" xml:lang="la">
                                <quote> seri </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> seminari </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> conseri </quote>
                            </cit>, de semine: <foreign xml:lang="gez">ዘርእ፡ ዘይዘራእ፡</foreign> (v. <foreign xml:lang="gez">ዘይዘርእ፡</foreign>) <ref cRef="Lev." loc="11,37">Lev. 11,37</ref>; <ref cRef="Gen." loc="1,11">Gen. 1,11</ref>; <ref cRef="Gen." loc="1,12">Gen. 1,12</ref>; <ref cRef="Gen." loc="1,29">Gen. 1,29</ref> (<foreign xml:lang="gez">ተዘሪኦ፡</foreign>); <ref cRef="Deut. " loc="29,22">Deut.  29,22</ref>; <ref cRef="Matth." loc="13,19">Matth. 13,19</ref> <lbl expand="sequens">seq.</lbl>; <ref cRef="Marc." loc="4,15">Marc. 4,15</ref> <lbl expand="sequens">seq.</lbl>; de agro: <foreign xml:lang="gez">ምድር፡ ዘኢይዘራእ፡</foreign> <ref cRef="Jer." loc="2,2">Jer. 2,2</ref>; <ref cRef="Jes." loc="19,7">Jes. 19,7</ref>; <ref cRef="Num." loc="20,5">Num. 20,5</ref>. <sense n="a" xml:id="D1a"> <lbl expand="translate">transl.</lbl>: <ref cRef="4 Esr. " loc="9,34">4 Esr.  9,34</ref>; <foreign xml:lang="gez">ቃል፡ ዘተዘርአ፡</foreign> <ref cRef="Jac." loc="1,21">Jac. 1,21</ref>.</sense> <sense n="b" xml:id="D1b"> de animatis: <foreign xml:lang="gez">ኅቡረ፡ ተዘራእነ፡ ማእከለ፡ ከርሥኪ፡</foreign> <ref cRef="Kuf." loc="35.">Kuf. 35.</ref> </sense> </sense> <sense n="2" xml:id="D2"> <cit type="translation" xml:lang="la">
                                <quote> spargi </quote>
                            </cit>, <cit type="translation" xml:lang="la">
