                  <lbl expand="pluralis">Pl.</lbl>
                  <foreign xml:lang="gez">መቃስም፡</foreign> et <foreign xml:lang="gez">መቃስምት፡</foreign>
                  <sense n="1" xml:id="D1">
                     <cit type="translation" xml:lang="la">
                        <quote>ominum captatio</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>sortilegium</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>augurium</quote>
                     </cit>, <cb n="0433" xml:id="c0433"/>
                     <cit type="translation" xml:lang="la">
                        <quote>divinatio</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>hariolatio</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>oraculum</quote>
                     </cit>
                     <ref cRef="Num." loc="23,23">Num. 23,23</ref>; <ref cRef="Deut." loc="18,10">Deut. 18,10</ref>; <ref cRef="Deut." loc="18,14">Deut. 18,14</ref>;
                        <foreign xml:lang="gez">ሕልም፡ ወሰገል፡ ወመቃስም፡</foreign> (v. <foreign xml:lang="gez">መቅስም፡</foreign> et <foreign xml:lang="gez">መቅሰም፡</foreign>)
                        <foreign xml:lang="gez">ከንቱ፡ ኵሎሙ፡</foreign>
                     <ref cRef="Sir." loc="31,5">Sir. 31,5</ref>; <foreign xml:lang="gez">ሰብአ፡
                        መቅስም፡</foreign>
                     <ref cRef="1 Reg." loc="6,2">1 Reg. 6,2</ref>; <foreign xml:lang="gez">በላዐም፡
                        ዘመቅሰም፡ </foreign>
                     <foreign xml:lang="grc">ὁ μάντις</foreign>? <ref cRef="Jos." loc="13,22">Jos.
                        13,22</ref>.</sense>
                  <sense n="2" xml:id="D2">
