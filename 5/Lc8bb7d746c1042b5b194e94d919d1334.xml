         <div>
            <entry xml:id="Lc8bb7d746c1042b5b194e94d919d1334" n="6206">
               <form>
                  <foreign xml:lang="gez">ከብደ</foreign>
               <rs type="root"/>
                    </form>
               <sense xml:lang="la" source="#dillmann" xml:id="Dmain"> <subc>I,1</subc> [<foreign xml:lang="he">כָּבֵד</foreign> <lbl expand="idem">id.</lbl>] <gramGrp> <pos expand="Subjunctivus">Subj.</pos> </gramGrp> <foreign xml:lang="gez">ይክበድ፡</foreign>, rarius <foreign xml:lang="gez">ይክብድ፡</foreign> (ut 1 <ref cRef="Reg." loc="3,2">Reg. 3,2</ref>; <ref cRef="2 Reg." loc="13,25">2 Reg. 13,25</ref> <lbl expand="varia lectio">var.</lbl>), <cit type="translation" xml:lang="la">
                            <quote> gravem esse </quote>
                        </cit>, et comparative <cit type="translation" xml:lang="la">
                            <quote> graviorem esse </quote>
                        </cit>, <lbl expand="sequens">seq.</lbl> <foreign xml:lang="gez">እምነ፡</foreign> compar.: <foreign xml:lang="gez">ምንት፡ ይከብድ፡ እምነ፡ ዐረር፡</foreign> <ref cRef="Sir." loc="22,14">Sir. 22,14</ref>; <ref cRef="Prov." loc="27,3">Prov. 27,3</ref>; <foreign xml:lang="gez">ይከብድ፡ እምኆጻ፡ ባሕር፡</foreign> <ref cRef="Job" loc="6,3">Job 6,3</ref>; <lbl expand="cum">c.</lbl> <gramGrp> <pos expand="pronomen suffixum">pron. suff.</pos> </gramGrp> : <cit type="translation" xml:lang="la">
                            <quote> gravare </quote>
                        </cit> aliquem vel <cit type="translation" xml:lang="la">
                            <quote> nimis gravem esse alicui </quote>
                        </cit>, ut: <foreign xml:lang="gez">ጾር፡ ከብደነ፡</foreign> <ref cRef="Clem." loc="180">Clem. f. 180</ref>; <ref cRef="Sir." loc="6,21">Sir. 6,21</ref>; <ref cRef="Ex." loc="18,18">Ex. 18,18</ref>; <foreign xml:lang="gez">ከብዳ፡ ኀጢአታ፡</foreign> <ref cRef="Jes." loc="24,20">Jes. 24,20</ref>; <ref cRef="4 Esr." loc="6,43">4 Esr. 6,43</ref>. <sense n="a" xml:id="Da"> <cit type="translation" xml:lang="la">
                                <quote> gravatum </quote>
                            </cit> vel <cit type="translation" xml:lang="la">
                                <quote> oneratum esse </quote>
                            </cit>: <foreign xml:lang="gez">ይከብደኒ፡ ርእስየ፡ ወኢጥዒኩ፡ ንዋመ፡</foreign> Rel. Bar.; <foreign xml:lang="gez">ወኢይክበድከ፡ ነፍስትከ፡ ውስተ፡ ምስካብከ፡</foreign> <ref cRef="Sir." loc="34,19">Sir. 34,19</ref>.</sense> <sense n="b" xml:id="Db"> <cit type="translation" xml:lang="la">
                                <quote> graviter incumbere </quote>
                            </cit> alicui, <lbl expand="cum">c.</lbl> <foreign xml:lang="gez">ላዕለ፡</foreign> <ref cRef="Ps. " loc="31,4">Ps.  31,4</ref>; <ref cRef="Ps." loc="37,4">Ps. 37,4</ref>; <ref cRef="1 Reg." loc="5,6">1 Reg. 5,6</ref>.</sense> <sense n="c" xml:id="Dc"> <cit type="translation" xml:lang="la">
                                <quote> ingravescere </quote>
                            </cit> i.e. <cit type="translation" xml:lang="la">
                                <quote> nimis vehementem fieri </quote>
                            </cit>, <foreign xml:lang="gez">ከብድ፡ ደዌሁ፡</foreign> <ref cRef="3 Reg." loc="17,17">3 Reg. 17,17</ref>; <lbl expand="cum">c.</lbl> <foreign xml:lang="gez">ዲበ፡</foreign> pers.: <foreign xml:lang="gez">ከብደ፡ ቀትል፡ ዲበ፡ ሰኦል፡</foreign> <ref cRef="1 Par." loc="10,3">1 Par. 10,3</ref> (aliâs <foreign xml:lang="gez">ጸንዐ፡</foreign>).</sense> <sense n="d" xml:id="Dd"> <cit type="translation" xml:lang="la">
                                <quote> molestum esse </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> oneri esse </quote>
                            </cit>, <lbl expand="cum">c.</lbl> <gramGrp> <pos expand="pronomen suffixum">pron. suff.</pos> </gramGrp> <cit type="translation" xml:lang="la">
                                <quote> (alicui) </quote>
                            </cit>: <foreign xml:lang="gez">ከበድኩከ፡</foreign> <foreign xml:lang="grc">εἰμὶ ἐπὶ σοὶ φορτίον</foreign> <ref cRef="Job" loc="7,20">Job 7,20</ref>; <lbl expand="cum">c.</lbl> <foreign xml:lang="gez">ላዕለ፡</foreign> pers. <ref cRef="2 Reg." loc="13,25">2 Reg. 13,25</ref>; 2 <ref cRef="Reg." loc="14,26">Reg. 14,26</ref>.</sense> <sense n="e" xml:id="De"> <cit type="translation" xml:lang="la">
                                <quote> ingravescere </quote>
                            </cit> i.e. <cit type="translation" xml:lang="la">
                                <quote> crescere </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> augescere </quote>
                            </cit> <ref cRef="4 Esr." loc="14,42">4 Esr. 14,42</ref>.</sense> <sense n="f" xml:id="Df"> <cit type="translation" xml:lang="la">
                                <quote> gravem </quote>
                            </cit> i.e. <cit type="translation" xml:lang="la">
                                <quote> hebetem esse </quote>
                            </cit> vel <cit type="translation" xml:lang="la">
                                <quote> fieri </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> hebescere </quote>
                            </cit>, de oculis <ref cRef="Gen." loc="48,10">Gen. 48,10</ref>; <ref cRef="Kuf." loc="27">Kuf. 27</ref>; <ref cRef="1 Reg." loc="3,2">1 Reg. 3,2</ref>; <ref cRef="Marc." loc="14,40">Marc. 14,40</ref>.</sense> <sense n="g" xml:id="Dg"> <cit type="translation" xml:lang="la">
                                <quote> obdurescere </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> indurari </quote>
                            </cit>, de animo: <foreign xml:lang="gez">ትከብድ፡ ነፍስየ፡ ላዕሌሆሙ፡</foreign> <ref cRef="Zach." loc="11,8">Zach. 11,8</ref>; <foreign xml:lang="gez">ከብደ፡ ልቡ፡</foreign> <ref cRef="Ex. " loc="7,14">Ex.  7,14</ref>; <ref cRef="Ex." loc="8,11">Ex. 8,11</ref>; <ref cRef="Ex." loc="9,7">Ex. 9,7</ref>. <cb n="0850" xml:id="c0850"/> </sense> </sense>
               
