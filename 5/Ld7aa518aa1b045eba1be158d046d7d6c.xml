               <sense xml:lang="la" source="#dillmann" xml:id="Dmain">
                  <gramGrp>
                     <pos expand="participium">part.</pos>
                  </gramGrp> (<subc>II,1</subc>) <lbl expand="cum">c.</lbl>
                  <lbl expand="terminatio">term.</lbl> Adj. <sense n="1" xml:id="D1">
                     <cit type="translation" xml:lang="la">
                        <quote>is qui extollit</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>sublevat</quote>
                     </cit>: Deus <foreign xml:lang="gez">መንሥኤ፡ ነዳያን፡ እምድር፡</foreign>
                     <ref cRef="Sx. Sen." loc="28 ">Sx. Sen. 28 </ref>Enc.</sense>
                  <sense n="2" xml:id="D2">
                     <cit type="translation" xml:lang="la">
                        <quote>is qui surgere facit</quote>
                     </cit>,<cit type="translation" xml:lang="la">
                        <quote> suscitat</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>excitat</quote>
                     </cit>: Deus <foreign xml:lang="gez">መንሥኤ፡ ውዱቃን፡</foreign>
                     <ref cRef="Macc." loc="17">Macc. f. 17</ref>; <foreign xml:lang="gez">መንሥኤ፡
                        ስቡር፡ እምዲበ፡ ዐራቱ፡</foreign>
                     <ref cRef="Sx. Hed." loc="14 ">Sx. Hed. 14 </ref>Enc.</sense>
                  <sense n="3" xml:id="D3">
                     <cit type="translation" xml:lang="la">
                        <quote>resuscitator</quote>
                     </cit>: <foreign xml:lang="gez">መንሥኢሆሙ፡ ለምውታን፡</foreign>
                     <ref cRef="Joh." loc="11,25">Joh. 11,25</ref>
                     <lbl expand="romanae editionis">rom.</lbl>; <ref cRef="Did." loc="25">Did.
                        25</ref>; <ref cRef="Genz." loc="81.">Genz. f. 81.</ref>
                  </sense>
               </sense>
               
