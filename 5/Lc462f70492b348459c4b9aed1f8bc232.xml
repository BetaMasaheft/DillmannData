            <entry xml:id="Lc462f70492b348459c4b9aed1f8bc232" n="9251">
               <form>
                  <foreign xml:lang="gez">ጻመወ</foreign>
               </form>
               <sense xml:lang="la" source="#dillmann" xml:id="Dmain"> quadril. I (<gramGrp> <pos expand="denominates">denom.</pos> </gramGrp>; nonnumquam <foreign xml:lang="gez">ጸመወ፡</foreign> scriptum), Imperf. et <gramGrp> <pos expand="Subjunctivus">Subj.</pos> </gramGrp> <foreign xml:lang="gez">ይጻሙ፡</foreign>, <cit type="translation" xml:lang="la">
                            <quote> laborare </quote>
                        </cit>, <cit type="translation" xml:lang="la">
                            <quote> labore defatigari </quote>
                        </cit>, <cit type="translation" xml:lang="la">
                            <quote> operam impendere </quote>
                        </cit>, plerumque <foreign xml:lang="grc">κοπιᾶν</foreign>, <foreign xml:lang="gez">ቦ፡ ዘይጔጕእ፡ ወይሰርሕ፡ ወይጻሙ፡</foreign> <ref cRef="Sir." loc="11,11">Sir. 11,11</ref>; <ref cRef="Gen." loc="49,15">Gen. 49,15</ref> (<foreign xml:lang="grc">πονεῖν</foreign>); <foreign xml:lang="gez">ከንቶ፡ ይጻምዉ፡</foreign> <ref cRef="Jes." loc="30,5">Jes. 30,5</ref>; <foreign xml:lang="gez">በከ፡ ጻመውኩ፡</foreign> <ref cRef="Jes." loc="49,4">Jes. 49,4</ref>; <ref cRef="Jer." loc="2,24">Jer. 2,24</ref>; <ref cRef="Ps." loc="126,1">Ps. 126,1</ref>; <ref cRef="4 Esr." loc="3,19">4 Esr. 3,19</ref>; <ref cRef="Joh." loc="4,38">Joh. 4,38</ref>; <ref cRef="Matth. " loc="6,28">Matth.  6,28</ref>; <ref cRef="2 Tim." loc="2,6">2 Tim. 2,6</ref>; <ref cRef="2 Tim." loc="4,5">2 Tim. 4,5</ref>; <ref cRef="Phil." loc="2,16">Phil. 2,16</ref>; <ref cRef="1 Thess." loc="5,12">1 Thess. 5,12</ref>; <ref cRef="1 Cor." loc="15,58">1 Cor. 15,58</ref>; <lbl expand="cum">c.</lbl> <foreign xml:lang="gez">ለ፡</foreign> pers.: <cit type="translation" xml:lang="la">
                            <quote> alicui </quote>
                        </cit> vel <cit type="translation" xml:lang="la">
                            <quote> pro </quote>
                        </cit> aliquo, <foreign xml:lang="gez">አኮ፡ ለባሕቲትየ፡ ዘጻመውኩ፡</foreign> <ref cRef="Sir." loc="24,34">Sir. 24,34</ref>; <ref cRef="Sir." loc="30,26">Sir. 30,26</ref>; <ref cRef="Lev." loc="25,53">Lev. 25,53</ref>; <lbl expand="cum">c.</lbl> <foreign xml:lang="gez">ለ፡</foreign> rei, in qua opera collocatur: <foreign xml:lang="gez">ዘይጻሙ፡ ለእኩይ፡</foreign> <foreign xml:lang="grc">κακόμοχθος</foreign> <ref cRef="Sap." loc="15,8">Sap. 15,8</ref>; <foreign xml:lang="gez">ይጻምዉ፡ አሕዛብ፡ ለከንቱ፡</foreign> <ref cRef="Jer." loc="28,58">Jer. 28,58</ref>; <ref cRef="Jes." loc="65,23">Jes. 65,23</ref>; <ref cRef="Job" loc="39,16">Job 39,16</ref>; <ref cRef="Koh." loc="5,15">Koh. 5,15</ref> <lbl expand="varia lectio">var.</lbl>; <ref cRef="Gal." loc="4,11">Gal. 4,11</ref>; vel <lbl expand="cum">c.</lbl> <foreign xml:lang="gez">በ፡</foreign> <ref cRef="Jos." loc="24,13">Jos. 24,13</ref>; <ref cRef="Sir." loc="6,19">Sir. 6,19</ref>; <ref cRef="Koh." loc="5,15">Koh. 5,15</ref> <lbl expand="varia lectio">var.</lbl>; vel <lbl expand="cum">c.</lbl> <gramGrp> <case value="accusativus">Acc.</case> </gramGrp> <foreign xml:lang="gez">ሐምሐም፡ ዘኢጻመውከ፡</foreign> <ref cRef="Jon." loc="4,10">Jon. 4,10</ref>; <lbl expand="cum">c.</lbl> <gramGrp> <case value="accusativus">Acc.</case> </gramGrp> laboris <foreign xml:lang="gez">ጻማሁ፡ ዘይጻሙ፡</foreign> (<foreign xml:lang="grc">μοχθεῖν</foreign>) <ref cRef="Koh." loc="1,3">Koh. 1,3</ref>; <foreign xml:lang="gez">ኅዳጠ፡ ትጻሙ፡ በቅኔሃ፡</foreign> <ref cRef="Sir." loc="6,19">Sir. 6,19</ref>; <cb n="1273" xml:id="c1273"/> <foreign xml:lang="gez">ውኁደ፡ ጻመውኩ፡</foreign> <ref cRef="Sir." loc="51,27">Sir. 51,27</ref>; <ref cRef="Joh." loc="4,38">Joh. 4,38</ref>. <sense n="a" xml:id="Da"> <cit type="translation" xml:lang="la">
                                <quote> labore parere </quote>
                            </cit> vel <cit type="translation" xml:lang="la">
                                <quote> acquirere </quote>
                            </cit> aliquid, <ref cRef="Hen." loc="53,2">Hen. 53,2</ref>.</sense> <sense n="b" xml:id="Db"> <cit type="translation" xml:lang="la">
                                <quote> molestiam </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> labores </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> aerumnam subire </quote>
                            </cit> vel <cit type="translation" xml:lang="la">
                                <quote> suscipere </quote>
                            </cit>: <foreign xml:lang="gez">ጻሙ፡ ለምህሮ፡</foreign> <ref cRef="2 Tim." loc="1,8">2 Tim. 1,8</ref>; <foreign xml:lang="gez">እንዘ፡ ይጻምዉ፡ ጻማ፡ ወእንዘ፡ ይትጋደሉ፡ ገድለ፡</foreign> <ref cRef="Sx. Ter" loc="27 Enc.">Sx. Ter 27 Enc.</ref>; <foreign xml:lang="gez">ጻመወ፡ ወተጋደለ፡ ዕጹበ፡</foreign> <ref cRef="Sx. Teq." loc="4 Enc.">Sx. Teq. 4 Enc.</ref>
                        </sense> <sense n="c" xml:id="Dc"> <cit type="translation" xml:lang="la">
                                <quote> vexari </quote>
                            </cit>: <foreign xml:lang="gez">ኢትጻሙ፡</foreign> <foreign xml:lang="grc">μὴ σκύλλου</foreign> <ref cRef="Luc." loc="7,6">Luc. 7,6</ref>; metu mortis, Lud. e <ref cRef="Vit. Dan." loc="">Vit. Dan. </ref>
                        </sense> </sense>
               
