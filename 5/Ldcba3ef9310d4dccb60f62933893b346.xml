         <div>
            <entry xml:id="Ldcba3ef9310d4dccb60f62933893b346" n="6192">
               <form>
                  <foreign xml:lang="gez">ክበብ</foreign>
               </form>
               <sense xml:lang="la" source="#dillmann" xml:id="Dmain">
                  <gramGrp>
                     <pos expand="Substantivum">subst.</pos>
                  </gramGrp>
                  <sense n="1" xml:id="D1">
                     <cit type="translation" xml:lang="la">
                        <quote>rotunditas</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>circulus</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>orbis</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>gyrus</quote>
                     </cit>: <foreign xml:lang="gez">ወክበብ፡</foreign> (v. <foreign xml:lang="gez">ወክበብ፡</foreign>) <foreign xml:lang="gez">ኵለንታሃ፡</foreign>
                     <foreign xml:lang="grc">στρογγύλη κυκλόθεν</foreign>
                     <ref cRef="2 Par." loc="4,2">2 Par. 4,2</ref>; <foreign xml:lang="gez">ክበቡ፡ ከመ፡
                        ፀሓይ፡ </foreign>
                     <ref cRef="Hen." loc="14,18">Hen. 14,18</ref>; <ref cRef="Hen." loc="18,4">Hen.
                        18,4</ref>; <ref cRef="Hen." loc="72,4">Hen. 72,4</ref>; <ref cRef="Hen." loc="78,3">Hen. 78,3</ref>; <ref cRef="Hen." loc="78,4">Hen. 78,4</ref>;
                        <ref cRef="Lit." loc="171,2">Lit. 171,2</ref>; <foreign xml:lang="gez">ይቁሙ፡
                        ካህናት፡ ክበቡ፡ ለምዉት፡</foreign>
                     <ref cRef="Genz." loc="36.">Genz. f. 36.</ref>
                  </sense>
                  <sense n="2" xml:id="D2">
                     <cit type="translation" xml:lang="la">
                        <quote>globus</quote>
                     </cit>
                     <lbl expand="videas">vid.</lbl>
                     <foreign xml:lang="gez">ክባብ፡</foreign>
                  </sense>
                  <sense n="3" xml:id="D3">
                     <cit type="translation" xml:lang="la">
                        <quote>glomus</quote>
                     </cit>: <foreign xml:lang="gez">ክበበ፡ አፍትልት፡</foreign> Chron. Ax.</sense>
                  <sense n="4" xml:id="D4">
                     <cit type="translation" xml:lang="la">
                        <quote>summa</quote>
                     </cit>, ex numeris redacta (<lbl expand="videas">vid.</lbl>
                     <foreign xml:lang="gez">መክብብ፡</foreign>): <foreign xml:lang="gez">ክበበ፡
                        ሤጡ፡</foreign>
                     <ref cRef="Lev." loc="27,23">Lev. 27,23</ref>.</sense>
               </sense>
               
