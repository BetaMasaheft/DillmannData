               <sense xml:lang="la" source="#dillmann" xml:id="Dmain"> <subc>I,1</subc> [<foreign xml:lang="he">סָפַק</foreign> <foreign xml:lang="ar">صَفَقَ صَفَحَ</foreign> <foreign xml:lang="he">טָפַח</foreign>] <gramGrp> <pos expand="Subjunctivus">Subj.</pos> </gramGrp> <foreign xml:lang="gez">ይጥፋሕ፡</foreign> <cit type="translation" xml:lang="la">
                            <quote> complodere </quote>
                        </cit> manus, <cit type="translation" xml:lang="la">
                            <quote> plaudere </quote>
                        </cit> manibus, alis; <lbl expand="sequens">seq.</lbl> <gramGrp> <case value="accusativus">Acc.</case> </gramGrp> manus, <ref cRef="Ps." loc="46,1">Ps. 46,1</ref>; <ref cRef="Ps." loc="97,9">Ps. 97,9</ref>; <ref cRef="4 Reg." loc="11,12">4 Reg. 11,12</ref>; <foreign xml:lang="gez">ይጠፍሕ፡ እዴሁ፡</foreign> <ref cRef="Prov." loc="17,18">Prov. 17,18</ref>; <foreign xml:lang="gez">ጥፋሕ፡ እዴከ፡</foreign> <ref cRef="Hez." loc="6,10">Hez. 6,10</ref>; <ref cRef="Hez." loc="21,12">Hez. 21,12</ref>; <ref cRef="Jes." loc="55,12">Jes. 55,12</ref>; <ref cRef="Jer." loc="5,31">Jer. 5,31</ref>; <foreign xml:lang="gez">ጥፋሕ፡ እዴከ፡ በዲበ፡ እዴከ፡</foreign> <ref cRef="Hez." loc="21,14">Hez. 21,14</ref>; <ref cRef="Hez." loc="21,17">Hez. 21,17</ref>; <ref cRef="Hez." loc="22,13">Hez. 22,13</ref>; <foreign xml:lang="gez">ወጠፍሐ፡ እደዊሁ፡ ላሜህ፡ በእንተ፡ ሐዘኑ፡ ላዕለ፡ ቃየል፡ ወበጥፍሐተ፡ እደዊሁ፡ ለከፎ፡ ለወልዱ፡ ወቀተሎ፡</foreign> <ref cRef="Clem." loc="15">Clem. f. 15</ref>; etiam omisso <gramGrp> <case value="accusativus">Acc.</case> </gramGrp>: <foreign xml:lang="gez">በጠፊሖትየ፡</foreign> <ref cRef="Gen." loc="4,23">Gen. 4,23</ref>; <foreign xml:lang="gez">እለ፡ ይጠፍሑ፡ ወየኀብሩ፡ ምስለ፡ ቃለ፡ ዕንዚራ፡</foreign> <foreign xml:lang="grc">ἐπικροτοῦντες</foreign> <ref cRef="Am." loc="6,5">Am. 6,5</ref>; <lbl expand="cum">c.</lbl> <foreign xml:lang="gez">በ፡</foreign> manus: <foreign xml:lang="gez">ትጠፍሕ፡ በእደዊሃ፡</foreign> <ref cRef="Jer." loc="31,26">Jer. 31,26</ref>; <ref cRef="Zeph." loc="2,15">Zeph. 2,15</ref>; <ref cRef="Sir." loc="12,18">Sir. 12,18</ref>; <ref cRef="Org." loc="3 ">Org. 3 </ref>(<lbl expand="videas">vid.</lbl> <foreign xml:lang="gez">ጽሕሰ፡</foreign>); <lbl expand="cum">c.</lbl> <foreign xml:lang="gez">ላዕለ፡</foreign> ejus, de quo quis laetans plaudit: <foreign xml:lang="gez">ጠፍሑ፡ እደዊሆሙ፡ ላዕሌኪ፡</foreign> <ref cRef="Thren." loc="2,15">Thren. 2,15</ref>; <ref cRef="Nah." loc="3,19">Nah. 3,19</ref>; vel <foreign xml:lang="gez">በላዕለ፡</foreign> <ref cRef="Job" loc="27,23">Job 27,23</ref>; alas: <foreign xml:lang="gez">መላእክት፡ ይጠፍሑ፡ ክነፊሆሙ፡</foreign> <ref cRef="Sx. Genb." loc="21.">Sx. Genb. 21.</ref> <sense n="a" xml:id="Da"> <cit type="translation" xml:lang="la">
                                <quote> plausum facere </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> crepitare </quote>
                            </cit>: <foreign xml:lang="gez">ይጠፍሑ፡ በምድር፡</foreign> (ubi Gr. habet: <foreign xml:lang="grc">σύροντες γῆν</foreign>) <ref cRef="Mich. " loc="7,17">Mich.  7,17</ref>.</sense> <sense n="b" xml:id="Db"> <cit type="translation" xml:lang="la">
                                <quote> plaudendo fingere </quote>
                            </cit> vel <cit type="translation" xml:lang="la">
                                <quote> formare </quote>
                            </cit> lateres, <ref cRef="Gen." loc="11,3">Gen. 11,3</ref>.</sense> </sense>
               
