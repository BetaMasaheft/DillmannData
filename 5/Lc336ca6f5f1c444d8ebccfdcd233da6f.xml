            <entry xml:id="Lc336ca6f5f1c444d8ebccfdcd233da6f" n="2808">
               <form>
                  <foreign xml:lang="gez">ሰንበት</foreign>
               <rs type="root"/>
                    </form>
               <sense xml:lang="la" source="#dillmann" xml:id="Dmain"> <gramGrp> <pos expand="Substantivum">subst.</pos> </gramGrp>, plerumque <gramGrp> <gen value="femininus">fem.</gen> </gramGrp>, <lbl expand="pluralis">Pl.</lbl> <foreign xml:lang="gez">ሰናብት፡</foreign> et <foreign xml:lang="gez">ሰንበታት፡</foreign> [<foreign xml:lang="he">אַבָּת</foreign>] 1) <cit type="translation" xml:lang="la">
                            <quote> sabbatum </quote>
                        </cit>, <cit type="translation" xml:lang="la">
                            <quote> dies hebdomadis septimus </quote>
                        </cit> <ref cRef="Ex." loc="16,23">Ex. 16,23</ref>; <ref cRef="Ex." loc="16,25">Ex. 16,25</ref>; <ref cRef="Ex." loc="16,29">Ex. 16,29</ref>; <ref cRef="Ex. " loc="16,30">Ex.  16,30</ref>; <foreign xml:lang="gez">ዕለተ፡ ሰንበት፡</foreign> <ref cRef="Ex." loc="20,8">Ex. 20,8</ref>; <ref cRef="Ex." loc="31,13">Ex. 31,13</ref>-15; <ref cRef="Matth." loc="12,2">Matth. 12,2</ref>; <ref cRef="Matth." loc="12,8">Matth. 12,8</ref>; <ref cRef="Marc." loc="6,2">Marc. 6,2</ref>; <ref cRef="Marc." loc="16,1">Marc. 16,1</ref>; <ref cRef="Hebr." loc="4,4">Hebr. 4,4</ref>; <foreign xml:lang="gez">ሰንበተ፡ ሰናብት፡</foreign> <ref cRef="Lev." loc="16,31">Lev. 16,31</ref>; <ref cRef="Lev." loc="23,32">Lev. 23,32</ref>; <foreign xml:lang="gez">እንተ፡ ቅድመ፡ ሰንበት፡</foreign> <foreign xml:lang="grc">προσάββατον</foreign> <ref cRef="Judith" loc="8,6">Judith 8,6</ref>; <ref cRef="Asc. Jes." loc="3,13">Asc. Jes. 3,13</ref>; <foreign xml:lang="gez">ሰናብት፡</foreign> <ref cRef="Ex." loc="31,16">Ex. 31,16</ref>; <foreign xml:lang="gez">ሰንበታት፡</foreign> <ref cRef="Am." loc="8,5">Am. 8,5</ref>; <ref cRef="Hez." loc="20,12">Hez. 20,12</ref>; <ref cRef="Jes." loc="56,2">Jes. 56,2</ref>; <ref cRef="Jes." loc="58,3">Jes. 58,3</ref>; <foreign xml:lang="gez">ዕለተ፡ ሰንበታት፡</foreign> <ref cRef="Jer." loc="17,21">Jer. 17,21</ref> seqq.; <foreign xml:lang="gez">መዋዕለ፡ ሰንበታት፡</foreign> <ref cRef="Judith" loc="10,2">Judith 10,2</ref>. <sense n="2" xml:id="D2"> Aethiopes Christiani cum et septimum et primum septimanae diem celebrarent, utrumque <foreign xml:lang="gez">ሰንበት፡</foreign> appellant, quare distinguere <cb n="0370" xml:id="c0370"/> solebant <sense n="a" xml:id="D2a"> <foreign xml:lang="gez">ሰንበተ፡ ቀዳሚት፡</foreign> <cit type="translation" xml:lang="la">
                                    <quote> sabbatum prius </quote>
                                </cit> Lud. comm. hist. p. 239 nr. 7; <ref cRef="Syn." loc=" in Proleg.">Syn.  in Proleg.</ref>; vel <foreign xml:lang="gez">ሰንበተ፡ አይሁድ፡</foreign> <cit type="translation" xml:lang="la">
                                    <quote> sabbatum Judaeorum </quote>
                                </cit> <ref cRef="Kedr f." loc="64">Kedr f. 64</ref>; <ref cRef="Can. Laod." loc="28 (29)">Can. Laod. 28 (29)</ref>.</sense> <sense n="b" xml:id="D2b"> <foreign xml:lang="gez">እሑድ፡ ሰንበት፡</foreign> <cit type="translation" xml:lang="la">
                                    <quote> feria prima </quote>
                                </cit>, <cit type="translation" xml:lang="la">
                                    <quote> dominica </quote>
                                </cit> <ref cRef="Marc." loc="16,2">Marc. 16,2</ref>; <ref cRef="Luc." loc="24,1">Luc. 24,1</ref>; vel <foreign xml:lang="gez">ሰንበተ፡ እሑድ፡</foreign> Lud. comm. hist. p. 239 nr. 8; <foreign xml:lang="gez">ሰንበተ፡ ክርስቲያን፡</foreign> <cit type="translation" xml:lang="la">
                                    <quote> sabbatum Christianorum </quote>
                                </cit> <ref cRef="Did." loc="10">Did. 10</ref>; <ref cRef="Mavâs." loc=" 70">Mavâs.  70</ref>. Sed passim <cit type="translation" xml:lang="la">
                                    <quote> dominicam </quote>
                                </cit> quoque (sicut diem septimum) simpliciter <foreign xml:lang="gez">ሰንበት፡</foreign> nominant, ut: <foreign xml:lang="gez">ተንሥአ፡ ክርስቶስ፡ በዕለተ፡ ሰንበት፡</foreign> <ref cRef="Did." loc="30">Did. 30</ref>; <foreign xml:lang="gez">መፍትው፡ ናዕርፍ፡ በሰንበታት፡</foreign> <ref cRef="Did." loc="38.">Did. 38.</ref> </sense> </sense> <sense n="3" xml:id="D3"> <cit type="translation" xml:lang="la">
                                <quote> sabbatum </quote>
                            </cit> i.e. <cit type="translation" xml:lang="la">
                                <quote> hebdomas </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> septimana </quote>
                            </cit> <ref cRef="Lev." loc="23,15">Lev. 23,15</ref>; <ref cRef="Lev." loc="25,8">Lev. 25,8</ref>; <ref cRef="Deut." loc="16,9">Deut. 16,9</ref>; <ref cRef="4 Esr." loc="4,39">4 Esr. 4,39</ref>; <ref cRef="Luc." loc="18,12">Luc. 18,12</ref>; <ref cRef="Tob." loc="2,1">Tob. 2,1</ref>; <foreign xml:lang="gez">በዓለ፡ ሰንበት፡</foreign> <cit type="translation" xml:lang="la">
                                <quote> festum hebdomadum </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> pentecoste </quote>
                            </cit> <ref cRef="Ex." loc="34,22">Ex. 34,22</ref>; <ref cRef="Deut. " loc="16,10">Deut.  16,10</ref>; <foreign xml:lang="gez">ሰንበታተ፡ መዋዕል፡ ፶ወ፪፡</foreign> <ref cRef="Kuf." loc="6">Kuf. 6</ref>; <foreign xml:lang="gez">ሰንበታተ፡ ዓመታት፡</foreign> <ref cRef="Kuf." loc="50">Kuf. 50</ref>; <ref cRef="Dan." loc="9,24-27">Dan. 9,24-27</ref>; <ref cRef="Hen. " loc="91,12-17">Hen.  91,12-17</ref>.</sense> <sense n="4" xml:id="D4"> <lbl expand="translate">transl.</lbl> sabbatum i.e. <cit type="translation" xml:lang="la">
                                <quote> feriae aetatis </quote>
                            </cit> sive <cit type="translation" xml:lang="la">
                                <quote> extrema aetas </quote>
                            </cit> (<lbl expand="oppositum, -o, -nitur">opp.</lbl> <foreign xml:lang="gez">መዋዕለ፡ ውርዙት፡</foreign>) <ref cRef="Hen." loc="10,17">Hen. 10,17</ref>.</sense> <sense n="5" xml:id="D5"> ut videtur, <cit type="translation" xml:lang="la">
                                <quote> dies festus </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> solemnia </quote>
                            </cit> in genere <ref cRef="Act." loc="19,28">Act. 19,28</ref>; <ref cRef="Act." loc="19,34">Act. 19,34</ref>.</sense> </sense>
               
            </entry>
