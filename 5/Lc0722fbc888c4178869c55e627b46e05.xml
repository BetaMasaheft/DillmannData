            <entry xml:id="Lc0722fbc888c4178869c55e627b46e05" n="5007">
               <form>
                  <foreign xml:lang="gez">ንስቲት</foreign>
               </form>
               <sense xml:lang="la" source="#dillmann" xml:id="Dmain"> (ut videtur pro <foreign xml:lang="gez">ንእስቲት፡</foreign>, terminatione <cit type="translation" xml:lang="la">
                            <quote> it </quote>
                        </cit> e <foreign xml:lang="gez">ንእስት፡</foreign> formatum) <sense n="1" xml:id="D1"> parvitas i.e. <cit type="translation" xml:lang="la">
                                <quote> adolescentia </quote>
                            </cit> <ref cRef="Lev." loc="22,13">Lev. 22,13</ref>.</sense> <sense n="2" xml:id="D2"> <cit type="translation" xml:lang="la">
                                <quote> paulum </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> paululum </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> aliquantulum </quote>
                            </cit>, Christus <foreign xml:lang="gez">አሥረቀ፡ ንስቲተ፡ መለኮቶ፡</foreign> <cit type="translation" xml:lang="la">
                                <quote> divinitatem suam paulum effulgere fecit </quote>
                            </cit>Lit. Orth. <sense n="a" xml:id="D2a"> de spatio: <foreign xml:lang="gez">ንስቲተ፡ ኀሊፍየ፡ እምኔሆሙ፡</foreign> <ref cRef="Cant." loc="3,4">Cant. 3,4</ref>; <ref cRef="Act." loc="27,28">Act. 27,28</ref>.</sense> <sense n="b" xml:id="D2b"> de tempore: <foreign xml:lang="gez">ነበረት፡ ንስቲተ፡</foreign> <ref cRef="Judith" loc="13,9">Judith 13,9</ref> (<cit type="translation" xml:lang="la">
                                    <quote> paulisper </quote>
                                </cit>); <ref cRef="Hen." loc="52,5">Hen. 52,5</ref>; <foreign xml:lang="gez">እምድኅረ፡ ንስቲት፡</foreign> <ref cRef="Genz." loc="84.">Genz. f. 84.</ref> </sense> <sense n="c" xml:id="D2c"> <foreign xml:lang="gez">ንስቲተ፡ ክመ፡ ተረፎሙ፡ ወይዌግሩኒ፡</foreign> <cit type="translation" xml:lang="la">
                                    <quote> non multum abest </quote>
                                </cit>, <cit type="translation" xml:lang="la">
                                    <quote> quin </quote>
                                </cit> <ref cRef="Ex." loc="17,4">Ex. 17,4</ref>; <foreign xml:lang="gez">ኀደግዎ፡ እንዘ፡ ንስቲት፡ ኢይሙት፡</foreign> <ref cRef="Sx. Nah." loc="5.">Sx. Nah. 5.</ref> ) <foreign xml:lang="gez">ንስቲተ፡</foreign> <cit type="translation" xml:lang="la">
                                    <quote> paene </quote>
                                </cit>, <foreign xml:lang="grc">παρὰ μικρόν</foreign> <ref cRef="Hez. " loc="16,47">Hez.  16,47</ref>. ) <foreign xml:lang="gez">በበንስቲት፡</foreign> <cit type="translation" xml:lang="la">
                                    <quote> paulatim </quote>
                                </cit>, <cit type="translation" xml:lang="la">
                                    <quote> sensim </quote>
                                </cit>: <ref cRef="Act." loc="18,23">Act. 18,23</ref> Platt; <ref cRef="Act." loc="26,28">Act. 26,28</ref> <lbl expand="romanae editionis">rom.</lbl>; <foreign xml:lang="gez">ኢወሀብኩ፡ በአሐዱ፡ ጊዜ፡ አላ፡ በበ፡ ንስቲት፡</foreign> Org.</sense> </sense> <sense n="3" xml:id="D3"> Adjectivi instar nominibus substantivis apponitur <sense n="a" xml:id="D3a"> <cit type="translation" xml:lang="la">
                                    <quote> paulum </quote>
                                </cit>, <cit type="translation" xml:lang="la">
                                    <quote> exiguum </quote>
                                </cit>: <foreign xml:lang="gez">ንስቲት፡ ጥበብ፡</foreign> <ref cRef="Koh. " loc="10,1">Koh.  10,1</ref> (cfr. <ref cRef="Hen." loc="63,1">Hen. 63,1</ref>; <ref cRef="Hen." loc="63,6">Hen. 63,6</ref>).</sense> <sense n="b" xml:id="D3b"> <cit type="translation" xml:lang="la">
                                    <quote> aliquot </quote>
                                </cit>, <cit type="translation" xml:lang="la">
                                    <quote> pauci </quote>
                                </cit>: <foreign xml:lang="gez">አምጽእ፡ ንስቲተ፡ በለሰ፡</foreign> Rel. Bar.</sense> <sense n="c" xml:id="D3c"> <cit type="translation" xml:lang="la">
                                    <quote> parvus </quote>
                                </cit>, <cit type="translation" xml:lang="la">
                                    <quote> parvulus </quote>
                                </cit>, <cit type="translation" xml:lang="la">
                                    <quote> pusillus </quote>
                                </cit>, <cit type="translation" xml:lang="la">
                                    <quote> exiguus </quote>
                                </cit>, de rebus: <foreign xml:lang="gez">ሀገር፡ ንስቲት፡</foreign> <ref cRef="Koh." loc="9,14">Koh. 9,14</ref>; <ref cRef="Gen." loc="19,20">Gen. 19,20</ref>; <foreign xml:lang="gez">ንስቲት፡ መክፈልት፡</foreign> <foreign xml:lang="grc">μικρὰ μερίς</foreign> <ref cRef="Prov." loc="15,16">Prov. 15,16</ref>; <ref cRef="Hez." loc="17,6">Hez. 17,6</ref>; <ref cRef="1 Reg." loc="2,19">1 Reg. 2,19</ref>; <ref cRef="Jac." loc="3,5">Jac. 3,5</ref>; <foreign xml:lang="gez">ንስቲት፡ መሶብ፡</foreign> <ref cRef="Sx. Genb." loc="6 ">Sx. Genb. 6 </ref>Enc.; de animalibus: <foreign xml:lang="gez">በግዕት፡ ንስቲት፡</foreign> <ref cRef="2 Reg." loc="12,3">2 Reg. 12,3</ref>; de hominibus (sc. de pueris et feminis): <foreign xml:lang="gez">ሕፃን፡ ንስቲት፡</foreign> <ref cRef="Jes." loc="11,6">Jes. 11,6</ref> (v. <foreign xml:lang="gez">ንኡስ፡</foreign>); <ref cRef="Cant." loc="8,8">Cant. 8,8</ref>; <ref cRef="Kuf." loc="30.">Kuf. 30.</ref> </sense> <sense n="d" xml:id="D3d"> de tempore: <cit type="translation" xml:lang="la">
                                    <quote> brevis </quote>
                                </cit>, <foreign xml:lang="gez">ከመ፡ የሀቦሙ፡ ዕረፍተ፡ ንስቲተ፡</foreign> <ref cRef="Hen." loc="63,1">Hen. 63,1</ref>; <ref cRef="Hen." loc="63,6">Hen. 63,6</ref>.</sense> </sense> </sense>
               
