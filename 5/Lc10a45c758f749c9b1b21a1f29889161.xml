            <entry xml:id="Lc10a45c758f749c9b1b21a1f29889161" n="7254">
               <form>
                  <foreign xml:lang="gez">ዐውድ</foreign>
               </form>
               <sense xml:id="Dmain" xml:lang="la" source="#dillmann"> usurpatur significandis adverbio et praepositioni <cit type="translation" xml:lang="la"><t:quote xmlns:t="http://www.tei-c.org/ns/1.0"> circa </t:quote></cit>. <sense n="1" xml:id="D1"> Vim substantivi integram tenet. <sense n="a" xml:id="D1a"> <foreign xml:lang="gez">ዐውዱ፡</foreign> (non <foreign xml:lang="gez">ዐውዶ፡</foreign>) <foreign xml:lang="gez">ለውእቱ፡ መንበር፡ ፳ወ፬ መናብርት፡</foreign> et (circuitus throni i.e.) circa thronum erant … <ref cRef="Apoc." loc="4,4">Apoc. 4,4</ref> <lbl expand="romanae editionis">rom.</lbl>; <foreign xml:lang="gez">የሐንጽ፡ ጥቅመ፡ ዐውድኪ፡</foreign> (v. <foreign xml:lang="gez">ዐውደኪ፡</foreign>) <ref cRef="Hez." loc="26,8">Hez. 26,8</ref>.
</sense> <sense n="b" xml:id="D1b"> post <gramGrp> <pos expand="pronomen">pron.</pos> <pos expand="relativum">rel.</pos> </gramGrp> vel <cb n="1001" xml:id="c1001"/> notam Genitivi: <foreign xml:lang="gez">ነጽሪ፡ ዘዐውድኪ፡</foreign> vide id quod (circuitus tuus i.e.) circa te est <ref cRef="Jes. " loc="49,18">Jes.  49,18</ref>; <foreign xml:lang="gez">እለ፡ ዐውድክሙ፡</foreign> <foreign xml:lang="grc">οἱ κύκλῳ ὑμῶν</foreign> <ref cRef="Hez." loc="36,3">Hez. 36,3</ref>; <ref cRef="Hez." loc="36,7">Hez. 36,7</ref>; <foreign xml:lang="gez">አሕዛብ፡ እለ፡ ዐውድነ፡</foreign> <ref cRef="2 Esr." loc="16,16">2 Esr. 16,16</ref>; <ref cRef="Ps." loc="88,8">Ps. 88,8</ref>; <ref cRef="1 Reg." loc="12,11">1 Reg. 12,11</ref>; <foreign xml:lang="gez">ኵሉ፡ ዘዐውዱ፡</foreign> (v. <foreign xml:lang="gez">ዐውዶ፡</foreign>) <ref cRef="Job" loc="1,10">Job 1,10</ref>; <foreign xml:lang="gez">ፀሩ፡ እለ፡ ዐውዱ፡</foreign> <ref cRef="Sir." loc="47,7">Sir. 47,7</ref>; <ref cRef="Num." loc="22,4">Num. 22,4</ref>.
</sense> <sense n="c" xml:id="D1c"> post praepositiones: <foreign xml:lang="gez">ውስተ፡ ኵሉ፡ ዐውዱ፡</foreign> <ref cRef="Ex." loc="29,16">Ex. 29,16</ref>; <foreign xml:lang="gez">ውስተ፡ ዐውድኪ፡</foreign> <ref cRef="Hez." loc="28,23">Hez. 28,23</ref>; <ref cRef="Lev." loc="1,11">Lev. 1,11</ref>; <ref cRef="Num." loc="2,53">Num. 2,53</ref>; <ref cRef="Jos." loc="6,3">Jos. 6,3</ref>; <ref cRef="Jud." loc="7,21">Jud. 7,21</ref>; <foreign xml:lang="gez">በዐውዳ፡</foreign> <ref cRef="Jer." loc="6,3">Jer. 6,3</ref>; <ref cRef="Apoc." loc="4,4">Apoc. 4,4</ref> Platt; <ref cRef="Apoc." loc="5,11">Apoc. 5,11</ref>; <foreign xml:lang="gez">እንተ፡ ዐውዱ፡</foreign> <ref cRef="Sir." loc="45,9">Sir. 45,9</ref>; <foreign xml:lang="gez">ዲቤኪ፡ እንተ፡ ዐውድኪ፡</foreign> <ref cRef="Hez." loc="23,24">Hez. 23,24</ref>; <foreign xml:lang="gez">እምነ፡ ዐውዶሙ፡</foreign> <ref cRef="Jos." loc="21,42">Jos. 21,42</ref>; <ref cRef="Job" loc="18,11">Job 18,11</ref>; <ref cRef="Sir." loc="46,5">Sir. 46,5</ref>; <ref cRef="Jes." loc="42,25">Jes. 42,25</ref>; <foreign xml:lang="gez">ኮኑ፡ ላዕሌሃ፡ እምዐውዳ፡</foreign> <ref cRef="Jer." loc="4,17">Jer. 4,17</ref>.</sense>
</sense> <sense n="2" xml:id="D2"> In <gramGrp> <case value="accusativus">Acc.</case> </gramGrp> <foreign xml:lang="gez">ዐውደ፡</foreign> <sense n="a" xml:id="D2a"> <gramGrp> <pos expand="adverbium, adverbialiter">adv.</pos> </gramGrp> <cit type="translation" xml:lang="la"><t:quote xmlns:t="http://www.tei-c.org/ns/1.0"> circa </t:quote></cit>, ut <ref cRef="Ps." loc="11,9">Ps. 11,9</ref>, quamquam sine pronominibus suffixis rarius usurpatur.
</sense> <sense n="b" xml:id="D2b"> <gramGrp> <pos expand="praepositio">praep.</pos> </gramGrp>: <ref cRef="Gen." loc="35,5">Gen. 35,5</ref>; <ref cRef="Ex." loc="7,24">Ex. 7,24</ref>; <ref cRef="Ex." loc="40,27">Ex. 40,27</ref>; <ref cRef="Num." loc="2,2">Num. 2,2</ref>; <ref cRef="Num." loc="11,24">Num. 11,24</ref>; <ref cRef="Ps. " loc="17,13">Ps.  17,13</ref>; <ref cRef="Ps." loc="77,32">Ps. 77,32</ref>; <ref cRef="Ps." loc="127,4">Ps. 127,4</ref>; <lbl expand="cum">c.</lbl> <gramGrp> <pos expand="pronomen suffixum">pron. suff.</pos> </gramGrp> (<lbl expand="videas">vid.</lbl> <bibl><ptr target="bm:Dillmann1857Gramm"/>gramm.</bibl> § 167,5): <ref cRef="Ps." loc="30,16">Ps. 30,16</ref>; <ref cRef="Ps." loc="33,8">Ps. 33,8</ref>; <ref cRef="Lev." loc="1,5">Lev. 1,5</ref>; <foreign xml:lang="gez">ነጽሪ፡ ዐውደኪ፡</foreign> <ref cRef="Jes." loc="60,4">Jes. 60,4</ref>; <foreign xml:lang="gez">ይቀውሙ፡ ዐውዶ፡</foreign> <ref cRef="Jes." loc="6,2">Jes. 6,2</ref>; <foreign xml:lang="gez">ዐውድየ፡</foreign> <ref cRef="Job" loc="29,5">Job 29,5</ref>; <ref cRef="Hez." loc="1,4">Hez. 1,4</ref>; <ref cRef="Zach." loc="2,5">Zach. 2,5</ref>; <ref cRef="Jos." loc="21,39">Jos. 21,39</ref>; <ref cRef="Marc." loc="3,34">Marc. 3,34</ref>.
</sense> 
</sense> </sense>
