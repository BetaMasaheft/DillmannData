               <sense xml:id="Dmain" xml:lang="la" source="#dillmann"> <subc>I,2</subc> [cogn. cum <foreign xml:lang="gez">ሮደ፡</foreign> et <foreign xml:lang="gez">ሮጸ፡</foreign> q.v. Notio primariae radicis in <cit type="translation" xml:lang="la"><quote> currendo </quote></cit>, <cit type="translation" xml:lang="la"><quote> discurrendo </quote></cit> esse videtur (<foreign xml:lang="he">רוּד</foreign> <foreign xml:lang="ar">رَادَ</foreign>); e notione <cit type="translation" xml:lang="la"><quote> libere vagandi </quote></cit> derivatur <cit type="translation" xml:lang="la"><quote> audacem </quote></cit>, <cit type="translation" xml:lang="la"><quote> pervicacem </quote></cit>, <cit type="translation" xml:lang="la"><quote> rebellem esse </quote></cit> (<foreign xml:lang="he">מָרַד</foreign> <foreign xml:lang="syr">ܡܪܰܕ</foreign> <foreign xml:lang="ar">مَرُدَ</foreign> <foreign xml:lang="gez">ረደደ፡</foreign>, <foreign xml:lang="gez">መርደደ፡</foreign>), e notione <cit type="translation" xml:lang="la"><quote> currendi </quote></cit> manat <cit type="translation" xml:lang="la"><quote> pellere </quote></cit>, <cit type="translation" xml:lang="la"><quote> propellere </quote></cit>, (<foreign xml:lang="ar">مَرَدَ</foreign> propulit navem) <cit type="translation" xml:lang="la"><quote> persequi </quote></cit> (<foreign xml:lang="he">רָדַף</foreign> <foreign xml:lang="gez">ሮደ፡</foreign>)] <cit type="translation" xml:lang="la"><quote> vires intendere in currendo </quote></cit>, <cit type="translation" xml:lang="la"><quote> raptim currere </quote></cit>, <cit type="translation" xml:lang="la"><quote> assilire </quote></cit>, <cit type="translation" xml:lang="la"><quote> insilire </quote></cit>, <cit type="translation" xml:lang="la"><quote> incursionem </quote></cit> vel <cit type="translation" xml:lang="la"><quote> impetum facere </quote></cit>: <foreign xml:lang="gez">ዘይሜርድ፡ ደክመ፡ እንዘ፡ ይረውጽ፡</foreign> <foreign xml:lang="grc">διέλιπεν ὁ τρέχων ἀπὸ τοῦ δρόμου αὐτοῦ</foreign> <ref cRef="Jer." loc="8,6">Jer. 8,6</ref>; <foreign xml:lang="gez">መረዱ፡ ይዴግንዎሙ፡</foreign> <ref cRef="Sap." loc="19,2">Sap. 19,2</ref>; <foreign xml:lang="gez">እንዘ፡ ይሜርዱ፡</foreign> <foreign xml:lang="grc">ἁλλόμενοι μεγάλα</foreign> <ref cRef="1 Reg." loc="10,2">1 Reg. 10,2</ref>; <ref cRef="Sx. Hed." loc="9 ">Sx. Hed. 9 </ref> Enc.; <foreign xml:lang="gez">ይሜርድ፡ ፍኖቶ፡</foreign> <ref cRef="Ps." loc="18,6">Ps. 18,6</ref>; <foreign xml:lang="gez">ውስተ፡ አርእስተ፡ አድባር፡ ይሜርዱ፡</foreign> <ref cRef="Joel" loc="2,5">Joel 2,5</ref>; saepius in Jsp. ut p. 272; <lbl expand="cum">c.</lbl> <foreign xml:lang="gez">ኀበ፡</foreign> gentis, in quam incursio fit <ref cRef="Jsp." loc="273">Jsp. p. 273</ref>; vel <lbl expand="cum">c.</lbl> <gramGrp> <case value="accusativus">Acc.</case> </gramGrp> loci, in quem impetus fit: <foreign xml:lang="gez">መረደ፡ ሀገሮሙ፡ ለሳምራውያን፡ ወአንኀለ፡ መቅደሰ፡</foreign> <ref cRef="Jsp." loc="299">Jsp. p. 299</ref>; <lbl expand="construitur cum">c.c.</lbl> <foreign xml:lang="gez">ላዕለ፡</foreign> pers. <cit type="translation" xml:lang="la"><quote> acrius currendo vires </quote></cit> alicujus <cit type="translation" xml:lang="la"><quote> immodice intendere </quote></cit>: <foreign xml:lang="gez">ለእመ፡ መረድነ፡ ላዕሌሆሙ፡</foreign> <ref cRef="Gen." loc="33,13">Gen. 33,13</ref>. </sense>
               
