                  <ref cRef="Jer. " loc="2,6">Jer. 2,6</ref>; <foreign xml:lang="gez">በድወ፡
                     ገዳም፡</foreign>
                  <ref cRef="Prov." loc="9,12">Prov. 9,12</ref>; <foreign xml:lang="gez">መካነ፡
                     በድው፡</foreign>
                  <ref cRef="Hen." loc="18,12">Hen. 18,12</ref>; <foreign xml:lang="gez">በድው፡
                     ፋራም፡</foreign>
