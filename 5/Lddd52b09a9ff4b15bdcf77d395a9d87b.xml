            <entry xml:id="Lddd52b09a9ff4b15bdcf77d395a9d87b" n="7461">
               <form>
                  <foreign xml:lang="gez">አዝለፈ</foreign>
               </form>
               <sense xml:lang="la" source="#dillmann" xml:id="Dmain"> <subc>II,1</subc>, rarius <foreign xml:lang="gez">አዘለፈ፡</foreign> <subc>II,2</subc> <cit type="translation" xml:lang="la">
                     <quote>continuare</quote>
                  </cit>, <cit type="translation" xml:lang="la">
                     <quote>pergere</quote>
                  </cit>, <cit type="translation" xml:lang="la">
                     <quote>perseverare</quote>
                  </cit>, <cit type="translation" xml:lang="la">
                     <quote>perpetuare</quote>
                  </cit>, <lbl expand="construitur cum">c.c.</lbl>
                  <gramGrp>
                     <case value="accusativus">Acc.</case>
                  </gramGrp> rei vel Infin.: <foreign xml:lang="gez">ዘአዝለፈ፡ ትዕግሥቶ፡</foreign>
                  <foreign xml:lang="grc">ὁ ὑπομείνας εἰς τέλος</foreign>
                  <ref cRef="Matth." loc="10,22">Matth. 10,22</ref>; <ref cRef="Matth." loc="24">Matth. 24</ref>;13; <ref cRef="Marc." loc="13,13">Marc. 13,13</ref> (ubi <lbl expand="edidit">ed.</lbl>
                  <lbl expand="romanae editionis">rom.</lbl>
                  <foreign xml:lang="gez">አዘለፈ፡</foreign> habet); <foreign xml:lang="gez">ዘያዘልፍ፡
                     ሐሰተ፡ </foreign> (pertinax mendacii) F.M. 23,1; <foreign xml:lang="gez">ሶበ፡
                     አዝለፈ፡ ጸሎተ፡ </foreign>
                  <ref cRef="Phlx." loc="51">Phlx. 51</ref>; <foreign xml:lang="gez">ዘአዝለፈ፡ ዘንተ፡
                     ኀጢአተ፡</foreign> F.M. 10,3; <foreign xml:lang="gez">ለእመ፡ አዝለፍነ፡ ዐቂበ፡</foreign>
                  <ref cRef="Hebr." loc="3,14">Hebr. 3,14</ref>; <foreign xml:lang="gez">ከመ፡ ያዝልፉ፡
                     ገቢረ፡ ትሩፋት፡</foreign> F.M. 2,2; <foreign xml:lang="gez">ኢያዝልፍ፡ ምሂረ፡ በቃል፡ ባሕቲቱ፡
                     አላ፡ ይኩኖሙ፡ አርአያ፡</foreign> F.M. 2,11; <foreign xml:lang="gez">አዘለፍነ፡
                     ነጽሮታ፡</foreign>
                  <ref cRef="Chrys. Ta." loc="8">Chrys. Ta. 8</ref>; <lbl expand="cum">c.</lbl>
                  <foreign xml:lang="gez">ላዕለ፡</foreign> seq. Infin., ut: <foreign xml:lang="gez">አዝለፈ፡ ላዕለ፡ ገቢረ፡ እኩይ፡</foreign>
                  <ref cRef="Chrys. Ta." loc="7">Chrys. Ta. 7</ref>; <lbl expand="sequens">seq.</lbl>
                  <foreign xml:lang="gez">እንዘ፡</foreign>, <ref cRef="Phlx." loc="63 ">Phlx. 63
                     </ref>(<lbl expand="videas">vid. sub</lbl>
                  <foreign xml:lang="gez">ደፍዐ፡</foreign>). (<ref cRef="Job" loc="39,13">Job
                     39,13</ref>: <foreign xml:lang="gez">ሰገኖ፡ ዘታዘልፍ፡ ወትነብር፡ </foreign> absque
                  auctoritate Graeci et sine sensu bono). – <lbl expand="Vocabularium Aethiopicum, -a -a.">Voc. Ae.</lbl>: <foreign xml:lang="gez">አውተረ፡ ወአዝለፈ፡ ዘ፡ አረዘመ።</foreign>
               </sense>
               
