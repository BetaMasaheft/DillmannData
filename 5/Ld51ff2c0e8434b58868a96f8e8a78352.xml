            <entry xml:id="Ld51ff2c0e8434b58868a96f8e8a78352" n="9810">
               <form>
                  <foreign xml:lang="gez">ፈለሰ</foreign>
               <rs type="root"/>
                    </form>
               <sense xml:lang="la" source="#dillmann" xml:id="Dmain"> <subc>I,1</subc> [amh. <lbl expand="idem">id.</lbl>; <lbl expand="vicinus">vic.</lbl> <foreign xml:lang="gez">ፈለጠ፡</foreign>; <lbl expand="videas">vid.</lbl> etiam Ges. thes. p. 1107] <gramGrp> <pos expand="Subjunctivus">Subj.</pos> </gramGrp> <foreign xml:lang="gez">ይፍልስ፡</foreign> <sense n="1" xml:id="D1"> de animatis: <cit type="translation" xml:lang="la">
                                <quote> migrare </quote>
                            </cit>, <sense n="a" xml:id="D1a"> <cit type="translation" xml:lang="la">
                                    <quote> transire </quote>
                                </cit>: <foreign xml:lang="gez">ፈለሰ፡ እምህየ፡</foreign> <foreign xml:lang="grc">μετέβη</foreign> <ref cRef="Matth." loc="11,1">Matth. 11,1</ref> <lbl expand="romanae editionis">rom.</lbl>; <foreign xml:lang="gez">ወፈሊሶ፡ እምህየ፡ ሖረ፡</foreign> <ref cRef="Matth." loc="12,9">Matth. 12,9</ref>; <foreign xml:lang="gez">ፍልስ፡ እምዝየ፡</foreign> <ref cRef="Joh." loc="7,3">Joh. 7,3</ref>; <foreign xml:lang="gez">ኢትፍልሱ፡ እምቤት፡ ለቤት፡</foreign> <ref cRef="Luc. " loc="10,7">Luc.  10,7</ref>; <foreign xml:lang="gez">ሖር፡ ፍልስ፡ ብሔረ፡ ይሁዳ፡</foreign> <foreign xml:lang="grc">ἐκχώρησον</foreign> <ref cRef="Am." loc="7,12">Am. 7,12</ref>. <sense n="α" xml:id="D1aα"> <cit type="translation" xml:lang="la">
                                        <quote> huc illuc migrare </quote>
                                    </cit>, <cit type="translation" xml:lang="la">
                                        <quote> vagari </quote>
                                    </cit>, <ref cRef="Ps." loc="108,9">Ps. 108,9</ref>.</sense> <sense n="β" xml:id="D1aβ"> <cit type="translation" xml:lang="la">
                                        <quote> transire ad partes </quote>
                                    </cit> alicujus: <foreign xml:lang="gez">ፈለሱ፡ ኀበ፡ ዳዊት፡</foreign> 1 <ref cRef="Par." loc="12,8">Par. 12,8</ref>; Hyrcanus <foreign xml:lang="gez">ፈለሰ፡ ኀበ፡ ሰዱቃዊያን፡</foreign> <ref cRef="Jsp." loc=" p. 300">Jsp.  p. 300</ref>; <foreign xml:lang="gez">ዘፈለሰ፡ እምአይሁድ፡ ኀበ፡ እስላም፡</foreign> <ref cRef="Abush. " loc="50.">Abush.  50.</ref> </sense> </sense> <sense n="b" xml:id="D1b"> <cit type="translation" xml:lang="la">
                                    <quote> secedere </quote>
                                </cit>: <foreign xml:lang="gez">ቅዱሳን፡ ፈለሱ፡ ገዳመ፡ ወመነኑ፡ ዓለመ፡</foreign> <ref cRef="Clem." loc="153.">Clem. f. 153.</ref> </sense> <sense n="c" xml:id="D1c"> <cit type="translation" xml:lang="la">
                                    <quote> excedere </quote>
                                </cit>, <cit type="translation" xml:lang="la">
                                    <quote> recedere </quote>
                                </cit>: astra <foreign xml:lang="gez">ኢይስሕቱ፡ ወኢይፈልሱ፡ እምነ፡ ዐቅሞሙ፡</foreign> <ref cRef="Sir." loc="43,10">Sir. 43,10</ref>; <foreign xml:lang="gez">ከመ፡ ኢይፍልሱ፡ እምዝንቱ፡ ግብር፡</foreign> <ref cRef="Esth." loc="9,27">Esth. 9,27</ref>.</sense> <sense n="d" xml:id="D1d"> <cit type="translation" xml:lang="la">
                                    <quote> emigrare </quote>
                                </cit>, <cit type="translation" xml:lang="la">
                                    <quote> demigrare </quote>
                                </cit>, <ref cRef="Kuf." loc="10">Kuf. 10</ref>; <ref cRef="4 Esr." loc="3,11">4 Esr. 3,11</ref>; in specie</sense> <sense n="e" xml:id="D1e"> <cit type="translation" xml:lang="la">
                                    <quote> deportari </quote>
                                </cit>, <cit type="translation" xml:lang="la">
                                    <quote> exsulare </quote>
                                </cit>: <foreign xml:lang="gez">ይሁዳ፡ ፈለሰ፡</foreign> (<foreign xml:lang="gez">ፍጹመ፡ ፍልሰተ፡</foreign>) <ref cRef="Jer." loc="13,19">Jer. 13,19</ref>; <ref cRef="Jer." loc="24,5">Jer. 24,5</ref>; <ref cRef="Jer." loc="31,11">Jer. 31,11</ref>; <ref cRef="Thren." loc="1,3">Thren. 1,3</ref>; <ref cRef="4 Reg." loc="17,23">4 Reg. 17,23</ref>; <ref cRef="4 Reg." loc="25,27">4 Reg. 25,27</ref>.</sense> <sense n="f" xml:id="D1f"> <cit type="translation" xml:lang="la">
                                    <quote> peregrinari </quote>
                                </cit>: <foreign xml:lang="gez">ፍልስ፡ ውስተ፡ ይእቲ፡ ምድር፡</foreign> <ref cRef="Kuf." loc="24">Kuf. 24</ref>; <ref cRef="Num." loc="15,14">Num. 15,14</ref>; <foreign xml:lang="gez">ፈላስያን፡ እለ፡ ፈለሱ፡ ውስተ፡ እስራኤል፡</foreign> <ref cRef="Hez." loc="14,7">Hez. 14,7</ref>; <foreign xml:lang="gez">ነገድ፡ ዘፈለሰ፡ ፈላሲ፡ ኀቤሁ፡</foreign> <ref cRef="Hez." loc="47,23">Hez. 47,23</ref>; <foreign xml:lang="gez">ኀበ፡ ፈለሱ፡</foreign> <foreign xml:lang="grc">ἡ παροικία αὐτῶν</foreign> <ref cRef="Judith" loc="5,9">Judith 5,9</ref>; <ref cRef="Hebr. " loc="11,9">Hebr.  11,9</ref>.</sense> <sense n="g" xml:id="D1g"> <cit type="translation" xml:lang="la">
                                    <quote> decedere </quote>
                                </cit> e vita, <cit type="translation" xml:lang="la">
                                    <quote> obire </quote>
                                </cit>: <ref cRef="Joh." loc="13,1">Joh. 13,1</ref>; <ref cRef="Phil." loc="1,23">Phil. 1,23</ref>; <foreign xml:lang="gez">ሀቦ፡ አክሊለ፡ ምክሕ፡ አመ፡ ይፈልስ፡ እምዝንቱ፡ ዓለም፡</foreign> <ref cRef="Did." loc="17">Did. 17</ref>; Lud. ex <ref cRef="Sx. Teq." loc="6 Enc.">Sx. Teq. 6 Enc.</ref>
                            </sense> </sense> <sense n="2" xml:id="D2"> de inanimatis et animatis, <sense n="a" xml:id="D2a"> <cit type="translation" xml:lang="la">
                                    <quote> transferri </quote>
                                </cit>: <foreign xml:lang="grc">μετῆλθε</foreign> <ref cRef="1 Reg." loc="5,8">1 Reg. 5,8</ref>; <foreign xml:lang="grc">μετατίθεσθαι</foreign> <ref cRef="Ps." loc="45,2">Ps. 45,2</ref>; <ref cRef="Hebr." loc="7,12">Hebr. 7,12</ref>; <foreign xml:lang="gez">ፍልስ፡</foreign> <foreign xml:lang="grc">μετάβηθι</foreign> (de monte) <ref cRef="Matth." loc="17,20">Matth. 17,20</ref>; Henoch <foreign xml:lang="gez">ፈለሰ፡</foreign> <foreign xml:lang="grc">οὐχ εὑρίσκετο</foreign> <ref cRef="Gen." loc="5,24">Gen. 5,24</ref>; <ref cRef="Hebr." loc="11,5">Hebr. 11,5</ref>; <foreign xml:lang="grc">μετετέθη</foreign> <ref cRef="Sap." loc="4,10">Sap. 4,10</ref>.</sense> <sense n="b" xml:id="D2b"> <cit type="translation" xml:lang="la">
                                    <quote> mutari </quote>
                                </cit>: <foreign xml:lang="gez">ከመ፡ የሀሉ፡ ዝክርከ፡ እንበለ፡ ፈሊስ፡</foreign> <ref cRef="Sx. Hed." loc="12 Enc.">Sx. Hed. 12 Enc.</ref>
                            </sense> </sense> </sense>
               
            </entry>
