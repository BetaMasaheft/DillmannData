               <sense xml:id="Dmain" xml:lang="la" source="#dillmann"> <gramGrp> <pos expand="Substantivum">subst.</pos> <gen value="not found">m.</gen> </gramGrp>, rarius <gramGrp> <gen value="femininus">fem.</gen> </gramGrp> (ut <ref cRef="3 Reg." loc="16,24">3 Reg. 16,24</ref> <lbl expand="varia lectio">var.</lbl>) [huic nomini plerique conferunt samar. <foreign xml:lang="he">טבר</foreign> <cit type="translation" xml:lang="la"><quote> mons </quote></cit>, <foreign xml:lang="he">טַבּוּר</foreign> <cit type="translation" xml:lang="la"><quote> locus editus </quote></cit>, <foreign xml:lang="he">טִיבוּר</foreign> <cit type="translation" xml:lang="la"><quote> umbilicus </quote></cit>; sed etiam arab. <foreign xml:lang="ar">دُبْرٌ</foreign> <cit type="translation" xml:lang="la"><quote> tergum </quote></cit> ab <cit type="translation" xml:lang="la"><quote> eminendo </quote></cit> dictum videtur, et significationes radicum <foreign xml:lang="ar">دبر</foreign> et <foreign xml:lang="he">דבר</foreign> <cit type="translation" xml:lang="la"><quote> pone esse </quote></cit> al. secundariae sunt; vicinum est I <foreign xml:lang="gez">ደበበ፡</foreign>] <lbl expand="pluralis">Pl.</lbl> <foreign xml:lang="gez">አድባር፡</foreign>, <lbl expand="pluralis">Pl.</lbl> <lbl expand="pluralis">Pl.</lbl> <foreign xml:lang="gez">አድባራት፡</foreign> <sense n="1" xml:id="D1"> <cit type="translation" xml:lang="la"><quote> mons </quote></cit> <ref cRef="Gen." loc="22,14">Gen. 22,14</ref>; <ref cRef="Ex." loc="19,2">Ex. 19,2</ref> <lbl expand="sequens">seq.</lbl>; <ref cRef="Ex." loc="24,">Ex. 24,</ref> 2 <lbl expand="sequens">seq.</lbl>; <ref cRef="Deut." loc="4,16">Deut. 4,16</ref>; <ref cRef="Ps." loc="2,6">Ps. 2,6</ref>; <ref cRef="Ps. " loc="14,1">Ps.  14,1</ref>; <ref cRef="Ps." loc="23,3">Ps. 23,3</ref>; <foreign xml:lang="gez">ደብር፡ ነዋኅ፡</foreign> <ref cRef="Job" loc="40,15">Job 40,15</ref>; <ref cRef="Jes." loc="14,13">Jes. 14,13</ref>; <ref cRef="Jes." loc="25,6">Jes. 25,6</ref>; <foreign xml:lang="gez">ደብር፡ ዐቢይ፡ ወነዋኅ፡</foreign> <ref cRef="Jes." loc="57,7">Jes. 57,7</ref>; <ref cRef="Jer." loc="3,6">Jer. 3,6</ref>; <foreign xml:lang="gez">ደብረ፡ ዘይት፡</foreign> <ref cRef="Zach." loc="14,4">Zach. 14,4</ref>; <foreign xml:lang="gez">ደብረ፡ ቅብእ፡</foreign> <ref cRef="2 Reg." loc="15,30">2 Reg. 15,30</ref>; <ref cRef="Matth." loc="4,8">Matth. 4,8</ref>; <ref cRef="Matth." loc="5,1">Matth. 5,1</ref>; <ref cRef="Matth." loc="5,14">Matth. 5,14</ref>; <ref cRef="Matth. " loc="17,20">Matth.  17,20</ref>; <ref cRef="Matth." loc="21,21">Matth. 21,21</ref> al. crebro; <foreign xml:lang="gez">ደብር፡ ብዱት፡</foreign> (<gramGrp> <gen value="femininus">fem.</gen> </gramGrp>) <ref cRef="Sx. Nah." loc="13">Sx. Nah. 13</ref>; <foreign xml:lang="gez">አድባር፡ ነዋኃት፡</foreign> <ref cRef="Gen." loc="7,19">Gen. 7,19</ref>; <ref cRef="Ps." loc="94,4">Ps. 94,4</ref>; <ref cRef="Jer. " loc="2,20">Jer.  2,20</ref>; <ref cRef="Deut." loc="12,2">Deut. 12,2</ref>; <foreign xml:lang="gez">አድባር፡ ነዋኃን፡</foreign> <ref cRef="Hen." loc="1,6">Hen. 1,6</ref>; <foreign xml:lang="gez">አድባር፡ ነዋኅ፡</foreign> <ref cRef="Jes." loc="14,13">Jes. 14,13</ref>; <ref cRef="Jes." loc="30,25">Jes. 30,25</ref>; <ref cRef="Job" loc="9,5">Job 9,5</ref>; <ref cRef="Job" loc="28,9">Job 28,9</ref>; <ref cRef="Sir." loc="43,16">Sir. 43,16</ref>; <foreign xml:lang="gez">አድባረ፡ እስራኤል፡</foreign> <ref cRef="Hez." loc="6,2">Hez. 6,2</ref>; <ref cRef="2 Reg." loc="1,21">2 Reg. 1,21</ref>; 4 <ref cRef="Reg." loc="17,10">Reg. 17,10</ref>; <ref cRef="Hen." loc="18,6">Hen. 18,6</ref>; <ref cRef="Hen." loc="24,2">Hen. 24,2</ref>; <ref cRef="Apoc." loc="17,9">Apoc. 17,9</ref> al.; de montibus ignivomis <lbl expand="videas">vid.</lbl> <ref cRef="Clem." loc="32.">Clem. f. 32.</ref> <sense n="a" xml:id="D1a"> in Gen.: <cit type="translation" xml:lang="la"><quote> montanus </quote></cit>: ut: <foreign xml:lang="gez">ሣዕረ፡ ደብር፡</foreign> <foreign xml:lang="grc">χόρτος ὀρεινός Ρ</foreign>rov. 27,24. 
</sense> <sense n="b" xml:id="D1b"> <foreign xml:lang="gez">ደብር፡</foreign> nonnumquam <cit type="translation" xml:lang="la"><quote> montes </quote></cit>, <cit type="translation" xml:lang="la"><quote> montanum </quote></cit>, <cit type="translation" xml:lang="la"><quote> regio montana </quote></cit>, ut <ref cRef="Deut." loc="33,2">Deut. 33,2</ref>; <ref cRef="Jos." loc="17,16">Jos. 17,16</ref>; <ref cRef="Jos." loc="20,7">Jos. 20,7</ref>; <ref cRef="Jos." loc="21,11">Jos. 21,11</ref>; <ref cRef="Jud." loc="1,19">Jud. 1,19</ref>; <ref cRef="1 Reg." loc="22,14">1 Reg. 22,14</ref>; <foreign xml:lang="gez">ደብረ፡ አሱር፡</foreign> <ref cRef="Kuf." loc="8,">Kuf. 8,</ref> et crebrius <foreign xml:lang="gez">አድባር፡</foreign> <ref cRef="Jos. " loc="11,21">Jos.  11,21</ref>; <foreign xml:lang="gez">አድባረ፡ አርሞን፡</foreign> <ref cRef="Sir." loc="24,13">Sir. 24,13</ref>; <foreign xml:lang="gez">አድባረ፡ አራራት፡</foreign> <ref cRef="Kuf." loc="8">Kuf. 8</ref>; <ref cRef="Gen." loc="8,4">Gen. 8,4</ref>; <foreign xml:lang="grc">ἡ ὀρεινή</foreign> <ref cRef="Jud." loc="1,9">Jud. 1,9</ref>; <ref cRef="Jos." loc="2,16">Jos. 2,16</ref>; <foreign xml:lang="gez">አሕጉረ፡ አድባር፡</foreign> <ref cRef="Jer." loc="40,13">Jer. 40,13</ref>; <ref cRef="Judith" loc="1,6">Judith 1,6</ref>; <ref cRef="Judith" loc="2,22">Judith 2,22</ref>; <ref cRef="Judith" loc="5,3">Judith 5,3</ref>. 
</sense> <sense n="c" xml:id="D1c"> <foreign xml:lang="gez">ደብረ፡ ሀገር፡</foreign> <ref cRef="2 Reg." loc="20,19">2 Reg. 20,19</ref> et <foreign xml:lang="gez">ደብረ፡ አህጉር፡</foreign> <cit type="translation" xml:lang="la"><quote> metropolis </quote></cit> <ref cRef="Jos." loc="14,15">Jos. 14,15</ref>; <ref cRef="Jos. " loc="15,13">Jos.  15,13</ref>; <ref cRef="Jos." loc="21,11">Jos. 21,11</ref>; <ref cRef="Jes." loc="1,26">Jes. 1,26</ref>. 
</sense> <sense n="d" xml:id="D1d"> (<foreign xml:lang="gez">በዓል፡</foreign>) <foreign xml:lang="gez">ዘደብረ፡ ታቦር፡</foreign> <cit type="translation" xml:lang="la"><quote> festum transfigurationis </quote></cit> <lbl expand="videas">vid. sub</lbl> <foreign xml:lang="gez">ታቦር፡</foreign>; <foreign xml:lang="gez">ዘደብረ፡ ዘይት፡</foreign> <cit type="translation" xml:lang="la"><quote> dominica montis oliveti </quote></cit> est dominica jejunii quadragesimalis quarta, Cod. Mus. Brit. XXXII (<ref cRef="Catal." loc="38">Catal. p. 38</ref>); <lbl expand="conferas">cfr.</lbl> Lud. Comm. hist. p. 386. 
</sense> 
</sense> <sense n="2" xml:id="D2"> <cit type="translation" xml:lang="la"><quote> finis </quote></cit>, <cit type="translation" xml:lang="la"><quote> terminus </quote></cit> (cfr. etiam amh. <foreign xml:lang="gez">ድምብር፡</foreign> nec non aeth. <foreign xml:lang="gez">ደወል፡</foreign>) terrae, regionis: <foreign xml:lang="gez">ደብር፡</foreign> <ref cRef="Gad. Ad." loc="167">Gad. Ad. f. 167</ref>; <foreign xml:lang="gez">አድባር፡</foreign> <foreign xml:lang="grc">ὅρια</foreign> <cit type="translation" xml:lang="la"><quote> fines </quote></cit> <ref cRef="Gen." loc="10,19">Gen. 10,19</ref>; <ref cRef="Gen." loc="47,21">Gen. 47,21</ref>; <ref cRef="Ex." loc="7,27">Ex. 7,27</ref>; <ref cRef="Ex." loc="10,4">Ex. 10,4</ref>; <ref cRef="Ex. " loc="10,14">Ex.  10,14</ref>; <ref cRef="Ex." loc="13,7">Ex. 13,7</ref>; <ref cRef="Ex." loc="23,31">Ex. 23,31</ref>; <ref cRef="Num." loc="32,33">Num. 32,33</ref>; <ref cRef="Deut." loc="3,16">Deut. 3,16</ref>; <ref cRef="Mich." loc="5,5">Mich. 5,5</ref>; <ref cRef="Matth." loc="4,13">Matth. 4,13</ref>; in specie <foreign xml:lang="gez">አድባራት፡</foreign> <cit type="translation" xml:lang="la"><quote> dioecesis </quote></cit> episcopi, F.M. 31,1 al. crebro; <ref cRef="Sx. Masc." loc="13.">Sx. Masc. 13.</ref> 
</sense> <sense n="3" xml:id="D3"> <sense n="a" xml:id="D3a"> <cit type="translation" xml:lang="la"><quote> monasterium </quote></cit> sive <cit type="translation" xml:lang="la"><quote> coenobium </quote></cit> (<lbl expand="videas">vid.</lbl> <bibl><ptr target="bm:Ludolf1681Historia"/></bibl>. <subc>III,3</subc>,40): Reg. Pach.; <ref cRef="Sx. Masc." loc="7">Sx. Masc. 7</ref>; <ref cRef="Sx. Teq." loc="1">Sx. Teq. 1</ref>; <ref cRef="Sx. Mag." loc="27">Sx. Mag. 27</ref>; <foreign xml:lang="gez">ደብረ፡ ሊባኖስ፡</foreign> monasterium Abyssiniae celeberrimum; nomina multorum aliorum recenset Lud. <lbl expand="lexicon">lex.</lbl> Aeth. <lbl expand="columna">col.</lbl> 503; <lbl expand="pluralis">Pl.</lbl> <foreign xml:lang="gez">አድባር፡</foreign>, ut: <foreign xml:lang="gez">አድባረ፡ ገዳም፡</foreign> Lud. ex <ref cRef="Sx. Jac." loc="14 ">Sx. Jac. 14 </ref>Enc.; plerumque <foreign xml:lang="gez">አድባራት፡</foreign> <ref cRef="Gad. T.H." loc="">Gad. T.H. </ref>; <ref cRef="Abush." loc="50">Abush. 50</ref>; Sx. <ref cRef="Masc." loc="7 ">Masc. 7 </ref>al. 
</sense> <sense n="b" xml:id="D3b"> <cit type="translation" xml:lang="la"><quote> regio monasteriorum </quote></cit>, ubi plura vel multa coenobia sunt, ut: <foreign xml:lang="gez">መዳልወ፡ አልባብ፡ ተሰምየት፡ እስከ፡ ዮም፡ ደብሩ፡</foreign> <ref cRef="Sx. Mag." loc="27 ">Sx. Mag. 27 </ref>Enc. (<lbl expand="videas">vid.</lbl> <foreign xml:lang="gez">መድሎት፡</foreign>); <foreign xml:lang="gez">አባ፡ መቃርስ፡ ዘደብረ፡ አስቄጥስ፡</foreign> <ref cRef="Sx. Masc." loc="10.">Sx. Masc. 10.</ref> 
</sense> 
</sense> <sense n="4" xml:id="D4"> <foreign xml:lang="gez">ደብር፡</foreign>, <lbl expand="pluralis">Pl.</lbl> <foreign xml:lang="gez">ደብራት፡</foreign> vid. sub <foreign xml:lang="gez">ዳብር፡</foreign> 
</sense> </sense>
               
