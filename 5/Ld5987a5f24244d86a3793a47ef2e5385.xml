            <entry xml:id="Ld5987a5f24244d86a3793a47ef2e5385" n="368">
               <form>
                  <foreign xml:lang="gez">ተለክአ</foreign>
               </form>
               <sense xml:id="Dmain" xml:lang="la" source="#dillmann"> <subc>III,1</subc> <gramGrp> <pos expand="passivum">pass.</pos> </gramGrp> et <gramGrp> <pos expand="reflexivum">refl.</pos> </gramGrp> <sense n="1" xml:id="D1"> <cit type="translation" xml:lang="la"><quote> affigi </quote></cit>, <cit type="translation" xml:lang="la"><quote> inhaerere </quote></cit>: <foreign xml:lang="gez">ወተለክአት፡ ሐመር፡ ማእከለ፡ እብን፡ ልዑላት፡</foreign> <ref cRef="Act." loc="27,41">Act. 27,41</ref> Platt; (<cit type="translation" xml:lang="la"><quote> repetierunt sermonem - </quote></cit> <foreign xml:lang="gez">ነገረ፡</foreign> -) <foreign xml:lang="gez">እስከ፡ ተለክአ፡ ውስተ፡ ልቡ፡</foreign> <cit type="translation" xml:lang="la"><quote> donec infixus esset </quote></cit> (inhaereret) <cit type="translation" xml:lang="la"><quote> ejus cordi </quote></cit> <ref cRef="Jsp. " loc="307.">Jsp.  p. 307.</ref> {<ref target="#c1426"/> <foreign xml:lang="gez">ሶበ፡ ተንጦልዐ፡ ሥጋሁ፡ ለኢየሱስ፡ በስፍሐተ፡ አእዳው፡ ወተለክኦ፡ አእጋር፡</foreign> M.M. f. 102}. 
</sense> <sense n="2" xml:id="D2"> impressi signis <cit type="translation" xml:lang="la"><quote> signari </quote></cit>: <foreign xml:lang="gez">አእጋሪከ፡ በቅንዋት፡ እለ፡ ተለክኡ፡</foreign> Cod. Mus. Br. LIV, 7, f; <foreign xml:lang="gez">ዲናር፡ ዘተለክአ፡ እምትካት፡</foreign> <cit type="translation" xml:lang="la"><quote> denarius signatus </quote></cit> (cusus) <cit type="translation" xml:lang="la"><quote> olim </quote></cit>, Hist. VII puer.; <foreign xml:lang="gez">ዲናር፡ ዘተለክአ፡ በስሙ፡ ለቄሳር፡</foreign> M.M. f. 317. 
</sense> <sense n="3" xml:id="D3"> <cit type="translation" xml:lang="la"><quote> imprimi </quote></cit>, <cit type="translation" xml:lang="la"><quote> inscribi </quote></cit>, <cit type="translation" xml:lang="la"><quote> consignari </quote></cit>: amorem in corde hominis, Org.; <foreign xml:lang="gez">ተለክአ፡ ዲብዲቆን፡ ዘማኅበረ፡ ነቢያት፡</foreign> <cit type="translation" xml:lang="la"><quote> consignatum est diptychon omnium prophetarum </quote></cit>, <ref cRef="Org." loc="3.">Org. 3.</ref> 
</sense> </sense>
               
            <sense xml:id="Tmain" xml:lang="en" source="#traces"><cit type="transcription" xml:lang="gez"><quote>talakʾa</quote></cit>, <foreign xml:lang="gez">ተለክዐ፡</foreign> <cit type="transcription" xml:lang="gez"><quote>talakʿa</quote></cit> <sense n="G" xml:id="TG"> <foreign xml:lang="gez">ተለክዐ፡</foreign> <cit type="transcription" xml:lang="gez"><quote>talakʿa</quote></cit> <subc>III,1</subc> <foreign xml:lang="gez">ይትለካዕ፡</foreign> «<cit type="translation" xml:lang="fr"><quote>être écrit</quote></cit>, <cit type="translation" xml:lang="fr"><quote>être imprimé</quote></cit>, etc.» ― <foreign xml:lang="am">ተጣፈ፡</foreign> Ms. <ref type="BM" target="BNFabb217">BNFabb217</ref>, fol. 16v. <lb/>
 <bibl><ptr target="bm:Grebaut1952SuppLexicon"/><citedRange unit="page"> 38</citedRange></bibl> 
</sense> <sense n="L" xml:id="TL"> <foreign xml:lang="gez">ተለክአ</foreign> <cit type="transcription" xml:lang="gez"><quote>talakʾa</quote></cit> passive {of <lbl expand="videas">vid.</lbl> <foreign xml:lang="gez">ለክአ</foreign>} <bibl><ptr target="bm:Leslau1987dic"/><citedRange unit="page"> 313a</citedRange></bibl> 
</sense> </sense></entry>
