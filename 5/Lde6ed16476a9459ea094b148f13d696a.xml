               <sense xml:lang="la" source="#dillmann" xml:id="Dmain"> <subc>II,1</subc> <cit type="translation" xml:lang="la">
                     <quote>moleste</quote>
                  </cit> vel <cit type="translation" xml:lang="la">
                     <quote>importune urgere</quote>
                  </cit>, <cit type="translation" xml:lang="la">
                     <quote>defatigare</quote>
                  </cit>, <cit type="translation" xml:lang="la">
                     <quote>molestum</quote>
                  </cit> vel <cit type="translation" xml:lang="la">
                     <quote>oneri esse</quote>
                  </cit>, <cit type="translation" xml:lang="la">
                     <quote>negotium facessere</quote>
                  </cit>, <lbl expand="construitur cum">c.c.</lbl>
                  <gramGrp>
                     <case value="accusativus">Acc.</case>
                  </gramGrp>
                  <lbl expand="persona">pers.</lbl>: <foreign xml:lang="grc">κόπους παρέχειν
