         <div>
            <entry xml:id="Lc78ed54ba6454ab5b1cdbd1f0b6bf278" n="3795">
               <form>
                  <foreign xml:lang="gez">አብልዐ</foreign>
               </form>
               <sense xml:lang="la" source="#dillmann" xml:id="Dmain"> <subc>II,1</subc> <sense n="1" xml:id="D1">
                     <cit type="translation" xml:lang="la">
                        <quote>comedendum dare</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>cibare</quote>
                     </cit>; <gramGrp>
                        <pos expand="absolute">abs.</pos>
                     </gramGrp>
                     <foreign xml:lang="gez">ታበልዕኒ፡ ወታሰትይኒ፡ </foreign>
                     <ref cRef="Sir." loc="29,25">Sir. 29,25</ref>; <lbl expand="cum">c.</lbl>
                     <gramGrp>
                        <case value="accusativus">Acc.</case>
                     </gramGrp>
                     <lbl expand="persona">pers.</lbl>: <foreign xml:lang="gez">እመ፡ ርኅበ፡ ጸላኢከ፡ አብልዖ፡ </foreign>
                     <ref cRef="Prov." loc="25,21">Prov. 25,21</ref>; <ref cRef="Matth." loc="25,35">Matth. 25,35</ref>; <ref cRef="Rom." loc="12,20">Rom. 12,20</ref>; <lbl expand="cum">c.</lbl>
                     <lbl expand="duplex">dupl.</lbl>
                     <gramGrp>
                        <case value="accusativus">Acc.</case>
                     </gramGrp>: <ref cRef="1 Cor." loc="3,2">1 Cor. 3,2</ref>; <ref cRef="Num." loc="11">Num. 11</ref>; <ref cRef="Num." loc="4">Num. 4</ref>; <foreign xml:lang="gez">አብልዐኒ፡ ዘብከ፡</foreign>
                     <ref cRef="Sir." loc="29,26">Sir. 29,26</ref>; <foreign xml:lang="gez">የኀሥሥ፡
                        ሎሙ፡ ዘያበልዖሙ፡</foreign>
                     <ref cRef="Job" loc="39,29">Job 39,29</ref>; <foreign xml:lang="gez">አነ፡ ኣበልዖሙ፡
                        ጻዕረ፡</foreign>
                     <ref cRef="Jer." loc="9,15">Jer. 9,15</ref>; <cit type="translation" xml:lang="la">
                        <quote>devorandum dare</quote>
                     </cit>: <foreign xml:lang="gez">አብልዐኒያ፡ ለይእቲ፡ መጽሐፍ፡</foreign>
                     <ref cRef="Hez." loc="3,2">Hez. 3,2</ref>.</sense>
                  <sense n="2" xml:id="D2">
                     <cit type="translation" xml:lang="la">
                        <quote>ad edendum adigere</quote>
                     </cit>, aliquem: <foreign xml:lang="gez">ከመ፡ ኢያብልዖ፡ </foreign>
                     <ref cRef="Dan." loc="1,8">Dan. 1,8</ref>; vel <cit type="translation" xml:lang="la">
                        <quote>depascere sinere</quote>
                     </cit>
                     <lbl expand="cum">c.</lbl>
                     <lbl expand="duplex">dupl.</lbl> Acc: <foreign xml:lang="gez">ለእመ፡ አብልዐ፡ ገራህተ፡
                        ሰብእ፡ እንስሳሁ፡</foreign>
                     <ref cRef="Ex." loc="22,4">Ex. 22,4</ref>.</sense>
               </sense>
               
