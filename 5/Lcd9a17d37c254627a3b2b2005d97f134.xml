                        <foreign xml:lang="gez">መቅደመ፡ ጳውሎስ፡</foreign> <bibl><ptr target="bm:Abbadie1859Cat"/><citedRange unit="page">148</citedRange></bibl>.
                     </sense>
                     <sense n="d" xml:id="D1d"> adverbialiter usurpatur <foreign xml:lang="gez">መቅድመ፡ </foreign>
                        <cit type="translation" xml:lang="la">
                           <quote>primum</quote>
                        </cit>, <cit type="translation" xml:lang="la">
                           <quote>primo</quote>
                        </cit>, <cit type="translation" xml:lang="la">
                           <quote>initio</quote>
                        </cit>, <cit type="translation" xml:lang="la">
                           <quote>primitus</quote>
                        </cit>, <cit type="translation" xml:lang="la">
                           <quote>potissimum</quote>
                        </cit>: <foreign xml:lang="grc">ἐν τῇ ἀρχῇ</foreign>
                        <ref cRef="Prov." loc="8,23">Prov. 8,23</ref>; <foreign xml:lang="grc">πρῶτον</foreign>
                        <ref cRef="Matth." loc="6,33">Matth. 6,33</ref>; <ref cRef="Clem." loc="268">Clem. f. 268</ref>; <ref cRef="1 Thess. " loc="4,16">1 Thess. 4,16</ref>
                        Platt; <foreign xml:lang="gez">መቅድመ፡</foreign> (<lbl expand="oppositum, -o, -nitur">opp.</lbl>
                        <foreign xml:lang="gez">ድኅረ፡</foreign>) <ref cRef="Jer." loc="27,17">Jer.
                           27,17</ref>
                        <lbl expand="versio altera">vers. alt.</lbl>; <foreign xml:lang="gez">ውእቱ፡
                           መቅድመ፡ ተቀብረ፡</foreign>
                        <ref cRef="Kuf." loc="4">Kuf. 4</ref>; <foreign xml:lang="gez">ውእቱ፡ መቅድመ፡
                           ገብረ፡ በዓለ፡ መጸለት፡</foreign>
                        <ref cRef="Kuf." loc="16">Kuf. 16</ref>; <foreign xml:lang="gez">መቅድመ፡ </foreign>
                        <cit type="translation" xml:lang="la">
                           <quote>ante omnia</quote>
                        </cit>
                        <ref cRef="Kedr f." loc="39">Kedr f. 39</ref>; <foreign xml:lang="gez">መቅድመ፡
                           ፈድፋደ፡ </foreign>
                        <cit type="translation" xml:lang="la">
                           <quote>maxime vero </quote>
                        </cit>seu<cit type="translation" xml:lang="la">
                           <quote> potissimum</quote>
                        </cit>
                        <ref cRef="Lit." loc="170,2">Lit. 170,2</ref>.</sense>
                     <sense n="e" xml:id="D1e"> praepos. <sense n="α" xml:id="D1eα">
                           <cit type="translation" xml:lang="la">
                              <quote>ante</quote>
                           </cit>
                           <foreign xml:lang="gez">መቅድመ፡ ኵሉ፡</foreign>
                           <cit type="translation" xml:lang="la">
                              <quote>ante omnia</quote>
                           </cit> F.M. 27,1; fortasse <ref cRef="Hen." loc="61,7">Hen.
                           61,7</ref>.</sense>
                        <sense n="β" xml:id="D1eβ">
                           <cit type="translation" xml:lang="la">
                              <quote>coram</quote>
                           </cit>
                           <ref cRef="Hen." loc="48,2">Hen. 48,2</ref>.</sense>
                        <sense n="2" xml:id="D1e2">
