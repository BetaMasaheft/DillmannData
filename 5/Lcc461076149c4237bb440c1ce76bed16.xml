            <entry xml:id="Lcc461076149c4237bb440c1ce76bed16" n="8027">
               <form>
                  <foreign xml:lang="gez">ድኁን</foreign>
               </form>
               <sense xml:lang="la" source="#dillmann" xml:id="Dmain"> <gramGrp> <pos expand="participium">part.</pos> </gramGrp> <sense n="1" xml:id="D1"> <cit type="translation" xml:lang="la">
                                <quote> incolumis </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> sospes </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> salvus </quote>
                            </cit>: <ref cRef="Jos." loc="10,21">Jos. 10,21</ref> <lbl expand="varia lectio">var.</lbl>; igne non consumti sed inventi sunt <foreign xml:lang="gez">ድኁናነ፡</foreign> <ref cRef="Sx. Hed." loc="7 Enc.">Sx. Hed. 7 Enc.</ref> <sense n="a" xml:id="D1a"> sensu physico: <sense n="α" xml:id="D1aα"> <cit type="translation" xml:lang="la">
                                        <quote> integer </quote>
                                    </cit>, <cit type="translation" xml:lang="la">
                                        <quote> sanus </quote>
                                    </cit>: judex sit <foreign xml:lang="gez">ድኁነ፡ ምስማዕ፡ ወንጻሬ፡</foreign> F.N. 43,1.</sense> <sense n="β" xml:id="D1aβ"> <cit type="translation" xml:lang="la">
                                        <quote> salubris </quote>
                                    </cit>, de bestiis <ref cRef="Gad. Ad." loc="7.">Gad. Ad. f. 7.</ref> </sense> </sense> <sense n="b" xml:id="D1b"> sensu theologico: <cit type="translation" xml:lang="la">
                                    <quote> salvatus </quote>
                                </cit>, <cit type="translation" xml:lang="la">
                                    <quote> divinam salutem adeptus </quote>
                                </cit>, <foreign xml:lang="gez">ኢድኁን፡ አንተ፡ በሞተ፡ ኢየሱስ፡</foreign> <ref cRef="M.M." loc="f. 127">M.M. f. 127</ref>; Deus <foreign xml:lang="gez">ይረስዮ፡</foreign> (mortuum) <foreign xml:lang="gez">ምስለ፡ ድኁናን፡</foreign> <ref cRef="Genz." loc="91.">Genz. f. 91.</ref> </sense> </sense> <sense n="2" xml:id="D2"> <cit type="translation" xml:lang="la">
                                <quote> liber </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> immunis </quote>
                            </cit>, a militia <ref cRef="Deut." loc="24,5">Deut. 24,5</ref>; vituperio: <foreign xml:lang="gez">ድኁን፡ አነ፡ እምሐሜት፡</foreign> <ref cRef="Chrys. ho." loc="4">Chrys. ho. 4</ref>; <foreign xml:lang="gez">ድኁን፡</foreign> (scriptum est <foreign xml:lang="gez">ድኑን፡</foreign>) <foreign xml:lang="gez">እምነውር፡</foreign> F.N. 43,1; peccato: <foreign xml:lang="gez">ክርስቶስ፡ ድኁን፡ ውእቱ፡ እምኀጣውእ፡</foreign> Chrys. ho. 15; culpa: <foreign xml:lang="gez">ድኁን፡ እምዕዳ፡</foreign> F.N. 33,2.</sense> </sense>
               
