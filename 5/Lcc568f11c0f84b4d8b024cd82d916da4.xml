         <div>
            <entry xml:id="Lcc568f11c0f84b4d8b024cd82d916da4" n="9941">
               <form>
                  <foreign xml:lang="gez">ፍቅር</foreign>
               </form>
               <sense xml:lang="la" source="#dillmann" xml:id="Dmain">
                  <gramGrp>
                     <pos expand="Substantivum">subst.</pos>
                  </gramGrp>, <gramGrp>
                     <gen value="masculinum">m.</gen>
                  </gramGrp> et <gramGrp>
                     <gen value="femininus">fem.</gen>
                  </gramGrp> (ut <ref cRef="Cant." loc="8,6">Cant. 8,6</ref>; <ref cRef="Prov." loc="5,19">Prov. 5,19</ref>; <ref cRef="Matth." loc="24,12">Matth.
                  24,12</ref>), <cit type="translation" xml:lang="la">
                     <quote>amor</quote>
                  </cit>, viri et feminae: <foreign xml:lang="gez">ይነድድ፡ ፍቅር፡ ከመ፡ እሳት፡ </foreign>
                  <ref cRef="Sir." loc="9,8">Sir. 9,8</ref>; <ref cRef="Cant." loc="2,5">Cant.
                     2,5</ref>; <ref cRef="Cant." loc="5,8">Cant. 5,8</ref>; <ref cRef="Cant." loc="8,6">Cant. 8,6</ref>; <ref cRef="Cant." loc="8,7">Cant. 8,7</ref>; <ref cRef="Prov." loc="5,19">Prov. 5,19</ref>; <foreign xml:lang="gez">በዛቲ፡ ፍቅር፡
                     እንዘ፡ ንትፋቀር፡</foreign>
                  <ref cRef="Prov." loc="5,19">Prov. 5,19</ref>; <foreign xml:lang="gez">ፍቅረ፡
                     ብእሲት፡</foreign> (<foreign xml:lang="grc">ἔρως</foreign>) <ref cRef="Prov." loc="24,51">Prov. 24,51</ref>; <ref cRef="2 Reg. " loc="13,15">2 Reg.
                     13,15</ref>; <ref cRef="Jer." loc="2,2">Jer. 2,2</ref>; <ref cRef="Jer." loc="2,33">Jer. 2,33</ref>; rerum: <ref cRef="Jac." loc="4,4">Jac. 4,4</ref>;
                     <foreign xml:lang="gez">ፍቅረ፡ ወርቅ፡ </foreign>
                  <ref cRef="Asc. Jes." loc="3,28">Asc. Jes. 3,28</ref>; <foreign xml:lang="gez">ፍቅረ፡ ወይን፡</foreign> I <ref cRef="Sal. Mar." loc="27 ">Sal. Mar. 27 </ref>
                     (<lbl expand="videas">vid. sub</lbl>
                  <foreign xml:lang="gez">ዔረግ፡</foreign>). <sense n="a" xml:id="Da">
                     <cit type="translation" xml:lang="la">
                        <quote>dilectio</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>caritas</quote>
                     </cit>, <ref cRef="Koh." loc="9,1">Koh. 9,1</ref>; <ref cRef="Koh." loc="9,6">Koh. 9,6</ref>; <ref cRef="Matth." loc="24,12">Matth. 24,12</ref>; amicorum
                        <ref cRef="2 Reg. " loc="1,26">2 Reg. 1,26</ref>; <foreign xml:lang="grc">φιλία</foreign> (<cit type="translation" xml:lang="la">
                        <quote>amicitia</quote>
                     </cit>) <ref cRef="Prov." loc="10,12">Prov. 10,12</ref>; <ref cRef="Prov. " loc="15,17">Prov. 15,17</ref>; <ref cRef="Prov." loc="27,5">Prov.
                     27,5</ref>; <foreign xml:lang="gez">ፍቅረ፡ ቢጽ፡</foreign>
                     <ref cRef="Did." loc="7 ">Did. 7 </ref>(de <cit type="translation" xml:lang="la">
                        <quote>caritate</quote>
                     </cit>
                     <cit type="translation" xml:lang="la">
                        <quote>mutua</quote>
                     </cit>
                     <foreign xml:lang="gez">ተፋቅሮ፡</foreign> frequentare solent); Dei in homines:
                        <foreign xml:lang="gez">ፍቅረ፡ ዘለዓለም፡ አፍቀርኩከ፡</foreign>
                     <ref cRef="Jer." loc="38,3">Jer. 38,3</ref>; <ref cRef="2 Cor." loc="13,12">2
                        Cor. 13,12</ref>; 2 <ref cRef="Cor." loc="13,14">Cor. 13,14</ref>; <ref cRef="1 Joh." loc="2,15">1 Joh. 2,15</ref>; <ref cRef="1 Joh." loc="3,17">1
                        Joh. 3,17</ref>; al.; <foreign xml:lang="gez">እግዚአብሔር፡ ፍቅር፡ ውእቱ፡</foreign>
                     <ref cRef="1 Joh." loc="4,8">1 Joh. 4,8</ref>; hominum in <ref cRef="Deum Christumve Luc." loc="11,42">Deum Christumve Luc. 11,42</ref>;
                        <ref cRef="Joh." loc="5,42">Joh. 5,42</ref>; <ref cRef="Joh." loc="15,9">Joh. 15,9</ref>; <ref cRef="Joh." loc="15,10">Joh. 15,10</ref>; <ref cRef="Rom." loc="8,35">Rom. 8,35</ref>; <ref cRef="1 Joh." loc="2,5">1 Joh.
                        2,5</ref> al.; <foreign xml:lang="gez">ገብሩ፡ ላዕሌየ፡ ፍቅረ፡</foreign>
                     <ref cRef="Cant." loc="2,4">Cant. 2,4</ref>; <lbl expand="cum">c.</lbl> etiam
                        <lbl expand="cum">c.</lbl>
                     <foreign xml:lang="gez">ለ፡ </foreign>
                     <cit type="translation" xml:lang="la">
                        <quote>in</quote>
                     </cit> vel <cit type="translation" xml:lang="la">
                        <quote>erga</quote>
                     </cit>, ut: <foreign xml:lang="gez">ፍቅርየ፡ ለእጓለ፡ እመሕያው፡</foreign>
                     <ref cRef="Sx. Pag." loc="3.">Sx. Pag. 3.</ref>
                  </sense>
                  <sense n="b" xml:id="Db">
                     <foreign xml:lang="gez">ፍቅረ፡ ነግዳን፡</foreign>
                     <ref cRef="Sx. Teq." loc="21 ">Sx. Teq. 21 </ref>vel <foreign xml:lang="gez">ፍቅረ፡ እንግዳ፡</foreign>
                     <ref cRef="Kid." loc="16,">Kid. f. 16,</ref>
                     <cit type="translation" xml:lang="la">
                        <quote>hospitalitas.</quote>
                     </cit>
                  </sense>
                  <sense n="c" xml:id="Dc">
                     <cit type="translation" xml:lang="la">
                        <quote>epulum</quote>
                     </cit>
                     <cit type="translation" xml:lang="la">
                        <quote>amoris</quote>
                     </cit>
                     <cit type="translation" xml:lang="la">
                        <quote>Christiani</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>agape</quote>
                     </cit>, <ref cRef="Judae" loc="12">Judae 12</ref>; <ref cRef="2 Petr." loc="2,13">2 Petr. 2,13</ref>.</sense>
                  <sense n="d" xml:id="Dd">
                     <foreign xml:lang="gez">በፍቅር፡</foreign>
                     <cit type="translation" xml:lang="la">
                        <quote>amanter</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>benevole</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>libenter</quote>
                     </cit>: <ref cRef="Act." loc="28,7">Act. 28,7</ref>
                     <lbl expand="romanae editionis">rom.</lbl>; Lit. Orth.; Lud. ex <ref cRef="Sx. Ter" loc="12 ">Sx. Ter 12 </ref>Enc.</sense>
               </sense>
               
            </entry>
