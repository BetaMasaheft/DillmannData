               <sense xml:lang="la" source="#dillmann" xml:id="Dmain"> quadril. I (etiam <foreign xml:lang="gez">አንዘረ፡</foreign>) [<foreign xml:lang="syr">ܢܨܰܪ</foreign> <cit type="translation" xml:lang="la">
                            <quote> sonare </quote>
                        </cit>, <cit type="translation" xml:lang="la">
                            <quote> resonare </quote>
                        </cit>, <cit type="translation" xml:lang="la">
                            <quote> strepere </quote>
                        </cit>]; passim tamquam stirps <subc>II,1</subc> radicis <foreign xml:lang="gez">ነዘረ፡</foreign> flectitur; <cit type="translation" xml:lang="la">
                            <quote> concentum musicum edere </quote>
                        </cit>, <cit type="translation" xml:lang="la">
                            <quote> organum </quote>
                        </cit>vel <cit type="translation" xml:lang="la">
                            <quote> fides pulsare </quote>
                        </cit>, <cit type="translation" xml:lang="la">
                            <quote> tibia cantare </quote>
                        </cit>, <ref cRef="2 Reg." loc="6,5">2 Reg. 6,5</ref>; <foreign xml:lang="gez">የዐነዝሩ፡ ወይሰነቅዉ፡</foreign> <ref cRef="Job" loc="21,12">Job 21,12</ref>; <foreign xml:lang="gez">ዳዊት፡ የዐነዝር፡ በዕንዚራ፡</foreign> <ref cRef="2 Reg." loc="6,14">2 Reg. 6,14</ref>; <foreign xml:lang="gez">በብዕዛ፡ የዐነዝሩ፡</foreign> (v. <foreign xml:lang="gez">ያነዝሩ፡</foreign>) <foreign xml:lang="gez">ወይገንዩ፡</foreign> <ref cRef="1 Par." loc="25,3">1 Par. 25,3</ref>; <ref cRef="1 Par." loc="25,5">1 Par. 25,5</ref>; <foreign xml:lang="gez">ዐንዘርነ፡</foreign> <foreign xml:lang="grc">ηὐλήσαμεν</foreign> <ref cRef="Matth." loc="11,17">Matth. 11,17</ref>. <sense n="a" xml:id="Da"> <cit type="translation" xml:lang="la">
                                <quote> canere </quote>
                            </cit> vel <cit type="translation" xml:lang="la">
                                <quote> strepere </quote>
                            </cit>, de regina apium, Lud. e Libr. Myst. (<lbl expand="videas">vid.</lbl> <foreign xml:lang="gez">ንህብ፡</foreign>).</sense> <sense n="b" xml:id="Db"> <cit type="translation" xml:lang="la">
                                <quote> concentu laudare </quote>
                            </cit>aliquem, <lbl expand="cum">c.</lbl> <gramGrp> <case value="accusativus">Acc.</case> </gramGrp> <lbl expand="persona">pers.</lbl>, <ref cRef="Sx. Ter" loc="28 ">Sx. Ter 28 </ref>Enc. (si quidem lectio bona est). – <cit type="translation" xml:lang="la">
                                <quote> Rad </quote>
                            </cit>. <cit type="translation" xml:lang="la">
                                <quote> Abb </quote>
                            </cit>.: <foreign xml:lang="gez">ዐንዘረ፡ ዘ፡ መታ፡</foreign> </sense> </sense>
               
            </entry>
