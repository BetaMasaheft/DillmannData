            <entry xml:id="L9e472a97584f4265a362c93e4d16b856" n="5690">
               <form>
                  <foreign xml:lang="gez">ተአኀወ</foreign>
               </form>
               <sense xml:lang="la" source="#dillmann" xml:id="Dmain"> <subc>III,1</subc> (si non <foreign xml:lang="gez">ተኣኀወ፡</foreign> <subc>III,3</subc>) <sense n="1" xml:id="D1">
                     <cit type="translation" xml:lang="la">
                        <quote>fraterno</quote>
                     </cit> vel <cit type="translation" xml:lang="la">
                        <quote>sororio</quote>
                     </cit>
                     <cit type="translation" xml:lang="la">
                        <quote>vinculo</quote>
                     </cit>
                     <cit type="translation" xml:lang="la">
                        <quote>inter</quote>
                     </cit>
                     <cit type="translation" xml:lang="la">
                        <quote>sese</quote>
                     </cit>
                     <cit type="translation" xml:lang="la">
                        <quote>junctos</quote>
                     </cit>
                     <cit type="translation" xml:lang="la">
                        <quote>esse</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>fratrem</quote>
                     </cit>
                     <cit type="translation" xml:lang="la">
                        <quote>esse</quote>
                     </cit> vel <cit type="translation" xml:lang="la">
                        <quote>fieri</quote>
                     </cit> alicujus: <foreign xml:lang="gez">አትናቴዎስ፡ ወኢራኒ፡ ዘተአኀዉ፡ </foreign>
                     <ref cRef="Sx. Hed." loc="3 ">Sx. Hed. 3 </ref>Enc.; Christus <foreign xml:lang="gez">ኢፈቀደ፡ ይትአኀዎሙ፡ ለኪሩቤል፡ </foreign>
                     <ref cRef="Org." loc="6">Org. 6</ref>; <foreign xml:lang="gez">ተአኅዎት፡
                        መንፈሳዊት፡</foreign> (inter monachos intercedens) F.N. 10,6.</sense>
                  <sense n="2" xml:id="D2">
                     <cit type="translation" xml:lang="la">
                        <quote>fraterno</quote>
                     </cit>
                     <cit type="translation" xml:lang="la">
                        <quote>affectu</quote>
                     </cit>
                     <cit type="translation" xml:lang="la">
                        <quote>sese</quote>
                     </cit>
                     <cit type="translation" xml:lang="la">
                        <quote>amplecti</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>amorem</quote>
                     </cit>
                     <cit type="translation" xml:lang="la">
                        <quote>fraternum</quote>
                     </cit>
                     <cit type="translation" xml:lang="la">
                        <quote>colere</quote>
                     </cit>
                     <sense n="a" xml:id="D2a"> de consanguineis: <foreign xml:lang="gez">አልቦ፡ ገቢር፡ ምስሌከ፡ ተአኅዎ፡ </foreign>
                        <ref cRef="Kuf." loc="37.">Kuf. 37.</ref>
                     </sense>
                     <sense n="b" xml:id="D2b"> de Christianis: <foreign xml:lang="gez">ተአኅዎ፡</foreign>
                        <foreign xml:lang="grc">φιλαδελφία</foreign>
                        <ref cRef="2 Petr." loc="1,7">2 Petr. 1,7</ref>.</sense>
                     <sense n="c" xml:id="D2c"> de hominibus: <foreign xml:lang="gez">ተአኅዎ፡ ምስለ፡ ኵሎ፡ </foreign>
                           (<note>allgemeine Bruderliebe</note>) <ref cRef="Syn." loc="315.">Syn. f.
                           315.</ref>
                     </sense>
                     <sense n="3" xml:id="D23">
                        <cit type="translation" xml:lang="la">
                           <quote>foedus</quote>
                        </cit>
                        <cit type="translation" xml:lang="la">
                           <quote>amicitiae</quote>
                        </cit>
                        <cit type="translation" xml:lang="la">
                           <quote>jungere</quote>
                        </cit>, <cit type="translation" xml:lang="la">
                           <quote>amicitiam</quote>
                        </cit> vel <cit type="translation" xml:lang="la">
                           <quote>pacem</quote>
                        </cit>
                        <cit type="translation" xml:lang="la">
                           <quote>colere</quote>
                        </cit>
                        <lbl expand="cum">c.</lbl> aliquo: <foreign xml:lang="gez">በከመ፡ ተአኅውከ፡ ምስለ፡ </foreign>
                        <foreign xml:lang="grc">ἐφιλίασας</foreign>
                        <ref cRef="2 Par." loc="20,37">2 Par. 20,37</ref>; <foreign xml:lang="gez">ምስለ፡ ኵሉ፡ ሰብእ፡ ተአኅዉ፡</foreign>
                        <foreign xml:lang="grc">εἰρηνεύοντες</foreign>
                        <ref cRef="Rom." loc="12,18">Rom. 12,18</ref>; <lbl expand="cum">c.</lbl>
                        <gramGrp>
                           <case value="accusativus">Acc.</case>
                        </gramGrp>
                        <lbl expand="persona">pers.</lbl>
                        <ref cRef="1 Thess." loc="5,13">1 Thess. 5,13</ref> ant.; <foreign xml:lang="gez">ተአኅዎ፡</foreign>
                        <cit type="translation" xml:lang="la">
                           <quote>amicitia</quote>
                        </cit>
                        <ref cRef="Fal." loc="16,">Fal. f. 16,</ref> al. – <lbl expand="Vocabularium Aethiopicum, -a -a.">Voc. Ae.</lbl>: <foreign xml:lang="gez">ተአኀወ፡ ዘ፡ ተፋቀረ።</foreign>
                     </sense>
                  </sense>
               </sense>
               
