            <entry xml:id="L8ea514098e2c47c788f90e90a957fac3" n="10126">
               <form>
                  <foreign xml:lang="gez">ፍጻሜ</foreign>
               </form>
               <sense xml:lang="la" source="#dillmann" xml:id="Dmain"> <gramGrp> <pos expand="Substantivum">subst.</pos> </gramGrp> <sense n="1" xml:id="D1"> <cit type="translation" xml:lang="la">
                                <quote> expletio </quote>
                            </cit> <sense n="a" xml:id="D1a"> <cit type="translation" xml:lang="la">
                                    <quote> plenitudo </quote>
                                </cit>, <cit type="translation" xml:lang="la">
                                    <quote> modus </quote>
                                </cit> <cit type="translation" xml:lang="la">
                                    <quote> justus </quote>
                                </cit> <cit type="translation" xml:lang="la">
                                    <quote> ac </quote>
                                </cit> <cit type="translation" xml:lang="la">
                                    <quote> plenus </quote>
                                </cit>: <foreign xml:lang="gez">ፍጻሜ፡ መጠነ፡ አካል፡ ዘዕደው፡ ፳ ዓመት፡ ወእመአኮ፡ ፳ወ፭ ዓመት፡ ወለአንስት፡ ፲ወ፪ ዓመት፡ ወእመአኮ፡ ፲ወ፭ ዓመት፡</foreign> F.N. 24,3.</sense> <sense n="b" xml:id="D1b"> <cit type="translation" xml:lang="la">
                                    <quote> completio </quote>
                                </cit> i.e. <cit type="translation" xml:lang="la">
                                    <quote> justa </quote>
                                </cit> <cit type="translation" xml:lang="la">
                                    <quote> observatio </quote>
                                </cit> legis <ref cRef="Rom." loc="11,12">Rom. 11,12</ref>.</sense> <sense n="c" xml:id="D1c"> <cit type="translation" xml:lang="la">
                                    <quote> initiatio </quote>
                                </cit>, <foreign xml:lang="grc">τελείωσις</foreign> (<lbl expand="videas">vid.</lbl> <foreign xml:lang="gez">ፈጸመ፡</foreign> n. 1 et <foreign xml:lang="gez">ተፈጸመ፡</foreign> n. 3,g), <ref cRef="Ex." loc="29,27">Ex. 29,27</ref>; <ref cRef="Lev." loc="7,37">Lev. 7,37</ref>; <ref cRef="Lev." loc="8,22">Lev. 8,22</ref>; <ref cRef="Lev." loc="8,31">Lev. 8,31</ref>; <ref cRef="Lev. " loc="8,33">Lev.  8,33</ref>.</sense> </sense> <sense n="2" xml:id="D2"> <cit type="translation" xml:lang="la">
                                <quote> consummatio </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> finis </quote>
                            </cit>, <foreign xml:lang="grc">περασμός</foreign> <ref cRef="Koh." loc="4,8">Koh. 4,8</ref>; <ref cRef="Koh." loc="4,16">Koh. 4,16</ref>; <ref cRef="Koh." loc="12,12">Koh. 12,12</ref>; <foreign xml:lang="grc">τέλος</foreign> Ps. in tit., <lbl expand="videas">vid.</lbl> Lud. annot. ad <ref cRef="Ps." loc="4,1">Ps. 4,1</ref>; <foreign xml:lang="gez">ፍጻሜ፡ ዘይመጽእ፡</foreign> <ref cRef="Hen." loc="10,2">Hen. 10,2</ref>; <foreign xml:lang="gez">በፍጻሜ፡ ራብዑ፡ ኢዮቤልው፡</foreign> <ref cRef="Kuf." loc="4">Kuf. 4</ref>; <foreign xml:lang="gez">እምቅድመ፡ ፍጻሜ፡ ዓመት፡</foreign> F.N. 24,5; <foreign xml:lang="gez">ከመ፡ ለፍጻሜ፡ ያብጽሕ፡ ወይግበር፡ ኵሎ፡ ሰርጐ፡ ቤትከ፡</foreign> 1 <ref cRef="Par." loc="29,19">Par. 29,19</ref>. <sense n="a" xml:id="D2a"> <cit type="translation" xml:lang="la">
                                    <quote> finis </quote>
                                </cit> <cit type="translation" xml:lang="la">
                                    <quote> propositus </quote>
                                </cit>: <ref cRef="1 Petr." loc="1,9">1 Petr. 1,9</ref>; <foreign xml:lang="gez">ፍጻሜ፡ ነፍስነ፡</foreign> <ref cRef="Lit." loc="168,2">Lit. 168,2</ref>.</sense> <sense n="b" xml:id="D2b"> <cit type="translation" xml:lang="la">
                                    <quote> extremum </quote>
                                </cit> (tempore et gradu): <foreign xml:lang="gez">ፍጻሜሁ፡ ለበቀል፡</foreign> <ref cRef="Sap." loc="12,27">Sap. 12,27</ref>.</sense> <sense n="c" xml:id="D2c"> <cit type="translation" xml:lang="la">
                                    <quote> terminus </quote>
                                </cit>: <foreign xml:lang="gez">ኵሎ፡ ፍጻሜ፡ ማኅበራ፡ ለቤተ፡ ክርስቲያን፡</foreign> (omnium finium ecclesiae – miserere), <ref cRef="Lit. Orth." loc="">Lit. Orth. </ref>
                            </sense> </sense> <sense n="3" xml:id="D3"> <cit type="translation" xml:lang="la">
                                <quote> perfectio </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> absolutio </quote>
                            </cit>: <foreign xml:lang="gez">እስመ፡ ኀልዮ፡ ኪያሃ፡ ፍጻሜ፡ አእምሮ፡</foreign> <foreign xml:lang="grc">τελειότης</foreign> <ref cRef="Sap." loc="6,16">Sap. 6,16</ref>; <ref cRef="Hebr. " loc="6,1">Hebr.  6,1</ref>; <lbl expand="secundum">sec.</lbl> Lud. etiam Adv. <cit type="translation" xml:lang="la">
                                <quote> prorsus </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> plane </quote>
                            </cit>, ut: <foreign xml:lang="gez">ፍጻሜ፡ ንቱግ፡</foreign> „plane evacuatum“.</sense> </sense>
               
