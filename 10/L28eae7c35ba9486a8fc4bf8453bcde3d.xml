               <sense xml:id="Dmain" xml:lang="la" source="#dillmann"> <subc>III,3</subc> <sense n="1" xml:id="D1"> <cit type="translation" xml:lang="la"><quote> sibi invicem similes esse </quote></cit>, <cit type="translation" xml:lang="la"><quote> inter se </quote></cit> <cb n="0173" xml:id="c0173"/> <cit type="translation" xml:lang="la"><quote> aequales esse </quote></cit>, <cit type="translation" xml:lang="la"><quote> aequiparare </quote></cit>: <ref cRef="Ex." loc="38,15">Ex. 38,15</ref>; <foreign xml:lang="gez">ዘኢይትማሰል፡ ነገር፡</foreign> <foreign xml:lang="grc">τοὐναντίον</foreign> <ref cRef="Gal." loc="2,7">Gal. 2,7</ref>; <lbl expand="cum">c.</lbl> <gramGrp> <case value="accusativus">Acc.</case> </gramGrp>: <foreign xml:lang="gez">ኢይትማሰላ፡ ወርቅ፡</foreign> <ref cRef="Job" loc="28,17">Job 28,17</ref>; <foreign xml:lang="gez">ከመ፡ ዘኢይትማሰል፡ ገጽ፡ ገጸ፡</foreign> <ref cRef="Prov." loc="27,19">Prov. 27,19</ref>; <foreign xml:lang="gez">መኑ፡ ይትማሰሎ፡ ለጣዕመ፡ ዚአሁ፡</foreign> <ref cRef="Clem." loc="202">Clem. f. 202</ref>; <ref cRef="Hen." loc="29,2">Hen. 29,2</ref>; <lbl expand="cum">c.</lbl> <foreign xml:lang="gez">ምስለ፡</foreign>, ut: <foreign xml:lang="gez">ኢይትማሰል፡ ምስለ፡ ባዕድ፡ ንብረቱ፡</foreign> <ref cRef="Sap." loc="2,15">Sap. 2,15</ref>; <lbl expand="cum">c.</lbl> <foreign xml:lang="gez">ከመ፡</foreign> <ref cRef="Hen. " loc="24">Hen.  24</ref>; <ref cRef="Hen." loc="3 ">Hen. 3 </ref> {DiL: <ref cRef="Hen." loc="24.">Hen. 24.</ref> 3}. 
</sense> <sense n="2" xml:id="D2"> <cit type="translation" xml:lang="la"><quote> imitari </quote></cit>, <cit type="translation" xml:lang="la"><quote> aemulari alios </quote></cit>: <foreign xml:lang="gez">ይትማሰሎሙ፡ ለገባርያነ፡ ብርት፡</foreign> <foreign xml:lang="grc">μιμεῖται</foreign> <ref cRef="Sap." loc="15,9">Sap. 15,9</ref>. 
</sense> </sense>
               
            <sense xml:id="Tmain" xml:lang="en" source="#traces"><cit type="transcription" xml:lang="gez"><quote>tamāsala</quote></cit> <sense n="G" xml:id="TG"> <foreign xml:lang="gez">ተማሰለ፡</foreign> <cit type="transcription" xml:lang="gez"><quote>tamāsala</quote></cit> <subc>III,3</subc> <foreign xml:lang="gez">ይትማሰል፡</foreign> «<cit type="translation" xml:lang="fr"><quote>se ressembler l’un l’autre</quote></cit>, <cit type="translation" xml:lang="fr"><quote>être égaux, pareils</quote></cit>» ― <foreign xml:lang="am">ተመሳሰለ፡</foreign> Ms. <ref type="BM" target="BNFabb217">BNFabb217</ref>, fol. 37v. <lb/>
 <bibl><ptr target="bm:Grebaut1952SuppLexicon"/><citedRange unit="page"> 88</citedRange></bibl> 
</sense> <sense n="L" xml:id="TL"><foreign xml:lang="gez">ተማሰለ፡</foreign> <cit type="transcription" xml:lang="gez"><quote>tamāsala</quote></cit> <cit type="translation" xml:lang="en"><quote>resemble one another</quote></cit>, <cit type="translation" xml:lang="en"><quote>be compared to one another</quote></cit>, <cit type="translation" xml:lang="en"><quote>be made equal to one another</quote></cit>, <cit type="translation" xml:lang="en"><quote>be equal to one another</quote></cit>, <cit type="translation" xml:lang="en"><quote>be like one another</quote></cit>, <cit type="translation" xml:lang="en"><quote>resemble</quote></cit> <bibl><ptr target="bm:Leslau1987dic"/><citedRange unit="page"> 365b</citedRange></bibl> 
</sense> </sense></entry>
