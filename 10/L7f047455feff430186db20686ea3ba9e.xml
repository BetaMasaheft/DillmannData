               <sense xml:id="Dmain" xml:lang="la" source="#dillmann"> <subc>I,1</subc> [<foreign xml:lang="ar">مَشَطَ</foreign> (discerpsit i.e.) <cit type="translation" xml:lang="la"><quote> pexuit </quote></cit>; <foreign xml:lang="ar">نَشَطَ</foreign>, <foreign xml:lang="ar">نَشَظَ</foreign> <cit type="translation" xml:lang="la"><quote> abripuit </quote></cit>] <gramGrp> <pos expand="Subjunctivus">Subj.</pos> </gramGrp> <foreign xml:lang="gez">ይምሥጥ፡</foreign> <sense n="1" xml:id="D1"> <cit type="translation" xml:lang="la"><quote> avellere </quote></cit>, <cit type="translation" xml:lang="la"><quote> decerpere </quote></cit>, <cit type="translation" xml:lang="la"><quote> deripere </quote></cit>: <foreign xml:lang="gez">ትመሥጥዎሙ፡ አምእስቲሆሙ፡ እምላዕሌሆሙ፡</foreign> <ref cRef="Mich." loc="3,2">Mich. 3,2</ref>; <foreign xml:lang="gez">መሠጥዎሙ፡ ለአማልክተ፡ ነኪር፡ ዘውስተ፡ እዘኒሆሙ፡</foreign> <ref cRef="Kuf." loc="31">Kuf. 31</ref>; <foreign xml:lang="gez">መሠጡ፡ እጓለ፡ ማውታ፡ እምውስተ፡ ጥብ፡</foreign> <ref cRef="Job" loc="24,9">Job 24,9</ref>.
</sense> <sense n="2" xml:id="D2"> <cit type="translation" xml:lang="la"><quote> discerpere </quote></cit>, <cit type="translation" xml:lang="la"><quote> dilaniare </quote></cit>: <foreign xml:lang="gez">ከመ፡ ኢይምሥጡካሃ፡ ከመ፡ ላህም፡ ለነፍስከ፡</foreign> <ref cRef="Sir." loc="6,2">Sir. 6,2</ref>; <foreign xml:lang="gez">ተኵላ፡ እንዘ፡ ይመሥጦሙ፡</foreign> (<foreign xml:lang="gez">ለአባግዕ፡</foreign>) <ref cRef="Clem." loc="131">Clem. f. 131</ref>; <foreign xml:lang="gez">ይመሥጥ፡</foreign> <foreign xml:lang="grc">ἐπιλήψεται</foreign> Jes.5,29; <ref cRef="Ps." loc="7,2">Ps. 7,2</ref>; <ref cRef="Hen." loc="90,11">Hen. 90,11</ref>; <ref cRef="Joh." loc="10,12">Joh. 10,12</ref>; <ref cRef="Org." loc="">Org. </ref> 
</sense> <sense n="3" xml:id="D3"> <cit type="translation" xml:lang="la"><quote> diripere </quote></cit>, <cit type="translation" xml:lang="la"><quote> rapere </quote></cit>, <cit type="translation" xml:lang="la"><quote> abripere </quote></cit>: <foreign xml:lang="gez">ይመሥጡ፡ ወየሀይዱ፡</foreign> <ref cRef="Hez." loc="22,25">Hez. 22,25</ref>; <foreign xml:lang="gez">እምእደ፡ ዘመሠጠ፡ ወእምእደ፡ ዘሄደ፡</foreign> <ref cRef="Jer." loc="21,12">Jer. 21,12</ref>; <foreign xml:lang="gez">መሠጡ፡ ኖላዌ፡ ምስለ፡ መርዔቱ፡</foreign> <ref cRef="Job " loc="24,2">Job  24,2</ref>; <ref cRef="Ps." loc="10,9">Ps. 10,9</ref>; <ref cRef="Ps." loc="34,12">Ps. 34,12</ref>; <ref cRef="Jud." loc="21,21">Jud. 21,21</ref>; <ref cRef="Kuf." loc="30">Kuf. 30</ref>; <ref cRef="Matth." loc="13,19">Matth. 13,19</ref>; <lbl expand="cum">c.</lbl> <lbl expand="duplex">dupl.</lbl> <gramGrp> <case value="accusativus">Acc.</case> </gramGrp> <ref cRef="Herm. " loc="5.">Herm.  p. 5.</ref> 
</sense> <sense n="4" xml:id="D4"> <lbl expand="cum">c.</lbl> <gramGrp> <case value="accusativus">Acc.</case> </gramGrp> <lbl expand="persona">pers.</lbl> <cit type="translation" xml:lang="la"><quote> rapere </quote></cit> i.e. <cit type="translation" xml:lang="la"><quote> raptim auferre </quote></cit> vel <cit type="translation" xml:lang="la"><quote> deducere </quote></cit> aliquem: <ref cRef="Job" loc="14,20">Job 14,20</ref>; <ref cRef="Joh." loc="6,15">Joh. 6,15</ref>; <ref cRef="Act." loc="8,39">Act. 8,39</ref>; <ref cRef="Act." loc="23,10">Act. 23,10</ref>; <ref cRef="1 Thess." loc="4,17">1 Thess. 4,17</ref> <lbl expand="romanae editionis">rom.</lbl>; <ref cRef="Hen." loc="39,3">Hen. 39,3</ref>; <ref cRef="Hen." loc="68,2">Hen. 68,2</ref>; in specie de homine raptae mentis in coelos deducto <ref cRef="2 Cor." loc="12,2">2 Cor. 12,2</ref>; <ref cRef="2 Cor. " loc="12,4">2 Cor.  12,4</ref>; <foreign xml:lang="gez">ምሥጥዎ፡ ለሕሊናክሙ፡ ኀበ፡ መልዕልት፡</foreign> (raptim tollite vel) <cit type="translation" xml:lang="la"><quote> elevate mentes vestras sursum </quote></cit> <ref cRef="Lith. Orth." loc="">Lith. Orth. </ref> 
</sense> <sense n="5" xml:id="D5"> simpliciter: <cit type="translation" xml:lang="la"><quote> tollere </quote></cit>, <cit type="translation" xml:lang="la"><quote> capere </quote></cit>, <cit type="translation" xml:lang="la"><quote> apprehendere </quote></cit> <ref cRef="Ex." loc="30,4">Ex. 30,4</ref>; <ref cRef="Enc." loc="">Enc. </ref> (Ludolfi).
</sense> <sense n="6" xml:id="D6"> in bonam partem: <cit type="translation" xml:lang="la"><quote> eripere </quote></cit>: <foreign xml:lang="gez">ወበርበርዎሙ፡ ወአልቦ፡ ዘያድኅኖሙ፡ ወይመሥጥዎሙ፡</foreign> <ref cRef="Jes." loc="42,22">Jes. 42,22</ref>. – Voc. Ae.: <foreign xml:lang="gez">መሠጠ፡ ዘ፡ ነተቀ፡</foreign> (<lbl expand="videas">vid.</lbl> <foreign xml:lang="gez">ነጠቀ፡</foreign> apud Isenb.).
</sense> </sense>
               
            <sense xml:id="Tmain" xml:lang="en" source="#traces"><cit type="transcription" xml:lang="gez"><quote>maśaṭa</quote></cit>, <foreign xml:lang="gez">መሰጠ፡</foreign> <cit type="transcription" xml:lang="gez"><quote>masaṭa</quote></cit> <sense n="G"> <foreign xml:lang="gez">መሰጠ፡</foreign> <cit type="transcription" xml:lang="gez"><quote>masaṭa</quote></cit> <subc>II,3</subc> <foreign xml:lang="gez">ይመስጥ፡</foreign>, <foreign xml:lang="gez">ይምስጥ፡</foreign> «<cit type="translation" xml:lang="fr"><quote>ravir</quote></cit>, <cit type="translation" xml:lang="fr"><quote>ôter des mains</quote></cit>» ― <foreign xml:lang="am">ነጠቀ፡</foreign> Ms. <ref type="BM" target="BNFabb217">BNFabb217</ref>, fol. 39r. <lb/>
 <bibl><ptr target="bm:Grebaut1952SuppLexicon"/><citedRange unit="page"> 92</citedRange></bibl> 
</sense> <sense n="L"> <foreign xml:lang="gez">መሠጠ</foreign> <cit type="transcription" xml:lang="gez"><quote>maśaṭa</quote></cit> (yǝmśǝṭ), (<bibl><ptr target="bm:Grebaut1952SuppLexicon"/><citedRange unit="page"> 92</citedRange></bibl>, <bibl><ptr target="bm:AlaqaTayya1965MSawasew"/></bibl>) <foreign xml:lang="gez">መሰጠ</foreign> <cit type="transcription" xml:lang="gez"><quote>masaṭa</quote></cit> <cit type="translation" xml:lang="en"><quote>tear to pieces</quote></cit>, <cit type="translation" xml:lang="en"><quote>tear away</quote></cit>, <cit type="translation" xml:lang="en"><quote>cut up</quote></cit>, <cit type="translation" xml:lang="en"><quote>dismember</quote></cit>, <cit type="translation" xml:lang="en"><quote>carry off</quote></cit>, <cit type="translation" xml:lang="en"><quote>carry away</quote></cit>, <cit type="translation" xml:lang="en"><quote>grab</quote></cit>, <cit type="translation" xml:lang="en"><quote>seize and carry off by force</quote></cit>, <cit type="translation" xml:lang="en"><quote>snatch away</quote></cit>, <cit type="translation" xml:lang="en"><quote>kidnap</quote></cit>, <cit type="translation" xml:lang="en"><quote>strip</quote></cit>, <cit type="translation" xml:lang="en"><quote>seize violently</quote></cit>, <cit type="translation" xml:lang="en"><quote>ravish</quote></cit>, <cit type="translation" xml:lang="en"><quote>enrapture</quote></cit>, <cit type="translation" xml:lang="en"><quote>entrance</quote></cit> <bibl><ptr target="bm:Leslau1987dic"/><citedRange unit="page"> 369a</citedRange></bibl> 
</sense> </sense></entry>
