                     <ref cRef="Sir." loc="17,23">Sir. 17,23</ref>; <ref cRef="Jes." loc="59,18">Jes. 59,18</ref>; <foreign xml:lang="gez">ዕለተ፡ ፍዳ፡</foreign>
                     <ref cRef="Jes." loc="61,2">Jes. 61,2</ref>; <ref cRef="Jes." loc="63,4">Jes.
                        63,4</ref>; <ref cRef="Jer." loc="28,6">Jer. 28,6</ref>; <foreign xml:lang="gez">ያገብእ፡ ለነ፡ ፍዳሃ፡</foreign>
                     <ref cRef="Gen." loc="50,15">Gen. 50,15</ref>; <foreign xml:lang="gez">ፍዳ፡ ደም፡
                        ንጹሕ፡ ይገብእ፡ ላዕሌክሙ፡</foreign>
                     <ref cRef="Jer." loc="33,15">Jer. 33,15</ref>; <foreign xml:lang="gez">በቀል፡
                        ወፍዳ፡ </foreign>
                     <ref cRef="Hos." loc="9,5">Hos. 9,5</ref>; <ref cRef="Col." loc="3,24">Col.
                        3,24</ref>; <ref cRef="Rom." loc="11,9">Rom. 11,9</ref>; <foreign xml:lang="grc">ἐπαγωγαί</foreign>, <foreign xml:lang="gez">ምሉእ፡ ፍዳሁ፡ ዲበ፡
                        ቤቱ፡</foreign>
                     <ref cRef="Sir." loc="23,11">Sir. 23,11</ref>; <ref cRef="Sir." loc="10,13">Sir. 10,13</ref>; <foreign xml:lang="grc">ἐκδίκησις</foreign>, <foreign xml:lang="gez">ፍዳሆሙ፡ ለኃጥኣን፡ ዕፄ፡</foreign>
                     <ref cRef="Sir." loc="7,17">Sir. 7,17</ref>; <foreign xml:lang="gez">ዕለተ፡ ፍዳከ፡ </foreign>
                     <ref cRef="Sir." loc="18,24">Sir. 18,24</ref>; <ref cRef="Mich." loc="7,4">Mich. 7,4</ref>; <foreign xml:lang="grc">ἐπισκοπή</foreign>
                     <ref cRef="Jes. " loc="10,3">Jes. 10,3</ref>; <foreign xml:lang="grc">ἀντιμισθία</foreign>
                     <ref cRef="Rom." loc="1,27">Rom. 1,27</ref>; <foreign xml:lang="gez">ይረክብ፡
                        ፍዳሆሙ፡</foreign>
                     <foreign xml:lang="grc">δίκην τίσουσι </foreign>
                     <ref cRef="2 Thess." loc="1,9">2 Thess. 1,9</ref>; <foreign xml:lang="grc">κρίσις</foreign>
                     <ref cRef="Luc." loc="11,31">Luc. 11,31</ref>
                     <lbl expand="romanae editionis">rom.</lbl>; 1 <ref cRef="Petr." loc="4,17">Petr. 4,17</ref>; num sit <foreign xml:lang="gez">ዘይትሐበየኒ፡ እምፍዳ፡ ምግባርየ፡
                        ዘግሩም፡ ጥቀ፡ </foreign> Org.; <foreign xml:lang="gez">አመ፡ አኀዝዋ፡</foreign>
                     (feminam) <foreign xml:lang="gez">ወአቅረብዋ፡ ለፍዳ፡</foreign> (judicio) <ref cRef="Sx. Ter" loc="5 ">Sx. Ter 5 </ref>Enc.; (etiam <ref cRef="Sx. Teq. " loc="29 ">Sx. Teq. 29 </ref>Enc., ubi Lud. significationem <cit type="translation" xml:lang="la">
                        <quote>meritum</quote>
                     </cit> statuit, significatio <cit type="translation" xml:lang="la">
                        <quote>poena</quote>
                     </cit>
                     <lbl expand="scilicet">sc.</lbl> merita sufficit).</sense>
                  <sense n="c" xml:id="Dc"> In <gramGrp>
                        <case value="accusativus">Acc.</case>
                     </gramGrp> et st. constr. usurpatur loco praepositionis <cit type="translation" xml:lang="la">
                        <quote>pro</quote>
                     </cit>, <foreign xml:lang="grc">ἀντί</foreign>
                     <ref cRef="Ex." loc="21,23">Ex. 21,23</ref>; <ref cRef="Ex." loc="21,24">Ex.
                        21,24</ref>; nec non <foreign xml:lang="gez">ህየንተ፡ ዘ፡</foreign> … <foreign xml:lang="gez">ፍዳ፡</foreign> a poëta novatur <cit type="translation" xml:lang="la">
                        <quote>propterea</quote>
                     </cit>
                     <cit type="translation" xml:lang="la">
                        <quote>quod</quote>
                     </cit>, ut: <foreign xml:lang="gez">ህየንተ፡ ዘአፍቀርኮሙ፡ ፍዳ፤</foreign>
                     <ref cRef="Sx. Teq." loc="9 ">Sx. Teq. 9 </ref> Enc. – <lbl expand="Vocabularium Aethiopicum, -a -a.">Voc. Ae.</lbl>: <foreign xml:lang="gez">ፍዳ፡ ዘ፡ ብድራት፡</foreign>
                  </sense>
               </sense>
               
