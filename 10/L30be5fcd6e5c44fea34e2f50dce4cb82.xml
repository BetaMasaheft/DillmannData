               <sense xml:id="Dmain" xml:lang="la" source="#dillmann"> <subc>I,1</subc> [<foreign xml:lang="he">חָטַב</foreign> <cit type="translation" xml:lang="la"><quote> cecidit lignum </quote></cit> (vic. <foreign xml:lang="he">חָצַב</foreign>); <foreign xml:lang="ar">حَطَبَ</foreign> <cit type="translation" xml:lang="la"><quote> lignatus est </quote></cit> (<foreign xml:lang="ar">حَطَبٌ</foreign> <cit type="translation" xml:lang="la"><quote> ligna caesa, quibus ignis accenditur </quote></cit>)] <gramGrp> <pos expand="Subjunctivus">Subj.</pos> </gramGrp> <foreign xml:lang="gez">ይሕጥብ፡</foreign> <sense n="1" xml:id="D1"> <cit type="translation" xml:lang="la"><quote> ligna </quote></cit>, <cit type="translation" xml:lang="la"><quote> findere </quote></cit> vel <cit type="translation" xml:lang="la"><quote> caedere </quote></cit>: <foreign xml:lang="gez">ዘየሐጥብ፡ ዕፀ፡</foreign> <foreign xml:lang="grc">ξυλοκόπος</foreign> <ref cRef="Jos." loc="9,21">Jos. 9,21</ref>; <foreign xml:lang="gez">ሤምናክሙ፡ ትሕጥቡ፡ ዕፀ፡</foreign> <ref cRef="Jos." loc="9,19">Jos. 9,19</ref>. 
</sense> <sense n="2" xml:id="D2"> <cit type="translation" xml:lang="la"><quote> lignari </quote></cit>: <foreign xml:lang="gez">ይሕጥብ፡ ዕፀ፡</foreign> <foreign xml:lang="grc">συναγαγεῖν ξύλα</foreign> <ref cRef="Deut." loc="19,5">Deut. 19,5</ref>; <foreign xml:lang="gez">ተሐጥብ፡ ዕፀወ፡</foreign> <foreign xml:lang="grc">συνέλεγε ξύλα</foreign> <ref cRef="3 Reg." loc="17,10">3 Reg. 17,10</ref>; <foreign xml:lang="gez">ሶበ፡ የሐጥብ፡ ዕፀወ፡ ይሤጽር፡ በበ፵ጾረ፡ ብእሲ፡ እስከ፡ ያነጢ፡ ዘይጸውር፡ ሎቱ፡</foreign> Gad. T.H. 
</sense> </sense>
               
            <sense xml:id="Tmain" xml:lang="en" source="#traces"> <cit type="transcription" xml:lang="gez"><quote>ḥaṭaba</quote></cit> <sense n="G" xml:id="TG"> <foreign xml:lang="gez">ሐጠበ፡</foreign> <cit type="transcription" xml:lang="gez"><quote>ḥaṭaba</quote></cit> <subc>I,1</subc> <foreign xml:lang="gez">የሐጥብ፡</foreign>, <foreign xml:lang="gez">ይሕጥብ፡</foreign> «<cit type="translation" xml:lang="fr"><quote>fendre du bois</quote></cit> (Ludolf), <cit type="translation" xml:lang="fr"><quote>recueillir du bois</quote></cit> (Abyssins)» ― <foreign xml:lang="am">እንጨት፡ ለቀመ፡</foreign> Ms. <ref type="BM" target="BNFabb217">BNFabb217</ref>, fol. 30r. N.B. <foreign xml:lang="am">እንጨት፡</foreign> <cit type="transcription" xml:lang="gez"><quote>ʾǝnč̣at</quote></cit> <cit type="translation" xml:lang="fr"><quote>bois</quote></cit> n’a jamais le sens d’<cit type="translation" xml:lang="fr"><quote>arbre</quote></cit>. <lb/>
 <bibl><ptr target="bm:Grebaut1952SuppLexicon"/><citedRange unit="page"> 66</citedRange></bibl> 
</sense> <sense n="L" xml:id="TL"> <foreign xml:lang="gez">ሐጠበ</foreign> <cit type="transcription" xml:lang="gez"><quote>ḥaṭaba</quote></cit> (yǝḥṭǝb) <cit type="translation" xml:lang="en"><quote>cut</quote></cit> (wood), <cit type="translation" xml:lang="en"><quote>split</quote></cit> (wood), <cit type="translation" xml:lang="en"><quote>gather</quote></cit> (wood) <bibl><ptr target="bm:Leslau1987dic"/><citedRange unit="page"> 248b</citedRange></bibl> 
</sense> </sense></entry>
