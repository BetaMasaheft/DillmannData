               <sense xml:id="Dmain" xml:lang="la" source="#dillmann"> <gramGrp> <pos expand="Substantivum">subst.</pos> </gramGrp> <sense n="1" xml:id="D1"> <cit type="translation" xml:lang="la"><quote> medium </quote></cit>, <cit type="translation" xml:lang="la"><quote> intimum </quote></cit>, <cit type="translation" xml:lang="la"><quote> internum </quote></cit>, <cit type="translation" xml:lang="la"><quote> interius </quote></cit>, <cit type="translation" xml:lang="la"><quote> interiora </quote></cit>, hominis: <foreign xml:lang="gez">በውስጥየ፡</foreign> <ref cRef="Ps." loc="108,21">Ps. 108,21</ref>; <ref cRef="Ps." loc="142,4">Ps. 142,4</ref>; <foreign xml:lang="gez">ደንገፀኒ፡ ልብየ፡ በውስጥየ፡</foreign> <cb n="0908" xml:id="c0908"/> <ref cRef="Genz." loc="5">Genz. f. 5</ref>; <ref cRef="Marc." loc="7,21">Marc. 7,21</ref>; <ref cRef="Matth." loc="23,25">Matth. 23,25</ref>; <foreign xml:lang="gez">በውስጡ፡</foreign> <ref cRef="Job" loc="21,24">Job 21,24</ref>; rei, ut: <foreign xml:lang="gez">ውስተ፡ ውስጡ፡ ለመንጦላዕት፡</foreign> <ref cRef="Lev." loc="16,2">Lev. 16,2</ref>. – Nota <foreign xml:lang="gez">በውስጤየ፡</foreign> <ref cRef="Ps." loc="38,4">Ps. 38,4</ref>. – <sense n="a" xml:id="D1a"> <cit type="translation" xml:lang="la"><quote> penetrale </quote></cit>: <foreign xml:lang="gez">ትበውእ፡ እምውስጥ፡ ለውስጥ፡</foreign> <foreign xml:lang="grc">ταμιεῖον</foreign> <ref cRef="2 Par." loc="18,24">2 Par. 18,24</ref>.</sense> <sense n="b" xml:id="D1b"> <foreign xml:lang="gez">ንዋየ፡ ውስጥ፡</foreign> <cit type="translation" xml:lang="la"><quote> intestina </quote></cit> <ref cRef="Lev." loc="1,13">Lev. 1,13</ref> <lbl expand="sequens">seq.</lbl>; <ref cRef="Kuf." loc="49">Kuf. 49</ref>; <ref cRef="Sap." loc="12,5">Sap. 12,5</ref> al.</sense> <sense n="c" xml:id="D1c"> internum orationis i.e. <cit type="translation" xml:lang="la"><quote> sensus </quote></cit>, <cit type="translation" xml:lang="la"><quote> sententia </quote></cit> ejus, Lud. <lbl expand="auctore, auctoritate">auct.</lbl> Greg.</sense> </sense> <sense n="2" xml:id="D2"> Adv. <sense n="a" xml:id="D2a"> <foreign xml:lang="gez">ውስጠ፡</foreign> raro <foreign xml:lang="gez">ውስጥ፡</foreign> (ut <ref cRef="Jes." loc="22,11">Jes. 22,11</ref>), <cit type="translation" xml:lang="la"><quote> intus </quote></cit>, <cit type="translation" xml:lang="la"><quote> intrinsecus </quote></cit>, <cit type="translation" xml:lang="la"><quote> intra </quote></cit>, <ref cRef="Gen. " loc="39,11">Gen.  39,11</ref>; <foreign xml:lang="gez">ወደዮን፡ውስጠ፡</foreign> <ref cRef="Ex." loc="40,18">Ex. 40,18</ref>; <foreign xml:lang="gez">ይኔጽር፡ ውስጠ፡</foreign> <ref cRef="2 Par." loc="4,4">2 Par. 4,4</ref>; <foreign xml:lang="gez">ወቦኡ፡ ውስጠ፡ ኀበ፡ ንጉሥ፡</foreign> <ref cRef="2 Par." loc="29,18">2 Par. 29,18</ref>; <ref cRef="Joh." loc="8,3">Joh. 8,3</ref>; <ref cRef="Joh." loc="20,26">Joh. 20,26</ref>; <ref cRef="Matth." loc="26,58">Matth. 26,58</ref>; <lbl expand="sequens">seq.</lbl> <foreign xml:lang="gez">እምነ፡</foreign> <cit type="translation" xml:lang="la"><quote> intra </quote></cit>, <cit type="translation" xml:lang="la"><quote> introrsum </quote></cit>: <foreign xml:lang="gez">ውስጠ፡ እምነ፡ መንጦላዕት፡</foreign> <ref cRef="Lev." loc="16,12">Lev. 16,12</ref>; <foreign xml:lang="gez">እለ፡ ውስጥ፡ እምነ፡</foreign> <foreign xml:lang="grc">ἐσώτερον</foreign> <ref cRef="Jes." loc="22,11">Jes. 22,11</ref>; <foreign xml:lang="gez">ውስጠ፡</foreign> (v. <foreign xml:lang="gez">ውስጥ፡</foreign>) <foreign xml:lang="gez">እምኔክሙ፡</foreign> <ref cRef="4 Esr." loc="14,36">4 Esr. 14,36</ref>.</sense> <sense n="b" xml:id="D2b"> <lbl expand="cum">c.</lbl> <gramGrp> <pos expand="pronomen suffixum">pron. suff.</pos> </gramGrp> : <foreign xml:lang="gez">ውስጡ፡</foreign>, <foreign xml:lang="gez">ውስጣ፡</foreign> <cit type="translation" xml:lang="la"><quote> intrinsecus </quote></cit>, <cit type="translation" xml:lang="la"><quote> introrsum </quote></cit>: <foreign xml:lang="gez">ዝንቱሰ፡ውስጡ፡ ጽቡር፡</foreign> <ref cRef="Dan. apocr." loc="13,7">Dan. apocr. 13,7</ref>; <foreign xml:lang="gez">ጽሕፍት፡ ይእቲ፡ ውስጣኒ፡ ወአፍአሃኒ፡</foreign> <ref cRef="Hez." loc="2,10">Hez. 2,10</ref>.</sense> <sense n="c" xml:id="D2c"> <foreign xml:lang="gez">በውስጥ፡</foreign> <cit type="translation" xml:lang="la"><quote> intus </quote></cit> <ref cRef="Act." loc="5,23">Act. 5,23</ref>; <ref cRef="Lit. Orth." loc="">Lit. Orth. </ref>; <ref cRef="Chron. Ax." loc="89.">Chron. Ax. f. 89.</ref> </sense> <sense n="d" xml:id="D2d"> <foreign xml:lang="gez">እምውስጥ፡</foreign> <cit type="translation" xml:lang="la"><quote> intrinsecus </quote></cit> <ref cRef="Marc." loc="7,23">Marc. 7,23</ref>, vel <foreign xml:lang="gez">እምውስጡ፡</foreign> <ref cRef="Job" loc="1,10">Job 1,10</ref>, vel <foreign xml:lang="gez">እምውስጣ፡</foreign> <ref cRef="Hez." loc="7,15">Hez. 7,15</ref>.</sense> <sense n="e" xml:id="D2e"> <foreign xml:lang="gez">እንተ፡ ውስጥ፡</foreign> <ref cRef="Lit." loc="173,3">Lit. 173,3</ref> vel <lbl expand="cum">c.</lbl> <gramGrp> <pos expand="pronomen suffixum">pron. suff.</pos> </gramGrp> <cit type="translation" xml:lang="la"><quote> intus </quote></cit>, <cit type="translation" xml:lang="la"><quote> intrinsecus </quote></cit>, <cit type="translation" xml:lang="la"><quote> introrsum </quote></cit>, <cit type="translation" xml:lang="la"><quote> intra </quote></cit>: <ref cRef="Gen." loc="6,14">Gen. 6,14</ref>; <foreign xml:lang="gez">እንተ፡ ውስጡ፡</foreign> <ref cRef="2 Par." loc="3,4">2 Par. 3,4</ref>; <ref cRef="Hez." loc="1,27">Hez. 1,27</ref>; <ref cRef="Eph. " loc="3,16">Eph.  3,16</ref>; <foreign xml:lang="gez">እንተ፡ ውስጣ፡</foreign> <ref cRef="Apoc." loc="5,1">Apoc. 5,1</ref>; <foreign xml:lang="gez">እንተ፡ ውስጦሙ፡</foreign> <ref cRef="Matth." loc="7,15">Matth. 7,15</ref>; <ref cRef="Matth." loc="23,27">Matth. 23,27</ref>; <foreign xml:lang="gez">እንተ፡ ውስጥክሙ፡</foreign> <ref cRef="Matth." loc="23,28">Matth. 23,28</ref>.</sense> </sense> </sense>
               
