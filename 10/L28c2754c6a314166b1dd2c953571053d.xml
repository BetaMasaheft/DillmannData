         <div>
            <entry xml:id="L28c2754c6a314166b1dd2c953571053d" n="5650">
               <form>
                  <foreign xml:lang="gez">አብደ</foreign>
               <rs type="root"/>
                    </form>
               <sense xml:lang="la" source="#dillmann" xml:id="Dmain"> <subc>I,1</subc> [potestas racidis prima est in <cit type="translation" xml:lang="la">
                            <quote> deerrando </quote>
                        </cit> et <cit type="translation" xml:lang="la">
                            <quote> vagando </quote>
                        </cit>; inde oriuntur 1) <cit type="translation" xml:lang="la">
                            <quote> ferum et a consuetudine alienum evasit </quote>
                        </cit> animal <foreign xml:lang="ar">أَبَدَ</foreign>; <cit type="translation" xml:lang="la">
                            <quote> mente alienari </quote>
                        </cit>, <cit type="translation" xml:lang="la">
                            <quote> insanire </quote>
                        </cit> <foreign xml:lang="gez">አብደ፡</foreign>; <cit type="translation" xml:lang="la">
                            <quote> efferari </quote>
                        </cit>, <cit type="translation" xml:lang="la">
                            <quote> rabere </quote>
                        </cit> <foreign xml:lang="ar">عَبِدَ أَبِدَ</foreign> <foreign xml:lang="gez">አብደ፡ ሀርበደ፡</foreign>; 2) <cit type="translation" xml:lang="la">
                            <quote> desertum esse </quote>
                        </cit> <foreign xml:lang="ar">أَبِدَ</foreign> (vic. <foreign xml:lang="gez">በድወ፡</foreign>, <foreign xml:lang="ar">بَدَا</foreign>); 3) <cit type="translation" xml:lang="la">
                            <quote> oberrare </quote>
                        </cit>, <cit type="translation" xml:lang="la">
                            <quote> amitti </quote>
                        </cit> <foreign xml:lang="he">אָבַד</foreign> <foreign xml:lang="syr">ܐܶܒܰܕ</foreign>; 4) <cit type="translation" xml:lang="la">
                            <quote> perire </quote>
                        </cit> <foreign xml:lang="he">אָבַד</foreign> <foreign xml:lang="syr">ܐܶܒܰܕ</foreign> <foreign xml:lang="gez">በድበደ፡</foreign> <foreign xml:lang="ar">بَدَا</foreign>] <gramGrp> <pos expand="Subjunctivus">Subj.</pos> </gramGrp> <foreign xml:lang="gez">ይእበድ፡</foreign> <sense n="1" xml:id="D1"> <cit type="translation" xml:lang="la">
                                <quote> oberrare </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> profugere </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> vagari </quote>
                            </cit> <ref cRef="1 Reg." loc="25,10">1 Reg. 25,10</ref>.</sense> <sense n="2" xml:id="D2"> mente errare vel capi i.e. <sense n="a" xml:id="D2a"> <cit type="translation" xml:lang="la">
                                    <quote> insanire </quote>
                                </cit>, <cit type="translation" xml:lang="la">
                                    <quote> amentem esse </quote>
                                </cit> vel <cit type="translation" xml:lang="la">
                                    <quote> fieri </quote>
                                </cit>: <foreign xml:lang="gez">አብደ፡</foreign> <foreign xml:lang="grc">ἐξέστη</foreign> <ref cRef="Marc." loc="3,21">Marc. 3,21</ref>; <foreign xml:lang="gez">ተአብዲ፡</foreign> <foreign xml:lang="grc">μαίνῃ</foreign> <ref cRef="Act." loc="12,15">Act. 12,15</ref>; <ref cRef="Act. " loc="26,24">Act.  26,24</ref>.</sense> <sense n="b" xml:id="D2b"> <cit type="translation" xml:lang="la">
                                    <quote> stultum esse </quote>
                                </cit> vel <cit type="translation" xml:lang="la">
                                    <quote> fieri </quote>
                                </cit>, <cit type="translation" xml:lang="la">
                                    <quote> imprudenter agere </quote>
                                </cit>, <cit type="translation" xml:lang="la">
                                    <quote> errorem committere </quote>
                                </cit>: <foreign xml:lang="gez">የአብዱ፡ መላእክተ፡ ጣኔዎስ፡</foreign> <foreign xml:lang="grc">μωροὶ ἔσονται</foreign> <ref cRef="Jes." loc="19,11">Jes. 19,11</ref>; <ref cRef="Rom." loc="1,22">Rom. 1,22</ref>; <ref cRef="1 Cor." loc="1,23">1 Cor. 1,23</ref>; <ref cRef="Clem." loc="189">Clem. f. 189</ref>; <foreign xml:lang="gez">እምኔክሙ፡ ኢየአብድ፡</foreign> (<foreign xml:lang="grc">ἀσυνετώτερος</foreign>) <ref cRef="Job" loc="13,2">Job 13,2</ref>; <foreign xml:lang="gez">እስመ፡ አብዱ፡ ኖሎት፡</foreign> <foreign xml:lang="grc">ἠφρονεύσαντο</foreign> <ref cRef="Jer." loc="10,21">Jer. 10,21</ref>; <ref cRef="Tit." loc="3,3">Tit. 3,3</ref> (<foreign xml:lang="grc">ἦμεν ἀνόητοι</foreign>); <ref cRef="Luc." loc="6,11">Luc. 6,11</ref>; <foreign xml:lang="grc">κορήσουσι</foreign> <ref cRef="Deut." loc="31,20">Deut. 31,20</ref>; <foreign xml:lang="gez">ኢትእበድ፡</foreign> <foreign xml:lang="grc">μὴ ἀγνόει</foreign> <ref cRef="Hos." loc="4,15">Hos. 4,15</ref>; <foreign xml:lang="gez">አብደ፡ ዐቢየ፡ እበደ፡</foreign> <ref cRef="1 Reg. " loc="14,24">1 Reg.  14,24</ref>.</sense> </sense> <sense n="3" xml:id="D3"> <cit type="translation" xml:lang="la">
                                <quote> efferari </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> furere </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> furore corripi </quote>
                            </cit> vel <cit type="translation" xml:lang="la">
                                <quote> exsultare </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> saevire </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> rabiosum esse </quote>
                            </cit> vel <cit type="translation" xml:lang="la">
                                <quote> fieri </quote>
                            </cit>: <foreign xml:lang="gez">አበድኩ፡</foreign> <ref cRef="2 Esr." loc="9,3">2 Esr. 9,3</ref> <lbl expand="varia lectio">var.</lbl>; <foreign xml:lang="gez">አብደ፡ መራዕይ፡</foreign> <ref cRef="Marc." loc="5,13">Marc. 5,13</ref>; <foreign xml:lang="gez">የአብዱ፡ ኵሎሙ፡ እለ፡ ነሰኮሙ፡</foreign> (<foreign xml:lang="gez">ከልብ፡ እቡድ፡</foreign>) <foreign xml:lang="gez">ኅቡረ፡ ምስሌሁ፡</foreign> <ref cRef="Did." loc="6">Did. 6</ref>; <lbl expand="cum">c.</lbl> <foreign xml:lang="gez">ላዕለ፡</foreign> vel <foreign xml:lang="gez">ዲበ፡</foreign> <cit type="translation" xml:lang="la">
                                <quote> contra </quote>
                            </cit> vel <cit type="translation" xml:lang="la">
                                <quote> in </quote>
                            </cit> aliquem: <ref cRef="Act." loc="26,11">Act. 26,11</ref> Platt; <ref cRef="Act." loc="19,16">Act. 19,16</ref>; <foreign xml:lang="gez">አብዱ፡ ላዕሌሁ፡</foreign> <ref cRef="M.M." loc=" f. 53">M.M.  f. 53</ref>; <foreign xml:lang="grc">ἐξηγριάνθη πρὸς αὐτόν</foreign> <ref cRef="Dan." loc="8,7">Dan. 8,7</ref>.</sense> </sense>
               
            </entry>
