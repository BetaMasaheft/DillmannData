               </form>
               <sense xml:lang="la" source="#dillmann" xml:id="Dmain"> <subc>II,1</subc> <cit type="translation" xml:lang="la">
                            <quote> exire facere </quote>
                        </cit> vel <cit type="translation" xml:lang="la">
                            <quote> jubere </quote>
                        </cit>, ut <ref cRef="Gen." loc="48,12">Gen. 48,12</ref>; <sense n="a" xml:id="Da"> <cit type="translation" xml:lang="la">
                                <quote> educere </quote>
                            </cit>: <foreign xml:lang="gez">አውፅኦ፡ አፍአ፡</foreign> <ref cRef="Gen." loc="15,5">Gen. 15,5</ref>; <foreign xml:lang="gez">አውፅኦሙ፡ ኀቤነ፡</foreign> <ref cRef="Gen." loc="19,5">Gen. 19,5</ref>; <ref cRef="Gen." loc="38,24">Gen. 38,24</ref>; <foreign xml:lang="gez">አውፅአነ፡ እምድረ፡ ግብጽ፡</foreign> <ref cRef="Ex." loc="13,14">Ex. 13,14</ref>; <ref cRef="Ex." loc="16,6">Ex. 16,6</ref>; <ref cRef="Ex." loc="18,1">Ex. 18,1</ref>; <ref cRef="Deut. " loc="17,5">Deut.  17,5</ref>; <foreign xml:lang="gez">አውፅእዎሙ፡ ኀበ፡ አንቀጽ፡</foreign> <ref cRef="Deut." loc="22,24">Deut. 22,24</ref>; <ref cRef="Ps." loc="67,7">Ps. 67,7</ref>; <foreign xml:lang="gez">አውፅአኒ፡ ውስተ፡ መርሕብ፡</foreign> <ref cRef="Ps." loc="17,22">Ps. 17,22</ref>; <ref cRef="Marc." loc="1,12">Marc. 1,12</ref>; <ref cRef="Marc. " loc="8,23">Marc.  8,23</ref>; <ref cRef="Luc." loc="24,50">Luc. 24,50</ref>.</sense> <sense n="b" xml:id="Db"> <cit type="translation" xml:lang="la">
                                <quote> ejicere </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> expellere </quote>
                            </cit>: <foreign xml:lang="gez">አውፅእዎሙ፡ አፍአ፡ እምገጹ፡</foreign> <foreign xml:lang="grc">ἐξέβαλον</foreign> <ref cRef="Ex." loc="10,11">Ex. 10,11</ref>; <ref cRef="Ex." loc="11,1">Ex. 11,1</ref>; <ref cRef="Ex. " loc="33,2">Ex.  33,2</ref>; <ref cRef="Gen." loc="3,23">Gen. 3,23</ref>; <ref cRef="Gen." loc="21,10">Gen. 21,10</ref>; <foreign xml:lang="gez">አውፅኦሙ፡ እምነ፡ ምድሮሙ፡</foreign> <ref cRef="Sir." loc="47,24">Sir. 47,24</ref>; <ref cRef="Prov." loc="22,10">Prov. 22,10</ref>; <ref cRef="Prov." loc="27,15">Prov. 27,15</ref>; <foreign xml:lang="gez">አውፅኦ፡</foreign> (Infin.) <ref cRef="Koh." loc="3,6">Koh. 3,6</ref>; daemones <ref cRef="Matth." loc="12,24">Matth. 12,24</ref> <lbl expand="sequens">seq.</lbl>; <ref cRef="Marc." loc="7,26">Marc. 7,26</ref>; uxorem matrimonio <ref cRef="Lev." loc="21,7">Lev. 21,7</ref>; <ref cRef="Sir." loc="7,26">Sir. 7,26</ref>; <foreign xml:lang="gez">እንተ፡ አውፅእዋ፡</foreign> <foreign xml:lang="grc">ἐκβεβλημένη</foreign> <ref cRef="Hez." loc="44,22">Hez. 44,22</ref>; aliquid, ut: <foreign xml:lang="gez">ተፋቅሮ፡ ታወፅኣ፡ ለፍርሀት፡</foreign> <ref cRef="1 Joh." loc="4,18">1 Joh. 4,18</ref>.</sense> <sense n="c" xml:id="Dc"> <cit type="translation" xml:lang="la">
                                <quote> emittere </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> dimittere </quote>
                            </cit>: <foreign xml:lang="gez">አውፃእኮን፡ ዕራቆን፡ ለእቤራት፡</foreign> <foreign xml:lang="grc">ἐξαπέστειλας</foreign> <ref cRef="Job" loc="22,9">Job 22,9</ref>; uxorem <ref cRef="1 Esr. " loc="9,36">1 Esr.  9,36</ref> (<foreign xml:lang="grc">ἀπέλυσαν</foreign>); <foreign xml:lang="gez">እመ፡ አድምዐተከ፡</foreign> (sc. <foreign xml:lang="gez">ኀጢአት፡</foreign>) <foreign xml:lang="gez">ኢታወፅአከ፡</foreign> <ref cRef="Sir." loc="21,2">Sir. 21,2</ref>.</sense> <sense n="d" xml:id="Dd"> <cit type="translation" xml:lang="la">
                                <quote> eripere </quote>
                            </cit> aliquem <ref cRef="2 Reg." loc="22,49">2 Reg. 22,49</ref>; <ref cRef="Ps." loc="142,13">Ps. 142,13</ref>; <cit type="translation" xml:lang="la">
                                <quote> liberare </quote>
                            </cit> <ref cRef="1 Petr. " loc="2,24">1 Petr.  2,24</ref>.</sense> <sense n="e" xml:id="De"> <cit type="translation" xml:lang="la">
                                <quote> efferre </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> foras efferre </quote>
                            </cit> aliquid, <ref cRef="Am." loc="6,10">Am. 6,10</ref>; <ref cRef="Ex." loc="12,46">Ex. 12,46</ref>; <ref cRef="Gen." loc="14,18">Gen. 14,18</ref>; <ref cRef="Deut. " loc="24,11">Deut.  24,11</ref>; <ref cRef="Act." loc="5,6">Act. 5,6</ref>.</sense> <sense n="f" xml:id="Df"> <cit type="translation" xml:lang="la">
                                <quote> producere </quote>
                            </cit>, homines ex utero: <foreign xml:lang="gez">ታወፅአኒ፡ እምከርሠ፡ እምየ፡</foreign> <ref cRef="Job" loc="10,18">Job 10,18</ref>; muscas e terra <ref cRef="Ex." loc="8,14">Ex. 8,14</ref>; plantas, animalia <ref cRef="Gen." loc="1,12">Gen. 1,12</ref>; <ref cRef="Gen. " loc="1,20">Gen.  1,20</ref>; <ref cRef="4 Esr." loc="7,17">4 Esr. 7,17</ref>; folia, fructus <ref cRef="Num." loc="17,23">Num. 17,23</ref>; <ref cRef="Gen." loc="40,10">Gen. 40,10</ref>; <ref cRef="Cant." loc="2,13">Cant. 2,13</ref>; <ref cRef="Ps. " loc="103,16">Ps.  103,16</ref>.</sense> <sense n="g" xml:id="Dg"> <cit type="translation" xml:lang="la">
                                <quote> extrahere </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> educere </quote>
                            </cit> aliquid vel aliquem, <ref cRef="Matth." loc="7,4">Matth. 7,4</ref>; <ref cRef="Ex." loc="4,6">Ex. 4,6</ref>; <ref cRef="Ex." loc="4,7">Ex. 4,7</ref>; <foreign xml:lang="gez">አውፅኡ፡ አምእስቲሆሙ፡ እምላዕለ፡ አዕፅምቲሆሙ፡</foreign> (<foreign xml:lang="grc">ἐξέδειραν</foreign>) <ref cRef="Mich." loc="3,3">Mich. 3,3</ref>; <foreign xml:lang="gez">ኣወፅእ፡ መጥባሕትየ፡ እምነ፡ ቀቀባ፡</foreign> <ref cRef="Hez." loc="21,3">Hez. 21,3</ref>; aliquem <ref cRef="Am." loc="9,2">Am. 9,2</ref>; animam <ref cRef="Ps." loc="103,30">Ps. 103,30</ref>. <sense n="α" xml:id="Dgα"> <cit type="translation" xml:lang="la">
                                    <quote> expromere </quote>
                                </cit>: <foreign xml:lang="gez">አውፅአት፡ ርእሶ፡ እምነ፡ ጽፍነታ፡</foreign> <foreign xml:lang="grc">προελοῦσα</foreign> <ref cRef="Judith" loc="13,15">Judith 13,15</ref>; <ref cRef="Gen. " loc="24,53">Gen.  24,53</ref>; <ref cRef="Luc." loc="10,35">Luc. 10,35</ref>.</sense> <sense n="β" xml:id="Dgβ"> <cit type="translation" xml:lang="la">
                                    <quote> exuere </quote>
                                </cit>, <cit type="translation" xml:lang="la">
                                    <quote> deponere </quote>
                                </cit> vestes <ref cRef="Cant." loc="5,3">Cant. 5,3</ref>.</sense> </sense> <sense n="h" xml:id="Dh"> <cit type="translation" xml:lang="la">
                                <quote> eruere </quote>
                            </cit> oculos: <foreign xml:lang="gez">አውፅአ፡ አዕይንቲሁ፡</foreign> <ref cRef="Jer. " loc="52,11">Jer.  52,11</ref>; <ref cRef="Jud." loc="16,20">Jud. 16,20</ref>; <ref cRef="1 Reg." loc="11,2">1 Reg. 11,2</ref>; <ref cRef="Matth." loc="5,29">Matth. 5,29</ref>; <ref cRef="Matth." loc="5,30">Matth. 5,30</ref>.</sense> <sense n="i" xml:id="Di"> <cit type="translation" xml:lang="la">
                                <quote> efferre </quote>
                            </cit> i.e. <sense n="α" xml:id="Diα"> <cit type="translation" xml:lang="la">
                                    <quote> proloqui </quote>
                                </cit>: <foreign xml:lang="gez">አውፅኦ፡ ቃል፡</foreign> <ref cRef="Koh." loc="5,1">Koh. 5,1</ref>; <ref cRef="Job" loc="15,13">Job 15,13</ref> (<foreign xml:lang="gez">እምአፍ፡</foreign>); <foreign xml:lang="gez">ነገረ፡ እምልብ፡</foreign> <ref cRef="Job" loc="8,10">Job 8,10</ref>; <ref cRef="Prov." loc="10,13">Prov. 10,13</ref>.</sense> <sense n="β" xml:id="Diβ"> <cit type="translation" xml:lang="la">
                                    <quote> in vulgus efferre </quote>
                                </cit>, <cit type="translation" xml:lang="la">
                                    <quote> evulgare </quote>
                                </cit>: <foreign xml:lang="gez">ያወፅእ፡ ቃለ፡</foreign> <ref cRef="Koh." loc="10,20">Koh. 10,20</ref>; <foreign xml:lang="gez">አውፅአ፡ ስመ፡ እኩየ፡ ሊተ፡</foreign> <ref cRef="Job " loc="31,30">Job  31,30</ref>; <ref cRef="Deut." loc="22,14">Deut. 22,14</ref>; <ref cRef="Luc." loc="6,22">Luc. 6,22</ref>; <foreign xml:lang="gez">ዘያወፅእ፡ ምክረ፡ ቢጹ፡</foreign> <foreign xml:lang="grc">μὴ συντηρῶν</foreign> <ref cRef="Sir." loc="13,12">Sir. 13,12</ref>; <foreign xml:lang="gez">ኢታውፅእ፡ ቃለ፡ ዘሰማዕከ፡</foreign> <ref cRef="Sir." loc="19,7">Sir. 19,7</ref>; <ref cRef="Sir." loc="19,10">Sir. 19,10</ref>; <ref cRef="Sir. " loc="19,12">Sir.  19,12</ref>; <foreign xml:lang="gez">ኀፍረት፡ አውፅኦ፡ ነገር፡ ዘሰማዕከ፡</foreign> <ref cRef="Sir. " loc="41,23">Sir.  41,23</ref>.</sense> </sense> <sense n="k" xml:id="Dk"> <cit type="translation" xml:lang="la">
                                <quote> edere </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> emittere </quote>
                            </cit> librum, F.N. p. 6; F.N. p. 7.</sense> <sense n="l" xml:id="Dl"> <cit type="translation" xml:lang="la">
                                <quote> exserere </quote>
                            </cit>, linguam <ref cRef="Herm." loc="21">Herm. p. 21</ref>; penem <ref cRef="Hen." loc="86,4">Hen. 86,4</ref>.</sense> <sense n="m" xml:id="Dm"> <cit type="translation" xml:lang="la">
                                <quote> emittere </quote>
                            </cit> sanguinem venis: <cb n="0946" xml:id="c0946"/> <foreign xml:lang="gez">ኢያወፅእ፡ ደመ፡</foreign> <ref cRef="M. Tom.">M. Tom.</ref> ; <cit type="translation" xml:lang="la">
                                <quote> elicere </quote>
                            </cit> humorem ex lapide, <ref cRef="Sx. Haml." loc="30 ">Sx. Haml. 30 </ref>Enc.; <cit type="translation" xml:lang="la">
                                <quote> emittere </quote>
                            </cit> liquorem, ut: ubera puellae <foreign xml:lang="gez">አውፅኡ፡ ሐሊበ፡</foreign> Chrys. ho.</sense> <sense n="n" xml:id="Dn"> <cit type="translation" xml:lang="la">
                                <quote> absistere facere </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> removere </quote>
                            </cit>: <foreign xml:lang="gez">ዘኢያውፅኣ፡ ለጽድቁ፡ እምነ፡ እግዚእየ፡</foreign> <ref cRef="Gen." loc="24,27">Gen. 24,27</ref>; <lbl expand="cum">c.</lbl> <gramGrp> <case value="accusativus">Acc.</case> </gramGrp> et <foreign xml:lang="gez">እምላዕለ፡</foreign> <ref cRef="Sir." loc="30,23">Sir. 30,23</ref> (<foreign xml:lang="grc">ἀπόστησον</foreign>).</sense> <sense n="o" xml:id="Do"> <cit type="translation" xml:lang="la">
                                <quote> expendere </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> erogare </quote>
                            </cit>: <foreign xml:lang="gez">ዘአባእከ፡ ወአውፃእከ፡</foreign> <ref cRef="Sir." loc="42,7">Sir. 42,7</ref>; <foreign xml:lang="gez">እለ፡ ታወፅኡ፡ ወርቀ፡ ወብሩረ፡ እምቍናማቲክሙ፡</foreign> <ref cRef="Jes." loc="46,6">Jes. 46,6</ref> (<lbl expand="videas">vid.</lbl> <foreign xml:lang="gez">አስተዋፅአ፡</foreign>)</sense> <sense n="p" xml:id="Dp"> <cit type="translation" xml:lang="la">
                                <quote> excipere </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> excerpere </quote>
                            </cit>: <foreign xml:lang="gez">ከመ፡ ያቀልል፡ አውፅኦ፡ ትእዛዝ፡</foreign> <bibl>
                                <ptr target="bm:Ludolf1691Commentarius"/>
                            </bibl> hist. p. 305 n. 16.</sense> </sense>
               
