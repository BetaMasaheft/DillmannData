               <sense xml:id="Dmain" xml:lang="la" source="#dillmann"> (<gramGrp> <pos expand="adjectivum">adj.</pos> </gramGrp> verb., deinde) <gramGrp> <pos expand="Substantivum">subst.</pos> </gramGrp>, <lbl expand="pluralis">Pl.</lbl> <foreign xml:lang="gez">መሲሓን፡</foreign> [nom. <gramGrp> <pos expand="not found">peregr.</pos> </gramGrp>, <lbl expand="scilicet">sc.</lbl> <foreign xml:lang="he">מָשִׁיַח</foreign> <foreign xml:lang="syr">ܡܫܺܝܚܳܐ</foreign> <foreign xml:lang="ar">مَسِيحٌ</foreign>] <foreign xml:lang="grc">χριστός</foreign> <cit type="translation" xml:lang="la"><quote> unctus </quote></cit> <sense n="1" xml:id="D1"> est epitethon honorificum regum Israeliticorum aliorumve unguendo consecratorum: <ref cRef="1 Reg." loc="2,10">1 Reg. 2,10</ref>; <ref cRef="1 Reg." loc="2,35">1 Reg. 2,35</ref>; <ref cRef="1 Reg." loc="12,5">1 Reg. 12,5</ref>; <ref cRef="1 Reg." loc="24,7">1 Reg. 24,7</ref> al.; <ref cRef="2 Par." loc="6,42">2 Par. 6,42</ref>; <ref cRef="2 Par." loc="22,7">2 Par. 22,7</ref>; <ref cRef="Am." loc="6,14">Am. 6,14</ref>; <ref cRef="Sir." loc="46,19">Sir. 46,19</ref>; <ref cRef="Ps." loc="17,54">Ps. 17,54</ref>; <ref cRef="Ps." loc="19,6">Ps. 19,6</ref>; <ref cRef="Ps." loc="27,11">Ps. 27,11</ref>; <ref cRef="Ps." loc="83,9">Ps. 83,9</ref>; <ref cRef="Ps. " loc="88,37">Ps.  88,37</ref>; <ref cRef="Ps." loc="88,50">Ps. 88,50</ref>; <ref cRef="Ps." loc="131,10">Ps. 131,10</ref>; <ref cRef="Thren." loc="4,20">Thren. 4,20</ref>; de Cyro <foreign xml:lang="gez">መሲሕየ፡ ቂሮስ፡</foreign> <ref cRef="Jes." loc="45,1">Jes. 45,1</ref>; <lbl expand="pluralis">Pl.</lbl> <foreign xml:lang="gez">መሲሓንየ፡</foreign> <ref cRef="Ps. " loc="104,14">Ps.  104,14</ref>; <ref cRef="1 Par." loc="16,22">1 Par. 16,22</ref>; epitheton regis <ref cRef="Aethiopiae Sx. Teq." loc="9 ">Aethiopiae Sx. Teq. 9 </ref>Enc. et Catalog. Cod. <ref cRef="Bibl. Bodl. Aeth." loc="77.">Bibl. Bodl. Aeth. p. 77.</ref> 
</sense> <sense n="2" xml:id="D2"> <cit type="translation" xml:lang="la"><quote> Messias </quote></cit>, <cit type="translation" xml:lang="la"><quote> Christus </quote></cit> (<ref cRef="Ps." loc="2,2">Ps. 2,2</ref>) <ref cRef="4 Esr." loc="5,29">4 Esr. 5,29</ref>; <ref cRef="4 Esr." loc="5,30">4 Esr. 5,30</ref>; <ref cRef="Hen." loc="48,10">Hen. 48,10</ref>; <ref cRef="Hen. " loc="52,4">Hen.  52,4</ref>; et saepissime in libris christianis, ut: <ref cRef="Matth." loc="16,16">Matth. 16,16</ref>; <ref cRef="Matth." loc="24,5">Matth. 24,5</ref>; <ref cRef="Luc." loc="2,26">Luc. 2,26</ref>; <ref cRef="Luc." loc="9,20">Luc. 9,20</ref>; <ref cRef="Act." loc="2,36">Act. 2,36</ref>; <ref cRef="1 Joh." loc="2,22">1 Joh. 2,22</ref>; <ref cRef="1 Joh." loc="5,1">1 Joh. 5,1</ref>; <ref cRef="Apoc." loc="11,15">Apoc. 11,15</ref>; <foreign xml:lang="gez">ሐሳዌ፡ መሲሕ፡</foreign> <cit type="translation" xml:lang="la"><quote> pseudomessias </quote></cit> { <ref target="#c1427"/> vel <cit type="translation" xml:lang="la"><quote> antichristus </quote></cit> } 1 <ref cRef="Joh." loc="2,18">Joh. 2,18</ref>; <foreign xml:lang="gez">ሐሳውያነ፡ መሲሕ፡</foreign> <ref cRef="1 Joh." loc="2,18">1 Joh. 2,18</ref>; <ref cRef="1 Joh." loc="2,22">1 Joh. 2,22</ref>; <ref cRef="Matth." loc="24,24">Matth. 24,24</ref> al. 
</sense> </sense>
               
