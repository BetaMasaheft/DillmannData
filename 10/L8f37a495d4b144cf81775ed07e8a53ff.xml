            <entry xml:id="L8f37a495d4b144cf81775ed07e8a53ff" n="2737">
               <form>
                  <foreign xml:lang="gez">ሰብዐቱ</foreign>
               </form>
               <sense xml:lang="la" source="#dillmann" xml:id="Dmain"> vel <foreign xml:lang="gez">ሰባዕቱ፡</foreign>,
                  raro sine suff. <cit type="translation" xml:lang="la">
                     <quote>û</quote>
                  </cit>, <lbl expand="scilicet">sc.</lbl>
                  <foreign xml:lang="gez">ሰብዐት፡</foreign> (ut <ref cRef="Jos." loc="6,13">Jos.
                     6,13</ref>), <gramGrp>
                     <pos expand="nomen">nom.</pos>
                     <pos expand="numerale">num.</pos>
                  </gramGrp>, plerumque masculinis junctum, <gramGrp>
                     <case value="accusativus">Acc.</case>
                  </gramGrp>
                  <foreign xml:lang="gez">ሰብዐተ፡</foreign> vel <foreign xml:lang="gez">ሰባዕተ፡</foreign>, <cit type="translation" xml:lang="la">
                     <quote>septem</quote>
                  </cit>
                  <ref cRef="Gen." loc="29,27">Gen. 29,27</ref>; <ref cRef="Gen." loc="41">Gen.
                     41</ref>; <ref cRef="Deut." loc="7,1">Deut. 7,1</ref>; <foreign xml:lang="gez">ሰብዐቱ፡ ኖሎት፡</foreign>
                  <ref cRef="Mich." loc="5,4">Mich. 5,4</ref>; <foreign xml:lang="gez">ሰብዐቱ፡
                     ዐይን፡</foreign> vel <foreign xml:lang="gez">አዕይንት፡</foreign>
                  <ref cRef="Zach." loc="3,9">Zach. 3,9</ref>; <ref cRef="Zach. " loc="4,2">Zach.
                     4,2</ref>; <foreign xml:lang="gez">ሰብዐቱ፡ ቈላተ፡</foreign> (Acc.) <ref cRef="Jes." loc="11,15">Jes. 11,15</ref>; <ref cRef="Tob." loc="3,8">Tob. 3,8</ref>; <ref cRef="Tob. " loc="12,15">Tob. 12,15</ref>; <cb n="0363" xml:id="c0363"/>
                  <ref cRef="4 Esr." loc="6,55">4 Esr. 6,55</ref>; <ref cRef="Apoc." loc="1,4">Apoc.
                     1,4</ref>; <ref cRef="Apoc." loc="1,11">Apoc. 1,11</ref>; <ref cRef="Apoc." loc="1,20">Apoc. 1,20</ref> Platt; <ref cRef="Apoc." loc="5,1">Apoc. 5,1</ref>;
                     <ref cRef="Apoc. " loc="5,6">Apoc. 5,6</ref>; <lbl expand="cum">c.</lbl> suff.
                     <foreign xml:lang="gez">ሰብዐቲሆሙ፡</foreign>
                  <ref cRef="1 Reg." loc="16,10">1 Reg. 16,10</ref>; <ref cRef="Kuf." loc="16">Kuf.
                     16</ref>; <ref cRef="Kuf." loc="34">Kuf. 34</ref>; <ref cRef="Marc." loc="12,22">Marc. 12,22</ref>; <foreign xml:lang="gez">ሰባዕቱ፡ ሰባዕቱ፡</foreign>
                  <cit type="translation" xml:lang="la">
                     <quote>septeni</quote>
                  </cit>
                  <ref cRef="Gen." loc="7,2">Gen. 7,2</ref>
                  <lbl expand="sequens">seq.</lbl> – <foreign xml:lang="gez">ሰብዐተ፡</foreign>
                  <cit type="translation" xml:lang="la">
                     <quote>septies</quote>
                  </cit>
                  <ref cRef="Jos." loc="6,16">Jos. 6,16</ref> (at legendum videtur <foreign xml:lang="gez">ሳብዕተ፡</foreign>
                  <cit type="translation" xml:lang="la">
                     <quote>septimum</quote>
                  </cit>). – <foreign xml:lang="gez">እለ፡ ሰባዕቱ፡</foreign> vocabat Ludolfi Gregorius
                     <cit type="translation" xml:lang="la">
                     <quote>septemtriones</quote>
                  </cit> vel <cit type="translation" xml:lang="la">
                     <quote>ursam majorem</quote>
                  </cit>. </sense>
               
