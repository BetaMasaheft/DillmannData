               <sense xml:id="Dmain" xml:lang="la" source="#dillmann"> <gramGrp> <pos expand="Substantivum">subst.</pos> </gramGrp>, <gramGrp> <gen value="not found">m.</gen> </gramGrp> et <gramGrp> <gen value="femininus">fem.</gen> </gramGrp>, <lbl expand="pluralis">Pl.</lbl> <foreign xml:lang="gez">አምዳር፡</foreign> et <foreign xml:lang="gez">ምድራት፡</foreign>, <lbl expand="pluralis">Pl.</lbl> <lbl expand="pluralis">Pl.</lbl> <foreign xml:lang="gez">አምዳራት፡</foreign> <sense n="1" xml:id="D1"> <cit type="translation" xml:lang="la"><quote> terra </quote></cit> i.e. <sense n="a" xml:id="D1a"> <cit type="translation" xml:lang="la"><quote> orbis terrarum </quote></cit> <ref cRef="Gen." loc="1,1">Gen. 1,1</ref>; <ref cRef="Gen." loc="1,2">Gen. 1,2</ref>; <ref cRef="Gen." loc="2,1">Gen. 2,1</ref>; <ref cRef="Gen." loc="9,19">Gen. 9,19</ref>; <ref cRef="Gen." loc="19,31">Gen. 19,31</ref>; <ref cRef="Jes. " loc="24,4">Jes.  24,4</ref>; <ref cRef="Jes." loc="65,17">Jes. 65,17</ref>; <ref cRef="Jes." loc="66,1">Jes. 66,1</ref>; <ref cRef="Jer." loc="4,28">Jer. 4,28</ref>; <ref cRef="Hen." loc="1,7">Hen. 1,7</ref>; <ref cRef="Hen." loc="2,3">Hen. 2,3</ref>; <ref cRef="Deut." loc="32,1">Deut. 32,1</ref>; <ref cRef="Matth. " loc="5,18">Matth.  5,18</ref>; <ref cRef="Matth." loc="24,35">Matth. 24,35</ref>; <ref cRef="Hebr." loc="12,26">Hebr. 12,26</ref>; <ref cRef="Eph." loc="1,10">Eph. 1,10</ref>; <foreign xml:lang="gez">ኵሉ፡ ምድር፡</foreign> <cit type="translation" xml:lang="la"><quote> omnes homines </quote></cit>, <ref cRef="Judith" loc="11,16">Judith 11,16</ref>; <ref cRef="Deut." loc="32,26">Deut. 32,26</ref>; <ref cRef="Matth. " loc="5,13">Matth.  5,13</ref>. 
</sense> <sense n="b" xml:id="D1b"> <cit type="translation" xml:lang="la"><quote> continens </quote></cit> (<lbl expand="oppositum, -o, -nitur">opp.</lbl> mari et fluviis): <ref cRef="Gen." loc="1,10">Gen. 1,10</ref>-12; <ref cRef="Gen." loc="1,28">Gen. 1,28</ref>; <ref cRef="Marc." loc="4,1">Marc. 4,1</ref>; <ref cRef="Luc." loc="5,11">Luc. 5,11</ref>; <ref cRef="Hebr." loc="11,29">Hebr. 11,29</ref>; <ref cRef="Jon." loc="2,11">Jon. 2,11</ref>. 
</sense> <sense n="c" xml:id="D1c"> <cit type="translation" xml:lang="la"><quote> regio </quote></cit> (<cit type="translation" xml:lang="la"><quote> provincia </quote></cit>, <cit type="translation" xml:lang="la"><quote> patria </quote></cit>): <ref cRef="Gen." loc="12,1">Gen. 12,1</ref>; <ref cRef="Gen." loc="13,9">Gen. 13,9</ref>; <ref cRef="Gen." loc="17,8">Gen. 17,8</ref>; <ref cRef="Matth." loc="2,20">Matth. 2,20</ref>; <ref cRef="Matth." loc="4,15">Matth. 4,15</ref>; <ref cRef="Matth." loc="9,26">Matth. 9,26</ref>; <ref cRef="Jer." loc="12,14">Jer. 12,14</ref>; <ref cRef="Jer." loc="22,29">Jer. 22,29</ref>; <foreign xml:lang="gez">ውስተዝ፡ ምድር፡</foreign> <ref cRef="Gen." loc="49,10">Gen. 49,10</ref>; <foreign xml:lang="gez">ኵሉ፡ ምድረ፡ ግብጽ፡</foreign> <ref cRef="Ex." loc="7,21">Ex. 7,21</ref>; <ref cRef="Jos." loc="8,1">Jos. 8,1</ref>; <ref cRef="1 Reg." loc="9,4">1 Reg. 9,4</ref>; <foreign xml:lang="gez">ምድረ፡ በድው፡</foreign> <foreign xml:lang="grc">ἡ ἄνυδρος γῆ</foreign> <ref cRef="Jes. " loc="41,19">Jes.  41,19</ref>; <foreign xml:lang="gez">ውስተ፡ ምድረ፡ ፄው፡</foreign> <foreign xml:lang="grc">ἐν ἁλίμοις</foreign> <ref cRef="Jer." loc="17,6">Jer. 17,6</ref>; <foreign xml:lang="gez">ምድረ፡ ሕያዋን፡</foreign> <ref cRef="Jer." loc="11,19">Jer. 11,19</ref>; <foreign xml:lang="gez">ምድረ፡ ገነት፡</foreign> (paradisi) <ref cRef="Clem." loc="171">Clem. f. 171</ref>; <foreign xml:lang="gez">አምዳር፡</foreign> et <foreign xml:lang="gez">አምዳራት፡</foreign> <ref cRef="Dan." loc="11,40">Dan. 11,40</ref>; <ref cRef="Dan." loc="11,42">Dan. 11,42</ref> <lbl expand="varia lectio">var.</lbl>; <foreign xml:lang="gez">ምድር፡</foreign> <foreign xml:lang="grc">ἐπαρχία</foreign> vel <cit type="translation" xml:lang="la"><quote> provincia Metropolitae </quote></cit> <ref cRef="Can. Ant." loc="7 ">Can. Ant. 7 </ref>(9). 
</sense> <sense n="d" xml:id="D1d"> <cit type="translation" xml:lang="la"><quote> ager </quote></cit>, <cit type="translation" xml:lang="la"><quote> agri </quote></cit> <ref cRef="Matth." loc="27,7">Matth. 27,7</ref>; <ref cRef="Jes." loc="23,10">Jes. 23,10</ref>; <ref cRef="Jes." loc="28,24">Jes. 28,24</ref>; <ref cRef="Gen." loc="47,18">Gen. 47,18</ref>; <ref cRef="Gen. " loc="47,19">Gen.  47,19</ref>; <ref cRef="Lev." loc="25,24">Lev. 25,24</ref>; <ref cRef="Lev." loc="25,25">Lev. 25,25</ref>; <foreign xml:lang="gez">ተገብረ፡ ምድረ፡</foreign> 2 <ref cRef="Reg." loc="9,10">Reg. 9,10</ref>; <foreign xml:lang="gez">ምድራት፡ ርሒባት፡</foreign> <ref cRef="1 Par." loc="4,10">1 Par. 4,10</ref>; <foreign xml:lang="gez">አድባር፡ ወአምዳር፡</foreign> (<foreign xml:lang="grc">ὀρεινή</foreign> et <foreign xml:lang="grc">πεδεινή</foreign>) <ref cRef="Deut." loc="11,11">Deut. 11,11</ref>. 
</sense> <sense n="e" xml:id="D1e"> <cit type="translation" xml:lang="la"><quote> humus </quote></cit>, <cit type="translation" xml:lang="la"><quote> solum terrae </quote></cit>: <foreign xml:lang="gez">ውስተ፡ ምድር፡</foreign> <foreign xml:lang="grc">χαμαί</foreign> <ref cRef="Judith" loc="12,15">Judith 12,15</ref>; <ref cRef="Judith " loc="14,18">Judith  14,18</ref>; <ref cRef="Thren." loc="2,10">Thren. 2,10</ref>; <ref cRef="Jes." loc="47,1">Jes. 47,1</ref>; <foreign xml:lang="grc">ἔδαφος</foreign> <ref cRef="Job" loc="9,8">Job 9,8</ref>; <ref cRef="Sir." loc="11,5">Sir. 11,5</ref>; <ref cRef="Jes." loc="26,5">Jes. 26,5</ref>; <foreign xml:lang="gez">ምድረ፡ ኮነ፡</foreign> <ref cRef="Judith" loc="5,18">Judith 5,18</ref>; <ref cRef="Matth." loc="10,29">Matth. 10,29</ref>; <ref cRef="Matth." loc="15,35">Matth. 15,35</ref>; <ref cRef="Joh." loc="8,6">Joh. 8,6</ref>; humus, quatenus plantarum ferax est: <ref cRef="Matth." loc="13,8">Matth. 13,8</ref> <lbl expand="romanae editionis">rom.</lbl>; <ref cRef="Hebr." loc="6,7">Hebr. 6,7</ref>; <ref cRef="Jac." loc="5,18">Jac. 5,18</ref>; <ref cRef="4 Esr." loc="9,35">4 Esr. 9,35</ref>. 
</sense> <sense n="f" xml:id="D1f"> <cit type="translation" xml:lang="la"><quote> terrae elementum </quote></cit> vel <cit type="translation" xml:lang="la"><quote> materia </quote></cit>, (<cit type="translation" xml:lang="la"><quote> pulvis </quote></cit>, <cit type="translation" xml:lang="la"><quote> lutum </quote></cit>) <ref cRef="Sir. " loc="17,1">Sir.  17,1</ref>; <foreign xml:lang="gez">እለ፡ እምድር፡</foreign> <foreign xml:lang="grc">γηγενεῖς</foreign> <ref cRef="Prov." loc="2,18">Prov. 2,18</ref>; <foreign xml:lang="gez">ዘውስተ፡ ምድር፡</foreign> <foreign xml:lang="grc">τὰ ἐπίγεια</foreign> <ref cRef="Phil." loc="3,19">Phil. 3,19</ref>. 
</sense> 
</sense> <sense n="2" xml:id="D2"> <lbl expand="translate">transl.</lbl> <cit type="translation" xml:lang="la"><quote> solum </quote></cit>, <cit type="translation" xml:lang="la"><quote> pavimentum </quote></cit>, <cit type="translation" xml:lang="la"><quote> fundus </quote></cit>: <foreign xml:lang="grc">ἔδαφος</foreign> tabernaculi <ref cRef="Num." loc="5,17">Num. 5,17</ref>; aedificii <ref cRef="3 Reg. " loc="6,15">3 Reg.  6,15</ref>; <ref cRef="Dan. apocr." loc="13,19">Dan. apocr. 13,19</ref>; <ref cRef="Hez." loc="41,16">Hez. 41,16</ref>; <ref cRef="Hen." loc="14,10">Hen. 14,10</ref>; <ref cRef="Hen." loc="14,17">Hen. 14,17</ref>; <foreign xml:lang="gez">ውስተ፡ ምድር፡</foreign> <foreign xml:lang="grc">ἐπὶ τῆς χελωνίδος</foreign> <ref cRef="Judith" loc="14,15">Judith 14,15</ref>; <foreign xml:lang="grc">ἐσχάρα</foreign> altaris <ref cRef="Ex. " loc="6,13">Ex.  6,13</ref>; <foreign xml:lang="gez">ምድረ፡ ግብ፡</foreign> <foreign xml:lang="grc">ἔδαφος λάκκου</foreign> <ref cRef="Dan." loc="6,24">Dan. 6,24</ref>; <foreign xml:lang="gez">ምድረ፡ ባሕር፡</foreign> <ref cRef="Job " loc="38,16">Job  38,16</ref>; hinc <foreign xml:lang="gez">እምድሩ፡</foreign> <cit type="translation" xml:lang="la"><quote> funditus </quote></cit>, ut: <foreign xml:lang="gez">እመ፡ እምድሩ፡ ትኰሲ፡ ሕማማተ፡</foreign> <foreign xml:lang="grc">εἰ παντελῶς ἀναιρεῖς τὸ πάθος</foreign> hom. Theodoti Anc. in <ref cRef="Cyr." loc="102 ">Cyr. f. 102 </ref>(de quo loco <cit type="translation" xml:lang="la"><quote> Voc. Ae. </quote></cit>: <foreign xml:lang="gez">እምድሩ፡ ዘ፡ ሐንድ፡ ዘንዱን፡ አካቶሂ፡ ይ፡</foreign>). – [In linguis cognatis: amh. <foreign xml:lang="gez">ምድር፡</foreign> <cit type="translation" xml:lang="la"><quote> terra </quote></cit> <cit type="translation" xml:lang="la"><quote> ; </quote></cit> arab. <foreign xml:lang="ar">مَدَرٌ</foreign> <cit type="translation" xml:lang="la"><quote> lutum siccum; pagi, oppida </quote></cit>; syr. <foreign xml:lang="syr">ܡܶܕܪܳܐ</foreign> <cit type="translation" xml:lang="la"><quote> gleba, cespes, terra </quote></cit>.] 
</sense> </sense>
