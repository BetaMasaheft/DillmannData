                     <lbl expand="sequens">seq.</lbl>; <ref cRef="1 Petr." loc="1,3">1 Petr.
                        1,3</ref>. De <foreign xml:lang="gez">ተንሥአ፡</foreign>
                     <cit type="translation" xml:lang="la">
                        <quote>tolli</quote>
                     </cit>,<cit type="translation" xml:lang="la">
                        <quote> auferri</quote>
                     </cit>,<cit type="translation" xml:lang="la">
                        <quote> capi</quote>
                     </cit>, <cit type="translation" xml:lang="la">
