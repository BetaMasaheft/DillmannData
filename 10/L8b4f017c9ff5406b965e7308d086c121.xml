            <entry xml:id="L8b4f017c9ff5406b965e7308d086c121" n="8456">
               <form>
                  <foreign xml:lang="gez">አግብአ</foreign>
               </form>
               <sense xml:lang="la" source="#dillmann" xml:id="Dmain"> II, 1 <sense n="1" xml:id="D1"> <cit type="translation" xml:lang="la">
                                <quote> revertere </quote>
                            </cit> <sense n="a" xml:id="D1a"> <cit type="translation" xml:lang="la">
                                    <quote> avertere </quote>
                                </cit>, <cit type="translation" xml:lang="la">
                                    <quote> convertere </quote>
                                </cit>: <foreign xml:lang="gez">ከመ፡ ያግብእዎሙ፡ ለአሕዛብ፡ ኀቤሆሙ፡</foreign> <ref cRef="Act." loc="20,30">Act. 20,30</ref>.</sense> <sense n="b" xml:id="D1b"> <cit type="translation" xml:lang="la">
                                    <quote> deflectere </quote>
                                </cit>, <cit type="translation" xml:lang="la">
                                    <quote> declinare facere </quote>
                                </cit>: <foreign xml:lang="gez">ከመ፡ እንተ፡ ታገብእ፡ ዐይነ፡ መዳልው፡</foreign> <ref cRef="Jes." loc="40,15">Jes. 40,15</ref>; <ref cRef="4 Esr." loc="1,35">4 Esr. 1,35</ref>.</sense> <sense n="c" xml:id="D1c"> <cit type="translation" xml:lang="la">
                                    <quote> retro vertere </quote>
                                </cit>: <foreign xml:lang="gez">ያገብኦሙ፡ ድኅሬሆሙ፡ ለጠቢባን፡</foreign> <ref cRef="Jes." loc="44,25">Jes. 44,25</ref>; <ref cRef="Ps." loc="20,12">Ps. 20,12</ref>; <ref cRef="Ps." loc="43,12">Ps. 43,12</ref>.</sense> <sense n="d" xml:id="D1d"> <cit type="translation" xml:lang="la">
                                    <quote> rejicere </quote>
                                </cit>: <foreign xml:lang="gez">ያገብኦ፡ ድኅረ፡ ዘባኑ፡</foreign> <ref cRef="Sir." loc="21,15">Sir. 21,15</ref>; <ref cRef="Ps." loc="49,18">Ps. 49,18</ref>. <cb n="1170" xml:id="c1170"/> </sense> <sense n="e" xml:id="D1e"> <cit type="translation" xml:lang="la">
                                    <quote> retrahere </quote>
                                </cit> <ref cRef="Hen." loc="60,16">Hen. 60,16</ref>.</sense> </sense> <sense n="2" xml:id="D2"> <cit type="translation" xml:lang="la">
                                <quote> redire facere </quote>
                            </cit>, <sense n="a" xml:id="D2a"> <cit type="translation" xml:lang="la">
                                    <quote> reducere </quote>
                                </cit>, aliquem vel aliquid: <foreign xml:lang="gez">እመ፡ አእተተ፡ መኑ፡ ያገብእ፡</foreign> <ref cRef="Job" loc="9,12">Job 9,12</ref>; <foreign xml:lang="gez">ቦኑ፡ እክል፡ አግብኦቶ፡</foreign> <ref cRef="2 Reg." loc="12,23">2 Reg. 12,23</ref>; <foreign xml:lang="gez">ዕራቅየ፡ አግብአኒ፡</foreign> <ref cRef="Ruth " loc="1,21">Ruth  1,21</ref>; <foreign xml:lang="gez">ያገብእ፡ ሌሊተ፡</foreign> <ref cRef="Job" loc="34,25">Job 34,25</ref>; <lbl expand="cum">c.</lbl> <gramGrp> <case value="accusativus">Acc.</case> </gramGrp> <lbl expand="sequens">seq.</lbl> <foreign xml:lang="gez">ውስተ፡</foreign> loci <ref cRef="Gen." loc="28,15">Gen. 28,15</ref>; <ref cRef="Gen." loc="48,21">Gen. 48,21</ref>; <ref cRef="Num. " loc="22,23">Num.  22,23</ref>; <foreign xml:lang="gez">ታገብአኒ፡ ውስተ፡ መሬት፡</foreign> <ref cRef="Job" loc="10,9">Job 10,9</ref>; <ref cRef="Sir." loc="17,1">Sir. 17,1</ref>; <ref cRef="Jer." loc="23,3">Jer. 23,3</ref>; <ref cRef="Jer." loc="37,3">Jer. 37,3</ref>; <foreign xml:lang="gez">አልቦ፡ ዘያገብኦ፡ ለሰብእ፡ ውስተ፡ ሕዝቡ፡</foreign> <foreign xml:lang="grc">συνάγων</foreign> <ref cRef="Jes." loc="13,14">Jes. 13,14</ref>; <lbl expand="sequens">seq.</lbl> <gramGrp> <case value="accusativus">Acc.</case> </gramGrp> loci: <foreign xml:lang="gez">አግብኦ፡ ብሔሮ፡</foreign> <ref cRef="Dan. apocr." loc="13,39">Dan. apocr. 13,39</ref>; <lbl expand="sequens">seq.</lbl> <foreign xml:lang="gez">ኀበ፡</foreign> pers.: <ref cRef="Gen." loc="42,37">Gen. 42,37</ref>; <foreign xml:lang="gez">ከመ፡ ያግብእ፡ ልበ፡ አብ፡ ኀበ፡ ውሉድ፡</foreign> <ref cRef="Sir." loc="48,10">Sir. 48,10</ref>.</sense> <sense n="b" xml:id="D2b"> <cit type="translation" xml:lang="la">
                                    <quote> reportare </quote>
                                </cit> vel <cit type="translation" xml:lang="la">
                                    <quote> referre </quote>
                                </cit> aliquid: <foreign xml:lang="gez">ያገብኡ፡ ለክሙ፡ ንዋየ፡ ቤተ፡ እግዚአብሔር፡ እምባቢሎን፡</foreign> <ref cRef="Jer." loc="34,13">Jer. 34,13</ref>; <ref cRef="Gen." loc="44,8">Gen. 44,8</ref>; <foreign xml:lang="gez">ርግብ፡ ዘሥልሰ፡ አግብአት፡ ቃለ፡ ለኖኅ፡</foreign> <ref cRef="Rel. Bar." loc="">Rel. Bar. </ref>
                            </sense> <sense n="c" xml:id="D2c"> <cit type="translation" xml:lang="la">
                                    <quote> remittere </quote>
                                </cit>, <ref cRef="1 Reg." loc="29,4">1 Reg. 29,4</ref>; <foreign xml:lang="gez">አግብአቶ፡ አስቴር፡ ለዝኩ፡ ዘመጽአ፡</foreign> <ref cRef="Esth." loc="4,15">Esth. 4,15</ref>.</sense> <sense n="d" xml:id="D2d"> <cit type="translation" xml:lang="la">
                                    <quote> restituere </quote>
                                </cit> aliquid vel aliquem in locum vel statum pristinum: <foreign xml:lang="gez">አግብእ፡ መጥባሕተከ፡ ውስተ፡ ቤቱ፡</foreign> <ref cRef="Matth." loc="26,52">Matth. 26,52</ref>; <ref cRef="1 Par." loc="21,27">1 Par. 21,27</ref>; <foreign xml:lang="gez">አግብኦ፡ ውስተ፡ ግብሩ፡</foreign> <ref cRef="Kuf." loc="39">Kuf. 39</ref>; <ref cRef="Gen." loc="40,13">Gen. 40,13</ref>; <ref cRef="Ps. " loc="15,5">Ps.  15,5</ref>; <ref cRef="Sx. Ter" loc="5 Enc.">Sx. Ter 5 Enc.</ref> (<lbl expand="videas">vid. sub</lbl> <foreign xml:lang="gez">ህላዌ፡</foreign>); animam i.e. <cit type="translation" xml:lang="la">
                                    <quote> recreare </quote>
                                </cit>: <foreign xml:lang="gez">ዘያገብኣ፡ ለነፍስየ፡</foreign> <ref cRef="Thren. " loc="1,16">Thren.  1,16</ref>; <ref cRef="Ruth" loc="4,15">Ruth 4,15</ref>.</sense> <sense n="e" xml:id="D2e"> <cit type="translation" xml:lang="la">
                                    <quote> reddere </quote>
                                </cit>, <lbl expand="cum">c.</lbl> <gramGrp> <case value="accusativus">Acc.</case> </gramGrp> rei et <foreign xml:lang="gez">ለ፡</foreign> pers., ut rem vi vel furto ablatam <ref cRef="Deut." loc="28,31">Deut. 28,31</ref>; <ref cRef="Jud." loc="17,3">Jud. 17,3</ref>; <ref cRef="Lev." loc="5,23">Lev. 5,23</ref>; <ref cRef="Num." loc="5,7">Num. 5,7</ref>; <ref cRef="2 Reg. " loc="9,7">2 Reg.  9,7</ref>; <ref cRef="Tob." loc="2,13">Tob. 2,13</ref>; amissam <ref cRef="Ex." loc="23,4">Ex. 23,4</ref>; <ref cRef="Deut." loc="22,2">Deut. 22,2</ref>; pignus <ref cRef="Ex." loc="22,25">Ex. 22,25</ref>; <ref cRef="Deut." loc="24,13">Deut. 24,13</ref>; <ref cRef="Hez." loc="18,7">Hez. 18,7</ref>; alia <ref cRef="2 Esr." loc="15,11">2 Esr. 15,11</ref>; <foreign xml:lang="gez">በርበረ፡ ማኅፈደ፡ ወኢያግብአ፡</foreign> <ref cRef="Job" loc="20,19">Job 20,19</ref>; <foreign xml:lang="gez">አግብአ፡ ፈርዖን፡ ሳራሃ፡ ለአብራም፡</foreign> <ref cRef="Kuf." loc="13">Kuf. 13</ref>; <ref cRef="4 Esr." loc="5,33">4 Esr. 5,33</ref>; <ref cRef="Apoc." loc="20,13">Apoc. 20,13</ref>; <ref cRef="Hen." loc="51,1">Hen. 51,1</ref>; <ref cRef="Matth." loc="27,3">Matth. 27,3</ref>; reddere responsum<bibl>
                                    <ptr target="bm:Ludolf1691Commentarius"/>
                                </bibl> p. 484. <sense n="α" xml:id="D2eα"> <cit type="translation" xml:lang="la">
                                        <quote> referre </quote>
                                    </cit> opus imperatum, postquam perfectum est, <ref cRef="Ex." loc="5,18">Ex. 5,18</ref>.</sense> <sense n="β" xml:id="D2eβ"> <cit type="translation" xml:lang="la">
                                        <quote> redire facere </quote>
                                    </cit> possessionem ad possessorem pristinum <ref cRef="Lev." loc="25,28">Lev. 25,28</ref>; <ref cRef="Lev. " loc="25,31">Lev.  25,31</ref>.</sense> <sense n="γ" xml:id="D2eγ"> <cit type="translation" xml:lang="la">
                                        <quote> retribuere </quote>
                                    </cit>, <cit type="translation" xml:lang="la">
                                        <quote> referre </quote>
                                    </cit>, <cit type="translation" xml:lang="la">
                                        <quote> rependere </quote>
                                    </cit>, <lbl expand="cum">c.</lbl> <gramGrp> <case value="accusativus">Acc.</case> </gramGrp> rei et <foreign xml:lang="gez">ላዕለ፡</foreign> pers. <ref cRef="Hez." loc="7,3">Hez. 7,3</ref>; <ref cRef="Hez." loc="7,4">Hez. 7,4</ref>; <foreign xml:lang="gez">ኣገብእ፡ ፍኖተኪ፡ ላዕለ፡ ርእስኪ፡</foreign> <ref cRef="Hez." loc="16,43">Hez. 16,43</ref>; <ref cRef="1 Reg. " loc="25,39">1 Reg.  25,39</ref>; <foreign xml:lang="gez">ከመ፡ ያግብእ፡ ላዕሌሆሙ፡ ዐውያተ፡ ነዳያን፡</foreign> <ref cRef="Job " loc="34,28">Job  34,28</ref>; <lbl expand="cum">c.</lbl> <gramGrp> <case value="accusativus">Acc.</case> </gramGrp> et <foreign xml:lang="gez">ለ፡</foreign> <ref cRef="Gen." loc="50,15">Gen. 50,15</ref>; <ref cRef="Jes. " loc="65,6">Jes.  65,6</ref>; <lbl expand="cum">c.</lbl> <gramGrp> <case value="accusativus">Acc.</case> </gramGrp> rei: <foreign xml:lang="gez">እግዚአብሔር፡ ያገብእ፡ ዕስየተ፡</foreign> <ref cRef="Sir." loc="32,13">Sir. 32,13</ref>; <foreign xml:lang="gez">እግዚአብሔር፡ ያገብእ፡ ፍትሐ፡</foreign> <ref cRef="Jes." loc="35,4">Jes. 35,4</ref>.</sense> <sense n="δ" xml:id="D2eδ"> <cit type="translation" xml:lang="la">
                                        <quote> reddere rationem </quote>
                                    </cit>, <lbl expand="sequens">seq.</lbl> <foreign xml:lang="gez">ሐሳበ፡</foreign> vel <foreign xml:lang="gez">ቃለ፡</foreign>, ut: <foreign xml:lang="gez">ያገብእ፡ ሐሳበ፡ ለዘይኴንን፡</foreign> <ref cRef="1 Petr." loc="4,5">1 Petr. 4,5</ref>; <foreign xml:lang="gez">በእንቲአሁ፡</foreign> <ref cRef="Matth." loc="12,36">Matth. 12,36</ref>; <foreign xml:lang="gez">ኣገብእ፡ ቃለ፡ በእንተ፡ ኵሉ፡ ዘገበርኩ፡</foreign> <ref cRef="Genz." loc="125">Genz. f. 125</ref>; etiam omisso <foreign xml:lang="gez">ቃለ፡</foreign>, ut: <foreign xml:lang="gez">ከመ፡ ያግብኡ፡ ኀቤሆሙ፡ ነገሥት፡</foreign> <ref cRef="Dan. " loc="6,2">Dan.  6,2</ref>.</sense> <sense n="ε" xml:id="D2eε"> reddere i.e. <cit type="translation" xml:lang="la">
                                        <quote> ferre sententiam </quote>
                                    </cit>, <lbl expand="sequens">seq.</lbl> <foreign xml:lang="gez">ፍትሐ፡</foreign> <ref cRef="Marc." loc="14,64">Marc. 14,64</ref>; <foreign xml:lang="gez">በጽድቅ፡ ያገብእ፡ ፍትሐ፡</foreign> <ref cRef="Jes." loc="42,3">Jes. 42,3</ref>; <ref cRef="Jes." loc="42,4">Jes. 42,4</ref>; <foreign xml:lang="gez">ያገብኡ፡ ሎቱ፡ ፍትሐ፡</foreign> <ref cRef="Sir." loc="8,14">Sir. 8,14</ref>; <foreign xml:lang="gez">አግብአ፡ ፍትሐ፡ ለዘሩባቤል፡</foreign> Lud. ex <ref cRef="Org. Edz.">Org. Edz.</ref> </sense> </sense> <sense n="f" xml:id="D2f"> <cit type="translation" xml:lang="la">
                                    <quote> reposcere </quote>
                                </cit>, <lbl expand="cum">c.</lbl> <gramGrp> <case value="accusativus">Acc.</case> </gramGrp> rei et <foreign xml:lang="gez">እምነ፡</foreign> pers., <ref cRef="F.N." loc=" 24,3">F.N.  24,3</ref> (<lbl expand="videas">vid. sub</lbl> <foreign xml:lang="gez">ሕፄ፡</foreign> col. 140).</sense> <sense n="g" xml:id="D2g"> <cit type="translation" xml:lang="la">
                                    <quote> referre </quote>
                                </cit> tributa, vectigalia, i.e. <cit type="translation" xml:lang="la">
                                    <quote> pendere </quote>
                                </cit>: <foreign xml:lang="gez">እለ፡ ያገብኡ፡ ጸባሕተ፡</foreign> <ref cRef="2 Reg." loc="8,2">2 Reg. 8,2</ref>; <ref cRef="Gen." loc="47,24">Gen. 47,24</ref> (ubi F <foreign xml:lang="gez">ታጋብኡ፡</foreign> II, 3 exhibet); <ref cRef="Gen." loc="47,26">Gen. 47,26</ref>; <foreign xml:lang="gez">አልብከ፡ ጸባሕት፡ ወኢያገብኡ፡ ለከ፡</foreign> <ref cRef="2 Esr." loc="4,13">2 Esr. 4,13</ref>; <ref cRef="Rom." loc="13,6">Rom. 13,6</ref>.</sense> <sense n="h" xml:id="D2h"> <cit type="translation" xml:lang="la">
                                    <quote> redigere </quote>
                                </cit> <sense n="α" xml:id="D2hα"> aliquid in ditionem alicujus: <foreign xml:lang="gez">አግብአ፡ ለፈርዖን፡ ኵሎ፡ ምድሮሙ፡</foreign> <ref cRef="Gen." loc="47,20">Gen. 47,20</ref>.</sense> <sense n="β" xml:id="D2hβ"> aliquem i.e. <cit type="translation" xml:lang="la">
                                        <quote> adigere </quote>
                                    </cit>, <lbl expand="sequens">seq.</lbl> <gramGrp> <pos expand="Subjunctivus">Subj.</pos> </gramGrp>: <foreign xml:lang="gez">ፍቅረ፡ ክርስቶስ፡ ያገብአነ፡</foreign> (Platt <foreign xml:lang="gez">ያጌብረነ፡</foreign>) <foreign xml:lang="gez">ናጥብዕ፡</foreign> <ref cRef="2 Cor. " loc="5,13">2 Cor.  5,13</ref>.</sense>
                            </sense> <sense n="i" xml:id="D2i"> <cit type="translation" xml:lang="la">
                                    <quote> transmutare </quote>
                                </cit> aliquid in aliquid (<lbl expand="videas">vid.</lbl> <foreign xml:lang="gez">ገብአ፡</foreign> <ref cRef="Nr." loc="1,">Nr. 1,</ref> i): <foreign xml:lang="gez">ትፍሥሕተክሙ፡ ውስተ፡ ትካዝ፡ አግብእዎ፡</foreign> <ref cRef="Jac." loc="4,9">Jac. 4,9</ref>.</sense>
                        </sense> <sense n="3" xml:id="D3"> facere ut aliquid veniat ad aliquem i.e. <cit type="translation" xml:lang="la">
                                <quote> tradere </quote>
