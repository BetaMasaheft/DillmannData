         <div>
            <entry xml:id="L17ccf661440648efb6e6048635970eef" n="7646">
               <form>
                  <foreign xml:lang="gez">ዘከረ</foreign>
               </form>
               <sense xml:lang="la" source="#dillmann" xml:id="Dmain"> <subc>I,2</subc> <sense n="1" xml:id="D1">
                     <cit type="translation" xml:lang="la">
                        <quote>memorem facere</quote>
                     </cit> aliquem, <cit type="translation" xml:lang="la">
                        <quote>in memoriam revocare</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>monere</quote>
                     </cit>, <lbl expand="cum">c.</lbl>
                     <gramGrp>
                        <case value="accusativus">Acc.</case>
                     </gramGrp> et <foreign xml:lang="gez">በእንተ፡</foreign>
                     <ref cRef="Gen." loc="40,14">Gen. 40,14</ref>; <lbl expand="cum">c.</lbl>
                     <lbl expand="duplex">dupl.</lbl>
                     <gramGrp>
                        <case value="accusativus">Acc.</case>
                     </gramGrp>: <foreign xml:lang="gez">እዜክረከ፡ ኵሎ፡</foreign>
                     <ref cRef="Kuf." loc="118">Kuf. p. 118</ref>; <lbl expand="sequens">seq.</lbl>
                     <foreign xml:lang="gez">ከመ፡</foreign>, ut: <foreign xml:lang="gez">ዘክሮሙ፡ ከመ፡
                        ይትአዘዙ፡</foreign>
                     <ref cRef="Tit." loc="3,1">Tit. 3,1</ref>; <ref cRef="2 Tim." loc="2,14">2 Tim.
                        2,14</ref>; <foreign xml:lang="gez">በዘክሮ፡</foreign>
                     <ref cRef="2 Petr." loc="1,13">2 Petr. 1,13</ref>; <ref cRef="2 Petr. " loc="3,1">2 Petr. 3,1</ref>.</sense>
                  <sense n="2" xml:id="D2">
                     <cit type="translation" xml:lang="la">
                        <quote>memorare</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>mentionem facere</quote>
                     </cit>: <ref cRef="Zach." loc="13,2">Zach. 13,2</ref>
                     <lbl expand="varia lectio">var.</lbl>
                  </sense>
               </sense>
               
