               <sense xml:id="Dmain" xml:lang="la" source="#dillmann"> <subc>I,1</subc> [<foreign xml:lang="he">רוּץ</foreign> <foreign xml:lang="syr">ܪܗܶܛ</foreign>; amh. <foreign xml:lang="gez">ሮጠ፡</foreign>; vicina sunt <foreign xml:lang="gez">ረየፀ፡</foreign> et <foreign xml:lang="gez">ሮደ፡</foreign>] <gramGrp> <pos expand="Subjunctivus">Subj.</pos> </gramGrp> <foreign xml:lang="gez">ይሩጽ፡</foreign> <cit type="translation" xml:lang="la"><quote> currere </quote></cit> (<cit type="translation" xml:lang="la"><quote> viam celerare </quote></cit>, <cit type="translation" xml:lang="la"><quote> volare </quote></cit>): <ref cRef="Prov." loc="4,12">Prov. 4,12</ref>; <ref cRef="2 Reg." loc="18,19">2 Reg. 18,19</ref>-25; <ref cRef="Num." loc="11,27">Num. 11,27</ref>; <foreign xml:lang="gez">ሩጽ፡</foreign> <ref cRef="Kuf." loc="43">Kuf. 43</ref>; <foreign xml:lang="gez">ረዊጽ፡</foreign> <foreign xml:lang="grc">ὁ δρόμος</foreign> <ref cRef="Koh." loc="9,11">Koh. 9,11</ref>; <ref cRef="Job" loc="9,25">Job 9,25</ref>; <foreign xml:lang="gez">ፍጡነ፡ ይመጽኡ፡ እንዘ፡ ይረውጹ፡</foreign> <ref cRef="Jes." loc="5,26">Jes. 5,26</ref>; <foreign xml:lang="gez">ይረውጹ፡ ከመ፡ ሀየል፡ ሐንካሳን፡</foreign> <ref cRef="Jes." loc="35,6">Jes. 35,6</ref>; <ref cRef="Jes." loc="40,31">Jes. 40,31</ref>; <ref cRef="Matth." loc="27,48">Matth. 27,48</ref>; <ref cRef="Matth." loc="28,8">Matth. 28,8</ref>; <ref cRef="Rom." loc="9,16">Rom. 9,16</ref>; <ref cRef="Joel" loc="2,7">Joel 2,7</ref>; <ref cRef="Joel" loc="2,9">Joel 2,9</ref>; <foreign xml:lang="grc">ὁρμᾶν</foreign> <ref cRef="Matth." loc="8,32">Matth. 8,32</ref> {DiL <foreign xml:lang="grc">ὁρμᾷν</foreign>}; de avibus <ref cRef="Job" loc="39,30">Job 39,30</ref>; curru <foreign xml:lang="gez">ሰረገላት፡ ዘይረውጽ፡</foreign> <ref cRef="Nah." loc="3,2">Nah. 3,2</ref>; vento <ref cRef="Jes." loc="5,28">Jes. 5,28</ref>; igne <ref cRef="Hen." loc="23,2">Hen. 23,2</ref>; fulgure <foreign xml:lang="gez">ከመ፡ ይረውጽ፡ መብረቅ፡ ቅድመ፡ ነጐድጓድ፡</foreign> <ref cRef="Sir." loc="35,10">Sir. 35,10</ref>; <ref cRef="Ex." loc="9,23">Ex. 9,23</ref>; <ref cRef="Nah." loc="2,5">Nah. 2,5</ref>; sole: <foreign xml:lang="gez">ፍጡነ፡ ይረውጽ፡ ፀሓይ፡</foreign> <ref cRef="1 Esr." loc="4,34">1 Esr. 4,34</ref>; <ref cRef="Hen." loc="72,37">Hen. 72,37</ref>; verbo <ref cRef="Ps." loc="147,4">Ps. 147,4</ref>; 2 <ref cRef="Thess." loc="3,1">Thess. 3,1</ref>; de navigantibus <ref cRef="Act." loc="21,1">Act. 21,1</ref>; <foreign xml:lang="grc">σπεύδειν</foreign> <foreign xml:lang="gez">እንዘ፡ ይረውጹ፡</foreign> <foreign xml:lang="grc">μετὰ σπουδῆς</foreign> <ref cRef="Dan. apocr." loc="1,50">Dan. apocr. 1,50</ref>; <foreign xml:lang="gez">ሩጹ፡ ወኢትቁሙ፡</foreign> <ref cRef="Jer." loc="4,6">Jer. 4,6</ref>; <foreign xml:lang="gez">ሩጽ፡ ጼውው፡</foreign> <foreign xml:lang="grc">ὀξέως</foreign> <ref cRef="Jes." loc="8,3">Jes. 8,3</ref>; <lbl expand="cum">c.</lbl> <gramGrp> <case value="accusativus">Acc.</case> </gramGrp> viae <ref cRef="Ps." loc="118,32">Ps. 118,32</ref>. <sense n="a" xml:id="Da"> <cit type="translation" xml:lang="la"><quote> currendo discedere </quote></cit>, <cit type="translation" xml:lang="la"><quote> se auferre </quote></cit>: <foreign xml:lang="gez">ሩጽ፡</foreign> <foreign xml:lang="grc">ἀπότρεχε</foreign> <ref cRef="Jer." loc="47,5">Jer. 47,5</ref>. 
</sense> <sense n="b" xml:id="Db"> <cit type="translation" xml:lang="la"><quote> praetercurrere </quote></cit>, <cit type="translation" xml:lang="la"><quote> cito praeterire </quote></cit>: <foreign xml:lang="gez">ከመ፡ ኅልፈተ፡ ዜና፡ ዘይረውጽ፡</foreign> <ref cRef="Sap." loc="5,9">Sap. 5,9</ref>. 
</sense> <sense n="c" xml:id="Dc"> <cit type="translation" xml:lang="la"><quote> circumcurrere </quote></cit>, <cit type="translation" xml:lang="la"><quote> discurrere </quote></cit>: <foreign xml:lang="gez">ሩጹ፡ ውስተ፡ ፍናዊሃ፡ ለኢየሩሳሌም፡</foreign> <ref cRef="Jer." loc="5,1">Jer. 5,1</ref>; <foreign xml:lang="gez">ይረውጽ፡ ኀበ፡ አባግዕ፡</foreign> <ref cRef="Herm." loc="56.">Herm. p. 56.</ref> 
</sense> <sense n="d" xml:id="Dd"> <cit type="translation" xml:lang="la"><quote> praecurrere </quote></cit> <lbl expand="cum">c.</lbl> <foreign xml:lang="gez">ቅድመ፡</foreign>, ut: <ref cRef="1 Reg." loc="8,11">1 Reg. 8,11</ref>; 2 <ref cRef="Reg." loc="15,1">Reg. 15,1</ref>. 
</sense> <sense n="e" xml:id="De"> <cit type="translation" xml:lang="la"><quote> cursu tedenre </quote></cit> aliquo, <cit type="translation" xml:lang="la"><quote> acurrere </quote></cit>, <cit type="translation" xml:lang="la"><quote> currendo petere </quote></cit>, <ref cRef="Marc." loc="9,15">Marc. 9,15</ref>; <ref cRef="Gen." loc="18,2">Gen. 18,2</ref>; <ref cRef="Gen." loc="33,4">Gen. 33,4</ref>; <lbl expand="cum">c.</lbl> <foreign xml:lang="gez">ኀበ፡</foreign> <ref cRef="Hebr." loc="12,1">Hebr. 12,1</ref>; <ref cRef="Gen. " loc="18,6">Gen.  18,6</ref>; <foreign xml:lang="gez">ሮጹ፡ ኀቤሃ፡</foreign> <ref cRef="Dan. apocr." loc="1,19">Dan. apocr. 1,19</ref>; <ref cRef="Luc." loc="24,12">Luc. 24,12</ref>; <foreign xml:lang="gez">ዘርእየ፡ በለሰ፡ ወይረውጽ፡ ኀቤሃ፡</foreign> <ref cRef="Jes." loc="28,4">Jes. 28,4</ref>; <foreign xml:lang="gez">እጐሊሁ፡ ይረውጹ፡ ኀበ፡ ደም፡</foreign> <ref cRef="Job" loc="39,30">Job 39,30</ref>; vel <lbl expand="cum">c.</lbl> <foreign xml:lang="gez">ለ፡</foreign>, ut: <foreign xml:lang="gez">ለኀጢአት፡ ይረውጻ፡ እገሪሆሙ፡</foreign> <ref cRef="Jes." loc="59,7">Jes. 59,7</ref>. 
</sense> <sense n="f" xml:id="Df"> <cit type="translation" xml:lang="la"><quote> currendo sequi </quote></cit>: <foreign xml:lang="gez">ሮጸ፡ ድኅሬሆሙ፡</foreign> <ref cRef="Kuf." loc="43.">Kuf. 43.</ref> 
</sense> <sense n="g" xml:id="Dg"> <cit type="translation" xml:lang="la"><quote> incursare </quote></cit>, <cit type="translation" xml:lang="la"><quote> incurrere </quote></cit>: <foreign xml:lang="gez">ሮጸ፡ ኀቤሁ፡ በኀይል፡</foreign> <ref cRef="Dan. apocr." loc="8,6">Dan. apocr. 8,6</ref>; <foreign xml:lang="gez">ሮዱኒ፡ ወሮጹ፡ ላዕሌየ፡</foreign> <ref cRef="Job " loc="16,14">Job  16,14</ref>. 
</sense> </sense>
               
