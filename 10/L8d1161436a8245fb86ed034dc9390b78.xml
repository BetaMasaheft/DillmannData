               <sense xml:lang="la" source="#dillmann" xml:id="Dmain"> <subc>I,1</subc> [<foreign xml:lang="he">יָבֵשׁ</foreign> <foreign xml:lang="syr">ܝܺܒܶܫ</foreign> <foreign xml:lang="ar">يَبِسَ</foreign>] <gramGrp> <pos expand="Subjunctivus">Subj.</pos> </gramGrp> <foreign xml:lang="gez">ይይበስ፡</foreign>, Imperat. <foreign xml:lang="gez">ይበስ፡</foreign>, <cit type="translation" xml:lang="la">
                            <quote> siccum </quote>
                        </cit> vel <cit type="translation" xml:lang="la">
                            <quote> aridum esse </quote>
                        </cit> vel <cit type="translation" xml:lang="la">
