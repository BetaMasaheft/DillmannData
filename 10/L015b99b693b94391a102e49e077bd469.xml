            <entry xml:id="L015b99b693b94391a102e49e077bd469" n="10122">
               <form>
                  <foreign xml:lang="gez">ተፈጸመ</foreign>
               </form>
               <sense xml:id="Dmain" xml:lang="la" source="#dillmann"> <subc>III,2</subc> (passim <subc>III,1</subc>) <sense n="1" xml:id="D1"> <cit type="translation" xml:lang="la"><quote> impleri </quote></cit>, <sense n="a" xml:id="D1a"> <cit type="translation" xml:lang="la"><quote> plenum </quote></cit> <cit type="translation" xml:lang="la"><quote> fieri </quote></cit>, de luna <ref cRef="Hen." loc="78,11">Hen. 78,11</ref>; <ref cRef="Hen." loc="78,13">Hen. 78,13</ref>.
</sense> <sense n="b" xml:id="D1b"> <cit type="translation" xml:lang="la"><quote> compleri </quote></cit>, <cit type="translation" xml:lang="la"><quote> completum </quote></cit> <cit type="translation" xml:lang="la"><quote> esse </quote></cit>: <foreign xml:lang="gez">ኢተፈጸመ፡ ኀጢአቶሙ፡ እስከ፡ ይእዜ፡</foreign> <ref cRef="Kuf." loc="14">Kuf. 14</ref>; <ref cRef="Gen." loc="15,16">Gen. 15,16</ref>; <ref cRef="1 Thess." loc="2,16">1 Thess. 2,16</ref>.
</sense> <sense n="c" xml:id="D1c"> de vaticiniis: <ref cRef="Asc. Jes." loc="11,38">Asc. Jes. 11,38</ref>; <ref cRef="Matth." loc="1,22">Matth. 1,22</ref>; <ref cRef="Matth." loc="2,15">Matth. 2,15</ref>; <ref cRef="Matth." loc="2,17">Matth. 2,17</ref>; <ref cRef="Matth." loc="2,23">Matth. 2,23</ref>; <ref cRef="Matth." loc="4,14">Matth. 4,14</ref>; <ref cRef="Matth." loc="8,17">Matth. 8,17</ref>; <ref cRef="Matth." loc="12,17">Matth. 12,17</ref> al.; <ref cRef="Luc." loc="22,37">Luc. 22,37</ref>; Lud. Comm. hist. p. 302 n. 4.</sense>
</sense> <sense n="2" xml:id="D2"> <cit type="translation" xml:lang="la"><quote> peragi </quote></cit>, <cit type="translation" xml:lang="la"><quote> perfici </quote></cit>: <ref cRef="Hen." loc="10,12">Hen. 10,12</ref>; <ref cRef="Hen." loc="16,1">Hen. 16,1</ref>; si adfuissem <foreign xml:lang="gez">እምኢተፈጸመ፡ ሎሙዝ፡</foreign> <ref cRef="Jsp." loc="388">Jsp. p. 388</ref>; <ref cRef="Jac." loc="1,15">Jac. 1,15</ref>; <cb n="1388" xml:id="c1388"/> <ref cRef="Luc." loc="22,37">Luc. 22,37</ref>; <foreign xml:lang="gez">ለእመ፡ ኢተፈጸመ፡</foreign> (<note>zu Stand kommt</note>) <foreign xml:lang="gez">ከዊነ፡ መርዓ፡</foreign> F.N. 24,3; F.N. 32,1.
</sense> <sense n="3" xml:id="D3"> <cit type="translation" xml:lang="la"><quote> consummari </quote></cit>, <cit type="translation" xml:lang="la"><quote> absolvi </quote></cit>, <cit type="translation" xml:lang="la"><quote> finiri </quote></cit>, <cit type="translation" xml:lang="la"><quote> ad </quote></cit> <cit type="translation" xml:lang="la"><quote> finem </quote></cit> <cit type="translation" xml:lang="la"><quote> perduci </quote></cit>: <ref cRef="Gen." loc="2,1">Gen. 2,1</ref>; <ref cRef="Gen." loc="6,16">Gen. 6,16</ref>; <ref cRef="Kuf." loc="2">Kuf. 2</ref>; <foreign xml:lang="gez">ወተፈጸመ፡ ቤቱ፡</foreign> <ref cRef="1 Esr." loc="7,5">1 Esr. 7,5</ref>; <ref cRef="2 Esr." loc="5,16">2 Esr. 5,16</ref>; <foreign xml:lang="gez">ወእምዝ፡ ተፈጺሞ፡ በዓል፡</foreign> <ref cRef="Tob." loc="10,7">Tob. 10,7</ref>; <ref cRef="Hen." loc="25,4">Hen. 25,4</ref>; <ref cRef="Joh." loc="19,28">Joh. 19,28</ref>; <ref cRef="Joh." loc="19,30">Joh. 19,30</ref>; <ref cRef="Matth." loc="10,23">Matth. 10,23</ref>; in specie de tempore: <ref cRef="Kuf." loc="3">Kuf. 3</ref>; <ref cRef="Kuf." loc="19">Kuf. 19</ref>; <ref cRef="Lev." loc="25,30">Lev. 25,30</ref>; <ref cRef="Deut." loc="34,8">Deut. 34,8</ref>; Tob.10,7; <foreign xml:lang="gez">ተፈጺሞ፡ መዋዕል፡</foreign> <ref cRef="Dan." loc="1,18">Dan. 1,18</ref>; <ref cRef="Job" loc="1,5">Job 1,5</ref>; <ref cRef="Luc." loc="2,21">Luc. 2,21</ref>; <ref cRef="Luc. " loc="4,2">Luc.  4,2</ref>; <ref cRef="Act." loc="7,23">Act. 7,23</ref>; <ref cRef="Apoc." loc="20,3">Apoc. 20,3</ref>; <ref cRef="Apoc." loc="20,5">Apoc. 20,5</ref>. <sense n="a" xml:id="D3a"> <cit type="translation" xml:lang="la"><quote> finem </quote></cit> <cit type="translation" xml:lang="la"><quote> habere </quote></cit> vel <cit type="translation" xml:lang="la"><quote> capere </quote></cit>, <cit type="translation" xml:lang="la"><quote> desinere </quote></cit>: <foreign xml:lang="gez">ኢተፈጸመ፡ ሣህሉ፡</foreign> <ref cRef="Thren." loc="3,22">Thren. 3,22</ref>; <ref cRef="1 Cor." loc="13,8">1 Cor. 13,8</ref>; <foreign xml:lang="gez">ዘኢይትፌጸም፡</foreign> <cit type="translation" xml:lang="la"><quote> infinitus </quote></cit>, Lit. Cyr.
</sense> <sense n="b" xml:id="D3b"> <cit type="translation" xml:lang="la"><quote> consumi </quote></cit>, <cit type="translation" xml:lang="la"><quote> absumi </quote></cit>: <foreign xml:lang="gez">ተፈጸመ፡ ማይ፡ እምዝቃ፡</foreign> <ref cRef="Kuf." loc="17">Kuf. 17</ref>; <ref cRef="3 Reg." loc="14,10">3 Reg. 14,10</ref>; <ref cRef="Hen." loc="19,1">Hen. 19,1</ref>.
</sense> <sense n="c" xml:id="D3c"> <cit type="translation" xml:lang="la"><quote> suprema </quote></cit> <cit type="translation" xml:lang="la"><quote> obire </quote></cit>, de animatis: <ref cRef="Apoc." loc="6,11">Apoc. 6,11</ref>; ex aëre decidit <foreign xml:lang="gez">ወተፈጸመ፡ ሶቤሃ፡ ወሞተ፡</foreign> <ref cRef="Clem." loc="254.">Clem. f. 254.</ref> 
</sense> <sense n="d" xml:id="D3d"> liber <cit type="translation" xml:lang="la"><quote> explicit </quote></cit>, ut: <foreign xml:lang="gez">ተፈጸመ፡ በዝየ፡ ጥበቢሁ፡ ለኢያሱ፡ ወልደ፡ ሲራክ፡</foreign> Sir. in subscr.; Asc. Jes. ed <ref cRef="Laur." loc="78">Laur. p. 78</ref>; Psalt. ed <ref cRef="Lud." loc="318">Lud. p. 318</ref>; <ref cRef="Herm." loc="108,">Herm. p. 108,</ref> al. creberrime.
</sense> <sense n="e" xml:id="D3e"> <cit type="translation" xml:lang="la"><quote> transigi </quote></cit>, <cit type="translation" xml:lang="la"><quote> dirimi </quote></cit>, de controversiis: <foreign xml:lang="gez">ኵሉ፡ ጋእዝ፡ ዘትከውን፡ ማእከሌሆሙ፡ ትትፌጸም፡ በመሐላ፡</foreign> F.N. 43,5.
</sense> <sense n="f" xml:id="D3f"> <cit type="translation" xml:lang="la"><quote> ad </quote></cit> <cit type="translation" xml:lang="la"><quote> perfectionem </quote></cit> <cit type="translation" xml:lang="la"><quote> perduci </quote></cit>, <cit type="translation" xml:lang="la"><quote> perfectum </quote></cit> <cit type="translation" xml:lang="la"><quote> fieri </quote></cit>: <ref cRef="Joh." loc="3,29">Joh. 3,29</ref>; <ref cRef="Luc." loc="22,16">Luc. 22,16</ref>; <ref cRef="1 Joh." loc="4,17">1 Joh. 4,17</ref>.
</sense> <sense n="g" xml:id="D3g"> <foreign xml:lang="grc">τελεῖσθαι</foreign> <cit type="translation" xml:lang="la"><quote> initiari </quote></cit> ab interpretibus vix intellectum <foreign xml:lang="gez">ተፈጸመ፡</foreign> versum est <ref cRef="Num." loc="25,3">Num. 25,3</ref>; <ref cRef="Ps. " loc="105,28">Ps.  105,28</ref> (quamquam videre licet sub <foreign xml:lang="gez">ፈጸመ፡</foreign> n. 1 phrasin <cit type="translation" xml:lang="la"><quote> implere </quote></cit> <cit type="translation" xml:lang="la"><quote> manus </quote></cit> i.e. <cit type="translation" xml:lang="la"><quote> initiare </quote></cit>).
</sense> 
</sense> </sense>
