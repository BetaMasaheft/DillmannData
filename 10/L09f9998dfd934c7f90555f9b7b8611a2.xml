                        <foreign xml:lang="gez">ወይደንግፁ፡ መሠረታተ፡ ሕምዝ፡</foreign> Lit. Diosc.;</sense>
                  <sense n="2" xml:id="D2">
                     <cit type="translation" xml:lang="la">
                        <quote>amaritudo</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>fel</quote>
                     </cit>
                     <foreign xml:lang="grc">πικρία</foreign>
                     <ref cRef="Job" loc="9,18">Job 9,18</ref>; <foreign xml:lang="gez">አጽገበኒ፡
                        ሕምዘ፡</foreign>
                     <ref cRef="Thren." loc="3,15">Thren. 3,15</ref>; <foreign xml:lang="grc">χολή</foreign>
                     <foreign xml:lang="gez">እግዚአብሔር፡ አስተየኒ፡ ማየ፡ ሕምዝ፡</foreign>
                     <ref cRef="Jer." loc="8,14">Jer. 8,14</ref>; <foreign xml:lang="gez">ምረተ፡
                        ሕምዝየ፡</foreign>
                     <foreign xml:lang="grc">πικρία καὶ χολή μου </foreign>
                     <ref cRef="Thren." loc="3,20">Thren. 3,20</ref>; <ref cRef="Deut." loc="29,17">Deut. 29,17</ref>; <foreign xml:lang="grc">χολέρα</foreign>
                     <ref cRef="Num. " loc="11,20">Num. 11,20</ref>.</sense>
                  <sense n="3" xml:id="D3">
                     <cit type="translation" xml:lang="la">
                        <quote>venenum</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>virus</quote>
                     </cit> (<foreign xml:lang="grc">χολὴ</foreign>, <foreign xml:lang="grc">θυμὸς</foreign>, <foreign xml:lang="grc">ἰὸς</foreign>, <foreign xml:lang="grc">φάρμακον</foreign>, <foreign xml:lang="grc">θάνατος</foreign>): <foreign xml:lang="gez">ሕምዘ፡ አርዌ፡ ምድር፡</foreign>
                     <ref cRef="Deut." loc="32,33">Deut. 32,33</ref>; <ref cRef="Job" loc="20,14">Job 20,14</ref>; <ref cRef="Job " loc="20,16">Job 20,16</ref>; <ref cRef="Ps." loc="13,5">Ps. 13,5</ref>; <ref cRef="Ps." loc="57,4">Ps.
                        57,4</ref>; <ref cRef="Ps." loc="139,3">Ps. 139,3</ref>; <ref cRef="Rom." loc="3,13">Rom. 3,13</ref>; <ref cRef="Prov." loc="23,32">Prov. 23,32</ref>;
                        <foreign xml:lang="gez">ሕምዝ፡ ዘይቀትል፡</foreign>
                     <ref cRef="Ps." loc="7,14">Ps. 7,14</ref>; <ref cRef="Jac." loc="3,8">Jac.
                        3,8</ref>; <foreign xml:lang="gez">ሕምዝ፡ ቀታሊ፡</foreign>
                     <ref cRef="Jsp." loc="285">Jsp. p. 285</ref>; <foreign xml:lang="gez">ሕምዘ፡ ባቲ፡
                        ውስቴታ፡ </foreign>
                     <ref cRef="4 Reg." loc="4,40">4 Reg. 4,40</ref>; <foreign xml:lang="gez">ሕምዘ፡
                        ሥራይ፡ ለተማስኖ፡</foreign>
                     <ref cRef="Sap." loc="1,14">Sap. 1,14</ref>; <foreign xml:lang="gez">ኵሉ፡
                        ዘፍጥረታተ፡ ሕምዝ፡</foreign>
                     <cit type="translation" xml:lang="la">
                        <quote>venefici cujuscumque generis</quote>
                     </cit>
                     <ref cRef="Lit." loc="169,1">Lit. 169,1</ref>.</sense>
               </sense>
               
