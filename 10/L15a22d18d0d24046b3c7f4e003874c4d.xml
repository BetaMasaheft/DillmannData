               <sense xml:lang="la" source="#dillmann" xml:id="Dmain">
                  <gramGrp>
                     <pos expand="Substantivum">subst.</pos>
                  </gramGrp>, <gramGrp>
                     <gen value="masculinum">m.</gen>
                  </gramGrp> et <gramGrp>
                     <gen value="femininus">fem.</gen>
                  </gramGrp>, <lbl expand="pluralis">Pl.</lbl>
                  <foreign xml:lang="gez">ፈቃዳት፡</foreign>
                  <sense n="1" xml:id="D1">
                     <cit type="translation" xml:lang="la">
                        <quote>appetitus</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>desiderium</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>studium</quote>
                     </cit>: <foreign xml:lang="gez">ኢትትሉ፡ ፈቃዳ፡ ለኀጢአት፡</foreign>
                     <ref cRef="Sir." loc="18,30">Sir. 18,30</ref>; <ref cRef="Gen." loc="6,5">Gen.
                        6,5</ref>; <ref cRef="Joh." loc="1,13">Joh. 1,13</ref>; <ref cRef="Eph." loc="2,3">Eph. 2,3</ref>; <foreign xml:lang="gez">ፈቃዱ፡ ተወርወት፡ ኀበ፡ ዐጸደ፡
                        ወይን፡</foreign>
                     <ref cRef="Sx. Tachs." loc="1 ">Sx. Tachs. 1 </ref>Enc.; appetitus cibi, Lud.
                     sine <lbl expand="auctore, auctoritate">auct.</lbl>; <foreign xml:lang="gez">ፈቃዳት፡</foreign>
                     <ref cRef="Phlx." loc="198.">Phlx. 198.</ref>
                  </sense>
                  <sense n="2" xml:id="D2">
                     <cit type="translation" xml:lang="la">
                        <quote>voluntas</quote>
                     </cit>: <ref cRef="Ps." loc="39,11">Ps. 39,11</ref>; <ref cRef="Ps." loc="144,20">Ps. 144,20</ref>; <foreign xml:lang="gez">ኵሎ፡ ፈቃድየ፡ ግበር፡ </foreign>
                     <ref cRef="Jes." loc="44,28">Jes. 44,28</ref>; <ref cRef="Jes." loc="48,14">Jes. 48,14</ref>; <ref cRef="Jes." loc="58,3">Jes. 58,3</ref>; <ref cRef="Jes." loc="58,13">Jes. 58,13</ref>; <ref cRef="Jes." loc="62,4">Jes.
                        62,4</ref>; <foreign xml:lang="gez">እስመዝ፡ ውእቱ፡ ፈቃድየ፡</foreign>
                     <ref cRef="Jer." loc="9,24">Jer. 9,24</ref>; <ref cRef="Koh." loc="5,3">Koh.
                        5,3</ref>; <ref cRef="Koh." loc="12,1">Koh. 12,1</ref>
                     <lbl expand="varia lectio">var.</lbl>; <ref cRef="Sir." loc="1,27">Sir.
                        1,27</ref>; <foreign xml:lang="gez">ፈቃዱ፡ ለእግዚአብሔር፡ ከመ፡ ኢትግበር፡ እኩየ፡</foreign>
                     <ref cRef="Sir. " loc="32,5">Sir. 32,5</ref>; <ref cRef="Sir." loc="41,4">Sir.
                        41,4</ref>; <ref cRef="Sir." loc="43,16">Sir. 43,16</ref>; <foreign xml:lang="gez">ግበር፡ ፈቃዳ፡ ለእምከ፡</foreign>
                     <ref cRef="Kuf. " loc="25">Kuf. 25</ref>; <foreign xml:lang="gez">በከመ፡
                        ፈቃዱ፡</foreign>
                     <ref cRef="Kuf." loc="22">Kuf. 22</ref>; <ref cRef="2 Tim." loc="1,9">2 Tim.
                        1,9</ref>; <foreign xml:lang="gez">ኵሉ፡ ኅሊና፡ ወፈቃዱ፡ ለሰብእ፡</foreign>
                     <ref cRef="Kuf." loc="7">Kuf. 7</ref>; <ref cRef="Matth." loc="6,10">Matth.
                        6,10</ref>; <ref cRef="Matth." loc="7,21">Matth. 7,21</ref>; <ref cRef="Matth." loc="12,50">Matth. 12,50</ref>; <ref cRef="Matth." loc="26,39">Matth. 26,39</ref>; <ref cRef="Matth." loc="26,42">Matth. 26,42</ref>; <ref cRef="Joh." loc="4,34">Joh. 4,34</ref>; <ref cRef="Joh." loc="6,38">Joh.
                        6,38</ref>; <ref cRef="Rom." loc="1,10">Rom. 1,10</ref>; <ref cRef="Rom. " loc="2,18">Rom. 2,18</ref>; <ref cRef="Rom." loc="10,1">Rom. 10,1</ref>;
                        <ref cRef="1 Thess." loc="5,18">1 Thess. 5,18</ref>; <ref cRef="Eph." loc="1,11">Eph. 1,11</ref>; <foreign xml:lang="gez">በፈቃደ፡ እግዚአብሔር፡
                     </foreign> Deo volente <ref cRef="Rom." loc="15,32">Rom. 15,32</ref>; <ref cRef="1 Cor." loc="1,1">1 Cor. 1,1</ref>; <foreign xml:lang="gez">በሥምረተ፡
                        ፈቃዱ፡ </foreign>
                     <ref cRef="Eph." loc="1,5">Eph. 1,5</ref>. <sense n="a" xml:id="D2a">
                        <cit type="translation" xml:lang="la">
                           <quote>delectatio</quote>
                        </cit>, <cit type="translation" xml:lang="la">
                           <quote>approbatio</quote>
                        </cit> (<note>Wohlgefallen</note>): <foreign xml:lang="gez">ኢኮነ፡ ቦቶን፡
                           ፈቃድየ፡</foreign>
                        <ref cRef="Koh." loc="12,1">Koh. 12,1</ref>; <foreign xml:lang="gez">ከመ፡
                           ይርከብ፡ ነገረ፡ ፈቃድ፡</foreign>
                        <ref cRef="Koh." loc="12,10">Koh. 12,10</ref>.</sense>
                     <sense n="b" xml:id="D2b">
                        <cit type="translation" xml:lang="la">
                           <quote>benevolentia</quote>
                        </cit>: <foreign xml:lang="gez">የኀሥሡ፡ ፈቃዶ፡</foreign>
                        <foreign xml:lang="grc">εὐδοκίαν</foreign>
                        <ref cRef="Sir." loc="2,16">Sir. 2,16</ref>; <foreign xml:lang="gez">ፈቃዱ፡
                           ለእግዚአብሔር፡ ታስተፌሥሖ፡ ለዓለም፡</foreign>
                        <ref cRef="Sir." loc="11,17">Sir. 11,17</ref>.</sense>
                     <sense n="c" xml:id="D2c">
                        <cit type="translation" xml:lang="la">
                           <quote>voluntas</quote>
                        </cit>
                        <cit type="translation" xml:lang="la">
                           <quote>promta</quote>
                        </cit>: <sense n="α" xml:id="D2cα">
                           <lbl expand="cum">c.</lbl>
                           <gramGrp>
                              <pos expand="praepositio">praep.</pos>
                           </gramGrp>
                           <foreign xml:lang="gez">በ፡ </foreign>
                           <cit type="translation" xml:lang="la">
                              <quote>voluntate</quote>
                           </cit>
                           <cit type="translation" xml:lang="la">
                              <quote>sua</quote>
                           </cit>, <cit type="translation" xml:lang="la">
                              <quote>sponte</quote>
                           </cit>, <cit type="translation" xml:lang="la">
                              <quote>ultro</quote>
                           </cit>: <foreign xml:lang="gez">በፈቃድ፡ ወኢበኵርህ፡</foreign> F.M. 1,3;
                              <foreign xml:lang="gez">በፈቃዱ፡</foreign>
                           <ref cRef="Ps. " loc="75,10">Ps. 75,10</ref>; <foreign xml:lang="gez">እለ፡
                              በፈቃዶሙ፡ ነበሩ፡</foreign>
                           <ref cRef="2 Esr." loc="21,2">2 Esr. 21,2</ref>; <ref cRef="2 Cor. " loc="8,5">2 Cor. 8,5</ref>; <ref cRef="Clem." loc="162">Clem. f.
                              162</ref>; <foreign xml:lang="gez">በፈቃደ፡ ርእሱ፡</foreign> F.M. 33,3;
                           F.M. 33,4.</sense>
                        <sense n="β" xml:id="D2cβ">
                           <foreign xml:lang="gez">ዘእንበለ፡ ፈቃዱ፡</foreign>
                           <cit type="translation" xml:lang="la">
                              <quote>invite</quote>
                           </cit>, <cit type="translation" xml:lang="la">
                              <quote>invitus</quote>
                           </cit>, F.M. 6,8; <ref cRef="Gad. T.H." loc="">Gad. T.H.</ref> (<lbl expand="videas">vid.</lbl>
                           <foreign xml:lang="gez">አዘመወ፡</foreign>).</sense>
                        <sense n="γ" xml:id="D2cγ"> Statui constr. subjunctum vel in Gen. vel <foreign xml:lang="gez">ዘበፈቃድ፡</foreign> vel <foreign xml:lang="gez">ዘእምፈቃድ፡ </foreign>
                           <cit type="translation" xml:lang="la">
                              <quote>voluntarius</quote>
                           </cit>, <cit type="translation" xml:lang="la">
                              <quote>spontaneus</quote>
                           </cit>: <foreign xml:lang="gez">መሥዋዕተ፡ ፈቃድ፡</foreign>
                           <ref cRef="Am." loc="4,5">Am. 4,5</ref>; <foreign xml:lang="gez">ዘበፈቃዶሙ፡ </foreign>
                           <foreign xml:lang="grc">τὰ ἑκούσια</foreign>
                           <ref cRef="Judith" loc="4,14">Judith 4,14</ref>; <ref cRef="Lev. " loc="7,16">Lev. 7,16</ref>; <ref cRef="Num." loc="15,3">Num.
                              15,3</ref>; (<foreign xml:lang="gez">መሥዋዕት፡</foreign>) <foreign xml:lang="gez">ዘፈቃድክሙ፡</foreign>
                           <ref cRef="Lev." loc="23,28">Lev. 23,28</ref>; <foreign xml:lang="gez">ዘእምፈቃድየ፡ እሠውዕ፡ ለከ፡</foreign>
                           <ref cRef="Ps." loc="53,6">Ps. 53,6</ref>.</sense>
                        <sense n="d" xml:id="D2cd">
                           <foreign xml:lang="gez">ፈቃዳት፡</foreign>
                           <cit type="translation" xml:lang="la">
                              <quote>mandata</quote>
                           </cit>, <cit type="translation" xml:lang="la">
                              <quote>praecepta</quote>
                           </cit>, <ref cRef="Kuf." loc="2">Kuf. 2</ref>; <ref cRef="Kuf." loc="21">Kuf. 21</ref>; <ref cRef="Sx. Jac." loc="13 ">Sx. Jac. 13
                           </ref>Enc.</sense>
                     </sense>
                  </sense>
               </sense>
               
            </entry>
