            <entry xml:id="L9fe5442cb2e04f28918a9cc3fba98776" n="948">
               <form>
                  <foreign xml:lang="gez">አስተሓየጸ</foreign>
               </form>
               <sense xml:id="Dmain" xml:lang="la" source="#dillmann"> <subc>IV,3</subc> (at plerumque <foreign xml:lang="gez">አስተሐየጸ፡</foreign> exscriptum) speculationem agere i.e. <sense n="1" xml:id="D1"> <cit type="translation" xml:lang="la"><quote> speculari </quote></cit>, <cit type="translation" xml:lang="la"><quote> explorare </quote></cit> (<foreign xml:lang="grc">σκοπεύειν</foreign>) <foreign xml:lang="gez">ዘያስተሓይጽ፡</foreign> <ref cRef="Hos." loc="5,1">Hos. 5,1</ref>; <foreign xml:lang="gez">እምርሑቅ፡ ያስተሓይጽ፡ አዕይንቲሁ፡</foreign> <ref cRef="Job" loc="39,29">Job 39,29</ref>; <ref cRef="Job" loc="16,12">Job 16,12</ref>; <foreign xml:lang="gez">ውስተ፡ ኵሉ፡ መንኰራኵሪሁ፡ ያስተሓይጽ፡ እግዚአብሔር፡</foreign> <ref cRef="Prov." loc="5,21">Prov. 5,21</ref>; <foreign xml:lang="gez">አስተሓይጹ፡ ለእመቦ፡</foreign> <ref cRef="Hebr." loc="12,15">Hebr. 12,15</ref>; <lbl expand="cum">c.</lbl> <gramGrp> <case value="accusativus">Acc.</case> </gramGrp>: <foreign xml:lang="gez">ያስተሓይጻ፡ እኩያነ፡</foreign> <ref cRef="Prov." loc="15,3">Prov. 15,3</ref>; <foreign xml:lang="gez">መጻእክሙ፡ ታስተሓይጹ፡ አሰረ፡ ምድር፡</foreign> <ref cRef="Kuf." loc="42">Kuf. 42</ref>; <ref cRef="Kuf." loc="44">Kuf. 44</ref>; <foreign xml:lang="gez">አስተሓይጽ፡ ፍኖተከ፡</foreign> <ref cRef="Nah." loc="2,2">Nah. 2,2</ref>; <ref cRef="Gal." loc="2,4">Gal. 2,4</ref>; <lbl expand="cum">c.</lbl> <foreign xml:lang="gez">ኀበ፡</foreign> (<foreign xml:lang="grc">ἀποβλέπειν εἰς</foreign>) <ref cRef="Ps." loc="9,31">Ps. 9,31</ref>.
</sense> <sense n="2" xml:id="D2"> <cit type="translation" xml:lang="la"><quote> accurate observare </quote></cit>, <cit type="translation" xml:lang="la"><quote> animum intendere </quote></cit> in rem: <foreign xml:lang="gez">ያስተሓይጹ፡ በሑያጼ፡ ወርኅ፡</foreign> <ref cRef="Kuf." loc="6">Kuf. 6</ref>; crebro in Chrys., ut <foreign xml:lang="gez">አስተሓይጽ፡ በልብከ፡ ወአእምር፡ መኑ፡ አንተ፡</foreign> <ref cRef="Tag." loc="11.">Tag. 11.</ref> 
</sense> <sense n="3" xml:id="D3"> in specie (sensu malo) <cit type="translation" xml:lang="la"><quote> aucupari </quote></cit>, <cit type="translation" xml:lang="la"><quote> insidiari </quote></cit> (observare doli faciendi causa), <lbl expand="construitur cum">c.c.</lbl> <gramGrp> <case value="accusativus">Acc.</case> </gramGrp>, <foreign xml:lang="grc">περιέχειν</foreign> <ref cRef="Ps." loc="16,10">Ps. 16,10</ref>; <foreign xml:lang="grc">φυλάσσειν</foreign> 55,6; <foreign xml:lang="grc">παρατηρεῖν</foreign> <ref cRef="Dan." loc="6,11">Dan. 6,11</ref>; <ref cRef="Ps." loc="36,12">Ps. 36,12</ref>; <foreign xml:lang="grc">ἐνεδρεύειν</foreign> <ref cRef="Job" loc="38,40">Job 38,40</ref>; <foreign xml:lang="gez">ያስተሓይጽ፡ በእኩይ፡</foreign> <ref cRef="Kuf." loc="28">Kuf. 28</ref>; <lbl expand="cum">c.</lbl> <foreign xml:lang="gez">ለ</foreign> ut <foreign xml:lang="gez">ያስተሓይጽ፡ ለድንግል፡</foreign> <cit type="translation" xml:lang="la"><quote> virgini insidiatur </quote></cit> <ref cRef="Clem." loc="163.">Clem. f. 163.</ref> 
</sense> </sense>
               
            <sense xml:id="Tmain" xml:lang="en" source="#traces"> <cit type="transcription" xml:lang="gez"><quote>ʾastaḥāyaṣa</quote></cit>, <foreign xml:lang="gez">አስተሐየጸ፡</foreign> <cit type="transcription" xml:lang="gez"><quote>ʾastaḥayaṣa</quote></cit> <sense n="G"> <foreign xml:lang="gez">አስተሓየጸ፡</foreign> <cit type="transcription" xml:lang="gez"><quote>ʾastaḥāyaṣa</quote></cit> <subc>IV,3</subc> «<cit type="translation" xml:lang="fr"><quote>observer avec une grande attention</quote></cit>» ― <foreign xml:lang="am">ተመለከተ፡</foreign> Ms. <ref type="BM" target="BNFabb217">BNFabb217</ref>, fol. 29r. <lb/>
 <bibl><ptr target="bm:Grebaut1952SuppLexicon"/><citedRange unit="page"> 62</citedRange></bibl> 
</sense> <sense n="L"> <foreign xml:lang="gez">አስተሓየጸ</foreign> <cit type="transcription" xml:lang="gez"><quote>ʾastaḥāyaṣa</quote></cit>, <foreign xml:lang="gez">አስተሐየጸ</foreign> <cit type="transcription" xml:lang="gez"><quote>ʾastaḥayaṣa</quote></cit> <cit type="translation" xml:lang="en"><quote>perceive</quote></cit>, <cit type="translation" xml:lang="en"><quote>view</quote></cit>, <cit type="translation" xml:lang="en"><quote>discern</quote></cit>, <cit type="translation" xml:lang="en"><quote>look at</quote></cit>, <cit type="translation" xml:lang="en"><quote>look on</quote></cit>, <cit type="translation" xml:lang="en"><quote>observe</quote></cit>, <cit type="translation" xml:lang="en"><quote>observe closely</quote></cit>, <cit type="translation" xml:lang="en"><quote>consider carefully</quote></cit>, <cit type="translation" xml:lang="en"><quote>search for</quote></cit>, <cit type="translation" xml:lang="en"><quote>watch</quote></cit>, <cit type="translation" xml:lang="en"><quote>examine</quote></cit>, <cit type="translation" xml:lang="en"><quote>spy</quote></cit>, <cit type="translation" xml:lang="en"><quote>explore</quote></cit>, <cit type="translation" xml:lang="en"><quote>scrutinize</quote></cit>, <cit type="translation" xml:lang="en"><quote>lie in wait</quote></cit>, <cit type="translation" xml:lang="en"><quote>be on the lookout for</quote></cit> <bibl><ptr target="bm:Leslau1987dic"/><citedRange unit="page"> 252a</citedRange></bibl> 
</sense> </sense></entry>
