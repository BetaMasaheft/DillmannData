               <sense xml:lang="la" source="#dillmann" xml:id="Dmain"> (etiam per <foreign xml:lang="gez">ኀ</foreign> et <foreign xml:lang="gez">ሀ</foreign>) <subc>I,1</subc> [proprie, ut videtur, <cit type="translation" xml:lang="la">
                            <quote> viribus contendere </quote>
                        </cit>, <lbl expand="collatio, -is vel collectivum,&#xA;                            -e">coll.</lbl> <foreign xml:lang="he">כֹּחַ</foreign>, nec non <foreign xml:lang="ar">كَاح</foreign> <cit type="translation" xml:lang="la">
                            <quote> pugnando vincere </quote>
                        </cit>] <sense n="1" xml:id="D1"> <cit type="translation" xml:lang="la">
                                <quote> contendere </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> niti ad </quote>
                            </cit> aliquid, <cit type="translation" xml:lang="la">
                                <quote> aemulari </quote>
                            </cit>: <foreign xml:lang="gez">ቅኆ፡</foreign> (<foreign xml:lang="gez">ቅናእ፡</foreign> Platt) <foreign xml:lang="gez">ለንስሓ፡</foreign> <ref cRef="Apoc." loc="3,19">Apoc. 3,19</ref> <lbl expand="romanae editionis">rom.</lbl>; <foreign xml:lang="gez">አምላክ፡ ዘኢይቅሕውዎ፡</foreign> <cit type="translation" xml:lang="la">
                                <quote> Deus cui nullus est aemulus </quote>
                            </cit> Lit. Joh.</sense> <sense n="2" xml:id="D2"> <cit type="translation" xml:lang="la">
                                <quote> certare </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> contendere cum </quote>
                            </cit> aliquo de praemio vel honore obtinendo, Lud. sine <lbl expand="auctore, auctoritate">auct.</lbl> <cb n="0417" xml:id="c0417"/> </sense> </sense>
               
