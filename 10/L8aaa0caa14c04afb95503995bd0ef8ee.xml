               <sense xml:lang="la" source="#dillmann" xml:id="Dmain"> <subc>I,1</subc> [<foreign xml:lang="ar">نَدَهَ</foreign> <cit type="translation" xml:lang="la">
                            <quote> egit </quote>
                        </cit>, <cit type="translation" xml:lang="la">
                            <quote> propulit </quote>
                        </cit>, <cit type="translation" xml:lang="la">
                            <quote> compulit </quote>
                        </cit> camelos; <foreign xml:lang="ar">نَدْأَةٌ</foreign> <cit type="translation" xml:lang="la">
                            <quote> multitudo pecorum </quote>
                        </cit>; <lbl expand="videas">vid.</lbl> etiam <foreign xml:lang="gez">ነድሐ፡</foreign>; amh. <foreign xml:lang="gez">ነዳ፡</foreign>] <cit type="translation" xml:lang="la">
                            <quote> agere </quote>
                        </cit>, <lbl expand="cum">c.</lbl> <gramGrp> <case value="accusativus">Acc.</case> </gramGrp>, ut: gregem, jumenta, <ref cRef="Gen." loc="32,20">Gen. 32,20</ref>; <ref cRef="1 Reg." loc="10,3">1 Reg. 10,3</ref>; <ref cRef="Jes." loc="11,6">Jes. 11,6</ref> (var. pro <foreign xml:lang="gez">የሐስሎሙ፡</foreign>); <ref cRef="Herm." loc="57.">Herm. p. 57.</ref> <sense n="a" xml:id="Da"> <cit type="translation" xml:lang="la">
                                <quote> agitare </quote>
                            </cit> vel <cit type="translation" xml:lang="la">
                                <quote> impellere </quote>
                            </cit> ad proficiscendum: <foreign xml:lang="gez">ይንድኡ፡ እንስሳሆሙ፡</foreign> <foreign xml:lang="grc">ἀναζευξάτωσαν</foreign> <ref cRef="Ex." loc="14,15">Ex. 14,15</ref>; <foreign xml:lang="gez">ነድአት፡ ሰረገላቲሃ፡</foreign> <foreign xml:lang="grc">ἔζευξε τὰς ἁμάξας αὐτῆς</foreign> <ref cRef="Judith" loc="15,11">Judith 15,11</ref>.</sense> <sense n="b" xml:id="Db"> <cit type="translation" xml:lang="la">
                                <quote> abigere </quote>
                            </cit> jumenta per vim rapta, <cit type="translation" xml:lang="la">
                                <quote> deducere </quote>
                            </cit> captivos: <ref cRef="Jud." loc="18,21">Jud. 18,21</ref>; <foreign xml:lang="gez">ማህረኩ፡ ወነድኡ፡ ምህርካሆሙ፡ ገመለ፡</foreign> caet. <ref cRef="1 Par." loc="5,21">1 Par. 5,21</ref>; <foreign xml:lang="gez">ወይነድእ፡ እንስሳሆሙ፡</foreign> <ref cRef="Dan." loc="11,24">Dan. 11,24</ref>; <foreign xml:lang="gez">እለ፡ ይነድኡ፡ መርዔተ፡ ዘኢኮነ፡ ሎሙ፡</foreign> F.N. 49; <foreign xml:lang="gez">ወነድእዋ፡ ምስለ፡ ወለታ፡</foreign> <ref cRef="Judith" loc="10,17">Judith 10,17</ref>; <foreign xml:lang="gez">ነድእዎን፡ ምስለ፡ ሕዝብ፡ ባቢሎን፡</foreign> Rel. Bar.</sense> <sense n="c" xml:id="Dc"> <cit type="translation" xml:lang="la">
                                <quote> exigere </quote>
                            </cit>: <foreign xml:lang="gez">ዘይነድኦሙ፡</foreign> <foreign xml:lang="grc">ἐξελαύνων</foreign> <ref cRef="Zach." loc="9,8">Zach. 9,8</ref>.</sense> <sense n="d" xml:id="Dd"> <lbl expand="translate">transl.</lbl>: <cit type="translation" xml:lang="la">
                                <quote> agere </quote>
                            </cit>, ut: <foreign xml:lang="gez">ይነድእ፡ ቃለ፡ ኀበ፡ ሥርዉ፡</foreign> <foreign xml:lang="grc">ἐπὶ τὸ πρωτότυπον ἄγει τὸν λόγον</foreign> (<cit type="translation" xml:lang="la">
                                <quote> ad exemplar primitivum deducit orationem </quote>
                            </cit>) <ref cRef="Chrys. ho." loc="33.">Chrys. ho. 33.</ref> — At pro <foreign xml:lang="gez">ለኵሉ፡ ዕለቱ፡ ይነድኦ፡</foreign> <foreign xml:lang="grc">πάντα ἐν πρωτότυπον ἄγει τὸν λὸγον</foreign> <ref cRef="Sir." loc="39,34">Sir. 39,34</ref> <foreign xml:lang="gez">ይንእዶ፡</foreign> legendum videtur. <cb n="0682" xml:id="c0682"/> </sense> </sense>
               
