         <div>
            <entry xml:id="L9e42581cd68248ecafafbeb9018d61b4" n="8360">
               <form>
                  <foreign xml:lang="gez">መገሥጽ</foreign>
               </form>
               <sense xml:lang="la" source="#dillmann" xml:id="Dmain">
                  <gramGrp>
                     <pos expand="participium">part.</pos>
                  </gramGrp>, <gramGrp>
                     <gen value="femininus">fem.</gen>
                  </gramGrp>
                  <foreign xml:lang="gez">መገሥጽት፡</foreign>
                  <sense n="1" xml:id="D1">
                     <cit type="translation" xml:lang="la">
                        <quote>castigator</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>reprehensor</quote>
                     </cit>, (<cit type="translation" xml:lang="la">
                        <quote>censor</quote>
                     </cit>), <foreign xml:lang="grc">ὁ ἐλέγχων</foreign>
                     <ref cRef="Chrys. Ta." loc="30.">Chrys. Ta. 30.</ref>
                  </sense>
                  <sense n="2" xml:id="D2">
                     <cit type="translation" xml:lang="la">
                        <quote>institutor</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>morum formator</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>paedagogus</quote>
                     </cit>: <foreign xml:lang="gez">መምህር፡ መገሥጽ፡</foreign>
                     <ref cRef="1 Tim." loc="3,2">1 Tim. 3,2</ref>; <foreign xml:lang="grc">παιδευτής </foreign>
                     <ref cRef="Hos." loc="5,2">Hos. 5,2</ref>; <foreign xml:lang="gez">ቀሲስ፡ በአምሳለ፡
                        መገሥጽ፡</foreign>
                     <ref cRef="Did." loc="1">Did. 1</ref>; <foreign xml:lang="gez">መምህር፡ ወመገሥጽ፡
                        ዘአልሀቀ፡ ደቂቆ፡ በንጹሕ፡</foreign>
                     <ref cRef="Did." loc="4">Did. 4</ref>; <foreign xml:lang="gez">መምህር፡ የዋህ፡ ወዕጉሥ፡
                        መገሥጽ፡</foreign>
                     <ref cRef="Did." loc="6">Did. 6</ref>; <foreign xml:lang="gez">ጳውሎስ፡ ኄር፡ ወመገሥጽ፡
                        ለአሕዛብ፡</foreign>
                     <ref cRef="Did." loc="33.">Did. 33.</ref>
                     <cb n="1153" xml:id="c1153"/>
                  </sense>
                  <sense n="3" xml:id="D3">
                     <cit type="translation" xml:lang="la">
                        <quote>monitor</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>hortator</quote>
                     </cit>: <foreign xml:lang="gez">አብ፡ መገሥጽ፡ </foreign>
                     <foreign xml:lang="grc">πατὴρ νουθετῶν</foreign>
                     <ref cRef="Sap." loc="11,11">Sap. 11,11</ref>; <foreign xml:lang="gez">መገሥጽት፡</foreign> (<foreign xml:lang="grc">παραίνεσις</foreign>) <ref cRef="Sap." loc="8,9">Sap. 8,9</ref>.</sense>
               </sense>
               
