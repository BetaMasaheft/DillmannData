               <sense xml:id="Dmain" xml:lang="la" source="#dillmann"> <gramGrp> <pos expand="participium">part.</pos> </gramGrp> et <gramGrp> <pos expand="adjectivum">adj.</pos> </gramGrp> <sense n="1" xml:id="D1"> <cit type="translation" xml:lang="la"><quote> durus </quote></cit>, <cit type="translation" xml:lang="la"><quote> asper </quote></cit>: <foreign xml:lang="gez">ግብር፡ ዕጹብ፡</foreign> <foreign xml:lang="grc">ἔργα σκληρά</foreign> <ref cRef="Ex." loc="1,14">Ex. 1,14</ref>; <ref cRef="Deut." loc="26,6">Deut. 26,6</ref>; <foreign xml:lang="gez">ጽኑዕ፡ ወዕጹብ፡ መዐቱ፡ ለእግዚአብሔር፡</foreign> <foreign xml:lang="grc">ἰσχυρὸν καὶ σκληρόν</foreign> <ref cRef="Jes." loc="28,2">Jes. 28,2</ref>; <foreign xml:lang="gez">ትእዛዝ፡ ዕጹብ፡</foreign> <foreign xml:lang="grc">γνώμη ἀναιδής</foreign> <ref cRef="Dan." loc="2,15">Dan. 2,15</ref>; de verbis: <ref cRef="Jes." loc="8,12">Jes. 8,12</ref>; <foreign xml:lang="gez">ዕጹብ፡ ዜና፡</foreign> <ref cRef="Jes." loc="21,2">Jes. 21,2</ref>; <foreign xml:lang="gez">አውሥኦሙ፡ ቃላተ፡ ዕጹባተ፡</foreign> <ref cRef="2 Par." loc="10,13">2 Par. 10,13</ref>; <foreign xml:lang="gez">ይትናገሩ፡ ዕጹባተ፡</foreign> <ref cRef="Hen." loc="27,2">Hen. 27,2</ref>; de factis: <foreign xml:lang="gez">ዕጹባተ፡ ገብረ፡</foreign> <ref cRef="Gen." loc="49,3">Gen. 49,3</ref>; consiliis: <foreign xml:lang="gez">እለ፡ መከሩ፡ ዕጹባተ፡</foreign> <ref cRef="Judith" loc="9,13">Judith 9,13</ref>. <sense n="a" xml:id="D1a"> de homine: <cit type="translation" xml:lang="la"><quote> immitis </quote></cit>, <cit type="translation" xml:lang="la"><quote> austerus </quote></cit> <ref cRef="Luc. " loc="19,21">Luc.  19,21</ref>.
</sense> <sense n="b" xml:id="D1b"> de sorte <cit type="translation" xml:lang="la"><quote> adversa </quote></cit>: <foreign xml:lang="gez">ዕጹብ፡ ይረክበኒ፡</foreign> <ref cRef="Job" loc="9,20">Job 9,20</ref>; <ref cRef="Ps." loc="59,3">Ps. 59,3</ref>.
</sense> <sense n="c" xml:id="D1c"> de bestiis: <cit type="translation" xml:lang="la"><quote> ferus </quote></cit>, <cit type="translation" xml:lang="la"><quote> ferox </quote></cit>, <foreign xml:lang="gez">አዘዘ፡ ይእስርዎ፡ ውስተ፡ እገሪሆሙ፡ ለአፍራስ፡ ዕጹባን፡ ዘእንበለ፡ ይትለጐም፡</foreign> <ref cRef="Sx. Sen." loc="6.">Sx. Sen. 6.</ref> 
</sense> 
</sense><sense n="2" xml:id="D2"> <cit type="translation" xml:lang="la"><quote> gravis </quote></cit>, <sense n="a" xml:id="D2a"> <cit type="translation" xml:lang="la"><quote> molestus </quote></cit>, <cit type="translation" xml:lang="la"><quote> oneri </quote></cit>: <ref cRef="Gen." loc="21,11">Gen. 21,11</ref>; <ref cRef="Gen." loc="21,12">Gen. 21,12</ref>; <foreign xml:lang="gez">ክቡድ፡ ወዕጹብ፡</foreign> <foreign xml:lang="grc">δύσχρηστος</foreign> <ref cRef="Jes. " loc="3,10">Jes.  3,10</ref>; <foreign xml:lang="gez">በኀበ፡ ጠቢብ፡ ዕጹብ፡ አስተሐቅሮ፡</foreign> <foreign xml:lang="grc">φρόνιμος βαρυνθήσεται ἀτιμίᾳ</foreign> <ref cRef="Sir." loc="21,24">Sir. 21,24</ref>; <ref cRef="Sir. " loc="21,25">Sir.  21,25</ref>; <ref cRef="Sir." loc="29,28">Sir. 29,28</ref>; <foreign xml:lang="gez">ዛቲ፡ ዕጽብት፡</foreign> <foreign xml:lang="grc">σκῶλον τοῦτο</foreign> <ref cRef="Ex." loc="10,7">Ex. 10,7</ref>; <foreign xml:lang="gez">በግብር፡ ዕጹብ፡</foreign> <cit type="translation" xml:lang="la"><quote> ex necessitate </quote></cit>vel <cit type="translation" xml:lang="la"><quote> coactus </quote></cit>F. 26,5.
</sense> <sense n="b" xml:id="D2b"> <cit type="translation" xml:lang="la"><quote> vehemens </quote></cit>: <foreign xml:lang="gez">ለእመ፡ ዕጹበ፡ ኮነ፡ ሕማሙ፡</foreign> <ref cRef="Clem. " loc="256.">Clem.  f. 256.</ref> 
</sense> <sense n="c" xml:id="D2c"> gravitate <cit type="translation" xml:lang="la"><quote> impeditus </quote></cit>: <foreign xml:lang="gez">ዘዕጹብ፡ ልሳኑ፡</foreign> <ref cRef="Hez." loc="3,6">Hez. 3,6</ref>.
</sense> <sense n="d" xml:id="D2d"> de pretio: <foreign xml:lang="gez">ዘዕጹብ፡ ሤጡ፡</foreign> <foreign xml:lang="grc">πολυτελής</foreign> <cit type="translation" xml:lang="la"><quote> pretiosus </quote></cit> <ref cRef="Sir." loc="45,11">Sir. 45,11</ref>; <ref cRef="1 Par. " loc="15,27">1 Par.  15,27</ref>; <ref cRef="Marc." loc="14,3">Marc. 14,3</ref>. al.</sense>
</sense> <sense n="3" xml:id="D3"> <cit type="translation" xml:lang="la"><quote> difficilis </quote></cit>, <cit type="translation" xml:lang="la"><quote> arduus </quote></cit>: <ref cRef="Dan." loc="2,11">Dan. 2,11</ref>; <foreign xml:lang="gez">ዕጹበ፡ ሰአልከ፡</foreign> <ref cRef="4 Reg." loc="2,10">4 Reg. 2,10</ref>; <ref cRef="Marc." loc="10,24">Marc. 10,24</ref>; <ref cRef="Hebr." loc="5,11">Hebr. 5,11</ref>; <ref cRef="Joh." loc="6,60">Joh. 6,60</ref>; <cb n="1021" xml:id="c1021"/> <foreign xml:lang="gez">ዕጹብ፡ ለተናግሮ፡</foreign> <ref cRef="Sap." loc="17,1">Sap. 17,1</ref>; <foreign xml:lang="gez">ግብር፡ ዕጽብት፡ ለገቢር፡</foreign> <ref cRef="Phlx." loc="3">Phlx. 3</ref>; <foreign xml:lang="gez">ዕጹብ፡ ቀሊል፡ ለከ፡</foreign> Deg.; <foreign xml:lang="gez">አእምሮታ፡ ለዛቲ፡ ዕጽብት፡ ላዕለ፡ ሰብእ፡</foreign> <ref cRef="Phlx." loc="170.">Phlx. 170.</ref> 
</sense> <sense n="4" xml:id="D4"> <cit type="translation" xml:lang="la"><quote> difficilis intellectu </quote></cit>, <cit type="translation" xml:lang="la"><quote> mirandus </quote></cit>, <cit type="translation" xml:lang="la"><quote> mirus </quote></cit>, <cit type="translation" xml:lang="la"><quote> stupendus </quote></cit>: <foreign xml:lang="gez">ዕጹብ፡ ኀይሉ፡</foreign> <foreign xml:lang="grc">θαυμαστή</foreign> <ref cRef="Sir." loc="43,29">Sir. 43,29</ref>; <ref cRef="Sir." loc="48,14">Sir. 48,14</ref>; <foreign xml:lang="gez">ህየ፡ ዕጹብ፡ ግብሩ፡ ወነኪር፡</foreign> <ref cRef="Sir." loc="43,25">Sir. 43,25</ref>; <foreign xml:lang="gez">ገብረ፡ ዕጹበ፡ ላዕሌሆሙ፡</foreign> <foreign xml:lang="grc">τέρατα</foreign> <ref cRef="Sir." loc="45,19">Sir. 45,19</ref>; <foreign xml:lang="gez">ዕጹብ፡ ለዐይን፡ ሥነ፡ ጽዕዱቱ፡</foreign> <foreign xml:lang="grc">ἐκθαυμάσει</foreign> <ref cRef="Sir." loc="43,18">Sir. 43,18</ref>; <ref cRef="Lit." loc="170,3">Lit. 170,3</ref>; <foreign xml:lang="gez">ዕጹብ፡ ከመ፡ ያስተባሪ፡</foreign> <ref cRef="Sir." loc="43,8">Sir. 43,8</ref>; <foreign xml:lang="gez">ዕጹብ፤ ይመስሎ፡ ዝንቱ፡ ለጦብያ፡</foreign> (<cit type="translation" xml:lang="la"><quote> mirum </quote></cit> <cit type="translation" xml:lang="la"><quote> ! </quote></cit>) <ref cRef="Tob." loc="7,2">Tob. 7,2</ref>; <foreign xml:lang="gez">ያማስን፡ ዕጹባተ፡</foreign> <foreign xml:lang="grc">θαυμαστὰ διαφθερεῖ</foreign> <ref cRef="Dan." loc="8,24">Dan. 8,24</ref>.
</sense> <sense n="5" xml:id="D5"> <cit type="translation" xml:lang="la"><quote> difficultatibus circumventus </quote></cit>: <foreign xml:lang="gez">ዕጹባን፡</foreign> <ref cRef="Org." loc="1.">Org. 1.</ref> 
</sense> <sense n="6" xml:id="D6"> <sense n="a" xml:id="D6a"> <foreign xml:lang="gez">እምዕጹብ፡</foreign> vel <foreign xml:lang="gez">በዕጹብ፡</foreign> <cit type="translation" xml:lang="la"><quote> difficulter </quote></cit>, <cit type="translation" xml:lang="la"><quote> aegre </quote></cit>, <cit type="translation" xml:lang="la"><quote> vix </quote></cit>: <foreign xml:lang="gez">እምዕጹብ፡</foreign> <foreign xml:lang="grc">μόλις</foreign> <ref cRef="Sir. " loc="21,20">Sir.  21,20</ref>; <ref cRef="Sir." loc="26,29">Sir. 26,29</ref>; <ref cRef="Sir." loc="29,6">Sir. 29,6</ref> (seq. <foreign xml:lang="gez">እመ፡</foreign>); <ref cRef="Sir. " loc="32,7">Sir.  32,7</ref>; <ref cRef="Prov." loc="11,31">Prov. 11,31</ref>; <ref cRef="Act." loc="27,8">Act. 27,8</ref> Platt; <ref cRef="Rom." loc="5,7">Rom. 5,7</ref>; <ref cRef="1 Petr." loc="4,18">1 Petr. 4,18</ref>; <foreign xml:lang="grc">μόγις</foreign> <ref cRef="Luc." loc="9,39">Luc. 9,39</ref>; <foreign xml:lang="grc">δυσκόλως</foreign> <ref cRef="Matth." loc="19,23">Matth. 19,23</ref>; <foreign xml:lang="gez">በዕጹብ፡</foreign> <ref cRef="Sap." loc="9,16">Sap. 9,16</ref>; <ref cRef="Act. " loc="14,18">Act.  14,18</ref>; <ref cRef="Act." loc="27,8">Act. 27,8</ref> <lbl expand="romanae editionis">rom.</lbl>; <ref cRef="Macc." loc="2.">Macc. f. 2.</ref> 
</sense> <sense n="b" xml:id="D6b"> <foreign xml:lang="gez">ዕጹበ፡</foreign> <cit type="translation" xml:lang="la"><quote> duriter </quote></cit>, <cit type="translation" xml:lang="la"><quote> moleste </quote></cit>: <foreign xml:lang="gez">ጻመወ፡ ወተጋደለ፡ ዕጹበ፡</foreign> <ref cRef="Sx. Teq." loc="4 ">Sx. Teq. 4 </ref>Enc.
</sense> 
</sense> </sense>
