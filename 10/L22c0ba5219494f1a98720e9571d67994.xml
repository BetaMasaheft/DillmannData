               <sense xml:id="Dmain" xml:lang="la" source="#dillmann"> <subc>I,1</subc> [respondent: <foreign xml:lang="he">בָּחַר</foreign> <cit type="translation" xml:lang="la"><quote> elegit, examinavit, exploravit </quote></cit> (<foreign xml:lang="he">בָּחַן</foreign>), <foreign xml:lang="syr">ܒܚܰܪ</foreign> <cit type="translation" xml:lang="la"><quote> examinavit, scrutatus est, probavit </quote></cit> (<foreign xml:lang="syr">ܒܰܚܶܢ</foreign>), arab. (praeter <foreign xml:lang="ar">مَخَرَ</foreign> VIII) in malam partem <foreign xml:lang="ar">مَكَرَ</foreign> <cit type="translation" xml:lang="la"><quote> dolum struxit </quote></cit> et <cb n="0199" xml:id="c0199"/> <foreign xml:lang="ar">مَحَنَ</foreign> <cit type="translation" xml:lang="la"><quote> tentavit </quote></cit>; amh. <foreign xml:lang="gez">መከረ፡</foreign> et <foreign xml:lang="gez">ሞከረ፡</foreign>] <gramGrp> <pos expand="Subjunctivus">Subj.</pos> </gramGrp> <foreign xml:lang="gez">ይምክር፡</foreign> <sense n="1" xml:id="D1"> <cit type="translation" xml:lang="la"><quote> exquirere </quote></cit>, <cit type="translation" xml:lang="la"><quote> explorare </quote></cit>, <cit type="translation" xml:lang="la"><quote> tentare </quote></cit>: <foreign xml:lang="gez">ምክር፡ እምውስተ፡ ሕዝብ፡ ዕደወ፡ ጻድቃነ፡</foreign> <foreign xml:lang="grc">σκέψαι</foreign> <ref cRef="Ex. " loc="18,21">Ex.  18,21</ref>; <foreign xml:lang="grc">κατασκέψασθαι</foreign> <ref cRef="Koh." loc="2,3">Koh. 2,3</ref>; <foreign xml:lang="gez">እምክርከ፡</foreign> (v. <foreign xml:lang="gez">ኣመክርከ፡</foreign>) <foreign xml:lang="gez">በፍሥሓ፡</foreign> <foreign xml:lang="grc">πειράσω σε ἐν εὐφροσύνῃ</foreign> <ref cRef="Koh." loc="2,1">Koh. 2,1</ref>; <foreign xml:lang="gez">እለ፡ ይመክሩ፡ ኰስየ፡</foreign> <foreign xml:lang="grc">ἀναιρεῖ πειρώμενοι</foreign> Cyr. c. <ref cRef="Pall. " loc="70.">Pall.  f. 70.</ref> 
</sense> <sense n="2" xml:id="D2"> exploratum approbare i.e. <cit type="translation" xml:lang="la"><quote> consulere </quote></cit> <sense n="a" xml:id="D2a"> <cit type="translation" xml:lang="la"><quote> consilium capere </quote></cit>, <cit type="translation" xml:lang="la"><quote> decernere </quote></cit>, nonnumquam: <cit type="translation" xml:lang="la"><quote> sibi proponere </quote></cit>, <cit type="translation" xml:lang="la"><quote> in animo habere </quote></cit>, <cit type="translation" xml:lang="la"><quote> moliri </quote></cit> (<foreign xml:lang="grc">βουελεύεσθαι</foreign> et <foreign xml:lang="grc">βούλεσθαι</foreign>), <lbl expand="construitur cum">c.c.</lbl> <gramGrp> <case value="accusativus">Acc.</case> </gramGrp> rei: <foreign xml:lang="gez">ምንተ፡ መከረ፡ እግዚአብሔር፡ ላዕለ፡ ግብጽ፡</foreign> <ref cRef="Jes." loc="19,12">Jes. 19,12</ref>; <ref cRef="Mich." loc="4,12">Mich. 4,12</ref>; <ref cRef="Mich." loc="6,5">Mich. 6,5</ref>; <foreign xml:lang="gez">ይመክሩ፡ ዕሙቀ፡ ምክረ፡</foreign> <ref cRef="Jes. " loc="29,15">Jes.  29,15</ref>; <foreign xml:lang="gez">መከሩ፡ ምክረ፡ እኩየ፡ ላዕለ፡ ርእሶሙ፡</foreign> <ref cRef="Jes." loc="3,9">Jes. 3,9</ref>; <ref cRef="Jes." loc="8,10">Jes. 8,10</ref>; <foreign xml:lang="gez">መከሩ፡ ኀጢአተ፡ ወገብሩ፡ እኩየ፡</foreign> <ref cRef="Mich." loc="2,1">Mich. 2,1</ref>; <foreign xml:lang="gez">ንጹሐ፡ ይመክር፡ መንፈስየ፡</foreign> <foreign xml:lang="grc">καθαρὰ νοήσει</foreign> <ref cRef="Job" loc="33,3">Job 33,3</ref>; <lbl expand="cum">c.</lbl> <gramGrp> <pos expand="Subjunctivus">Subj.</pos> </gramGrp> verbi <ref cRef="Matth." loc="1,19">Matth. 1,19</ref>; <ref cRef="Marc." loc="15,15">Marc. 15,15</ref>; <foreign xml:lang="gez">በመካን፡ ኀበ፡ አንትሙ፡ መከርክሙ፡ ትባኡ፡</foreign> <ref cRef="Jer." loc="49,22">Jer. 49,22</ref>; <foreign xml:lang="gez">ዘመከርኩ፡ እግበር፡ ላዕሌሆሙ፡</foreign> <ref cRef="Jer." loc="18,8">Jer. 18,8</ref>; <ref cRef="Sir." loc="12,16">Sir. 12,16</ref>; <foreign xml:lang="gez">እግዚአብሔር፡ መከረ፡ ይጽደቅ፡</foreign> <ref cRef="Jes." loc="42,21">Jes. 42,21</ref>, vel <lbl expand="cum">c.</lbl> <foreign xml:lang="gez">ከመ፡</foreign> seq. <gramGrp> <pos expand="Subjunctivus">Subj.</pos> </gramGrp>, ut: <ref cRef="Act." loc="15,2">Act. 15,2</ref>; <ref cRef="Kuf." loc="32">Kuf. 32</ref>; <ref cRef="Zach." loc="8,15">Zach. 8,15</ref>; vel <lbl expand="cum">c.</lbl> <gramGrp> <case value="accusativus">Acc.</case> </gramGrp> Infin., ut: <foreign xml:lang="gez">ዘቀተለ፡ በኢያእምሮቱ፡ ወኢመከረ፡ ቀቲሎቶ፡</foreign> F.M. 4,2.
</sense> <sense n="b" xml:id="D2b"> <cit type="translation" xml:lang="la"><quote> consilium dare </quote></cit>, <cit type="translation" xml:lang="la"><quote> suadere </quote></cit>, <lbl expand="cum">c.</lbl> <gramGrp> <case value="accusativus">Acc.</case> </gramGrp> <lbl expand="persona">pers.</lbl> <ref cRef="Ex." loc="18,19">Ex. 18,19</ref> <lbl expand="varia lectio">var.</lbl>; <ref cRef="Job" loc="26,3">Job 26,3</ref> <lbl expand="varia lectio">var.</lbl> (<lbl expand="videas">vid.</lbl> <subc>II,1</subc>).
</sense> <sense n="c" xml:id="D2c"> <cit type="translation" xml:lang="la"><quote> consultare </quote></cit>, <cit type="translation" xml:lang="la"><quote> deliberare </quote></cit>: <foreign xml:lang="gez">መከሩ፡ ምስለ፡ ሊቃውንት፡</foreign> <ref cRef="Matth." loc="28,12">Matth. 28,12</ref>.
</sense> <sense n="d" xml:id="D2d"> <cit type="translation" xml:lang="la"><quote> exquirere consilium </quote></cit> alicujus <lbl expand="cum">c.</lbl> <lbl expand="duplex">dupl.</lbl> <gramGrp> <case value="accusativus">Acc.</case> </gramGrp>: <foreign xml:lang="gez">ውስተ፡ እንግልጋ፡ ኢይመክርዎሙ፡ ምክረ፡</foreign> <ref cRef="Sir." loc="38,33">Sir. 38,33</ref> <lbl expand="varia lectio">var.</lbl> 
</sense> 
</sense> </sense>
               
