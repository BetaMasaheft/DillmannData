               <sense xml:id="Dmain" xml:lang="la" source="#dillmann"> <gramGrp> <pos expand="nomen">n.</pos> <pos expand="numerale">num.</pos> </gramGrp> (i.e. <foreign xml:lang="gez">ዐሠርት፡</foreign> cum suff. <cit type="translation" xml:lang="la"><quote> û </quote></cit>), plerumque nominibus <gramGrp> <gen value="not found">m.</gen> </gramGrp> junctum, <gramGrp> <case value="accusativus">Acc.</case> </gramGrp> <foreign xml:lang="gez">ዐሠርተ፡</foreign>, <cit type="translation" xml:lang="la"><quote> decem </quote></cit>: <ref cRef="Gen." loc="14,4">Gen. 14,4</ref>; <ref cRef="Gen." loc="32,23">Gen. 32,23</ref>; <ref cRef="Gen." loc="37,2">Gen. 37,2</ref>; <ref cRef="Gen." loc="37,9">Gen. 37,9</ref>; <ref cRef="Ex. " loc="16,36">Ex.  16,36</ref>; <ref cRef="Ruth" loc="4,2">Ruth 4,2</ref>; <ref cRef="1 Reg." loc="1,8">1 Reg. 1,8</ref>; <ref cRef="1 Reg." loc="25,5">1 Reg. 25,5</ref>; <ref cRef="Jes." loc="38,5">Jes. 38,5</ref>; <ref cRef="Jes." loc="38,8">Jes. 38,8</ref>; <foreign xml:lang="gez">ዐሠርቱ፡ ወአሐዱ፡ ወርኅ፡</foreign> <ref cRef="Zach." loc="1,7">Zach. 1,7</ref>; <ref cRef="Hez." loc="26,1">Hez. 26,1</ref>; <cb n="0960" xml:id="c0960"/> <foreign xml:lang="gez">አመ፡ ተርፉ፡ ዐሠርቱ፡</foreign> <ref cRef="Am." loc="6,9">Am. 6,9</ref>; <ref cRef="4 Esr." loc="14,19">4 Esr. 14,19</ref>; <ref cRef="Judith" loc="1,1">Judith 1,1</ref>; <ref cRef="Ps." loc="91,3">Ps. 91,3</ref>; <ref cRef="Matth." loc="9,20">Matth. 9,20</ref>; <ref cRef="Matth." loc="10,1">Matth. 10,1</ref>; <ref cRef="Matth." loc="20,24">Matth. 20,24</ref>; <foreign xml:lang="gez">ዐሠርቱ፡ ምእት፡</foreign> <cit type="translation" xml:lang="la"><quote> mille </quote></cit> <ref cRef="Ps. " loc="104,7">Ps.  104,7</ref>; <ref cRef="2 Petr." loc="3,8">2 Petr. 3,8</ref>; <ref cRef="Apoc." loc="11,3">Apoc. 11,3</ref>; etiam alia suff. admittit, ut: <foreign xml:lang="gez">ዐሠርቲሆሙ፡</foreign> <ref cRef="4 Esr." loc="3,60">4 Esr. 3,60</ref>. </sense>
               
