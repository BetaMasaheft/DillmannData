               <sense xml:lang="la" source="#dillmann" xml:id="Dmain"> et <foreign xml:lang="gez">ነዊኅ፡</foreign> <gramGrp> <pos expand="adjectivum">adj.</pos> </gramGrp>, <gramGrp> <gen value="femininus">fem.</gen> </gramGrp> <foreign xml:lang="gez">ነዋኅ፡</foreign>, <lbl expand="pluralis">Pl.</lbl> <gramGrp> <gen value="not found">m.</gen> </gramGrp> <foreign xml:lang="gez">ነዊኃን፡</foreign> et <foreign xml:lang="gez">ነዋኃን፡</foreign> (<ref cRef="Hen." loc="1,6">Hen. 1,6</ref>), <gramGrp> <gen value="femininus">fem.</gen> </gramGrp> <foreign xml:lang="gez">ነዋኃት፡</foreign>, <lbl expand="cum">c.</lbl> <foreign xml:lang="gez">ነዋኅት፡</foreign> <sense n="1" xml:id="D1"> <cit type="translation" xml:lang="la">
                                <quote> longus </quote>
                            </cit>: <foreign xml:lang="gez">ሐብል፡ ነዋኅ፡</foreign> <ref cRef="Jes." loc="5,18">Jes. 5,18</ref>; <foreign xml:lang="gez">አሕባል፡ ነዊኃን፡</foreign> <ref cRef="Hen." loc="61,1">Hen. 61,1</ref>. <sense n="a" xml:id="D1a"> de tempore: <foreign xml:lang="gez">ነዋኅ፡ ሕይወት፡</foreign> <ref cRef="Dan." loc="7,12">Dan. 7,12</ref>; <foreign xml:lang="gez">ለነዋኅ፡ መዋዕል፡</foreign> <ref cRef="Ps." loc="20,4">Ps. 20,4</ref>; <ref cRef="Ps." loc="92,7">Ps. 92,7</ref>; <ref cRef="Jes." loc="34,10">Jes. 34,10</ref>; <ref cRef="Thren. " loc="5,20">Thren.  5,20</ref>; <ref cRef="Bar." loc="3,14">Bar. 3,14</ref>; <foreign xml:lang="gez">ለጕንዱይ፡ መዋዕል፡ ወለነዋኅ፡ ዓመታት፡</foreign> <ref cRef="Hez." loc="12,27">Hez. 12,27</ref>; <foreign xml:lang="gez">ንዋም፡ ነዊኅ፡</foreign> <ref cRef="Genz. " loc="83.">Genz.  f. 83.</ref> </sense> <sense n="b" xml:id="D1b"> de animo i.e. <cit type="translation" xml:lang="la">
                                    <quote> patiens </quote>
                                </cit>: <foreign xml:lang="gez">ነዊኀ፡ መንፈስ፡</foreign> <ref cRef="Chrys. Ta." loc="32.">Chrys. Ta. 32.</ref> </sense> <sense n="c" xml:id="D1c"> de oratione: <ref cRef="Marc." loc="12,40">Marc. 12,40</ref>; <ref cRef="Luc." loc="20,47">Luc. 20,47</ref>.</sense> <sense n="d" xml:id="D1d"> <gramGrp> <pos expand="adverbium, adverbialiter">adv.</pos> </gramGrp> <foreign xml:lang="gez">ነዋኀ፡</foreign> <cit type="translation" xml:lang="la">
                                    <quote> diu </quote>
                                </cit>, ut: <foreign xml:lang="gez">ዘርእ፡ ዘነዋኀ፡ የሐዩ፡</foreign> <ref cRef="Jes." loc="53,10">Jes. 53,10</ref>; <foreign xml:lang="gez">ዘእንበለ፡ ይነስሑ፡ ነዋኀ፡</foreign> <ref cRef="Clem." loc="236">Clem. f. 236</ref>; <foreign xml:lang="gez">ጸለየ፡ ነዋኀ፡</foreign> <ref cRef="Sx. Masc." loc="1">Sx. Masc. 1</ref>; <ref cRef="Kuf." loc="82.">Kuf. p. 82.</ref> </sense> </sense> <sense n="2" xml:id="D2"> <cit type="translation" xml:lang="la">
                                <quote> longinquus </quote>
                            </cit>: <foreign xml:lang="gez">እስከ፡ ነዋኅ፡ ብሔር፡</foreign> <ref cRef="Mich." loc="4,3">Mich. 4,3</ref>; <foreign xml:lang="gez">እስመ፡ ነዋኅ፡ ኀበ፡ አምሰጠከ፡</foreign> <ref cRef="Sir." loc="27,20">Sir. 27,20</ref>; <foreign xml:lang="gez">እስከ፡ ነዋኅ፡</foreign> <foreign xml:lang="grc">εἰς μακράν</foreign> <ref cRef="Sir." loc="24,32">Sir. 24,32</ref>. Hinc <foreign xml:lang="gez">ነዋኀ፡</foreign> vel <foreign xml:lang="gez">ነዊኀ፡</foreign> <gramGrp> <pos expand="adverbium, adverbialiter">adv.</pos> </gramGrp> <cit type="translation" xml:lang="la">
                                <quote> longe </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> procul </quote>
                            </cit>: <ref cRef="Job" loc="18,17">Job 18,17</ref>; <ref cRef="Job" loc="18,18">Job 18,18</ref>; <foreign xml:lang="gez">ነዋኀ፡ ጐዩ፡</foreign> <ref cRef="Jes." loc="22,3">Jes. 22,3</ref>; <foreign xml:lang="gez">ነዋኀ፡ ይርሕቁ፡ እምኔየ፡</foreign> <ref cRef="Jes." loc="29,13">Jes. 29,13</ref>; <ref cRef="Matth." loc="15,8">Matth. 15,8</ref>; <ref cRef="Jer." loc="2,5">Jer. 2,5</ref>; <ref cRef="Gen." loc="44,4">Gen. 44,4</ref>; <ref cRef="Marc." loc="7,6">Marc. 7,6</ref> (<foreign xml:lang="gez">ነዊኀ፡</foreign> et <foreign xml:lang="gez">ነዋኀ፡</foreign>); <foreign xml:lang="gez">ርሑቃን፡ ነዋኀ፡</foreign> <ref cRef="Joel" loc="4,8">Joel 4,8</ref>; <foreign xml:lang="gez">ይበጽሕ፡ ነዊኀ፡</foreign> <ref cRef="Hen. " loc="32,4">Hen.  32,4</ref>.</sense> <sense n="3" xml:id="D3"> <cit type="translation" xml:lang="la">
                                <quote> procerus </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> altus </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> elatus </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> excelsus </quote>
                            </cit>: <foreign xml:lang="gez">አሕዛብ፡ ነዋኃት፡</foreign> <lbl expand="vel">v.</lbl> <foreign xml:lang="gez">ነዊኃን፡</foreign> <ref cRef="Jes." loc="18,2">Jes. 18,2</ref>; <foreign xml:lang="gez">ሕዝብ፡ ዐቢይ፡ ወነዋኅት፡</foreign> <ref cRef="Deut. " loc="9,2">Deut.  9,2</ref>; <foreign xml:lang="gez">ኮነ፡ ነዊኀ፡ በቆሙ፡</foreign> (staturâ) <ref cRef="Sx. Genb." loc="13">Sx. Genb. 13</ref>; <foreign xml:lang="gez">ይሬኢ፡ ኵሎ፡ ነዋኀ፡</foreign> <foreign xml:lang="grc">πᾶν ὑψηλὸν ὁρᾷ</foreign> <ref cRef="Job" loc="41,25">Job 41,25</ref>; <foreign xml:lang="gez">ኵሉ፡ ነዋኅ፡ ወልዑል፡</foreign> <ref cRef="Jes." loc="2,12">Jes. 2,12</ref>; <ref cRef="Hez." loc="21,26">Hez. 21,26</ref>; <ref cRef="Ps." loc="137,7">Ps. 137,7</ref>; <foreign xml:lang="gez">መንበሩ፡ ነዋኅ፡ ወልዑል፡</foreign> <ref cRef="Jes." loc="6,1">Jes. 6,1</ref>; <foreign xml:lang="gez">ውስተ፡ ርእስ፡ ነዋኅ፡</foreign> <ref cRef="Jes." loc="17,6">Jes. 17,6</ref>; <foreign xml:lang="gez">ዕፅ፡ ነዋኅ፡</foreign> <ref cRef="Gen." loc="12,6">Gen. 12,6</ref>; <ref cRef="Sir. " loc="50,10">Sir.  50,10</ref>; <foreign xml:lang="gez">ነዋኅት፡ አዕዋም፡</foreign> <ref cRef="Jes." loc="37,24">Jes. 37,24</ref>; <foreign xml:lang="gez">ነዋኅ፡ ጥቅም፡</foreign> <ref cRef="Apoc." loc="21,12">Apoc. 21,12</ref>; <foreign xml:lang="gez">ደብር፡ ነዊኅ፡</foreign> <ref cRef="Hen." loc="25,3">Hen. 25,3</ref>; <ref cRef="Jes." loc="14,13">Jes. 14,13</ref>; <ref cRef="Jes." loc="28,4">Jes. 28,4</ref>; <ref cRef="Jes." loc="40,9">Jes. 40,9</ref>; <ref cRef="Jes." loc="57,7">Jes. 57,7</ref>; <ref cRef="Jer. " loc="2,20">Jer.  2,20</ref>; <ref cRef="Jer." loc="3,6">Jer. 3,6</ref>; <ref cRef="Job" loc="40,15">Job 40,15</ref>; <ref cRef="Matth." loc="4,8">Matth. 4,8</ref>; <foreign xml:lang="gez">አርእስተ፡ አድባር፡ ነዋኃት፡</foreign> <lbl expand="vel">v.</lbl> <foreign xml:lang="gez">ነዋኅት፡</foreign> <ref cRef="Judith" loc="5,1">Judith 5,1</ref>; <ref cRef="Ps. " loc="94,4">Ps.  94,4</ref>; <ref cRef="Gen." loc="7,19">Gen. 7,19</ref>; <ref cRef="Hen." loc="1,6">Hen. 1,6</ref>; <ref cRef="Kuf." loc="5.">Kuf. 5.</ref> <sense n="a" xml:id="D3a"> de voce: <foreign xml:lang="gez">ትጼውዕ፡ በነዋኅ፡ ስብከት፡</foreign> <ref cRef="Prov. " loc="9,3">Prov.  9,3</ref>.</sense> <sense n="b" xml:id="D3b"> de animo: <foreign xml:lang="gez">ነዋኀ፡ መንፈስ፡</foreign> <ref cRef="Chrys. Ta." loc="19">Chrys. Ta. 19</ref> (<lbl expand="videas">vid. sub</lbl> <foreign xml:lang="gez">ርሒብ፡</foreign>).</sense> <sense n="c" xml:id="D3c"> <cit type="translation" xml:lang="la">
                                    <quote> altus </quote>
                                </cit> i.e. <cit type="translation" xml:lang="la">
                                    <quote> profundus </quote>
                                </cit>: <foreign xml:lang="gez">ግብ፡ ነዋኅ፡</foreign> Lud. e <ref cRef="Vit. Greg. Arm." loc="">Vit. Greg. Arm. </ref> — Voc. Ae.: <foreign xml:lang="gez">ነዋኅ፡ ዘ፡ ረዥም።</foreign> </sense> </sense> </sense>
               
