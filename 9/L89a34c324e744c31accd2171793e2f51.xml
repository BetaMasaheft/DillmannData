               <sense xml:lang="la" source="#dillmann" xml:id="Dmain"> <subc>III,2</subc> <sense n="1" xml:id="D1">
                     <cit type="translation" xml:lang="la">
                        <quote>inquinari</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>conspurcari</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>se inquinare</quote>
                     </cit>: <foreign xml:lang="gez">ይረኵሱ፡ ባቲ፡ ወይዴነሱ፡</foreign>
                     <ref cRef="Chrys. ho." loc="31">Chrys. ho. 31</ref>; <foreign xml:lang="gez">እመቦ፡ ዘአበሰ፡ ወተደነሰ፡ በዛቲ፡ ኀጢአት፡</foreign>
                     <ref cRef="F.M." loc="5">F.M. 5</ref>,2; <foreign xml:lang="gez">እመሂ፡ በቃል፡ አበሱ፤
                        ወእመሂ፡ በገቢር፡ ረኵሱ፤ ወእመሂ፡ በክሡት፡ ተደነሱ፡</foreign> Genz. f. 118.</sense>
                  <sense n="2" xml:id="D2">
                     <cit type="translation" xml:lang="la">
                        <quote>mercedem meretriciam accipere</quote>
                     </cit>: <foreign xml:lang="gez">ኢትዴነሲ፡ እንከ፡ </foreign>
                     <ref cRef="Hez." loc="16,41">Hez. 16,41</ref>.</sense>
               </sense>
               
