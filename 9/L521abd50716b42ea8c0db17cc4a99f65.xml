            <entry xml:id="L521abd50716b42ea8c0db17cc4a99f65" n="5215" type="hom">
               <form>
                  <foreign xml:lang="gez">ነገደ</foreign>
                        <seg type="hom">I</seg>
                    <rs type="root"/>
                    </form>
               <sense xml:lang="la" source="#dillmann" xml:id="Dmain"> <subc>I,1</subc> [amh. <lbl expand="idem">id.</lbl>; potestas radicis prima in <cit type="translation" xml:lang="la">
                            <quote> trahendo </quote>
                        </cit> (<foreign xml:lang="syr">ܢܓܰܕ</foreign>) est; inde propagatur significatus <cit type="translation" xml:lang="la">
                            <quote> proficisci </quote>
                        </cit>, in specie de agmine in longum protracto et de itinere in longinquum extenso, sicut germ. ziehen, <lbl expand="videas">vid.</lbl> etiam Ges. thes. p. 826 sub <lbl expand="radix">rad.</lbl> <foreign xml:lang="he">מָשַׁךְ</foreign>] <gramGrp> <pos expand="Subjunctivus">Subj.</pos> </gramGrp> <foreign xml:lang="gez">ይንግድ፡</foreign>, <cit type="translation" xml:lang="la">
                            <quote> peregre abire </quote>
                        </cit> vel <cit type="translation" xml:lang="la">
                            <quote> proficisci </quote>
                        </cit>, <cit type="translation" xml:lang="la">
                            <quote> peregre esse </quote>
                        </cit>, <cit type="translation" xml:lang="la">
                            <quote> peregrinari </quote>
                        </cit>, <cit type="translation" xml:lang="la">
                            <quote> iter facere </quote>
                        </cit>, <foreign xml:lang="grc">ἀποδημεῖν</foreign> <ref cRef="Matth." loc="21,33">Matth. 21,33</ref>; <ref cRef="Matth. " loc="25,14">Matth.  25,14</ref>; <ref cRef="Matth." loc="25,15">Matth. 25,15</ref>; <ref cRef="Marc." loc="12,1">Marc. 12,1</ref>; <ref cRef="2 Cor." loc="5,9">2 Cor. 5,9</ref>; <foreign xml:lang="gez">ኢይክል፡ ነጊደ፡</foreign> <cit type="translation" xml:lang="la">
                            <quote> non possum iter facere </quote>
                        </cit> <ref cRef="Kuf." loc="31">Kuf. 31</ref>; <foreign xml:lang="gez">እለ፡ ይነግዱ፡ በባሕር፡ ወበየብስ፡</foreign> <ref cRef="Kid." loc="18">Kid. f. 18</ref>; <lbl expand="cum">c.</lbl> <gramGrp> <case value="accusativus">Acc.</case> </gramGrp> loci, quo tenditur <ref cRef="Luc." loc="15,13">Luc. 15,13</ref>; <ref cRef="Luc." loc="20,9">Luc. 20,9</ref>; <foreign xml:lang="gez">ብእሲ፡ ዘነገደ፡ ርሑቀ፡ ፍኖተ፡</foreign> <ref cRef="Fal." loc="38">Fal. f. 38</ref>; <lbl expand="cum">c.</lbl> <foreign xml:lang="gez">እምነ፡</foreign> loci, unde disceditur: <foreign xml:lang="gez">ሶበ፡ ይነግድ፡ እምብሔሩ፡</foreign> <ref cRef="Prov." loc="27,8">Prov. 27,8</ref>; <ref cRef="2 Cor." loc="5,6">2 Cor. 5,6</ref> Platt. <sense n="a" xml:id="Da"> <cit type="translation" xml:lang="la">
                                <quote> peragrare </quote>
                            </cit>, <lbl expand="cum">c.</lbl> <gramGrp> <case value="accusativus">Acc.</case> </gramGrp> terrae: <foreign xml:lang="gez">ነገድነ፡ ገዳመ፡ በድው፡</foreign> <ref cRef="Sap." loc="5,7">Sap. 5,7</ref>; <foreign xml:lang="gez">አንሶሰዉ፡ ወነገዱ፡ ብሔረ፡ በድው፡</foreign> <ref cRef="Sap. " loc="11,2">Sap.  11,2</ref>.</sense> <sense n="b" xml:id="Db"> <lbl expand="translate">transl.</lbl>: <cit type="translation" xml:lang="la">
                                <quote> procul discedere </quote>
                            </cit> ab aliquo, ut: <foreign xml:lang="gez">ነገዱ፡ እምኔየ፡</foreign> <foreign xml:lang="grc">ἀπεπήδησαν ἀπ᾽ ἐμοῦ</foreign> <ref cRef="Hos. " loc="7,13">Hos.  7,13</ref>.</sense> <sense n="c" xml:id="Dc"> in specie: <cit type="translation" xml:lang="la">
                                <quote> negotiandi </quote>
                            </cit> vel <cit type="translation" xml:lang="la">
                                <quote> mercandi </quote>
                            </cit> causa, <cit type="translation" xml:lang="la">
                                <quote> mercaturam facere </quote>
                            </cit> <ref cRef="2 Par." loc="8,16">2 Par. 8,16</ref>; <foreign xml:lang="gez">እለ፡ ይነግዱ፡</foreign> <foreign xml:lang="grc">ἔμποροι</foreign> <ref cRef="3 Reg." loc="10,15">3 Reg. 10,15</ref>; <foreign xml:lang="grc">ἀγοράζων</foreign> <foreign xml:lang="gez">ዘይነግድ፡</foreign> <ref cRef="Sir." loc="37,11">Sir. 37,11</ref>; <lbl expand="cum">c.</lbl> <foreign xml:lang="gez">በ፡</foreign> mercis, ut: <foreign xml:lang="gez">ነገደ፡ በመክሊት፡</foreign> <ref cRef="Sx. Teq." loc="27 ">Sx. Teq. 27 </ref>(e <ref cRef="Matth." loc="25">Matth. 25</ref>).</sense> <sense n="d" xml:id="Dd"> <cit type="translation" xml:lang="la">
                                <quote> iter </quote>
                            </cit> <cit type="translation" xml:lang="la">
                                <quote> facere navi </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> navigare </quote>
                            </cit>, <lbl expand="cum">c.</lbl> <foreign xml:lang="gez">በ፡</foreign> vel <foreign xml:lang="gez">ውስተ፡</foreign> aquae, ut: <foreign xml:lang="gez">ነገደ፡ ውስተ፡ ባሕር፡</foreign> <ref cRef="Jsp." loc="318">Jsp. p. 318</ref>; <foreign xml:lang="gez">እለ፡ ይነግዱ፡ በባሕር፡ ወበየብስ፡</foreign> <ref cRef="Kid." loc="18">Kid. f. 18</ref>; <foreign xml:lang="gez">እንዘ፡ ይፈቅድ፡ ይንግድ፡ ውስተ፡ ሞገዳት፡</foreign> <ref cRef="Sap." loc="14,1">Sap. 14,1</ref>; vel <lbl expand="cum">c.</lbl> <gramGrp> <case value="accusativus">Acc.</case> </gramGrp> aquae: <foreign xml:lang="gez">እለ፡ ይነግድዋ፡ ለባሕር፡</foreign> <ref cRef="Sir." loc="43,24">Sir. 43,24</ref>; <ref cRef="Jes." loc="42,10">Jes. 42,10</ref>; <foreign xml:lang="gez">ዘይነግድ፡ በሐመር፡ ባሕረ፡</foreign> <ref cRef="Kuf." loc="50">Kuf. 50</ref>; <gramGrp> <pos expand="absolute">abs.</pos> </gramGrp>: <foreign xml:lang="gez">ዐርገ፡ ውስተ፡ ሐመር፡ ይንግድ፡ ምስሌሆሙ፡</foreign> <ref cRef="Jon." loc="1,3">Jon. 1,3</ref>; <ref cRef="Act." loc="27,2">Act. 27,2</ref> Platt; <lbl expand="cum">c.</lbl> <gramGrp> <case value="accusativus">Acc.</case> </gramGrp> loci, quo tenditur: <foreign xml:lang="gez">ነገደ፡ ቆጵሮስ፡</foreign> <ref cRef="Act." loc="15,39">Act. 15,39</ref>; <foreign xml:lang="gez">ነገደ፡ ሶርያ፡</foreign> <ref cRef="Act." loc="18,18">Act. 18,18</ref> <lbl expand="romanae editionis">rom.</lbl>; <foreign xml:lang="gez">ነገደ፡ መርሶ፡</foreign> Sx. <ref cRef="Genb." loc="26 ">Genb. 26 </ref>Enc.; <foreign xml:lang="gez">መርሶ፡ አሕማር፡ በጽንፈ፡ ባሕር፡ ለነጊድ፡</foreign> <ref cRef="Jsp. " loc="280">Jsp.  p. 280</ref>; etiam de nave praedicatur, ut: <foreign xml:lang="gez">ረከበ፡ ሐመረ፡ ዘይነግድ፡ ብሔረ፡ ተርሴስ፡</foreign> <ref cRef="Jon." loc="1,3">Jon. 1,3</ref>.</sense> <sense n="e" xml:id="De"> <cit type="translation" xml:lang="la">
                                <quote> peregrinando loca sacra adire </quote>
                            </cit> (wallfahrten), ut: <foreign xml:lang="gez">ነገደ፡ ኢየሩሳሌም፡ ፪ ጊዜ፡</foreign> <ref cRef="Sx. Masc." loc="18.">Sx. Masc. 18.</ref> </sense> </sense>
               
            </entry>
