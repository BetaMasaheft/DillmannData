         <div>
            <entry xml:id="L93fba24780cb47a997e70efa10fcb41b" n="7130">
               <form>
                  <foreign xml:lang="gez">አዕቀጸ</foreign>
               </form>
               <sense xml:lang="la" source="#dillmann" xml:id="Dmain"> <subc>II,1</subc> <sense n="1" xml:id="D1"> <cit type="translation" xml:lang="la">
                                <quote> laqueis implicare </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> illaqueare </quote>
                            </cit>pedes alicujus, {<ref target="#c1433"/> <cit type="translation" xml:lang="la">
                                <quote> supplantare </quote>
                            </cit> }, pede supposito vel laqueo <cit type="translation" xml:lang="la">
                                <quote> improvidum prosternere </quote>
                            </cit>: <foreign xml:lang="grc">συμποδίζειν</foreign> <ref cRef="Gen." loc="22,9">Gen. 22,9</ref> (nisi <foreign xml:lang="gez">ዐቀደ፡</foreign> emendare malueris); <ref cRef="Ps." loc="17,43">Ps. 17,43</ref>; <ref cRef="Ps." loc="77,35">Ps. 77,35</ref>; <foreign xml:lang="grc">πτερνίζειν</foreign> <ref cRef="Gen." loc="27,36">Gen. 27,36</ref>; <foreign xml:lang="gez">በውስተ፡ ከርሥ፡ አዕቀጾ፡ ለእኁሁ፡</foreign> <ref cRef="Hos." loc="12,3">Hos. 12,3</ref>; <foreign xml:lang="gez">ኵሉ፡ እኊ፡ አዕቅጾ፡ ያዐቅጽ፡</foreign> <ref cRef="Jer." loc="9,4">Jer. 9,4</ref>; <foreign xml:lang="grc">σκελίζειν</foreign> <foreign xml:lang="gez">አነ፡ ኣዐቅጾሙ፡</foreign> <ref cRef="Jer." loc="10,18">Jer. 10,18</ref>; <foreign xml:lang="grc">ὑποσκελίζειν</foreign> <ref cRef="Ps." loc="16,13">Ps. 16,13</ref>; <ref cRef="Ps." loc="139,5">Ps. 139,5</ref>; <ref cRef="Sx. Teq." loc="24 ">Sx. Teq. 24 </ref> Enc.; <ref cRef="Genz." loc="12.">Genz. 12.</ref> </sense> <sense n="2" xml:id="D2"> <lbl expand="translate">transl.</lbl> <sense n="a" xml:id="D2a"> <cit type="translation" xml:lang="la">
                                    <quote> insidiari </quote>
                                </cit>, <cit type="translation" xml:lang="la">
                                    <quote> defraudare </quote>
                                </cit>: <foreign xml:lang="gez">ያዐቅጾኑ፡ ሰብእ፡ ለእግዚአብሔር፡</foreign> <foreign xml:lang="grc">πτερνιεῖ</foreign> <ref cRef="Mal. " loc="3,8">Mal.  3,8</ref>.</sense> <sense n="b" xml:id="D2b"> <cit type="translation" xml:lang="la">
                                    <quote> dolo </quote>
                                </cit> vel <cit type="translation" xml:lang="la">
                                    <quote> fraude detorquere </quote>
                                </cit>: <foreign xml:lang="gez">አዕቅጾቶሙ፡ እምፍኖተ፡ ጽድቅ፡</foreign> <ref cRef="Kuf." loc="1.">Kuf. 1.</ref> – <cit type="translation" xml:lang="la">
                                    <quote> Voc. Ae. </quote>
                                </cit>: <foreign xml:lang="gez">አዕቀጸ፡ ዘ፡ አሰረ።</foreign> </sense> </sense> </sense>
               
            <sense xml:lang="en" source="#traces" xml:id="Tmain">
                        <cit type="transcription" xml:lang="gez">
                            <quote>ʾaʿqaṣa</quote>
                        </cit> <cit type="translation" xml:lang="en">
                            <quote>hinder</quote>
                        </cit>, <cit type="translation" xml:lang="en">
                            <quote>impede</quote>
                        </cit> ‘<cit type="translation" xml:lang="it">
                            <quote>ostacolare</quote>
                        </cit>’ <foreign xml:lang="gez">ወአዕቀጾሙ፡ በኀይለ፡ መጻሕፍት፡ ወሞኦሙ፡ በፍትሕ፡</foreign> ‘<cit type="translation" xml:lang="it">
                            <quote>Ma li ostacolò con la potenza dei libri e li sconfisse con la giustizia</quote>
                        </cit>’ <bibl>
                            <ptr target="bm:Tedros2009TawaldaMadehnVittore"/>
                            <citedRange unit="page"> 112 (192) l. 8-9 (ed.); 113 (193) l. 8-9 (tr.)</citedRange>
                        </bibl> </sense>
