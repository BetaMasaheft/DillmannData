               <sense xml:lang="la" source="#dillmann" xml:id="Dmain"> <subc>I,1</subc> [amh. et <foreign xml:lang="he">יָרַד</foreign> <lbl expand="idem">id.</lbl>; <foreign xml:lang="ar">وَرَدَ</foreign> plerumque significatus speciales habet] <gramGrp> <pos expand="Subjunctivus">Subj.</pos> </gramGrp> <foreign xml:lang="gez">ይረድ፡</foreign>; Imperat. <foreign xml:lang="gez">ረድ፡</foreign>, <cit type="translation" xml:lang="la">
                            <quote> descendere </quote>
                        </cit>, <ref cRef="Ex." loc="19,24">Ex. 19,24</ref>; <ref cRef="Jes." loc="47,1">Jes. 47,1</ref>; <ref cRef="Ps." loc="17,11">Ps. 17,11</ref>; <ref cRef="Matth." loc="24,17">Matth. 24,17</ref>; <ref cRef="Marc." loc="1,10">Marc. 1,10</ref>; <foreign xml:lang="gez">ወሪዶት፡</foreign> <ref cRef="Hen." loc="63,10">Hen. 63,10</ref>; <ref cRef="Joh." loc="4,49">Joh. 4,49</ref>; <lbl expand="cum">c.</lbl> <foreign xml:lang="gez">እምነ፡</foreign> loci: <foreign xml:lang="gez">ጐንደየ፡ ወሪደ፡ እምደብር፡</foreign> <ref cRef="Ex." loc="32,1">Ex. 32,1</ref>; <ref cRef="Judith" loc="10,10">Judith 10,10</ref>; <foreign xml:lang="gez">እምነ፡ ሰረገላ፡</foreign> <ref cRef="Jud. " loc="4,15">Jud.  4,15</ref>; <foreign xml:lang="gez">እምሰማይ፡</foreign> <ref cRef="Matth." loc="28,2">Matth. 28,2</ref>; <ref cRef="Joh." loc="1,32">Joh. 1,32</ref>; <foreign xml:lang="gez">እመስቀል፡</foreign> <ref cRef="Marc." loc="15,30">Marc. 15,30</ref>; <foreign xml:lang="gez">ረዲ፡ እምነ፡ ክብርኪ፡</foreign> <ref cRef="Jer." loc="31,18">Jer. 31,18</ref>; <lbl expand="cum">c.</lbl> <foreign xml:lang="gez">እምዲበ፡</foreign> <ref cRef="Ex." loc="19,14">Ex. 19,14</ref>; <ref cRef="Matth. " loc="14,29">Matth.  14,29</ref>; <lbl expand="cum">c.</lbl> <foreign xml:lang="gez">እምውስተ፡</foreign> <ref cRef="Gen." loc="24,64">Gen. 24,64</ref>; <lbl expand="cum">c.</lbl> <foreign xml:lang="gez">እምላዕለ፡</foreign> <ref cRef="1 Reg." loc="25,23">1 Reg. 25,23</ref>; <lbl expand="cum">c.</lbl> <foreign xml:lang="gez">በ፡</foreign> scalae <ref cRef="Gen." loc="28,12">Gen. 28,12</ref>; <lbl expand="cum">c.</lbl> <foreign xml:lang="gez">ውስተ፡</foreign> loci: <foreign xml:lang="gez">ሑር፡ ረድ፡ ውስተ፡ ቤተ፡ ንጉሥ፡</foreign> <ref cRef="Jer." loc="22,1">Jer. 22,1</ref>; <ref cRef="Cant." loc="6,2">Cant. 6,2</ref>; <ref cRef="Gen." loc="12,10">Gen. 12,10</ref>; <ref cRef="Gen." loc="46,3">Gen. 46,3</ref>; <ref cRef="Gen." loc="46,4">Gen. 46,4</ref>; <ref cRef="Ex." loc="19,18">Ex. 19,18</ref>; <ref cRef="2 Reg." loc="11,10">2 Reg. 11,10</ref>; <ref cRef="Marc." loc="13,15">Marc. 13,15</ref>; <ref cRef="Joh." loc="6,16">Joh. 6,16</ref>; <lbl expand="cum">c.</lbl> <gramGrp> <case value="accusativus">Acc.</case> </gramGrp> loci: <foreign xml:lang="gez">ወወረደ፡ ቤተ፡ ንጉሥ፡</foreign> <ref cRef="Jer." loc="43,12">Jer. 43,12</ref>; <foreign xml:lang="gez">እለ፡ ተሐውሩ፡ ትወርዱ፡ ብሔረ፡ ግብጽ፡</foreign> <ref cRef="Jes." loc="30,2">Jes. 30,2</ref>; <ref cRef="Jes." loc="31,1">Jes. 31,1</ref>; <ref cRef="Jes." loc="52,4">Jes. 52,4</ref>; <foreign xml:lang="gez">ወረዱ፡ ሲኦል፡</foreign> (v. <foreign xml:lang="gez">ሲኦለ፡</foreign>) <ref cRef="Bar. " loc="3,19">Bar.  3,19</ref>; <ref cRef="Jon." loc="1,3">Jon. 1,3</ref>; <ref cRef="Joh." loc="2,12">Joh. 2,12</ref>; <ref cRef="Act." loc="7,15">Act. 7,15</ref>; raro <lbl expand="cum">c.</lbl> <foreign xml:lang="gez">ለ፡</foreign> (versus) loci, <ref cRef="Luc." loc="10,30">Luc. 10,30</ref> (cfr. <ref cRef="Act." loc="8,26">Act. 8,26</ref>); <lbl expand="cum">c.</lbl> <foreign xml:lang="gez">ኀበ፡</foreign> pers. (ad): <ref cRef="Joh." loc="1,52">Joh. 1,52</ref>; <ref cRef="Act." loc="10,21">Act. 10,21</ref>; <ref cRef="Act." loc="14,11">Act. 14,11</ref>; <lbl expand="cum">c.</lbl> <gramGrp> <case value="accusativus">Acc.</case> </gramGrp> viae qua quis descendit, <ref cRef="Luc." loc="10,31">Luc. 10,31</ref>, vel loci per quem descendit, <ref cRef="Asc. Jes." loc="10,8">Asc. Jes. 10,8</ref>; <lbl expand="cum">c.</lbl> <foreign xml:lang="gez">ላዕለ፡</foreign> vel <foreign xml:lang="gez">ዲበ፡</foreign> super aliquem, <ref cRef="Luc." loc="3,32">Luc. 3,32</ref>; <ref cRef="Joh." loc="1,33">Joh. 1,33</ref>; <cit type="translation" xml:lang="la">
                            <quote> irruere </quote>
                        </cit> super aliquem <ref cRef="Jos." loc="11,7">Jos. 11,7</ref>. De rebus inanimatis <sense n="a" xml:id="Da"> <cit type="translation" xml:lang="la">
                                <quote> defluere </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> decidere </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> delabi </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> deferri </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> demitti </quote>
                            </cit>: <foreign xml:lang="gez">ደም፡ ዘይወርድ፡ ውስተ፡ ምድር፡</foreign> <ref cRef="Luc." loc="22,44">Luc. 22,44</ref>; pluvia de coelo <ref cRef="Jes." loc="55,10">Jes. 55,10</ref>; <ref cRef="Matth." loc="7,25">Matth. 7,25</ref>; <foreign xml:lang="gez">አኀዘ፡ ማይ፡ ይረድ፡ ውስተ፡ ቀላይ፡</foreign> <ref cRef="Kuf." loc="5">Kuf. 5</ref>; <foreign xml:lang="gez">ይወርድ፡ ጠል፡</foreign> <ref cRef="Num." loc="11,9">Num. 11,9</ref>; <foreign xml:lang="gez">ነቅዕ፡ ዘይወርድ፡ እምድብር፡</foreign> <ref cRef="Deut." loc="9,21">Deut. 9,21</ref>; <ref cRef="Ps." loc="71,6">Ps. 71,6</ref>; <ref cRef="Ps." loc="132,3">Ps. 132,3</ref>; <foreign xml:lang="gez">እሳት፡ እምሰማይ፡</foreign> <ref cRef="Luc." loc="9,54">Luc. 9,54</ref>; <foreign xml:lang="gez">በረድ፡ እምሰማይ፡</foreign> <ref cRef="Apoc." loc="16,21">Apoc. 16,21</ref>; <ref cRef="Hen." loc="18,11">Hen. 18,11</ref>; <ref cRef="Hen." loc="101,2">Hen. 101,2</ref>; <foreign xml:lang="gez">በከመ፡ ወረደ፡ መዐትየ፡ ላዕለ፡</foreign> … <foreign xml:lang="grc">ἔσταξε</foreign> <ref cRef="Jer." loc="49,18">Jer. 49,18</ref>; <foreign xml:lang="gez">ወረደ፡ ዕፃ፡ ላዕለ፡ ዮናስ፡</foreign> (<cit type="translation" xml:lang="la">
                                <quote> cadere </quote>
                            </cit>) <ref cRef="Jon." loc="1,7">Jon. 1,7</ref>; <ref cRef="Esth." loc="3,7">Esth. 3,7</ref>; <foreign xml:lang="gez">ልብሰ፡ መትከፍት፡ ዘይወርድ፡ ውስተ፡ ክልኤሆን፡ መታክፊሁ፡</foreign> <ref cRef="Ex." loc="36,11">Ex. 36,11</ref>; <foreign xml:lang="gez">ፍርሀት፡ ወረዓድ፡ ወረደ፡ ላዕሌሁ፡</foreign> <foreign xml:lang="grc">ἐπέπεσεν</foreign> <ref cRef="Luc." loc="1,12">Luc. 1,12</ref>.</sense> <sense n="b" xml:id="Db"> <cit type="translation" xml:lang="la">
                                <quote> desidere </quote>
                            </cit> <ref cRef="Ps." loc="103,9">Ps. 103,9</ref>.</sense> <sense n="c" xml:id="Dc"> <cit type="translation" xml:lang="la">
                                <quote> deorsum tendere </quote>
                            </cit>, de terminis, <ref cRef="Num." loc="34,11">Num. 34,11</ref>; <ref cRef="Num." loc="34,12">Num. 34,12</ref>; <ref cRef="Jos." loc="17,9">Jos. 17,9</ref>; <ref cRef="Jos. " loc="18,13">Jos.  18,13</ref>.</sense> </sense>
               
