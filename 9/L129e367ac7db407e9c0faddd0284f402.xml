               <sense xml:lang="la" source="#dillmann" xml:id="Dmain"> <gramGrp> <pos expand="adjectivum">adj.</pos> </gramGrp> (forma <gramGrp> <pos expand="participium">part.</pos> </gramGrp>), <gramGrp> <gen value="femininus">fem.</gen> </gramGrp> <foreign xml:lang="gez">ካልእት፡</foreign>, <lbl expand="pluralis">Pl.</lbl> <foreign xml:lang="gez">ካልኣን፡</foreign>, <gramGrp> <gen value="not found">m.</gen> </gramGrp> <foreign xml:lang="gez">ካልኣት፡</foreign> <gramGrp> <gen value="femininus">fem.</gen> </gramGrp>, <sense n="1" xml:id="D1"> <cit type="translation" xml:lang="la">
