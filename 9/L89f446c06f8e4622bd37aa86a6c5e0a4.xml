                  </cit>, <cit type="translation" xml:lang="la">
                     <quote>impudicitia</quote>
                  </cit>: <foreign xml:lang="grc">ἀσέλγεια</foreign>
                  <ref cRef="Marc." loc="7,22">Marc. 7,22</ref> Platt; <ref cRef="2 Cor." loc="11,21">2 Cor. 11,21</ref>; <ref cRef="Gal." loc="5,19">Gal. 5,19</ref>;
                     <foreign xml:lang="grc">ἀσωτία</foreign>
                  <ref cRef="Eph." loc="5,18">Eph. 5,18</ref>; <ref cRef="Tit." loc="1,6">Tit.
                     1,6</ref>; <ref cRef="Luc." loc="15,13">Luc. 15,13</ref>; <foreign xml:lang="grc">ἀσυνθεσία</foreign>
                  <ref cRef="Jer." loc="3,7">Jer. 3,7</ref>; <foreign xml:lang="grc">χρεμετισμός </foreign>
