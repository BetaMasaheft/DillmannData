               <sense xml:lang="la" source="#dillmann" xml:id="Dmain"> <gramGrp> <pos expand="Substantivum">subst.</pos> </gramGrp> (etiam per <foreign xml:lang="gez">ኅ፡</foreign>), <gramGrp> <gen value="femininus">fem.</gen> </gramGrp> et <gramGrp> <gen value="not found">m.</gen> </gramGrp>, <lbl expand="pluralis">Pl.</lbl> <foreign xml:lang="gez">እራሓት፡</foreign> [ad <foreign xml:lang="ar">رَحَّ</foreign>, <foreign xml:lang="ar">رَحْرَحَ</foreign>, quibus potestas <cit type="translation" xml:lang="la">
                            <quote> lati </quote>
                        </cit>, <cit type="translation" xml:lang="la">
                            <quote> ampli </quote>
                        </cit> et <cit type="translation" xml:lang="la">
                            <quote> aequalis </quote>
                        </cit> (flach) inest, referendum videtur] <cit type="translation" xml:lang="la">
                            <quote> palma </quote>
                        </cit> manus cum <cit type="translation" xml:lang="la">
                            <quote> carpo </quote>
                        </cit>, <foreign xml:lang="grc">καρπός</foreign>, <foreign xml:lang="gez">እራሓ፡ ሰፍሐት፡ ለነዳይ፡</foreign> <ref cRef="Prov." loc="29,38">Prov. 29,38</ref>; <ref cRef="Did." loc="3">Did. 3</ref>; <foreign xml:lang="gez">እራሓተ፡ እደዊሁ፡</foreign> <ref cRef="1 Reg." loc="5,4">1 Reg. 5,4</ref>; <foreign xml:lang="grc">τὰ ἴχνη τῶν χειρῶν</foreign> <ref cRef="4 Reg." loc="9,35">4 Reg. 9,35</ref>; <foreign xml:lang="gez">ሰላም፡ ለእራሓትኪ፡</foreign> I <ref cRef="Sal. Mar." loc="24">Sal. Mar. 24</ref>; precantium est <foreign xml:lang="gez">አንሥኦተ፡ እደዊነ፡ እንዘ፡ ስፉሓት፡ እራሓቲሆን፡</foreign> F.N. 14; <foreign xml:lang="gez">አንስት፡ ትሰፍሕ፡ እደዊሃ፡ ወእራሓ፡ ለውሂብ፡</foreign> <ref cRef="Clem." loc="190">Clem. f. 190</ref>; <foreign xml:lang="gez">እራሕ፡ ዘይምን፡</foreign> <ref cRef="Sx. Masc." loc="14 ">Sx. Masc. 14 </ref>Enc.; <foreign xml:lang="gez">እራሐ፡ መዝራዕት፡</foreign> Lud. e Mss. Colb. <sense n="a" xml:id="Da"> <cit type="translation" xml:lang="la">
                                <quote> manus </quote>
                            </cit>: <foreign xml:lang="gez">ኵሉ፡ ውስተ፡ እራሕነ፡</foreign> (dicit Deus) <ref cRef="Clem. " loc="49">Clem.  f. 49</ref>; <ref cRef="Clem." loc="51">Clem. f. 51</ref>; <foreign xml:lang="gez">በእራሑ፡ ፈጠረ፡ ኵሎ፡</foreign> <ref cRef="Clem. " loc="218.">Clem.  f. 218.</ref> </sense> <sense n="b" xml:id="Db"> <cit type="translation" xml:lang="la">
                                <quote> planta </quote>
                            </cit>pedis: <foreign xml:lang="gez">ኢገብረ፡</foreign> (Pachomius) <foreign xml:lang="gez">አሣእነ፡ ለእራሐ፡ እግሩ፡</foreign> <ref cRef="Sx. Genb." loc="14 ">Sx. Genb. 14 </ref>Enc. – <lbl expand="Vocabularium Aethiopicum, -a -a">Voc. Ae.</lbl>: <foreign xml:lang="gez">እራሕ፡ ዘ፡ መበላ፡ እጅ፡</foreign> (v. <foreign xml:lang="gez">መብያእጅ</foreign>).</sense> </sense>
               
