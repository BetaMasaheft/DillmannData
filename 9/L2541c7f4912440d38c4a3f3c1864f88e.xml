            <entry xml:id="L2541c7f4912440d38c4a3f3c1864f88e" n="8442">
               <form>
                  <foreign xml:lang="gez">መስተገብር</foreign>
               </form>
               <sense xml:lang="la" source="#dillmann" xml:id="Dmain">
                  <gramGrp>
                     <pos expand="participium">part.</pos>
                  </gramGrp> (IV, 2), passim <foreign xml:lang="gez">መስተጋብር፡</foreign> (IV, <sense n="3" xml:id="D3">
                     <gramGrp>
                        <gen value="femininus">fem.</gen>
                     </gramGrp>
                     <foreign xml:lang="gez">መስተገብርት፡</foreign>,</sense>
                  <sense n="1" xml:id="D1">
                     <cit type="translation" xml:lang="la">
                        <quote>laboriosus</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>operosus</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>industrius</quote>
                     </cit>: <foreign xml:lang="gez">ንህብ፡ መስተገብርት፡ ይእቲ፡</foreign>
                     <ref cRef="Prov." loc="6,8">Prov. 6,8</ref>; <ref cRef="Did." loc="13">Did.
                        13</ref>; <foreign xml:lang="gez">መስተገብር፡ ውእቱ፡ ዘይትጌበር፡ ዘልፈ፡</foreign> F.M.
                     23,1.</sense>
                  <sense n="2" xml:id="D2">
                     <cit type="translation" xml:lang="la">
                        <quote>operarius</quote>
                     </cit>
                     <sense n="a" xml:id="D2a">
                        <cit type="translation" xml:lang="la">
                           <quote>fabricator</quote>
                        </cit>, <cit type="translation" xml:lang="la">
                           <quote>opifex</quote>
                        </cit>: ejus parentes erant <foreign xml:lang="gez">መስተገብራነ፡ ማህው፡</foreign>
                        <ref cRef="Sx. Hed." loc="21">Sx. Hed. 21</ref>; <foreign xml:lang="grc">ἐργάται </foreign> in aedibus aedificandis <ref cRef="Chrys. Ta." loc="12">Chrys. Ta. 12</ref>; <foreign xml:lang="gez">መስተጋብራነ፡ ቤተ፡
                           ክርስቲያን፡</foreign> <ref cRef="Gad. Lal.">Gad. Lal.</ref> </sense>
                     <sense n="b" xml:id="D2b">
                        <cit type="translation" xml:lang="la">
                           <quote>agri cultor</quote>
                        </cit>, <cit type="translation" xml:lang="la">
                           <quote>agricola</quote>
                        </cit>, <ref cRef="Gen." loc="4,2">Gen. 4,2</ref>; <foreign xml:lang="gez">መስተገብራነ፡ ምድር፡</foreign>
                        <ref cRef="Did." loc="13">Did. 13</ref>; <ref cRef="Sx. Hed." loc="26">Sx.
                           Hed. 26</ref>; <foreign xml:lang="grc">γηπόνοι </foreign>
                        <ref cRef="Chrys. ho." loc="12.">Chrys. ho. 12.</ref>
                     </sense>
                     <sense n="c" xml:id="D2c">
                        <cit type="translation" xml:lang="la">
                           <quote>mercator</quote>
                        </cit>, <cit type="translation" xml:lang="la">
                           <quote>negotiator</quote>
                        </cit>: <foreign xml:lang="gez">መስተገብራን፡ </foreign>
                        <foreign xml:lang="grc">ἔμποροι</foreign>
                        <ref cRef="Hez." loc="27,18">Hez. 27,18</ref>; <ref cRef="Jes." loc="45,14">Jes. 45,14</ref>
                        <lbl expand="versio altera">vers. alt.</lbl>; <ref cRef="3 Reg." loc="10,28">3 Reg. 10,28</ref>
                        <lbl expand="varia lectio">var.</lbl>; <ref cRef="Jes." loc="23,2">Jes.
                           23,2</ref>
                        <lbl expand="varia lectio">var.</lbl>; <foreign xml:lang="gez">መስተጋብራን፡ </foreign>
                        <ref cRef="Jes." loc="23,8">Jes. 23,8</ref>
                        <lbl expand="varia lectio">var.</lbl>
                     </sense>
                  </sense>
               </sense>
               
