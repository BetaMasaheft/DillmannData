                  <foreign xml:lang="grc">γαλήνη μεγάλη </foreign>
                  <ref cRef="Matth." loc="8,26">Matth. 8,26</ref>; <ref cRef="Marc." loc="4,39">Marc. 4,39</ref>; <ref cRef="Luc." loc="8,24">Luc. 8,24</ref> (ubi <lbl expand="romanae editionis">ed. rom. </lbl>
