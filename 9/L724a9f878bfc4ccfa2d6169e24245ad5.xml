            <entry xml:id="L724a9f878bfc4ccfa2d6169e24245ad5" n="2546">
               <form>
                  <foreign xml:lang="gez">አስምዐ</foreign>
               </form>
               <sense xml:lang="la" source="#dillmann" xml:id="Dmain"> <subc>II,1</subc> <sense n="1" xml:id="D1"> <sense n="a" xml:id="D1a"> <cit type="translation" xml:lang="la">
                                    <quote> facere ut audiat </quote>
                                </cit> aliquis: <foreign xml:lang="gez">ለጽሙማን፡ ያሰምዖሙ፡</foreign> <ref cRef="Marc." loc="7,37">Marc. 7,37</ref> (i.e auditum restituere); <foreign xml:lang="gez">ኣስምዕ፡ አሕዛበ፡</foreign> (v. <foreign xml:lang="gez">ለአሕዛብ፡</foreign>) <ref cRef="Jes." loc="45,1">Jes. 45,1</ref>; <ref cRef="Rom." loc="1,5">Rom. 1,5</ref>; <lbl expand="cum">c.</lbl> <lbl expand="duplex">dupl.</lbl> <gramGrp> <case value="accusativus">Acc.</case> </gramGrp>: <foreign xml:lang="gez">አስምዐኒ፡ ቃለከ፡</foreign> <ref cRef="Cant. " loc="2,14">Cant.  2,14</ref>; <ref cRef="Ps." loc="50,9">Ps. 50,9</ref>; <ref cRef="Sir." loc="45,5">Sir. 45,5</ref>; <foreign xml:lang="gez">ኢታስምዖ፡ ቃለ፡ ዘይብል፡ ዘይብል፡ ረሐቁ፡ እምኔነ፡ ኵልክሙ፡</foreign> <ref cRef="Genz." loc="80.">Genz. f. 80.</ref> </sense> <sense n="b" xml:id="D1b"> <cit type="translation" xml:lang="la">
                                    <quote> facere ut audiatur </quote>
                                </cit> aliquid: <foreign xml:lang="gez">ያሰምዕ፡ እግዚአብሔር፡ ቃለ፡ ስብሐቲሁ፡</foreign> <foreign xml:lang="grc">ἀκουστὴν ποιεῖ</foreign> <ref cRef="Jes. " loc="30,30">Jes.  30,30</ref>; <foreign xml:lang="gez">ያሰምዖ፡ ድምፆ፡ በዐቢይ፡ ቃል፡</foreign> <ref cRef="Sir. " loc="50,16">Sir.  50,16</ref>.</sense> <sense n="c" xml:id="D1c"> <cit type="translation" xml:lang="la">
                                    <quote> nuntiare </quote>
                                </cit>, <cit type="translation" xml:lang="la">
                                    <quote> annuntiare </quote>
                                </cit>: <foreign xml:lang="gez">አስማዕኩከ፡</foreign> <ref cRef="Jes." loc="48,5">Jes. 48,5</ref>; <ref cRef="Jes." loc="48,6">Jes. 48,6</ref>; <foreign xml:lang="gez">አስምዑ፡ በኢየሩሳሌም፡</foreign> <ref cRef="Jer." loc="4,5">Jer. 4,5</ref>; <lbl expand="cum">c.</lbl> <foreign xml:lang="gez">ለ፡</foreign> pers. <ref cRef="Ex." loc="21,29">Ex. 21,29</ref>; <ref cRef="Ex. " loc="21,36">Ex.  21,36</ref>.</sense> <sense n="d" xml:id="D1d"> <cit type="translation" xml:lang="la">
                                    <quote> acclamare </quote>
                                </cit>, <cit type="translation" xml:lang="la">
                                    <quote> inclamare </quote>
                                </cit> alicui: <foreign xml:lang="gez">አስምዕዎ፡ ቃለ፡ ሕሡመ፡ ወወገርዎ፡ በአእባን፡</foreign> <ref cRef="Jsp." loc="360">Jsp. p. 360</ref>; <ref cRef="Hen." loc="89,53">Hen. 89,53</ref>.</sense> <sense n="e" xml:id="D1e"> <cit type="translation" xml:lang="la">
                                    <quote> vocem edere </quote>
                                </cit> i.e. <cit type="translation" xml:lang="la">
                                    <quote> alta voce canere </quote>
                                </cit>: <foreign xml:lang="gez">ወያሰምዑ፡ መዘምራን፡</foreign> <ref cRef="2 Esr." loc="22,42">2 Esr. 22,42</ref> <lbl expand="versio nova">vers. nov.</lbl> </sense> </sense> <sense n="2" xml:id="D2"> <cit type="translation" xml:lang="la">
                                <quote> testari </quote>
                            </cit> <sense n="a" xml:id="D2a"> <cit type="translation" xml:lang="la">
                                    <quote> testem adhibere </quote>
                                </cit> vel <cit type="translation" xml:lang="la">
                                    <quote> invocare </quote>
                                </cit>, <cit type="translation" xml:lang="la">
                                    <quote> contestari </quote>
                                </cit> aliquem: <foreign xml:lang="gez">አስምዕ፡ ሰማዕተ፡</foreign> <ref cRef="Jer." loc="39,44">Jer. 39,44</ref>; <foreign xml:lang="gez">አስምዕ፡ ሰብአ፡ ምእመናነ፡</foreign> <ref cRef="Jes." loc="8,2">Jes. 8,2</ref>; <foreign xml:lang="gez">ናሰምዕ፡ ለክሙ፡ ሰማየ፡ ወምድረ፡</foreign> <ref cRef="Judith" loc="7,28">Judith 7,28</ref>; <ref cRef="Deut." loc="8,19">Deut. 8,19</ref>; <ref cRef="Sir." loc="46,19">Sir. 46,19</ref>; <ref cRef="Hen." loc="100,11">Hen. 100,11</ref>; 2 <ref cRef="Tim." loc="4,1">Tim. 4,1</ref> <lbl expand="romanae editionis">rom.</lbl> </sense> <sense n="b" xml:id="D2b"> <cit type="translation" xml:lang="la">
                                    <quote> obtestari </quote>
                                </cit>, <cit type="translation" xml:lang="la">
                                    <quote> teste invocato affirmare </quote>
                                </cit>: <foreign xml:lang="gez">በሰማዕት፡ አስምዐ፡ ለነ፡</foreign> <ref cRef="Gen." loc="43,3">Gen. 43,3</ref>; <ref cRef="3 Reg." loc="2,42">3 Reg. 2,42</ref>; <ref cRef="Ps." loc="49,8">Ps. 49,8</ref>; <ref cRef="Hebr." loc="2,6">Hebr. 2,6</ref> <lbl expand="romanae editionis">rom.</lbl>; <ref cRef="Kuf." loc="4">Kuf. 4</ref>; <lbl expand="cum">c.</lbl> <gramGrp> <case value="accusativus">Acc.</case> </gramGrp> <lbl expand="persona">pers.</lbl>: <foreign xml:lang="gez">አስምዖ፡ ሎቱ፡</foreign> <ref cRef="Zach. " loc="3,6">Zach.  3,6</ref>; vel <cit type="translation" xml:lang="la">
                                    <quote> teste invocato annuntiare </quote>
                                </cit> alicui aliquid: <foreign xml:lang="gez">አስምዕ፡ ላቲ፡ ለኢየሩሳሌም፡ ኀጢአታ፡</foreign> <ref cRef="Hez." loc="16,2">Hez. 16,2</ref>; vel <cit type="translation" xml:lang="la">
                                    <quote> teste invocato hortari et implorare </quote>
                                </cit>: <foreign xml:lang="gez">አስምዕ፡ ላዕሌሆሙ፡ ከመ፡ ኢየሀቡ፡</foreign> <ref cRef="Kuf." loc="30">Kuf. 30</ref>; <ref cRef="Deut." loc="32,46">Deut. 32,46</ref>; <ref cRef="Ex." loc="19,23">Ex. 19,23</ref>; <ref cRef="Hen." loc="81,6">Hen. 81,6</ref>; <ref cRef="Hen. " loc="91,3">Hen.  91,3</ref>.</sense> <sense n="d" xml:id="D2d"> <cit type="translation" xml:lang="la">
                                    <quote> testimonium perhibere </quote>
                                </cit>, <cit type="translation" xml:lang="la">
                                    <quote> testem esse </quote>
                                </cit>: <foreign xml:lang="gez">ስምዐ፡ ያሰምዕ፡ ላቲ፡</foreign> <ref cRef="Ex." loc="21,10">Ex. 21,10</ref>; <foreign xml:lang="gez">እስመ፡ እግዚአብሔር፡ አስምዐ፡ ማእከሌከ፡ ወማእከለ፡ ብእሲትከ፡</foreign> <ref cRef="Mal." loc="2,14">Mal. 2,14</ref>.</sense> </sense> </sense>
               
            </entry>
