         <div>
            <entry xml:id="L91afcbb8c1994825839e3b0b49c78f58" n="8183">
               <form>
                  <foreign xml:lang="gez">ደገደግ</foreign>
               </form>
               <sense xml:lang="la" source="#dillmann" xml:id="Dmain">, <foreign xml:lang="gez">ደግደግ፡</foreign>,
                     <foreign xml:lang="gez">ድግድግ፡</foreign>, <foreign xml:lang="gez">ደግዳግ፡</foreign>
                  <gramGrp>
                     <pos expand="adjectivum">adj.</pos>
                  </gramGrp>
                  <sense n="1" xml:id="D1">
                     <cit type="translation" xml:lang="la">
                        <quote>macer</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>macilentus</quote>
                     </cit>, <ref cRef="Gen." loc="41,3">Gen. 41,3</ref>; <ref cRef="Gen." loc="41,4">Gen. 41,4</ref>; <ref cRef="Gen." loc="41,19">Gen. 41,19</ref>;
                        <ref cRef="Gen." loc="41,20">Gen. 41,20</ref>; <ref cRef="Gen. " loc="41,27">Gen. 41,27</ref>
                     <lbl expand="cum">c.</lbl> varr. (<lbl expand="videas">vid.</lbl>
                     <lbl expand="annotatio">ann.</lbl> ad <ref cRef="Gen." loc="41,3">Gen.
                        41,3</ref>); <foreign xml:lang="gez">ድግድጋን፡ </foreign>
                     <ref cRef="Dan." loc="1,10">Dan. 1,10</ref>
                     <lbl expand="varia lectio">var.</lbl> — <cit type="translation" xml:lang="la">
                        <quote>Voc</quote>
                     </cit>. <cit type="translation" xml:lang="la">
                        <quote>Ae.</quote>
                     </cit>: <foreign xml:lang="gez">ደግደግ፡ ብ፡ ከስ፡</foreign>
                  </sense>
                  <sense n="2" xml:id="D2"> Usurpatur de <cit type="translation" xml:lang="la">
                        <quote>die</quote>
                     </cit> vel <cit type="translation" xml:lang="la">
                        <quote>dici luce adhuc macra</quote>
                     </cit> vel <cit type="translation" xml:lang="la">
                        <quote>tenui</quote>
                     </cit> (cfr. <foreign xml:lang="he">עוֹד הַיּוֹם גָּדוֹל</foreign>
                     <ref cRef="Gen." loc="29,7">Gen. 29,7</ref>), i.e. <cit type="translation" xml:lang="la">
                        <quote>prima lux</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>diluculum</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>mane</quote>
                     </cit>: <foreign xml:lang="gez">እስከ፡ ደገደግ፡</foreign> (v. <foreign xml:lang="gez">ደግደግ፡</foreign> vel <foreign xml:lang="gez">ደግዳግ፡</foreign>)
                        <foreign xml:lang="grc">ἕως πρωΐ</foreign>
                     <ref cRef="Ex. " loc="29,34">Ex. 29,34</ref>.</sense>
               </sense>
               
