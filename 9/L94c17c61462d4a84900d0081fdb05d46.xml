            <entry xml:id="L94c17c61462d4a84900d0081fdb05d46" n="6990">
               <form>
                  <foreign xml:lang="gez">አዕረቀ</foreign>
               </form>
               <sense xml:lang="la" source="#dillmann" xml:id="Dmain"> <subc>II,1</subc> <sense n="1" xml:id="D1"> <cit type="translation" xml:lang="la">
                                <quote> nudare </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> denudare </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> exuere </quote>
                            </cit> <cit type="translation" xml:lang="la">
                                <quote> veste </quote>
                            </cit>, <lbl expand="cum">c.</lbl> <gramGrp> <case value="accusativus">Acc.</case> </gramGrp> <lbl expand="persona">pers.</lbl>: <ref cRef="Apoc." loc="17,16">Apoc. 17,16</ref>; <foreign xml:lang="gez">ያዕርቅዎሙ፡</foreign> (sc. baptizandos) F.N. 3; <foreign xml:lang="gez">መልክእከ፡</foreign> (o Christe) <foreign xml:lang="gez">አይሁድ፡ ዘአዕረቅዎ፡</foreign> Cod. Mus. Brit. LIV, n. 7; <lbl expand="cum">c.</lbl> <foreign xml:lang="gez">እምነ፡</foreign> vestis: <foreign xml:lang="gez">ኢታዕርቆ፡ እግዚኦ፡ ለእኁነ፡ እገሌ፡ እምልብሰ፡ ጥምቀት፡</foreign> <ref cRef="Genz." loc="77">Genz. f. 77</ref>; <lbl expand="cum">c.</lbl> <lbl expand="duplex">dupl.</lbl> <gramGrp> <case value="accusativus">Acc.</case> </gramGrp>: <foreign xml:lang="gez">አዘዘ፡ ከመ፡ ያዕርቅዎ፡ አልባሲሁ፡</foreign> <ref cRef="Sx. Jac." loc="9">Sx. Jac. 9</ref>; <ref cRef="Sx. Hed." loc="11 Enc.">Sx. Hed. 11 Enc.</ref>; <lbl expand="translate">transl.</lbl> <sense n="a" xml:id="D1a"> <cit type="translation" xml:lang="la">
                                    <quote> privare </quote>
                                </cit>: <foreign xml:lang="gez">አዕርቆተ፡ ሰብእ፡</foreign> <ref cRef="Hen." loc="102,9">Hen. 102,9</ref>; <foreign xml:lang="gez">አዕሪቆሙ፡ እምነፍሰ፡ ሰብእ፡ ወልቡና፡ ለሥጋ፡</foreign> <foreign xml:lang="grc">ψυχῆς λογικῆς τὴν σάρκα γυμνώσαντες</foreign> Cyr. ad Theod. f. 13.</sense> <sense n="b" xml:id="D1b"> <cit type="translation" xml:lang="la">
                                    <quote> orbare </quote>
                                </cit>, <cit type="translation" xml:lang="la">
                                    <quote> nudare </quote>
                                </cit> terram incolis: <ref cRef="Hen." loc="84,5">Hen. 84,5</ref>.</sense> </sense> <sense n="2" xml:id="D2"> <cit type="translation" xml:lang="la">
                                <quote> detegere </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> retegere </quote>
                            </cit> vestem: <foreign xml:lang="gez">አዕረቁ፡ ክዳነ፡</foreign> (sc. altaris) <ref cRef="Sx. Teq." loc="2.">Sx. Teq. 2.</ref> </sense> <sense n="3" xml:id="D3"> <cit type="translation" xml:lang="la">
                                <quote> vacuefacere </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> evacuare </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> exinanire </quote>
                            </cit>: <foreign xml:lang="gez">እቤሎሙ፡ ያዕርቁ፡ ኵሎ፡ መዛግብተ፡</foreign> <ref cRef="2 Esr." loc="23,9">2 Esr. 23,9</ref>; <foreign xml:lang="gez">ከመ፡ ያዕርቅ፡ ነፍሰ፡ ጽምእተ፡</foreign> <ref cRef="Jes." loc="32,6">Jes. 32,6</ref>.</sense> </sense>
               
