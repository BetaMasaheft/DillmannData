      <body>
         <div>
            <entry xml:id="L4334a70e62064048922739eeed0c85f8" n="5287">
               <form>
                  <foreign xml:lang="gez">አንጻር</foreign>
               </form>
               <sense xml:lang="la" source="#dillmann" xml:id="Dmain">
                  <gramGrp>
                     <pos expand="Substantivum">subst.</pos>
                  </gramGrp> (ut videtur <lbl expand="pluralis">Pl.</lbl> a <lbl expand="singularis">sing.</lbl> deperdito), <lbl expand="pluralis">Pl.</lbl>
                  <foreign xml:lang="gez">አንጻራት፡</foreign> (<ref cRef="Kuf." loc="56">Kuf. p.
                     56</ref>), proprie: <cit type="translation" xml:lang="la">
                     <quote>conspectus</quote>
                  </cit>; usurpatur pro <sense n="1" xml:id="D1">
                     <gramGrp>
                        <pos expand="adverbium, adverbialiter">adv.</pos>
                     </gramGrp>: <foreign xml:lang="gez">አንጻረ፡</foreign>
                     <cit type="translation" xml:lang="la">
                        <quote>ex adverso</quote>
                     </cit>, <foreign xml:lang="gez">ነበረት፡ አንጻረ፡</foreign>
                     <ref cRef="Kuf." loc="66,">Kuf. p. 66,</ref>
                     <lbl expand="annotatio">ann.</lbl> 15.</sense>
                  <sense n="2" xml:id="D2">
                     <gramGrp>
                        <pos expand="praepositio">praep.</pos>
                     </gramGrp>
                     <sense n="a" xml:id="D2a">
                        <cit type="translation" xml:lang="la">
                           <quote>ex adverso</quote>
                        </cit>, <cit type="translation" xml:lang="la">
                           <quote>in regione</quote>
                        </cit>, <foreign xml:lang="grc">ἀπέναντι </foreign>
                        <ref cRef="Gen." loc="25,9">Gen. 25,9</ref>; <ref cRef="Judith" loc="7,18">Judith 7,18</ref>; <foreign xml:lang="gez">አንጻርየ፡</foreign>
                        <ref cRef="Hez. " loc="40,2">Hez. 40,2</ref>; <foreign xml:lang="grc">κατέναντι</foreign>
                        <ref cRef="Marc." loc="13,3">Marc. 13,3</ref>; <ref cRef="2 Par." loc="6,12">2 Par. 6,12</ref>; <ref cRef="Ex. " loc="32,5">Ex. 32,5</ref>; <foreign xml:lang="grc">ἐξεναντίας</foreign>
                        <ref cRef="Ex." loc="14,2">Ex. 14,2</ref>; <foreign xml:lang="gez">አንበሮሙ፡
                           አንጻራቲሆሙ፡ በበገጾሙ፡</foreign>
                        <ref cRef="Kuf." loc="56">Kuf. p. 56</ref>; <foreign xml:lang="gez">ኀደረ፡
                           አንጻረ፡ ኬብሮን፡</foreign>
                        <ref cRef="Kuf." loc="19">Kuf. 19</ref>; <foreign xml:lang="grc">κατὰ
                           πρόσωπον</foreign>
                        <ref cRef="Gen." loc="33,18">Gen. 33,18</ref>.</sense>
                     <sense n="b" xml:id="D2b">
                        <cit type="translation" xml:lang="la">
                           <quote>versus</quote>
                        </cit>: <foreign xml:lang="gez">አንጻረ፡ ኢየሩሳሌም፡</foreign>
                        <ref cRef="Dan." loc="6,10">Dan. 6,10</ref>; <ref cRef="Gen. " loc="12">Gen.
                           12</ref>; <ref cRef="Gen." loc="8.">Gen. 8.</ref>
                     </sense>
                     <sense n="c" xml:id="D2c">
                        <cit type="translation" xml:lang="la">
                           <quote>adversum</quote>
                        </cit>, <cit type="translation" xml:lang="la">
                           <quote>obviam</quote>
                        </cit>: <foreign xml:lang="gez">አብርሃም፡ ይረውጽ፡ አንጻሪሆሙ፡ </foreign>
                        <ref cRef="Kuf." loc="11.">Kuf. 11.</ref>
                     </sense>
                     <sense n="3" xml:id="D23"> aliis praepositionibus subjungitur: <foreign xml:lang="gez">መንገለ፡
                           አንጻረ፡ ቤቱ፡</foreign>
                        <foreign xml:lang="grc">κατέναντι οἰκίας αὐτοῦ </foreign>
                        <ref cRef="2 Esr." loc="13,10">2 Esr. 13,10</ref>; <foreign xml:lang="gez">ውስተ፡ አንጻረ፡ ሀገር፡</foreign>
                        <ref cRef="Gen. " loc="34,20">Gen. 34,20</ref>; <foreign xml:lang="gez">በአንጻሪሆሙ፡</foreign>
                        <cit type="translation" xml:lang="la">
                           <quote>in conspectu eorum</quote>
                        </cit>, <foreign xml:lang="grc">ἐναντίον αὐτῶν</foreign>
                        <ref cRef="2 Par." loc="7,6">2 Par. 7,6</ref>; <ref cRef="Ps." loc="22,6">Ps. 22,6</ref> (<foreign xml:lang="grc">ἐξεναντίας</foreign>); <foreign xml:lang="gez">እምአንጻርየ፡</foreign>
                        <cit type="translation" xml:lang="la">
                           <quote>e conspectu meo</quote>
                        </cit>
                        <ref cRef="Cant." loc="6,5">Cant. 6,5</ref>; <foreign xml:lang="gez">እምአንጻረ፡
                           ይእቲ፡ ዕፅ፡</foreign> (<cit type="translation" xml:lang="la">
                           <quote>e regione arboris</quote>
                        </cit>) <ref cRef="4 Esr. " loc="14,1">4 Esr. 14,1</ref>
                        <lbl expand="varia lectio">var.</lbl>; <foreign xml:lang="gez">ኀበ፡ አንጻረ፡
                           ፀሩ፡</foreign>
                        <cit type="translation" xml:lang="la">
                           <quote>adversus hostes suos</quote>
                        </cit>
                        <ref cRef="Lit." loc="174,2">Lit. 174,2</ref>.</sense>
                  </sense>
               </sense>
               
            </entry>
