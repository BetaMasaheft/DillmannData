               <sense xml:id="Dmain" xml:lang="la" source="#dillmann"> <subc>I,1</subc> [<foreign xml:lang="he">שׂוּם</foreign>, <foreign xml:lang="he">שִׂים</foreign> <foreign xml:lang="syr">ܣܳܡ</foreign> <foreign xml:lang="ar">سَامَ</foreign>] <gramGrp> <pos expand="Subjunctivus">Subj.</pos> </gramGrp> <foreign xml:lang="gez">ይሢም፡</foreign> <cit type="translation" xml:lang="la"><quote> ponere </quote></cit>, <cit type="translation" xml:lang="la"><quote> collocare </quote></cit>, <lbl expand="cum">c.</lbl> <gramGrp> <case value="accusativus">Acc.</case> </gramGrp> <ref cRef="Jud." loc="20,36">Jud. 20,36</ref>; <ref cRef="Deut." loc="27,15">Deut. 27,15</ref>; <foreign xml:lang="gez">ሤሞ፡ ህየ፡</foreign> <ref cRef="Gen." loc="2,8">Gen. 2,8</ref>; <lbl expand="cum">c.</lbl> <gramGrp> <pos expand="praepositio">praep.</pos> </gramGrp> loci: <ref cRef="Gen." loc="1,17">Gen. 1,17</ref>; <ref cRef="Ex." loc="2,3">Ex. 2,3</ref>; <ref cRef="Deut." loc="28,13">Deut. 28,13</ref>; <ref cRef="1 Reg." loc="8,11">1 Reg. 8,11</ref>; <ref cRef="2 Reg. " loc="8,6">2 Reg.  8,6</ref>; <ref cRef="2 Reg." loc="8,14">2 Reg. 8,14</ref>; <ref cRef="Ps." loc="17,47">Ps. 17,47</ref>; <ref cRef="Sir." loc="22,27">Sir. 22,27</ref> (<lbl expand="videas">vid.</lbl> <foreign xml:lang="gez">አቀመ፡</foreign>, <foreign xml:lang="gez">አንበረ፡</foreign>, <foreign xml:lang="gez">ረሰየ፡</foreign>, <foreign xml:lang="gez">ወደየ፡</foreign>). <sense n="a" xml:id="Da"> <cit type="translation" xml:lang="la"><quote> proponere </quote></cit> <lbl expand="cum">c.</lbl> <foreign xml:lang="gez">ቅድመ፡</foreign> <ref cRef="4 Esr." loc="2,4">4 Esr. 2,4</ref>; <foreign xml:lang="gez">እሠይም፡ ቅድመ፡ ገጽክሙ፡ ፍኖተ፡ ሕይወት፡</foreign> <ref cRef="Jer." loc="21,8">Jer. 21,8</ref>. 
</sense> <sense n="b" xml:id="Db"> <cit type="translation" xml:lang="la"><quote> imponere </quote></cit>, <cit type="translation" xml:lang="la"><quote> superimponere </quote></cit> <lbl expand="cum">c.</lbl> <foreign xml:lang="gez">ላዕለ፡</foreign>, ut: <foreign xml:lang="gez">ሤመ፡ ላዕሌሆሙ፡ መንፈሰ፡ ቅዱሰ፡</foreign> <ref cRef="Jes." loc="63,11">Jes. 63,11</ref>; <foreign xml:lang="gez">ከመ፡ እሢም፡ ስምየ፡ ላዕሌሁ፡</foreign> <ref cRef="Kuf." loc="1">Kuf. 1</ref>; vel <lbl expand="cum">c.</lbl> <foreign xml:lang="gez">ዲበ፡</foreign>, ut: <foreign xml:lang="gez">እሠይም፡ መንፈስየ፡ ዲበ፡ ዘርእከ፡</foreign> <ref cRef="Jes." loc="44,3">Jes. 44,3</ref>. 
</sense> <sense n="c" xml:id="Dc"> <cit type="translation" xml:lang="la"><quote> applicare </quote></cit> vel <cit type="translation" xml:lang="la"><quote> inducere </quote></cit> vestem: <foreign xml:lang="gez">ሤመ፡ አልባሰ፡</foreign> Lud. ex <ref cRef="Sx. Genb." loc="19 ">Sx. Genb. 19 </ref>Enc. 
</sense> <sense n="d" xml:id="Dd"> potissimum: <cit type="translation" xml:lang="la"><quote> constituere </quote></cit>, <cit type="translation" xml:lang="la"><quote> praeponere </quote></cit>, <cit type="translation" xml:lang="la"><quote> praeficere </quote></cit>, <lbl expand="cum">c.</lbl> <gramGrp> <case value="accusativus">Acc.</case> </gramGrp> <ref cRef="Hebr." loc="7,28">Hebr. 7,28</ref>; <ref cRef="1 Tim." loc="5,22">1 Tim. 5,22</ref>; <foreign xml:lang="gez">አንትሙ፡ ጳጳሳት፡ ኢትሢሙ፡ ኵሎ፡ ዘረከብክሙ፡</foreign> <cb n="0262" xml:id="c0262"/> <ref cRef="Clem." loc="130">Clem. f. 130</ref>; <foreign xml:lang="gez">እሠይም፡ ኖላዌ፡ አሐደ፡</foreign> <ref cRef="Hez." loc="34,23">Hez. 34,23</ref>; <foreign xml:lang="gez">በኀበ፡ ሤሙከ፡</foreign> <ref cRef="Sir." loc="30,31">Sir. 30,31</ref>; <foreign xml:lang="gez">ሤመ፡ የዋሃነ፡ ህየንቴሆሙ፡</foreign> <ref cRef="Sir." loc="10,14">Sir. 10,14</ref>; <ref cRef="Kuf." loc="32">Kuf. 32</ref>; <lbl expand="cum">c.</lbl> <lbl expand="duplex">dupl.</lbl> <gramGrp> <case value="accusativus">Acc.</case> </gramGrp>: <ref cRef="1 Reg." loc="8,1">1 Reg. 8,1</ref>; <ref cRef="1 Reg." loc="25,30">1 Reg. 25,30</ref>; <ref cRef="1 Reg." loc="28,2">1 Reg. 28,2</ref>; <foreign xml:lang="gez">እመ፡ ሤሙከ፡ መልአከ፡</foreign> <ref cRef="Sir." loc="35,1">Sir. 35,1</ref>; <ref cRef="Jes." loc="22,23">Jes. 22,23</ref>; <foreign xml:lang="gez">እግዚአብሔር፡ ሤመከ፡ ካህነ፡</foreign> <ref cRef="Jer." loc="36,26">Jer. 36,26</ref>; <lbl expand="cum">c.</lbl> <foreign xml:lang="gez">ለ፡</foreign> vel <foreign xml:lang="gez">ላዕለ፡</foreign> vel <foreign xml:lang="gez">ዲበ፡</foreign> vel <foreign xml:lang="gez">ውስተ፡</foreign> ejus, cui praeponitur: <foreign xml:lang="gez">ሢሙ፡ ለክሙ፡ መልአከ፡</foreign> <ref cRef="Judith" loc="14,2">Judith 14,2</ref>; <ref cRef="Deut." loc="17,14">Deut. 17,14</ref>; <ref cRef="Deut." loc="17,15">Deut. 17,15</ref>; <ref cRef="Dan." loc="2,48">Dan. 2,48</ref>; <ref cRef="Dan." loc="3,12">Dan. 3,12</ref>; <ref cRef="Sir." loc="17,17">Sir. 17,17</ref>; <ref cRef="Jer." loc="1,10">Jer. 1,10</ref>; <ref cRef="Marc." loc="12,1">Marc. 12,1</ref>; <ref cRef="Tit." loc="1,5">Tit. 1,5</ref>; <foreign xml:lang="gez">ዘሤመ፡ ንጉሥ፡ ላዕለ፡ ምድር፡</foreign> <ref cRef="Jer." loc="48,2">Jer. 48,2</ref>; <ref cRef="Gen." loc="41,33">Gen. 41,33</ref>; <ref cRef="Sir." loc="22,27">Sir. 22,27</ref>; <ref cRef="Dan." loc="6,3">Dan. 6,3</ref>; <ref cRef="Esth." loc="8,2">Esth. 8,2</ref>; <ref cRef="Ps." loc="9,21">Ps. 9,21</ref>; <foreign xml:lang="gez">ሤሞ፡ ለዮሴፍ፡ ዲበ፡ ኵሉ፡ ንዋዩ፡</foreign> <ref cRef="Kuf." loc="39">Kuf. 39</ref>; <ref cRef="Kuf." loc="40">Kuf. 40</ref>; <ref cRef="Ps." loc="8,6">Ps. 8,6</ref>; dan. 1,11; <foreign xml:lang="gez">እሠይመከ፡ ውስተ፡ ኵሉ፡ ምድር፡</foreign> <ref cRef="Gen." loc="41,41">Gen. 41,41</ref>; <ref cRef="Matth." loc="24,45">Matth. 24,45</ref>; <ref cRef="Matth." loc="25,21">Matth. 25,21</ref>; <foreign xml:lang="gez">ይሢምዎ፡ ውስተ፡ መልእክተ፡ ቤተ፡ ክርስቲያን፡</foreign> <ref cRef="Clem." loc="131.">Clem. f. 131.</ref> 
</sense> <sense n="e" xml:id="De"> <cit type="translation" xml:lang="la"><quote> convertere in </quote></cit> <lbl expand="cum">c.</lbl> <foreign xml:lang="gez">ኀበ፡</foreign>, ut: <foreign xml:lang="gez">እሠይም፡ አዕይንተ፡ ኀቤከ፡</foreign> <ref cRef="Jer." loc="47,4">Jer. 47,4</ref>. 
</sense> <sense n="f" xml:id="Df"> <cit type="translation" xml:lang="la"><quote> statuere </quote></cit>: <foreign xml:lang="gez">አነ፡ ሤምኩ፡ ኪዳነ፡ ኀበ፡ አበዊክሙ፡</foreign> <ref cRef="Jer. " loc="41,13">Jer.  41,13</ref>; <ref cRef="Gen." loc="17,2">Gen. 17,2</ref>; <ref cRef="Gen." loc="17,4">Gen. 17,4</ref>; <ref cRef="2 Par." loc="16,3">2 Par. 16,3</ref>; <ref cRef="Ex." loc="34,10">Ex. 34,10</ref>. 
</sense> <sense n="g" xml:id="Dg"> ex Hebraismo: <cit type="translation" xml:lang="la"><quote> facere </quote></cit>, <cit type="translation" xml:lang="la"><quote> reddere </quote></cit>: <foreign xml:lang="gez">እሠይሞ፡ ውስተ፡ ሕዝብ፡</foreign> <ref cRef="Kuf." loc="15.">Kuf. 15.</ref> – <lbl expand="Vocabularium Aethiopicum, -a -a">Voc. Ae.</lbl>: <foreign xml:lang="gez">ሤመ፡ ዘ፡ ሾመ፡ አስቀመጠሂ፡ ይ፡</foreign> 
</sense> </sense>
               
            <sense xml:id="Tmain" xml:lang="en" source="#traces"> <cit type="transcription" xml:lang="gez"><quote>śema</quote></cit> <sense n="G" xml:id="TG"> <foreign xml:lang="gez">ሤመ፡</foreign> <cit type="transcription" xml:lang="gez"><quote>śema</quote></cit> <subc>I,1</subc> <foreign xml:lang="gez">ይሠይም፡</foreign>, <foreign xml:lang="gez">ይሢም፡</foreign> «<cit type="translation" xml:lang="fr"><quote>préposer</quote></cit>, <cit type="translation" xml:lang="fr"><quote>mettre en charge</quote></cit>, <cit type="translation" xml:lang="fr"><quote>donner un emploi</quote></cit>, <cit type="translation" xml:lang="fr"><quote>faire monter</quote></cit>» ― <foreign xml:lang="am">ሾመ፡</foreign> Ms. <ref type="BM" target="BNFabb217">BNFabb217</ref>, fol. 53v. <lb/>
 <bibl><ptr target="bm:Grebaut1952SuppLexicon"/><citedRange unit="page"> 135</citedRange></bibl> 
</sense> <sense n="L" xml:id="TL"> <cit type="transcription" xml:lang="gez"><quote>śym</quote></cit> I, <foreign xml:lang="gez">ሤመ</foreign> <cit type="transcription" xml:lang="gez"><quote>śema</quote></cit> (yǝśim) <cit type="translation" xml:lang="en"><quote>set</quote></cit>, <cit type="translation" xml:lang="en"><quote>set in order</quote></cit>, <cit type="translation" xml:lang="en"><quote>put</quote></cit>, <cit type="translation" xml:lang="en"><quote>place</quote></cit>, <cit type="translation" xml:lang="en"><quote>appoint</quote></cit> (to an office), <cit type="translation" xml:lang="en"><quote>ordain</quote></cit>, <cit type="translation" xml:lang="en"><quote>charge</quote></cit>, <cit type="translation" xml:lang="en"><quote>establish</quote></cit>, <cit type="translation" xml:lang="en"><quote>designate</quote></cit>, <cit type="translation" xml:lang="en"><quote>institute</quote></cit>, <cit type="translation" xml:lang="en"><quote>promote</quote></cit>, <cit type="translation" xml:lang="en"><quote>consecrate</quote></cit>, (<bibl><ptr target="bm:Yaqob1927Sawasew"/></bibl>, <bibl><ptr target="bm:daMag1953"/></bibl>) <cit type="translation" xml:lang="en"><quote>hide</quote></cit> (that is, ‘place something aside’), <cit type="translation" xml:lang="en"><quote>insert</quote></cit> <bibl><ptr target="bm:Leslau1987dic"/><citedRange unit="page"> 539b</citedRange></bibl> 
</sense> </sense></entry>
