               <sense xml:lang="la" source="#dillmann" xml:id="Dmain"> <gramGrp> <pos expand="not found">subst.</pos> </gramGrp> <lbl expand="pluralis">Pl.</lbl> <foreign xml:lang="gez">ሕሠማት፡</foreign> <sense n="1" xml:id="D1"> <cit type="translation" xml:lang="la">
                                <quote> foeditas </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> turpitudo </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> deformitas </quote>
                            </cit>: <foreign xml:lang="gez">ኢታስተአክዮ፡ ለሰብእ፡ እንበይነ፡ ሕሠሚሁ፡</foreign> <ref cRef="Sir." loc="11,2">Sir. 11,2</ref>; <foreign xml:lang="gez">ኢይክሉ፡ ይክድኑ፡ ሕሠሚሆሙ፡ በተሰርግዎ፡</foreign> <ref cRef="Chrys. Ta." loc="15">Chrys. Ta. 15</ref>; <foreign xml:lang="gez">ሕሠመ፡ ራእየ፡ ብእሲቱ፡</foreign> <ref cRef="F.N." loc="24,1">F.N. 24,1</ref>.</sense> <sense n="2" xml:id="D2"> <cit type="translation" xml:lang="la">
                                <quote> macula </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> laesio </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> malum </quote>
                            </cit>: <foreign xml:lang="gez">ኢተረክበ፡ እምላዕሌሁ፡ ሕሠም፡</foreign> (v. <foreign xml:lang="gez">ሕሡም፡</foreign>) <foreign xml:lang="grc">διαφθορά</foreign> <ref cRef="Dan." loc="6,23">Dan. 6,23</ref>; <cit type="translation" xml:lang="la">
                                <quote> ornatus sanctitatis </quote>
                            </cit> <foreign xml:lang="gez">ዘኢይረክቦ፡ ሕሠም፡</foreign> <cit type="translation" xml:lang="la">
                                <quote> nulli corruptioni obnoxius </quote>
                            </cit> <ref cRef="Org." loc="">Org. </ref>; <foreign xml:lang="gez">ኵሉ፡ ዘይረክባ፡ እምሕሠማት፡</foreign> <cit type="translation" xml:lang="la">
                                <quote> quodvis malum, quod ei accidit </quote>
                            </cit> <ref cRef="F.N." loc=" 28">F.N.  28</ref>.</sense> <sense n="3" xml:id="D3"> ethice: <cit type="translation" xml:lang="la">
                                <quote> foeditas </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> malefactum </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> dedecus </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> probrum </quote>
                            </cit>: <foreign xml:lang="gez">ሰይእ፡ ሕሠም፡</foreign> <ref cRef="2 Reg." loc="18,13">2 Reg. 18,13</ref> <lbl expand="annotatio">ann.</lbl>; <foreign xml:lang="gez">አህ፡ እምዛቲ፡ ሕሠም፡</foreign> <ref cRef="Chrys. Ta." loc="15">Chrys. Ta. 15</ref>; <foreign xml:lang="gez">ኵሉ፡ ግብረ፡ ሕሠም፡</foreign> <ref cRef="Jsp." loc="299">Jsp. p. 299</ref>; <foreign xml:lang="gez">ገቢረ፡ ሕሠማት፡</foreign> <cit type="translation" xml:lang="la">
                                <quote> turpiter agere </quote>
                            </cit> <ref cRef="Fal." loc="">Fal. </ref>; <foreign xml:lang="gez">ዘይከውና፡ ሕሠመ፡</foreign> <cit type="translation" xml:lang="la">
                                <quote> quod mulieri dedecori est </quote>
                            </cit> <ref cRef="F.N." loc=" 24,3">F.N.  24,3</ref>; <foreign xml:lang="gez">በእንተ፡ ሕሠመ፡ ግዕዙ፡</foreign> <ref cRef="F.N." loc=" 24,3">F.N.  24,3</ref>.</sense> </sense>
               
