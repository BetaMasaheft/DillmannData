         <div>
            <entry xml:id="L926ee87b968f4eb9a95f8b9cfc3780c5" n="9633">
               <form>
                  <foreign xml:lang="gez">ጸፍጸፍ</foreign>
               </form>
               <sense xml:lang="la" source="#dillmann" xml:id="Dmain">
                  <gramGrp>
                     <pos expand="Substantivum">subst.</pos>
                  </gramGrp>
                  <sense n="1" xml:id="D1">
                     <cit type="translation" xml:lang="la">
                        <quote>pavimentum e lapidibus stratum</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>tesselatum</quote>
                     </cit>: <foreign xml:lang="gez">ቶልስሮጣን፡ ዘውእቱ፡ ጸፍጸፍ፡</foreign>
                     <foreign xml:lang="grc">λιθόστρωτον</foreign> 2 <ref cRef="Par." loc="7,3">Par.
                        7,3</ref>; <ref cRef="Hez." loc="40,18">Hez. 40,18</ref> et <ref cRef="Hez." loc="42,3">Hez. 42,3</ref>
                     <lbl expand="versio nova">vers. nov.</lbl>; <foreign xml:lang="gez">አዘዘ፡ ይኅፅቡ፡
                        ጸፍጸፎ፡</foreign> (<foreign xml:lang="gez">ለመቅደስ፡</foreign>) <foreign xml:lang="gez">በማይ፡</foreign>
                     <ref cRef="Jsp." loc="294">Jsp. p. 294</ref>; <ref cRef="Macc." loc="3 ">Macc.
                        f. 3 </ref>(<lbl expand="videas">vid. sub</lbl>
                     <foreign xml:lang="gez">ንጹፍ፡</foreign>); <ref cRef="Hen." loc="14,10">Hen.
                        14,10</ref>; <foreign xml:lang="gez">ወጥቀ፡ ሠናይ፡ ጸፍጸፉ፡</foreign>
                     <ref cRef="Esth." loc="1,6">Esth. 1,6</ref>; <foreign xml:lang="gez">ሰሌዳ፡ በረድ፡
                        ጸፍጸፈ፡ ዐውዱ፡ </foreign> Lit. Epiph.</sense>
                  <sense n="2" xml:id="D2">
                     <cit type="translation" xml:lang="la">
                        <quote>tabulae</quote>
                     </cit> vel <cit type="translation" xml:lang="la">
                        <quote>tesserae</quote>
                     </cit>, in coelo stratae: <ref cRef="Hen." loc="81,1">Hen. 81,1</ref>; <ref cRef="Hen." loc="81,2">Hen. 81,2</ref>; <ref cRef="Hen. " loc="93,2">Hen.
                        93,2</ref>; <ref cRef="Hen." loc="103,2">Hen. 103,2</ref>; <ref cRef="Hen." loc="106,19">Hen. 106,19</ref>.</sense>
               </sense>
               
