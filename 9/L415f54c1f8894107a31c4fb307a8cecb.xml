               <sense xml:id="Dmain" xml:lang="la" source="#dillmann"> <subc>I,1</subc> (etiam per <foreign xml:lang="gez">ሰ</foreign> et <foreign xml:lang="gez">አ</foreign>) [amh. <foreign xml:lang="gez">ሣለ፡</foreign>; <foreign xml:lang="he">יָצַר</foreign>, <foreign xml:lang="he">צוּר</foreign>, <foreign xml:lang="syr">ܨܳܪ</foreign>, <foreign xml:lang="ar">صَوَّرَ</foreign> (<foreign xml:lang="ar">سَوَّلَ</foreign>)] <gramGrp> <pos expand="Subjunctivus">Subj.</pos> </gramGrp> <foreign xml:lang="gez">ይሥዐል፡</foreign> <sense n="1" xml:id="D1"> <cit type="translation" xml:lang="la"><quote> formare </quote></cit>, <cit type="translation" xml:lang="la"><quote> figurare </quote></cit>, <cit type="translation" xml:lang="la"><quote> effigiare </quote></cit>: (Deus filius) <foreign xml:lang="gez">ዘይሥዕሎሙ፡ ለሕፃናት፡ ውስተ፡ ማኅፀን፡ ተወልደ፡</foreign> Deg.; <foreign xml:lang="gez">እግዚአብሔር፡ ፈጠረ፡ ሥጋክሙ፡ ወመንፈሰክሙ፡ ወሠዐለክሙ፡ በክሂሎቱ፡</foreign> <ref cRef="Jsp. " loc="290.">Jsp.  p. 290.</ref> 
</sense> <sense n="2" xml:id="D2"> <cit type="translation" xml:lang="la"><quote> delineare </quote></cit>, <cit type="translation" xml:lang="la"><quote> designare </quote></cit>, <cit type="translation" xml:lang="la"><quote> describere </quote></cit>, <cit type="translation" xml:lang="la"><quote> adumbrare </quote></cit>, (<cit type="translation" xml:lang="la"><quote> insculpere </quote></cit>): <foreign xml:lang="gez">ንሣእ፡ ግንፋለ፡ ወሠዐል፡ ውስቴቱ፡ ሀገረ፡ ኢየሩሳሌም፡</foreign> <foreign xml:lang="grc">διαγράψεις</foreign> <ref cRef="Hez." loc="4,1">Hez. 4,1</ref> (cfr. <ref cRef="Hez. " loc="43,11">Hez.  43,11</ref>); <foreign xml:lang="gez">ናሁ፡ በእደዊየ፡ ሠዐልኩ፡ አረፋቲከ፡</foreign> <foreign xml:lang="grc">ἐζωγράφηκα</foreign> <ref cRef="Jes." loc="49,15">Jes. 49,15</ref>; <ref cRef="3 Reg." loc="6,27">3 Reg. 6,27</ref>; <foreign xml:lang="gez">አዘዘ፡ ይግበሩ፡ ጻሕለ፡</foreign> <cb n="0259" xml:id="c0259"/> <foreign xml:lang="gez">ዘወርቅ፡ ወይሥዐሉ፡ ላዕሌሁ፡ ሥዕለ፡ ምድረ፡ ግብጽ፡ በምልኣ፡</foreign> <ref cRef="Jsp." loc="287.">Jsp. p. 287.</ref> 
</sense> <sense n="3" xml:id="D3"> <cit type="translation" xml:lang="la"><quote> pingere </quote></cit>, <cit type="translation" xml:lang="la"><quote> effingere </quote></cit>: <foreign xml:lang="gez">እፈቅድ፡ ከመ፡ ትሥዐሊ፡ ሊተ፡ ሥዕሎሙ፡</foreign> <ref cRef="Jsp." loc="322">Jsp. p. 322</ref>; <foreign xml:lang="gez">ለዛቲ፡ ሥዕል፡ ሠዐላ፡ ሉቃስ፡ ወንጌላዊ፡</foreign> <ref cRef="Sx. Masc." loc="10.">Sx. Masc. 10.</ref> – <lbl expand="Vocabularium Aethiopicum, -a -a">Voc. Ae.</lbl>: <foreign xml:lang="gez">ሠዐለ፡ ዐለመ፡ ብ፡ ጻፈ።</foreign> 
</sense> </sense>
               
            <sense xml:id="Tmain" xml:lang="en" source="#traces"><cit type="transcription" xml:lang="gez"><quote>śaʿala</quote></cit>, <foreign xml:lang="gez">ሠአለ፡</foreign> <cit type="transcription" xml:lang="gez"><quote>śaʾala</quote></cit> <sense n="G" xml:id="TG"> <foreign xml:lang="gez">ሠዐለ፡</foreign> <cit type="transcription" xml:lang="gez"><quote>śaʿala</quote></cit> <subc>I,1</subc> <foreign xml:lang="gez">ይሥዕል፡</foreign>, <foreign xml:lang="gez">ይሥዐል፡</foreign> «<cit type="translation" xml:lang="fr"><quote>peindre</quote></cit>» ― <foreign xml:lang="am">ሣለ፡</foreign> Ms. <ref type="BM" target="BNFabb217">BNFabb217</ref>, fol. 53v. <lb/>
 <bibl><ptr target="bm:Grebaut1952SuppLexicon"/><citedRange unit="page"> 134</citedRange></bibl> 
</sense> <sense n="L" xml:id="TL"> <foreign xml:lang="gez">ሠዐለ</foreign> <cit type="transcription" xml:lang="gez"><quote>śaʿala</quote></cit> (yǝśʿal), (<bibl><ptr target="bm:Yaqob1927Sawasew"/></bibl>) <foreign xml:lang="gez">ሠአለ</foreign> <cit type="transcription" xml:lang="gez"><quote>śaʾala</quote></cit> <cit type="translation" xml:lang="en"><quote>form</quote></cit>, <cit type="translation" xml:lang="en"><quote>shape</quote></cit>, <cit type="translation" xml:lang="en"><quote>fashion</quote></cit>, <cit type="translation" xml:lang="en"><quote>carve</quote></cit>, <cit type="translation" xml:lang="en"><quote>depict</quote></cit>, <cit type="translation" xml:lang="en"><quote>portray</quote></cit>, <cit type="translation" xml:lang="en"><quote>paint</quote></cit> <bibl><ptr target="bm:Leslau1987dic"/><citedRange unit="page"> 524b</citedRange></bibl> 
</sense> </sense></entry>
