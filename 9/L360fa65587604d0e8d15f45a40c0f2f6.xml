               </form>
               <sense xml:lang="la" source="#dillmann" xml:id="Dmain"> <subc>III,1</subc> (nonnisi semel <ref cRef="3 Reg." loc="22,38">3 Reg. 22,38</ref> in uno codice Perf. <foreign xml:lang="gez">ተኀፀበ፡</foreign> inveni) <sense n="1" xml:id="D1"> reflex. <cit type="translation" xml:lang="la">
                        <quote>se</quote>
                     </cit> vel <cit type="translation" xml:lang="la">
                        <quote>sibi</quote>
                     </cit> corpus, membra al. <cit type="translation" xml:lang="la">
                        <quote>lavare</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>abluere</quote>
                     </cit>
                     <ref cRef="Ruth" loc="3,3">Ruth 3,3</ref>; <ref cRef="Job " loc="11,15">Job
                        11,15</ref>; <ref cRef="Job" loc="9,30">Job 9,30</ref>; <ref cRef="Kuf." loc="33">Kuf. 33</ref>; <ref cRef="2 Reg." loc="11,4">2 Reg. 11,4</ref>;
                        <foreign xml:lang="gez">ዘይትኀፀብ፡ እምድኅረ፡ ገሰሰ፡ በድነ፡</foreign>
                     <foreign xml:lang="grc">βαπτιζόμενος ἀπὸ νεκροῦ</foreign>
                     <ref cRef="Sir. " loc="31,30">Sir. 31,30</ref>; <foreign xml:lang="gez">ተኀፀቡ፡
                        ወንጹሓነ፡ ኩኑ፡</foreign>
                     <ref cRef="Jes." loc="1,16">Jes. 1,16</ref>; <ref cRef="Tob." loc="2,5">Tob.
                        2,5</ref>; <foreign xml:lang="gez">ፈተወት፡ ትትኀፀብ፡</foreign>
                     <ref cRef="Dan. apocr." loc="1,15">Dan. apocr. 1,15</ref>; <ref cRef="2 Par." loc="4,6">2 Par. 4,6</ref>; <ref cRef="Ex. " loc="30,18">Ex. 30,18</ref>;
                        <foreign xml:lang="gez">ምንተ፡ ይበቍዖ፡ ተኀፅቦ፡</foreign>
                     <ref cRef="Sir." loc="31,30">Sir. 31,30</ref>; <foreign xml:lang="gez">በውስተ፡
                        ተከዚ፡</foreign>
                     <ref cRef="Ex." loc="2,5">Ex. 2,5</ref>; <ref cRef="Kuf." loc="47">Kuf.
                        47</ref>; <ref cRef="Tob." loc="6,3">Tob. 6,3</ref>; <ref cRef="Joh." loc="5,4">Joh. 5,4</ref>; <ref cRef="Joh. " loc="9,7">Joh. 9,7</ref>; <lbl expand="construitur cum">c.c.</lbl>
                     <gramGrp>
                        <case value="accusativus">Acc.</case>
                     </gramGrp> corporis vel membri: <ref cRef="Gen." loc="43,31">Gen. 43,31</ref>;
                        <ref cRef="Kuf." loc="22">Kuf. 22</ref>; <foreign xml:lang="gez">ተኀፅበት፡ ሥጋሃ፡
                        በማይ፡</foreign>
                     <ref cRef="Judith" loc="10,3">Judith 10,3</ref>; <ref cRef="Cant." loc="5,3">Cant. 5,3</ref>; <ref cRef="Ex." loc="30,20">Ex. 30,20</ref>; <ref cRef="Lev." loc="16,4">Lev. 16,4</ref>; <ref cRef="Lev. " loc="17,16">Lev.
                        17,16</ref>; <ref cRef="Lev." loc="22,6">Lev. 22,6</ref>; <ref cRef="Matth." loc="6,17">Matth. 6,17</ref>; <ref cRef="Matth." loc="27,24">Matth.
                        27,24</ref>; <ref cRef="Marc." loc="7,3">Marc. 7,3</ref>; <ref cRef="Joh." loc="13,10">Joh. 13,10</ref>.</sense>
                  <sense n="2" xml:id="D2">
                     <gramGrp>
                        <pos expand="passivum">pass.</pos>
                     </gramGrp>
                     <cit type="translation" xml:lang="la">
                        <quote>lavari</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>ablui</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>lotum esse</quote>
                     </cit>: <foreign xml:lang="gez">ተኀፂቦ፡ ውእቱ፡ ልብስ፡</foreign>
                     <ref cRef="Lev." loc="13,58">Lev. 13,58</ref>; <ref cRef="Joh." loc="13,10">Joh. 13,10</ref>; <foreign xml:lang="gez">ትትኀፀብ፡ ምድር፡ እምኵሉ፡ ሙስና፡</foreign>
