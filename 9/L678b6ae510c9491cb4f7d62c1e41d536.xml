            <entry xml:id="L678b6ae510c9491cb4f7d62c1e41d536" n="8205">
               <form>
                  <foreign xml:lang="gez">መድፍን</foreign>
               </form>
               <sense xml:lang="la" source="#dillmann" xml:id="Dmain">
                  <gramGrp>
                     <pos expand="Substantivum">subst.</pos>
                  </gramGrp> (<gramGrp>
                     <gen value="masculinum">m.</gen>
                  </gramGrp>), <lbl expand="pluralis">Pl.</lbl>
                  <foreign xml:lang="gez">መዳፍንት፡</foreign>
                  <sense n="1" xml:id="D1">
                     <cit type="translation" xml:lang="la">
                        <quote>thesaurus</quote>
                     </cit>: <sense n="a" xml:id="D1a"> sub terra vel alibi reconditus, <foreign xml:lang="grc">θησαυρός</foreign>
                        <ref cRef="Job" loc="3,21">Job 3,21</ref>; <foreign xml:lang="gez">ከመ፡ መድፍን፡
                           ክቡት፡</foreign>
                        <ref cRef="Sir." loc="20,30">Sir. 20,30</ref>; <ref cRef="Sir." loc="29,11">Sir. 29,11</ref>; <foreign xml:lang="gez">ረኪበ፡ መድፍን፡</foreign>
                        <ref cRef="Sir." loc="40,18">Sir. 40,18</ref>; <ref cRef="Prov." loc="2,4">Prov. 2,4</ref>; <ref cRef="Matth." loc="13,44">Matth. 13,44</ref>; <ref cRef="Clem." loc="222">Clem. f. 222</ref>; <lbl expand="pluralis">Pl.</lbl>
                        <ref cRef="Gen. " loc="43,23">Gen. 43,23</ref>.</sense>
                     <sense n="b" xml:id="D1b"> in quo quid reconditur, <cit type="translation" xml:lang="la">
                           <quote>promptuarium</quote>
                        </cit>
                        <ref cRef="Sir." loc="29,10">Sir. 29,10</ref>.</sense>
                     <sense n="2" xml:id="D12">
                        <cit type="translation" xml:lang="la">
                           <quote>monumentum sepulchrale</quote>
                        </cit>, <foreign xml:lang="grc">σκόπελον</foreign>
                        <ref cRef="4 Reg. " loc="23,17">4 Reg. 23,17</ref>.</sense>
                  </sense>
               </sense>
               
