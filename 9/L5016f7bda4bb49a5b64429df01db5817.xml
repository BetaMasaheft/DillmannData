            <entry xml:id="L5016f7bda4bb49a5b64429df01db5817" n="6061">
               <form>
                  <foreign xml:lang="gez">ተከሥተ</foreign>
               </form>
               <sense xml:lang="la" source="#dillmann" xml:id="Dmain"> <subc>III,1</subc> <sense n="1" xml:id="D1">
                     <gramGrp>
                        <pos expand="reflexivum">Refl.</pos>
                     </gramGrp>
                     <cit type="translation" xml:lang="la">
                        <quote>se retergere</quote>
                     </cit> vel <cit type="translation" xml:lang="la">
                        <quote>nudare</quote>
                     </cit>, Kuf. p. 15; <ref cRef="Kuf." loc="30">Kuf. p. 30</ref>; <ref cRef="2 Reg." loc="6,20">2 Reg. 6,20</ref>.</sense>
                  <sense n="2" xml:id="D2"> Pass. <sense n="a" xml:id="D2a">
                        <cit type="translation" xml:lang="la">
                           <quote>retegi</quote>
                        </cit>, <cit type="translation" xml:lang="la">
                           <quote>detegi</quote>
                        </cit>: <foreign xml:lang="gez">ግላ፡</foreign>
                        <ref cRef="2 Cor." loc="3,14">2 Cor. 3,14</ref>; <foreign xml:lang="gez">ተከሥተ፡ ሰኰናኪ፡</foreign>
                        <ref cRef="Jer." loc="13,22">Jer. 13,22</ref>; <foreign xml:lang="gez">አልቦ፡
                           ክዱን፡ ዘኢይትከሠት፡</foreign>
                        <ref cRef="Marc." loc="4,22">Marc. 4,22</ref>; <foreign xml:lang="gez">ይትከሠት፡ መሠረታቲሃ፡</foreign>
                        <ref cRef="Hez." loc="13,14">Hez. 13,14</ref>; <ref cRef="Ps." loc="17,17">Ps. 17,17</ref>.</sense>
                     <sense n="b" xml:id="D2b">
                        <cit type="translation" xml:lang="la">
                           <quote>aperiri</quote>
                        </cit>, <cit type="translation" xml:lang="la">
                           <quote>patefieri</quote>
                        </cit>: <foreign xml:lang="gez">መቃብራት፡ ተከሥቱ፡ </foreign>
                        <ref cRef="Clem." loc="218">Clem. f. 218</ref>; <ref cRef="Matth." loc="27,52">Matth. 27,52</ref>; <foreign xml:lang="gez">አፍ፡</foreign>
                        <ref cRef="Luc. " loc="1,64">Luc. 1,64</ref>; <foreign xml:lang="gez">እዘን፡</foreign>
                        <ref cRef="Marc." loc="7,35">Marc. 7,35</ref>; <foreign xml:lang="gez">አዕይንት፡</foreign>
                        <ref cRef="1 Reg." loc="14,27">1 Reg. 14,27</ref>; <ref cRef="1 Reg." loc="14,29">1 Reg. 14,29</ref>; <foreign xml:lang="gez">ተከሢቶ፡ </foreign>
                        <ref cRef="Asc. Jes." loc="9,37">Asc. Jes. 9,37</ref>; <ref cRef="Matth." loc="9,30">Matth. 9,30</ref>; <ref cRef="Matth." loc="20,33">Matth.
                           20,33</ref>; <ref cRef="Joh." loc="9,10">Joh. 9,10</ref>; <foreign xml:lang="gez">ልብ፡</foreign>
                        <ref cRef="Act." loc="2,37">Act. 2,37</ref>; <cb n="0833" xml:id="c0833"/>
                        <foreign xml:lang="gez">መጻሕፍት፡</foreign>
                        <ref cRef="4 Esr." loc="4,22">4 Esr. 4,22</ref>; <ref cRef="Hen." loc="47,3">Hen. 47,3</ref>.</sense>
                     <sense n="c" xml:id="D2c">
                        <lbl expand="translate">transl.</lbl>
                        <sense n="α" xml:id="D2cα">
                           <cit type="translation" xml:lang="la">
                              <quote>revelari</quote>
                           </cit> (de revelatione divina): <foreign xml:lang="gez">ኢተከሥተ፡ ሕግ፡ እስከ፡
                              አሜሃ፡</foreign>
                           <ref cRef="Kuf." loc="33">Kuf. 33</ref>; <foreign xml:lang="gez">ተከሥተ፡
                              ሎቱ፡ ከመ፡ ይግበር፡ </foreign>
                           <ref cRef="Kuf." loc="32">Kuf. 32</ref>; <ref cRef="1 Reg." loc="3,7">1
                              Reg. 3,7</ref>; <ref cRef="Dan." loc="2,19">Dan. 2,19</ref>; <ref cRef="Dan." loc="10,1">Dan. 10,1</ref>; <ref cRef="Jes." loc="31,1">Jes. 31,1</ref>; <ref cRef="Hen." loc="52,5">Hen. 52,5</ref>; 1 <ref cRef="Cor." loc="14,30">Cor. 14,30</ref>. <ref cRef="Eph." loc="3,5">Eph. 3,5</ref>.</sense>
                        <sense n="β" xml:id="D2cβ">
                           <cit type="translation" xml:lang="la">
                              <quote>manifestum fieri</quote>
                           </cit>, <cit type="translation" xml:lang="la">
                              <quote>patefieri</quote>
                           </cit>, <cit type="translation" xml:lang="la">
                              <quote>apparere</quote>
                           </cit>: <foreign xml:lang="gez">ተከሥተት፡ ምሕረትየ፡</foreign>
                           <ref cRef="Jes." loc="56,1">Jes. 56,1</ref>; <foreign xml:lang="gez">ኅቡኣት፡ ኢተከሥቱ፡ ለክሙ፡</foreign> Hen.16,3; <ref cRef="Hen." loc="38,3">Hen. 38,3</ref>; <foreign xml:lang="gez">ምድር፡ ተከሥተት፡</foreign>
                           <ref cRef="Hen." loc="89,8">Hen. 89,8</ref>; <ref cRef="2 Thess." loc="2,3">2 Thess. 2,3</ref>; <ref cRef="2 Thess. " loc="2,8">2 Thess.
                              2,8</ref>.</sense>
                     </sense>
                  </sense>
               </sense>
               
