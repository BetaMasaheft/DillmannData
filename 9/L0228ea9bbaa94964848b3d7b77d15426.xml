               <sense xml:id="Dmain" xml:lang="la" source="#dillmann"> <subc>IV,3</subc> <cit type="translation" xml:lang="la"><quote> res inter se similes facere </quote></cit> vel <cit type="translation" xml:lang="la"><quote> putare </quote></cit> i.e. <sense n="a" xml:id="Da"> <cit type="translation" xml:lang="la"><quote> ad similitudinem alius effingere </quote></cit>: <foreign xml:lang="gez">በአምሳለ፡ ዚአከ፡ አስተማሰልከ፡</foreign> <ref cRef="4 Esr." loc="8,50">4 Esr. 8,50</ref>; <ref cRef="Jes." loc="40,18">Jes. 40,18</ref>; <ref cRef="Sir." loc="38,27">Sir. 38,27</ref>; <foreign xml:lang="gez">አው፡ አስተማሰሎ፡ በእንስሳ፡</foreign> <ref cRef="Sap." loc="13,14">Sap. 13,14</ref>. 
</sense> <sense n="b" xml:id="Db"> <cit type="translation" xml:lang="la"><quote> assimilare </quote></cit>, <lbl expand="cum">c.</lbl> <foreign xml:lang="gez">ከመ፡</foreign> <ref cRef="4 Esr." loc="8,52">4 Esr. 8,52</ref>; <lbl expand="cum">c.</lbl> <foreign xml:lang="gez">በ፡</foreign>, ut: <foreign xml:lang="gez">እስራኤል፡ እለ፡ በበኵርከ፡ አስተማሰልከ፡</foreign> <ref cRef="Sir." loc="36,17">Sir. 36,17</ref>. 
</sense> <sense n="c" xml:id="Dc"> <cit type="translation" xml:lang="la"><quote> comparare </quote></cit>: <lbl expand="cum">c.</lbl> <foreign xml:lang="gez">በ፡</foreign>, ut: <foreign xml:lang="gez">በመኑ፡ ታስተማስሉኒ፡</foreign> <ref cRef="Jes." loc="40,25">Jes. 40,25</ref>; <ref cRef="Jes." loc="46,5">Jes. 46,5</ref>; <ref cRef="Thren." loc="2,13">Thren. 2,13</ref>; <ref cRef="Sap. " loc="7,9">Sap.  7,9</ref>; <ref cRef="Marc." loc="4,30">Marc. 4,30</ref>; <ref cRef="Luc." loc="13,18">Luc. 13,18</ref> Platt; <ref cRef="Luc." loc="13,20">Luc. 13,20</ref> Platt; <lbl expand="cum">c.</lbl> <foreign xml:lang="gez">ለ፡</foreign> ut <ref cRef="Cant." loc="1,9">Cant. 1,9</ref>; <lbl expand="cum">c.</lbl> <gramGrp> <case value="accusativus">Acc.</case> </gramGrp> <ref cRef="Matth." loc="11,16">Matth. 11,16</ref> <lbl expand="romanae editionis">rom.</lbl> 
</sense> <sense n="d" xml:id="Dd"> <cit type="translation" xml:lang="la"><quote> similem putare </quote></cit> rem alicui: <lbl expand="cum">c.</lbl> <foreign xml:lang="gez">በ፡</foreign> <ref cRef="Act. " loc="17,29">Act.  17,29</ref>. 
</sense> <sense n="e" xml:id="De"> <cit type="translation" xml:lang="la"><quote> parabolas </quote></cit>, <cit type="translation" xml:lang="la"><quote> aenigmata </quote></cit> al. <cit type="translation" xml:lang="la"><quote> proponere </quote></cit>: <foreign xml:lang="gez">ከመ፡ ታመክሮ፡ በአስተማስሎ፡</foreign> <foreign xml:lang="grc">ἐν αἰνίγμασι</foreign> <ref cRef="2 Par. " loc="9,1">2 Par.  9,1</ref>. 
</sense> </sense>
               
