      <body>
         <div>
            <entry xml:id="L1548f5cbda9d448bacfb80534e8c721a" n="4640">
               <form>
                  <foreign xml:lang="gez">ኂሩት</foreign>
               </form>
               <sense xml:lang="la" source="#dillmann" xml:id="Dmain">
                  <gramGrp>
                     <pos expand="Substantivum">subst.</pos>
                  </gramGrp>, <lbl expand="pluralis">Pl.</lbl>
                  <foreign xml:lang="gez">ኂሩታት፡</foreign>, <cit type="translation" xml:lang="la">
                     <quote>bonitas</quote>
                  </cit>, <cit type="translation" xml:lang="la">
                     <quote>praestantia</quote>
                  </cit>, <cit type="translation" xml:lang="la">
                     <quote>virtus</quote>
                  </cit>. Usurpatur de variis corporis et animi qualitatibus: <sense n="a" xml:id="Da">
                     <cit type="translation" xml:lang="la">
                        <quote>Praestantia formae</quote>
                     </cit> vel <cit type="translation" xml:lang="la">
                        <quote>suavitas morum</quote>
                     </cit> intelligenda videtur <ref cRef="Sir." loc="42,14">Sir. 42,14</ref>
                        (<foreign xml:lang="gez">ይኄይስ፡ እከዪሁ፡ ለብእሲ፡ እምነ፡ ኂሩታ፡ ለብእሲት፡</foreign>);
                        <foreign xml:lang="gez">ኂሩተ፡ ትዝምድ፡</foreign>
                     <foreign xml:lang="grc">εὐγένεια </foreign>
                     <cit type="translation" xml:lang="la">
                        <quote>nobilitas generis</quote>
                     </cit>
                     <ref cRef="Sap." loc="8,3">Sap. 8,3</ref>; <cit type="translation" xml:lang="la">
                        <quote>honestas</quote>
                     </cit>
                     <foreign xml:lang="grc">εὐσχημοσύνη</foreign>
                     <foreign xml:lang="gez">በኂሩት፡ </foreign>
                     <foreign xml:lang="grc">εὐσχημόνως</foreign>
                     <ref cRef="1 Thess." loc="4,12">1 Thess. 4,12</ref>; <cit type="translation" xml:lang="la">
                        <quote>pietas</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>sanctitas</quote>
                     </cit>
                     <foreign xml:lang="grc">ὁσίοτης</foreign>
                     <ref cRef="Sap." loc="9,3">Sap. 9,3</ref>; <ref cRef="Prov." loc="14,32">Prov.
                        14,32</ref>; <cit type="translation" xml:lang="la">
                        <quote>liberalitas</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>benevolentia</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>beneficentia</quote>
                     </cit>
                     <ref cRef="Gal." loc="5,22">Gal. 5,22</ref> (<foreign xml:lang="grc">ἀγαθωσύνη</foreign>).</sense>
                  <sense n="b" xml:id="Db"> at plerumque: <cit type="translation" xml:lang="la">
                        <quote>probitas</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>honestas</quote>
                     </cit>, <cit type="translation" xml:lang="la">
                        <quote>virtus</quote>
                     </cit>: de Deo <foreign xml:lang="grc">ἀγαθότης</foreign>
                     <ref cRef="Sap." loc="7,26">Sap. 7,26</ref>; <foreign xml:lang="grc">χρηστότης</foreign>
                     <ref cRef="Ps." loc="67,11">Ps. 67,11</ref>; <ref cRef="Ps." loc="118,68">Ps.
                        118,68</ref>; <ref cRef="4 Esr." loc="8,42">4 Esr. 8,42</ref>; de homine:
                        <foreign xml:lang="gez">በኂሩተ፡ ጻድቃን፡</foreign>
                     <ref cRef="Prov." loc="11,10">Prov. 11,10</ref>; <ref cRef="Job" loc="40,3">Job
                        40,3</ref> (LXX); <foreign xml:lang="gez">አልቦ፡ እኩይ፡ ዘእንበለ፡ ኂሩት፡</foreign>
                     <ref cRef="Kuf." loc="35">Kuf. 35</ref>; <foreign xml:lang="gez">አልቦ፡ ላዕሌሁ፡
                        ኂሩት፡</foreign>
                     <ref cRef="Kuf." loc="35">Kuf. 35</ref>; <foreign xml:lang="gez">መንፈሰ፡ ኂሩት፡ </foreign>
                     <ref cRef="Hen." loc="61,11">Hen. 61,11</ref>; <ref cRef="Jsp." loc="361">Jsp.
                        p. 361</ref>; <foreign xml:lang="grc">ἀρετή</foreign>
                     <ref cRef="Sap." loc="4,1">Sap. 4,1</ref>; <foreign xml:lang="gez">ርእሰ፡
                        ኂሩታት፡</foreign>
                     <cit type="translation" xml:lang="la">
                        <quote>virtutum summa</quote>
                     </cit> vel <cit type="translation" xml:lang="la">
                        <quote>nobilissima</quote>
                     </cit>, <ref cRef="Phlx." loc="3">Phlx. 3</ref>; Vespasiani <foreign xml:lang="gez">ኂሩታት፡ </foreign>
                     <cit type="translation" xml:lang="la">
                        <quote>virtutes</quote>
                     </cit> vel <cit type="translation" xml:lang="la">
                        <quote>laudes</quote>
                     </cit>, <ref cRef="Jsp." loc="363.">Jsp. p. 363.</ref>
                  </sense>
               </sense>
               
            </entry>
