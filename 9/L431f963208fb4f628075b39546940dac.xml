               <sense xml:lang="la" source="#dillmann" xml:id="Dmain"> <subc>I,2</subc> [amh. <lbl expand="idem">id.</lbl>; <foreign xml:lang="ar">نَظَرَ</foreign> <lbl expand="idem">id.</lbl>; <foreign xml:lang="he">נָצַר</foreign> <foreign xml:lang="syr">ܢܛܰܪ</foreign> (<foreign xml:lang="ar">نطر</foreign>) <cit type="translation" xml:lang="la">
