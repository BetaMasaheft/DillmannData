                  <foreign xml:lang="gez">ጠበ</foreign>
               <rs type="root"/>
                    </form>
               <sense xml:lang="la" source="#dillmann" xml:id="Dmain"> et <foreign xml:lang="gez">ጠበበ፡</foreign> <subc>I,1</subc> [<foreign xml:lang="ar">طَبَّ</foreign> <foreign xml:lang="syr">ܛܰܒ</foreign>] <gramGrp> <pos expand="Subjunctivus">Subj.</pos> </gramGrp> <foreign xml:lang="gez">ይጥበብ፡ ይጥብብ፡</foreign> <sense n="1" xml:id="D1"> <cit type="translation" xml:lang="la">
                                <quote> sapere </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> sapientia praeditum esse </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> sapientem </quote>
                            </cit> vel <cit type="translation" xml:lang="la">
                                <quote> prudentem esse </quote>
                            </cit> vel <cit type="translation" xml:lang="la">
                                <quote> fieri </quote>
                            </cit>: <foreign xml:lang="gez">ጥበቡ፡</foreign> <foreign xml:lang="grc">σοφίσθητε</foreign> <ref cRef="Prov." loc="8,33">Prov. 8,33</ref>; <foreign xml:lang="gez">ጥበብ፡ ወኀሊ፡</foreign> <ref cRef="Jes." loc="44,28">Jes. 44,28</ref>; <foreign xml:lang="gez">እጥብብ፡</foreign> <ref cRef="Koh." loc="7,23">Koh. 7,23</ref> <lbl expand="varia lectio">var.</lbl>; <foreign xml:lang="gez">ጠበብኩ፡</foreign> <ref cRef="Koh." loc="2,15">Koh. 2,15</ref>; <foreign xml:lang="gez">ዘጠበበ፡ በሕዝቡ፡</foreign> <ref cRef="Sir." loc="37,26">Sir. 37,26</ref>; <foreign xml:lang="gez">ይጠቡ፡</foreign> <ref cRef="Ps." loc="93,8">Ps. 93,8</ref>; <foreign xml:lang="gez">ይጥበቡ፡</foreign> <ref cRef="Rom." loc="1,22">Rom. 1,22</ref>; <foreign xml:lang="gez">በዝንቱ፡ ይትመካሕ፡ ዘይትሜካሕ፤ ጠቢብ፡ ወያእምር፡ ኪያየ፡</foreign> <ref cRef="Jer." loc="9,24">Jer. 9,24</ref>; <cb n="1228" xml:id="c1228"/> <foreign xml:lang="gez">ይልህቅ፡ በአእምሮ፡ ወበጥቦ፡</foreign> (legendum videtur <foreign xml:lang="gez">ጠቦ፡</foreign>) <ref cRef="Sx. Pag." loc="4">Sx. Pag. 4</ref>; comparative: <cit type="translation" xml:lang="la">
                                <quote> sapientiorem </quote>
                            </cit> vel <cit type="translation" xml:lang="la">
                                <quote> prudentiorem esse </quote>
                            </cit> vel <cit type="translation" xml:lang="la">
                                <quote> fieri </quote>
                            </cit>: <foreign xml:lang="gez">ጠበብከ፡ አንተ፡</foreign> <foreign xml:lang="grc">σοφώτερος</foreign> <ref cRef="1 Esr." loc="4,42">1 Esr. 4,42</ref>; <lbl expand="cum">c.</lbl> <gramGrp> <pos expand="pronomen">pron.</pos> </gramGrp> suff. (aliquo): <foreign xml:lang="gez">አልቦ፡ ዘይጠብበከ፡</foreign> <ref cRef="Gen." loc="41,39">Gen. 41,39</ref>, vel <lbl expand="sequens">seq.</lbl> <foreign xml:lang="gez">እምነ፡</foreign> <ref cRef="3 Reg." loc="4,27">3 Reg. 4,27</ref> (<foreign xml:lang="gez">ጠበ፡</foreign> et <foreign xml:lang="gez">ጠበበ፡</foreign>); <foreign xml:lang="gez">አንተኑ፡ ትጠብብ፡ እምዳንኤል፡</foreign> <ref cRef="Hez." loc="28,3">Hez. 28,3</ref>; <ref cRef="Ps." loc="118,99">Ps. 118,99</ref>; <ref cRef="Ps." loc="118,100">Ps. 118,100</ref>; <ref cRef="Luc." loc="16,8">Luc. 16,8</ref>; <ref cRef="1 Cor. " loc="1,25">1 Cor.  1,25</ref>. <sense n="a" xml:id="D1a"> <cit type="translation" xml:lang="la">
                                    <quote> intelligentem </quote>
                                </cit>, <cit type="translation" xml:lang="la">
                                    <quote> doctum </quote>
                                </cit>, <cit type="translation" xml:lang="la">
                                    <quote> eruditum </quote>
                                </cit>, <cit type="translation" xml:lang="la">
                                    <quote> disciplina imbutum esse </quote>
                                </cit> vel <cit type="translation" xml:lang="la">
                                    <quote> fieri </quote>
                                </cit>: <foreign xml:lang="gez">ትጥበብ፡</foreign> vel <foreign xml:lang="gez">ትጥብብ፡</foreign> <foreign xml:lang="grc">παιδεύεσθαι</foreign> <ref cRef="Sir." loc="6,32">Sir. 6,32</ref>; <ref cRef="Sir." loc="19,24">Sir. 19,24</ref>; <foreign xml:lang="gez">ኢይጠብብ፡ አምጣነ፡ ሕያው፡ ውእቱ፡</foreign> <ref cRef="Sir." loc="23,15">Sir. 23,15</ref>; <foreign xml:lang="gez">እለ፡ ይፈቅዱ፡ ይጥበቡ፡</foreign> (v. <foreign xml:lang="gez">ይጥብቡ፡</foreign>) <ref cRef="Sir." loc="30,26">Sir. 30,26</ref>; <foreign xml:lang="gez">ምንትኑ፡ ይጠብብ፡</foreign> <foreign xml:lang="grc">τί σοφισθήσεται</foreign> <ref cRef="Sir." loc="38,25">Sir. 38,25</ref>; <foreign xml:lang="grc">γνῶσις</foreign> <ref cRef="1 Cor. " loc="13,8">1 Cor.  13,8</ref>.</sense> <sense n="b" xml:id="D1b"> <cit type="translation" xml:lang="la">
                                    <quote> philosophari </quote>
                                </cit>: <foreign xml:lang="gez">አርእየኒ፡ እስኩ፡ ዘጠበ፡ እንዘ፡ ሕሙም፡ ውእቱ፡ ወጽዑር፡</foreign> <foreign xml:lang="grc">δεῖξόν μοι τὸν ἐν αὐτῷ τῷ πάθει φιλοσοφοῦντα</foreign> <ref cRef="Chrys. Ta." loc="4 ">Chrys. Ta. 4 </ref>(pluries).</sense> <sense n="c" xml:id="D1c"> <cit type="translation" xml:lang="la">
                                    <quote> resipiscere </quote>
                                </cit>, <cit type="translation" xml:lang="la">
                                    <quote> sobrium esse </quote>
                                </cit> vel <cit type="translation" xml:lang="la">
                                    <quote> fieri </quote>
                                </cit>: <foreign xml:lang="gez">ጥበቡ፡ ወአጥብቡ፡ ልበክሙ፡</foreign> <foreign xml:lang="grc">νήψατε</foreign> <ref cRef="1 Petr." loc="5,8">1 Petr. 5,8</ref>; <ref cRef="1 Thess." loc="5,6">1 Thess. 5,6</ref>; <ref cRef="2 Tim." loc="4,5">2 Tim. 4,5</ref>; <foreign xml:lang="gez">ጠበ፡ ንጉሥ፡ እምዕንባዜሁ፡</foreign> Gad. T.H.</sense> <sense n="2" xml:id="D12"> <cit type="translation" xml:lang="la">
                                    <quote> peritum </quote>
                                </cit>, <cit type="translation" xml:lang="la">
                                    <quote> gnarum esse </quote>
                                </cit> vel <cit type="translation" xml:lang="la">
                                    <quote> fieri </quote>
                                </cit> rei, <lbl expand="cum">c.</lbl> <foreign xml:lang="gez">በ፡</foreign> rei: <foreign xml:lang="gez">ይጠብቡ፡ በግብሮሙ፡</foreign> <ref cRef="Sir." loc="38,31">Sir. 38,31</ref>; <foreign xml:lang="gez">ከመ፡ ይጥበቡ፡</foreign> (v. <foreign xml:lang="gez">ይጠበቡ፡</foreign>) <foreign xml:lang="gez">በኪኖሙ፡</foreign> <ref cRef="Sir. " loc="38,34">Sir.  38,34</ref>; <lbl expand="cum">c.</lbl> <gramGrp> <case value="accusativus">Acc.</case> </gramGrp> rei: <foreign xml:lang="gez">እለ፡ ይጠብቡ፡ ኪያሃ፡</foreign> (<foreign xml:lang="gez">ለመጻሕፍት፡</foreign>) <ref cRef="Syn." loc="287.">Syn. f. 287.</ref> </sense> <sense n="3" xml:id="D13"> in malam partem: <cit type="translation" xml:lang="la">
                                    <quote> callidum </quote>
                                </cit>, <cit type="translation" xml:lang="la">
                                    <quote> astutum esse </quote>
                                </cit>, <ref cRef="Gen." loc="3,1">Gen. 3,1</ref>; <ref cRef="Sir. " loc="19,25">Sir.  19,25</ref>.</sense> </sense> </sense>
               
