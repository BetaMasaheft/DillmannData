               <sense xml:lang="la" source="#dillmann" xml:id="Dmain"> <gramGrp> <pos expand="Substantivum">subst.</pos> </gramGrp> <sense n="1" xml:id="D1"> <cit type="translation" xml:lang="la">
                                <quote> paratio </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> praeparatio </quote>
                            </cit>, <foreign xml:lang="grc">ἑτοιμασία</foreign> <ref cRef="Ps." loc=" 88,14">Ps.  88,14</ref>; <foreign xml:lang="grc">κατόρθωσις</foreign> <ref cRef="Ps." loc=" 96,2">Ps.  96,2</ref>.</sense> <sense n="2" xml:id="D2"> <cit type="translation" xml:lang="la">
                                <quote> convenientia </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> congruentia </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> decorum </quote>
                            </cit>: <foreign xml:lang="gez">እስመ፡ ከመዝ፡ ተድላ፡ ውእቱ፡ እፈጽም፡</foreign> <foreign xml:lang="grc">πρέπον ἐστὶ πληρῶσαι</foreign> <ref cRef="Matth." loc="3,15">Matth. 3,15</ref> <lbl expand="romanae editionis">rom.</lbl> <sense n="a" xml:id="D2a"> <cit type="translation" xml:lang="la">
                                    <quote> decus </quote>
                                </cit>, <cit type="translation" xml:lang="la">
                                    <quote> honor </quote>
                                </cit>: <ref cRef="4 Esr." loc="10,64">4 Esr. 10,64</ref>; <foreign xml:lang="gez">ተድላ፡ ዚአሁ፡ የኀሥሥ፡</foreign> <foreign xml:lang="grc">δόξαν</foreign> <ref cRef="Joh." loc="7,18">Joh. 7,18</ref>; <ref cRef="Joh. " loc="8,50">Joh.  8,50</ref>; <foreign xml:lang="gez">በተድላ፡</foreign> <foreign xml:lang="grc">ἐν εὐπρεπείαις</foreign> <ref cRef="Job" loc="36,11">Job 36,11</ref>.</sense> <sense n="b" xml:id="D2b"> <cit type="translation" xml:lang="la">
                                    <quote> dignitas </quote>
                                </cit> <ref cRef="Hen." loc="89,44">Hen. 89,44</ref>; <foreign xml:lang="gez">ረሰይከነ፡ ተድላነ፡</foreign> <ref cRef="Lud. Comm. hist. p. 825 Nr." loc="73">Lud. Comm. hist. p. 825 Nr. 73</ref>; <ref cRef="Lit." loc="165,4">Lit. 165,4</ref>.</sense> </sense> <sense n="3" xml:id="D3"> <cit type="translation" xml:lang="la">
                                <quote> commodum </quote>
                            </cit>: <foreign xml:lang="gez">ተድላ፡ ርእስየ፡</foreign> <foreign xml:lang="grc">συμφέρον</foreign> <ref cRef="1 Cor." loc="10,33">1 Cor. 10,33</ref>.</sense> <sense n="4" xml:id="D4"> <cit type="translation" xml:lang="la">
                                <quote> affluentia rerum bonarum </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> fortunae </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> prosperitas </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> felicitas </quote>
                            </cit>: <foreign xml:lang="gez">መዛግብቲሆሙ፡ እምላእ፡ ተድላ፡</foreign> <foreign xml:lang="grc">ἀγαθῶν</foreign> <ref cRef="Prov." loc="8,21">Prov. 8,21</ref>; <foreign xml:lang="gez">ከመ፡ ትደለወ፡ አመ፡ ተድላሁ፡</foreign> (<lbl expand="oppositum, -o, -nitur">opp.</lbl> <foreign xml:lang="gez">ተጽናሱ፡</foreign>) <ref cRef="Sir." loc="29,2">Sir. 29,2</ref>; <foreign xml:lang="grc">εὐθηνία</foreign> <ref cRef="Ps. " loc="29,7">Ps.  29,7</ref>.</sense> <sense n="5" xml:id="D5"> <cit type="translation" xml:lang="la">
                                <quote> vita commoda </quote>
                            </cit> vel <cit type="translation" xml:lang="la">
                                <quote> laeta </quote>
                            </cit> vel <cit type="translation" xml:lang="la">
                                <quote> jucunda </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> amoenitas </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> laetitia </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> jucunditas </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> voluptas </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> deliciae </quote>
                            </cit>: <foreign xml:lang="grc">τρυφή</foreign> <ref cRef="Prov." loc="19,10">Prov. 19,10</ref>; <foreign xml:lang="gez">አክሊለ፡ ተድላ፡</foreign> <ref cRef="Prov." loc="4,9">Prov. 4,9</ref>; <foreign xml:lang="gez">ገነተ፡ ተድላ፡</foreign> <ref cRef="Gen." loc="3,23">Gen. 3,23</ref>; <ref cRef="Kuf." loc="7">Kuf. p. 7</ref>; <foreign xml:lang="gez">ኢያፍቱከ፡ ተድላሆሙ፡ ለረሲዓን፡</foreign> <foreign xml:lang="grc">εὐδοκία</foreign> <ref cRef="Sir." loc="9,12">Sir. 9,12</ref>; <foreign xml:lang="gez">ረሲ፡ ፍግዐ፡ ወተድላ፡ ውስተ፡ ገጻ፡ ለምድር፡</foreign> <ref cRef="Lit." loc="164,4">Lit. 164,4</ref>; <foreign xml:lang="gez">ፍሥሓ፡</foreign> et <foreign xml:lang="gez">ተድላ፡</foreign> <ref cRef="Org. " loc="1">Org.  1</ref>; <foreign xml:lang="gez">መንግሥተ፡ ሰማያት፡ ፈለሰ፡ ወሖረ፡ በተድላ፡</foreign> <ref cRef="Sx. Tachs." loc="8 Enc.">Sx. Tachs. 8 Enc.</ref>; <foreign xml:lang="gez">ሐናጺ፡ ለነጋድያን፡ ማኅደረ፡ ተድላ፡</foreign> <ref cRef="Sx. Masc." loc="7 Enc.">Sx. Masc. 7 Enc.</ref>
                        </sense> </sense>
               
