               <sense xml:id="Dmain" xml:lang="la" source="#dillmann"> vel <foreign xml:lang="gez">መሥወርት፡</foreign> <gramGrp> <pos expand="Substantivum">subst.</pos> </gramGrp>, hucusque nonnisi in <lbl expand="pluralis">Pl.</lbl> <foreign xml:lang="gez">መሣውር፡</foreign> repertum; ut videtur: <cit type="translation" xml:lang="la"><quote> infundibulum </quote></cit> vel <cit type="translation" xml:lang="la"><quote> eas quo fluidum infunditur </quote></cit>: <foreign xml:lang="gez">ሰብዐቱ፡ መሣውር፡ በዘ፡ ይሰቅይዋ፡ ለመኃትዊሃ፡</foreign> <foreign xml:lang="grc">ἐπαρυστρίδες</foreign> <ref cRef="Zach." loc="4,2">Zach. 4,2</ref>; etiam <ref cRef="Macc." loc="18">Macc. f. 18</ref>; <ref cRef="Kebr. Nag." loc="17">Kebr. Nag. 17</ref>; <ref cRef="Clem." loc="87">Clem. f. 87</ref>; <foreign xml:lang="gez">መሣውር፡</foreign> vel <foreign xml:lang="gez">መሳውር፡</foreign> inter vasa templi vel ecclesiae simul cum candelabris et lampadibus memorantur. – <lbl expand="Vocabularium Aethiopicum, -a -a">Voc. Ae.</lbl>: <foreign xml:lang="gez">መሳውር፡ ዘ፡ መቈሽቈሻ፡</foreign>; <cit type="translation" xml:lang="la"><quote> aliud </quote></cit>: <foreign xml:lang="gez">መሳውር፡ ዘ፡ ሸኛ።</foreign> Ludolfi Aethiops <cit type="translation" xml:lang="la"><quote> rostra lychnorum </quote></cit> exponebat. </sense>
               
            <sense xml:id="Tmain" xml:lang="en" source="#traces"> <cit type="transcription" xml:lang="gez"><quote>maśwar</quote></cit>, <cit type="transcription" xml:lang="gez"><quote>maśwart</quote></cit>, <lbl expand="pluralis">Pl.</lbl> <foreign xml:lang="gez">መሣውር፡</foreign> <cit type="transcription" xml:lang="gez"><quote>maśāwǝr</quote></cit> <sense n="G" xml:id="TG"> <foreign xml:lang="gez">ወሥወር፡</foreign> <cit type="transcription" xml:lang="gez"><quote>maśwar</quote></cit>, <lbl expand="pluralis">Pl.</lbl> <foreign xml:lang="gez">መሣውር፡</foreign> <cit type="transcription" xml:lang="gez"><quote>maśāwǝr</quote></cit> «<cit type="translation" xml:lang="fr"><quote>lampe</quote></cit>» ― <foreign xml:lang="am">ማብርያ፡</foreign> Ms. <ref type="BM" target="BNFabb217">BNFabb217</ref>, fol. 35v. <lb/>
 <bibl><ptr target="bm:Grebaut1952SuppLexicon"/><citedRange unit="page"> 82</citedRange></bibl> 
</sense> <sense n="L" xml:id="TL"> <cit type="transcription" xml:lang="gez"><quote>śwr</quote></cit>, <foreign xml:lang="gez">ወሥወር</foreign> <cit type="transcription" xml:lang="gez"><quote>maśwar</quote></cit>, <foreign xml:lang="gez">መሥወርት</foreign> <cit type="transcription" xml:lang="gez"><quote>maśwart</quote></cit>, (<lbl expand="pluralis">Pl.</lbl> <foreign xml:lang="gez">መሣውር</foreign> <cit type="transcription" xml:lang="gez"><quote>maśāwǝr</quote></cit>) <cit type="translation" xml:lang="en"><quote>funnel</quote></cit>, (Lt) <cit type="translation" xml:lang="en"><quote>dry measure</quote></cit>, (<bibl><ptr target="bm:Ludolf1699Lexicon"/></bibl>) <cit type="translation" xml:lang="en"><quote>container for oil</quote></cit>, <cit type="translation" xml:lang="en"><quote>means of stirring the fire</quote></cit>, <cit type="translation" xml:lang="en"><quote>poker</quote></cit> <bibl><ptr target="bm:Leslau1987dic"/><citedRange unit="page"> 539a</citedRange></bibl> 
</sense> </sense></entry>
