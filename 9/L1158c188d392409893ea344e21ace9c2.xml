               <sense xml:lang="la" source="#dillmann" xml:id="Dmain"> <gramGrp> <pos expand="Substantivum">subst.</pos> </gramGrp>, plerumque <gramGrp> <gen value="not found">m.</gen> </gramGrp>, (<gramGrp> <gen value="femininus">fem.</gen> </gramGrp>, ut <ref cRef="Org." loc="4">Org. 4</ref>), <lbl expand="Singularis">Sing.</lbl> et <lbl expand="collatio, -is vel collectivum,&#xA;                            -e">coll.</lbl>, <lbl expand="pluralis">Pl.</lbl> <foreign xml:lang="gez">ፍሬያት፡</foreign>, rarius <foreign xml:lang="gez">ፍርያት፡</foreign> (<foreign xml:lang="gez">ፍረያት፡</foreign> <ref cRef="Sap." loc="10,7">Sap. 10,7</ref> in <lbl expand="d'Abbadie">Abb.</lbl> LV; <ref cRef="Marc." loc="4,29">Marc. 4,29</ref> <lbl expand="romanae editionis">rom.</lbl>), <sense n="1" xml:id="D1"> <cit type="translation" xml:lang="la">
