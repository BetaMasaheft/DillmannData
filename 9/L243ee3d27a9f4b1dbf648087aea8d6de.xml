               <sense xml:id="Dmain" xml:lang="la" source="#dillmann"> <gramGrp> <pos expand="Substantivum">subst.</pos> </gramGrp>, plerumque <gramGrp> <gen value="not found">m.</gen> </gramGrp>, <lbl expand="pluralis">Pl.</lbl> <foreign xml:lang="gez">አስማት፡</foreign> [<foreign xml:lang="he">שֵׁם</foreign> <foreign xml:lang="syr">ܫܶܡ</foreign> <foreign xml:lang="ar">إِسْمٌ</foreign>] <sense n="1" xml:id="D1"> <cit type="translation" xml:lang="la"><t:quote xmlns:t="http://www.tei-c.org/ns/1.0"> nomen </t:quote></cit> <ref cRef="Gen." loc="2,19">Gen. 2,19</ref>; <ref cRef="Gen. " loc="2,20">Gen.  2,20</ref>; <ref cRef="Gen." loc="3,20">Gen. 3,20</ref>; <ref cRef="Gen." loc="4,19">Gen. 4,19</ref>; <ref cRef="Gen." loc="10,25">Gen. 10,25</ref>; <ref cRef="Gen." loc="11,29">Gen. 11,29</ref>; <ref cRef="Gen." loc="25,13">Gen. 25,13</ref>; <ref cRef="Gen." loc="26,20">Gen. 26,20</ref>; <ref cRef="Ex." loc="1,1">Ex. 1,1</ref>; <ref cRef="Dan." loc="1,7">Dan. 1,7</ref>; <ref cRef="Matth." loc="1,21">Matth. 1,21</ref>; <ref cRef="Matth." loc="10,2">Matth. 10,2</ref>, al. sexcenties; <foreign xml:lang="gez">ደሴት፡ እንተ፡ ስማ፡ ፍጥሞ፡</foreign> <ref cRef="Apoc." loc="1,9">Apoc. 1,9</ref>; <foreign xml:lang="gez">እምትካት፡ ስምከ፡ ላዕሌነ፡</foreign> <ref cRef="Jes." loc="36,16">Jes. 36,16</ref>; <foreign xml:lang="gez">አምኅ፡ ቢጸከ፡ በበአስማቲሆሙ፡</foreign> <cit type="translation" xml:lang="la"><t:quote xmlns:t="http://www.tei-c.org/ns/1.0"> nominatim </t:quote></cit> <ref cRef="3 Joh." loc="15">3 Joh. 15</ref>; <ref cRef="Joh." loc="10,3">Joh. 10,3</ref>; <ref cRef="Num." loc="1,17">Num. 1,17</ref>; <ref cRef="Num." loc="4,32">Num. 4,32</ref>. <sense n="a" xml:id="D1a"> <foreign xml:lang="gez">መኑ፡ ስምከ፡</foreign> <cit type="translation" xml:lang="la"><t:quote xmlns:t="http://www.tei-c.org/ns/1.0"> quod est nomen tuum? </t:quote></cit> <ref cRef="Jud. " loc="13,17">Jud.  13,17</ref>;<foreign xml:lang="gez">መኑ፡ ስሙ፡ ወመኑ፡ ስመ፡ ወልዱ፡</foreign> <ref cRef="Prov." loc="24,27">Prov. 24,27</ref>; Clem. f. 221 (<lbl expand="videas">vid.</lbl> <foreign xml:lang="gez">መኑ፡</foreign>). 
</sense> <sense n="b" xml:id="D1b"> <foreign xml:lang="gez">ስመ፡ ጥምቀት፡</foreign> <cit type="translation" xml:lang="la"><t:quote xmlns:t="http://www.tei-c.org/ns/1.0"> praenomen </t:quote></cit> (baptizato inditum), ut: <foreign xml:lang="gez">ስመ፡ ጥምቀቱ፡ ዘሚካኤል፡</foreign> <ref cRef="Cod. Didascaliae Stuttg. " loc="117.">Cod. Didascaliae Stuttg.  p. 117.</ref> 
</sense> <sense n="c" xml:id="D1c"> <foreign xml:lang="gez">ዘአልቦ፡ ስመ፡</foreign> <cit type="translation" xml:lang="la"><t:quote xmlns:t="http://www.tei-c.org/ns/1.0"> anonymus </t:quote></cit>, ut <ref cRef="Asc. Jes. " loc="4,22">Asc. Jes.  4,22</ref>. 
</sense> <sense n="d" xml:id="D1d"> <foreign xml:lang="gez">በስመ፡ እገሌ፡</foreign> <cit type="translation" xml:lang="la"><t:quote xmlns:t="http://www.tei-c.org/ns/1.0"> nomine et auctoritate </t:quote></cit> alicujus: <foreign xml:lang="gez">ኢይነግር፡ በስመ፡ ዚአሁ፡</foreign> <ref cRef="Jer." loc="20,9">Jer. 20,9</ref>; <ref cRef="Deut." loc="18,22">Deut. 18,22</ref>; <ref cRef="Joh. " loc="5,43">Joh.  5,43</ref>; <ref cRef="Joh." loc="10,25">Joh. 10,25</ref>; <ref cRef="Act." loc="4,7">Act. 4,7</ref>. 
</sense> <sense n="e" xml:id="D1e"> <cit type="translation" xml:lang="la"><t:quote xmlns:t="http://www.tei-c.org/ns/1.0"> nomen </t:quote></cit> opponitur rei: <cb n="0341" xml:id="c0341"/> <foreign xml:lang="gez">ጥበብ፡ በከመ፡ ስማ፡ ይእቲ፡</foreign> <ref cRef="Sir." loc="6,22">Sir. 6,22</ref>; <foreign xml:lang="gez">ዐርክ፡ ለስም፡ በከ፡ ዘይትዓረክ፡</foreign> <ref cRef="Sir. " loc="37,1">Sir.  37,1</ref>; <ref cRef="1 Reg." loc="25,25">1 Reg. 25,25</ref>. 
</sense> <sense n="f" xml:id="D1f"> <cit type="translation" xml:lang="la"><t:quote xmlns:t="http://www.tei-c.org/ns/1.0"> nomen </t:quote></cit> i.e. <cit type="translation" xml:lang="la"><t:quote xmlns:t="http://www.tei-c.org/ns/1.0"> signum </t:quote></cit> vel <cit type="translation" xml:lang="la"><t:quote xmlns:t="http://www.tei-c.org/ns/1.0"> monumentum </t:quote></cit> <ref cRef="2 Reg." loc="8,13">2 Reg. 8,13</ref>; <ref cRef="2 Reg." loc="18,18">2 Reg. 18,18</ref> E. 
</sense> 
</sense> <sense n="2" xml:id="D2"> <cit type="translation" xml:lang="la"><t:quote xmlns:t="http://www.tei-c.org/ns/1.0"> nomen </t:quote></cit> i.e. <cit type="translation" xml:lang="la"><t:quote xmlns:t="http://www.tei-c.org/ns/1.0"> fama </t:quote></cit> <sense n="a" xml:id="D2a"> <cit type="translation" xml:lang="la"><t:quote xmlns:t="http://www.tei-c.org/ns/1.0"> bona </t:quote></cit>, <cit type="translation" xml:lang="la"><t:quote xmlns:t="http://www.tei-c.org/ns/1.0"> mala </t:quote></cit>: <foreign xml:lang="gez">ስም፡ ሠናይ፡</foreign> <ref cRef="Koh." loc="7,1">Koh. 7,1</ref>; <ref cRef="Sir." loc="41,13">Sir. 41,13</ref>; <ref cRef="Prov." loc="22,1">Prov. 22,1</ref>; <foreign xml:lang="gez">እኩይ፡</foreign> <ref cRef="Sir." loc="6,1">Sir. 6,1</ref>; <foreign xml:lang="gez">ዘአውፅአ፡ ስመ፡ እኩየ፡ ለቢጹ፡</foreign> <ref cRef="Clem." loc="120">Clem. f. 120</ref>; <ref cRef="Deut. " loc="22,14">Deut.  22,14</ref>; <ref cRef="Luc." loc="6,22">Luc. 6,22</ref>; <ref cRef="Job" loc="31,30">Job 31,30</ref>; <foreign xml:lang="gez">ከመ፡ እግበር፡ ስመ፡ እኩየ፡ ሊተ፡</foreign> <ref cRef="2 Esr." loc="16,13">2 Esr. 16,13</ref>. 
</sense> <sense n="b" xml:id="D2b"> <cit type="translation" xml:lang="la"><t:quote xmlns:t="http://www.tei-c.org/ns/1.0"> fama post mortem </t:quote></cit>, <cit type="translation" xml:lang="la"><t:quote xmlns:t="http://www.tei-c.org/ns/1.0"> memoria </t:quote></cit>: <foreign xml:lang="gez">እለ፡ ኀደጉ፡ ስመ፡ ክቡረ፡</foreign> <ref cRef="Sir." loc="44,8">Sir. 44,8</ref>; <foreign xml:lang="gez">ይወርስ፡ ስመ፡ ዘለዓለም፡</foreign> <ref cRef="Sir." loc="15,6">Sir. 15,6</ref>; <foreign xml:lang="gez">ይቀውም፡ ስሙ፡ ለትውልደ፡ ትውልድ፡</foreign> <ref cRef="Sir." loc="39,9">Sir. 39,9</ref>; <foreign xml:lang="gez">ኣጠፍእ፡ ስሞሙ፡ ወዘርኦሙ፡</foreign> <ref cRef="Jes. " loc="14,22">Jes.  14,22</ref>; <foreign xml:lang="gez">እሁቦሙ፡ ስመ፡ ዘለዓለም፡</foreign> <ref cRef="Jes." loc="56,5">Jes. 56,5</ref>; <ref cRef="Ps." loc="71,17">Ps. 71,17</ref>; <ref cRef="Deut." loc="7,24">Deut. 7,24</ref>; <ref cRef="Deut." loc="9,14">Deut. 9,14</ref>. 
</sense> <sense n="c" xml:id="D2c"> <cit type="translation" xml:lang="la"><t:quote xmlns:t="http://www.tei-c.org/ns/1.0"> nomen celebre </t:quote></cit>, <cit type="translation" xml:lang="la"><t:quote xmlns:t="http://www.tei-c.org/ns/1.0"> fama </t:quote></cit>: <foreign xml:lang="gez">ዕደወ፡ ስም፡</foreign> <ref cRef="Gen." loc="6,4">Gen. 6,4</ref>; <foreign xml:lang="gez">ገብረ፡ ለከ፡ ስመ፡ ዐቢየ፡</foreign> <ref cRef="Deut. " loc="26,19">Deut.  26,19</ref>; <foreign xml:lang="gez">ገበርከ፡ ለከ፡ ስመ፡</foreign> <ref cRef="Jer." loc="39,20">Jer. 39,20</ref>; <foreign xml:lang="gez">ኮነ፡ በከመ፡ ስሙ፡ ዐቢይ፡</foreign> <ref cRef="Sir." loc="46,1">Sir. 46,1</ref>; <foreign xml:lang="gez">ውሉድ፡ ወሕንጻ፡ ሀገር፡ ያዐብዩ፡ ስመ፡</foreign> <ref cRef="Sir." loc="40,19">Sir. 40,19</ref>; <foreign xml:lang="gez">ኀሊ፡ ስመ፡ ትገብር፡ ለከ፡</foreign> <ref cRef="Sir." loc="41,12">Sir. 41,12</ref>. – In libris Mss. <foreign xml:lang="gez">ስም፡</foreign> passim confunditur cum <foreign xml:lang="gez">ስምዕ፡</foreign> 
</sense> 
</sense> </sense>
               
