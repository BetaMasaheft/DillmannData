            <entry xml:id="L3165c6521bff4ac7be21f89e7ab71450" n="218">
               <form>
                  <foreign xml:lang="gez">ልሑድ</foreign>
               </form>
               <sense xml:lang="la" source="#dillmann" xml:id="Dmain">
                  <gramGrp>
                     <pos expand="participium">part.</pos>
                  </gramGrp>, <gramGrp>
                     <gen value="femininus">fem.</gen>
                  </gramGrp>
                  <foreign xml:lang="gez">ልሕድ፡</foreign>, nonnumquam <foreign xml:lang="gez">ላሕድ፡</foreign>
                  <gramGrp>
                     <pos expand="participium">part.</pos>
                     <gen value="masculinum">m.</gen>
                  </gramGrp> et <gramGrp>
                     <gen value="femininus">fem.</gen>
                  </gramGrp>
                  <cit type="translation" xml:lang="la">
                     <quote>tener</quote>
                  </cit>, <cit type="translation" xml:lang="la">
                     <quote>mollis</quote>
                  </cit>, <cit type="translation" xml:lang="la">
                     <quote>delicate habitus</quote>
                  </cit>, <cit type="translation" xml:lang="la">
                     <quote>suavis</quote>
                  </cit>, <foreign xml:lang="gez">ኢትሰመዪ፡ እንከ፡ ልሕደ፡ ወሥብሕተ፡</foreign>
                  <foreign xml:lang="grc">ἁπαλὴ καὶ τρυφερά</foreign>
                  <ref cRef="Jes." loc="47,1">Jes. 47,1</ref>; <foreign xml:lang="gez">አንቲ፡
                     ላሕድ፡</foreign> (v. <foreign xml:lang="gez">ልሕድ፡</foreign>) <foreign xml:lang="grc">τρυφερά</foreign>
                  <ref cRef="Jes." loc="47,8">Jes. 47,8</ref>; <ref cRef="Jer." loc="26,28">Jer.
                     26,28</ref>; <ref cRef="Mich." loc="1,16">Mich. 1,16</ref>; <foreign xml:lang="gez">ልሕደ፡ ተመሰልኪ፡</foreign>
                  <ref cRef="Jer." loc="6,2">Jer. 6,2</ref> (vers. alt.); <foreign xml:lang="gez">ሕፃን፡ ላሕድ፡</foreign> (v. <foreign xml:lang="gez">ልሕድ፡</foreign>) <foreign xml:lang="grc">παιδίον ἐντρυφῶν</foreign>
                  <ref cRef="Jer." loc="38,20">Jer. 38,20</ref>; <foreign xml:lang="gez">ኦልሕድ፡
                     ኢትዮጵያ፡</foreign>
                  <lbl expand="Novum Testamentum">N.T.</lbl>
                        <lbl expand="romanae editionis">rom.</lbl> (in <lbl expand="Epilogus">Epil.</lbl> in Ep. Joann.); <cit type="translation" xml:lang="la">
                     <quote>pueri</quote>
                  </cit>
                  <foreign xml:lang="gez">ልኁዳን፡</foreign> Lud. sine <lbl expand="auctore, auctoritate">auct.</lbl> – <lbl expand="Vocabularium Aethiopicum, -a -a.">Voc. Ae.</lbl>: <foreign xml:lang="gez">ልሕድ፡ ብ፡ ድልድል፡</foreign>; <cit type="translation" xml:lang="la">
                     <quote>aliud</quote>
                  </cit>: <foreign xml:lang="gez">ልሕድ፡ ዘ፡ ሕድስ፡ በዓልቴትሂ፡ ድልድልሂ፡ ይ፡</foreign> (ad
                  Jesaiam). </sense>
               
