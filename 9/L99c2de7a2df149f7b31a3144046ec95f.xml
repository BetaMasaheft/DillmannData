            <entry xml:id="L99c2de7a2df149f7b31a3144046ec95f" n="9846">
               <form>
                  <foreign xml:lang="gez">ተፋለጠ</foreign>
               </form>
               <sense xml:lang="la" source="#dillmann" xml:id="Dmain"> <subc>III,3</subc> <cit type="translation" xml:lang="la">
                            <quote> se sejungere alium ab alio </quote>
                        </cit>, <cit type="translation" xml:lang="la">
                            <quote> a se invicem discedere </quote>
                        </cit> vel <cit type="translation" xml:lang="la">
                            <quote> secedere </quote>
                        </cit>: <foreign xml:lang="gez">ወተፋለጡ፡ እንዘ፡ የሐውሩ፡ ፍኖተ፡</foreign> <ref cRef="3 Reg." loc="18,6">3 Reg. 18,6</ref>; <ref cRef="Jos." loc="7,14">Jos. 7,14</ref> (at <foreign xml:lang="gez">ተፈልጠ፡</foreign> <ref cRef="Jos." loc="7,17">Jos. 7,17</ref>; <ref cRef="Jos." loc="7,18">Jos. 7,18</ref>); <foreign xml:lang="gez">ዘዘዚአሆሙ፡</foreign> <ref cRef="Act." loc="15,39">Act. 15,39</ref>; <foreign xml:lang="gez">በበይናቲሆሙ፡</foreign> <ref cRef="Dan. ap." loc="1,13">Dan. ap. 1,13</ref>. <sense n="a" xml:id="Da"> dissidio: <ref cRef="Luc." loc="12,52">Luc. 12,52</ref> Platt.</sense> <sense n="b" xml:id="Db"> divortio: <foreign xml:lang="gez">በተፋልጦቶሙ፡</foreign> F.M. 5,2.</sense> <sense n="c" xml:id="Dc"> i.q. <foreign xml:lang="gez">ተፈልጠ፡</foreign> <cit type="translation" xml:lang="la">
                                <quote> se sejungere </quote>
                            </cit> ab aliquo: <foreign xml:lang="gez">ወእምጋዲ፡ ተፋለጡ፡</foreign> (v. <foreign xml:lang="gez">ተፈልጡ፡</foreign>) <ref cRef="1 Par." loc="12,8">1 Par. 12,8</ref>.</sense> </sense>
               
