               <sense xml:id="Dmain" xml:lang="la" source="#dillmann"> <subc>III,1</subc> <sense n="1" xml:id="D1"> <cit type="translation" xml:lang="la"><quote> possideri </quote></cit>, <cit type="translation" xml:lang="la"><quote> dominio teneri </quote></cit>: <foreign xml:lang="gez">ዘኢይትመለክ፡</foreign> <cit type="translation" xml:lang="la"><quote> quod non possidetur </quote></cit> F.N. 39,3. 
</sense> <sense n="2" xml:id="D2"> <cit type="translation" xml:lang="la"><quote> imperio </quote></cit> alicujus <cit type="translation" xml:lang="la"><quote> subjectum esse </quote></cit> Lud. e praefatione Syn. 
</sense> <sense n="3" xml:id="D3"> <cit type="translation" xml:lang="la"><quote> dominum </quote></cit>, <cb n="0151" xml:id="c0151"/> <cit type="translation" xml:lang="la"><quote> regnatorem constitui </quote></cit> Lud. sine <lbl expand="auctore, auctoritate">auct.</lbl>; <ref cRef="Chr. L. Atq." loc="31">Chr. L. Atq. f. 31</ref>. 
</sense> </sense>
               
            <sense xml:id="Tmain" xml:lang="en" source="#traces"> <cit type="transcription" xml:lang="gez"><quote>tamalka</quote></cit> <sense n="G" xml:id="TG"> <foreign xml:lang="gez">ተመልከ፡</foreign> <cit type="transcription" xml:lang="gez"><quote>tamalka</quote></cit> <subc>III,1</subc> <foreign xml:lang="gez">ይትመለክ፡</foreign> «<cit type="translation" xml:lang="fr"><quote>être adoré</quote></cit>, <cit type="translation" xml:lang="fr"><quote>être constitué</quote></cit> seigneur, roi» ― <foreign xml:lang="am">ተመለከ፡</foreign>, <foreign xml:lang="am">ነገሠ፡</foreign> Ms. <ref type="BM" target="BNFabb217">BNFabb217</ref>, fol. 33v. <lb/>
 <bibl><ptr target="bm:Grebaut1952SuppLexicon"/><citedRange unit="page"> 76</citedRange></bibl> 
</sense> <sense n="L" xml:id="TL"> <foreign xml:lang="gez">ተመልከ</foreign> <cit type="transcription" xml:lang="gez"><quote>tamalka</quote></cit> passive of <lbl expand="videas">vid.</lbl> <foreign xml:lang="gez">መለከ</foreign> <cit type="transcription" xml:lang="gez"><quote>malaka</quote></cit>; also <cit type="translation" xml:lang="en"><quote>be subjected to</quote></cit>, <cit type="translation" xml:lang="en"><quote>be made (constituted) master</quote></cit> <bibl><ptr target="bm:Leslau1987dic"/><citedRange unit="page"> 343b</citedRange></bibl> 
</sense> <sense n="C" xml:id="TC"> <lbl expand="videas">vid.</lbl> also <ref type="external" target="https://betamasaheft.eu/Dillmann/lemma/L1115u9os7v2f26rn2k0ppa0jnzu8a9mk"><foreign xml:lang="gez">ተመልከ፡</foreign></ref> 
</sense> </sense></entry>
