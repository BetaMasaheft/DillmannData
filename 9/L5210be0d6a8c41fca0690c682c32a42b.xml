         <div>
            <entry xml:id="L5210be0d6a8c41fca0690c682c32a42b" n="9315" type="hom">
               <form>
                  <foreign xml:lang="gez">ጸበ</foreign>
                        <seg type="hom">I</seg>
                    <rs type="root"/>
                    </form>
               <sense xml:lang="la" source="#dillmann" xml:id="Dmain"> (vel <foreign xml:lang="gez">ፀበ፡</foreign>) et <foreign xml:lang="gez">ጸበበ፡</foreign> <subc>I,1</subc> [<foreign xml:lang="ar">ضبّ</foreign> <cit type="translation" xml:lang="la">
                            <quote> arctius adhaesit </quote>
                        </cit>; ab omni <cit type="translation" xml:lang="la">
                            <quote> parte comprehendit </quote>
                        </cit> rem; (<foreign xml:lang="ar">ضبأ ضبن</foreign> IV); <foreign xml:lang="ar">ضَفٌّ</foreign> et <foreign xml:lang="ar">ظَفٌّ</foreign>; amh. <foreign xml:lang="gez">ጸበበ፡</foreign> et <foreign xml:lang="gez">ጠበበ፡</foreign>] <gramGrp> <pos expand="Subjunctivus">Subj.</pos> </gramGrp> <foreign xml:lang="gez">ይጽብብ፡</foreign>, <sense n="1" xml:id="D1"> <cit type="translation" xml:lang="la">
                                <quote> angustum </quote>
                            </cit> vel <cit type="translation" xml:lang="la">
                                <quote> nimis angustum esse </quote>
                            </cit> vel <cit type="translation" xml:lang="la">
                                <quote> fieri </quote>
                            </cit> alicui (c. <gramGrp> <pos expand="pronomen">pron.</pos> </gramGrp> suff.), <cit type="translation" xml:lang="la">
                                <quote> coangustare </quote>
                            </cit> aliquem: <foreign xml:lang="gez">ለይጽብቦ፡</foreign> fiat ei angustum, <ref cRef="Org." loc="3">Org. 3</ref>; <foreign xml:lang="gez">ዝንቱ፡ ማኅደርነ፡ ይጸብበነ፡</foreign> <foreign xml:lang="grc">στενὸς ἀφ᾽ ἡμῶν</foreign> <ref cRef="4 Reg." loc="6,1">4 Reg. 6,1</ref>; <foreign xml:lang="grc">στενοχωρεῖ σε</foreign> <ref cRef="Jos." loc="17,15">Jos. 17,15</ref>; <foreign xml:lang="gez">ይጸብበነ፡ ብሔር፡</foreign> <foreign xml:lang="grc">στενός μοι</foreign> <ref cRef="Jes." loc="49,20">Jes. 49,20</ref>; <foreign xml:lang="gez">ትጸብቢዮሙ፡ ለእለ፡ ይነብሩ፡ ውስቴትኪ፡</foreign> <ref cRef="Jes." loc="49,19">Jes. 49,19</ref>; <foreign xml:lang="gez">ጸበኒ፡</foreign> (v. <foreign xml:lang="gez">ጸበበኒ፡</foreign>) <foreign xml:lang="gez">ዓለም፡</foreign> I <ref cRef="Sal. Mar." loc="17">Sal. Mar. 17</ref>; <foreign xml:lang="gez">ዘሰማይ፡ ኢያግመሮ፡ ኢጸበቦ፡ ማኅፀነ፡ ድንግል፡</foreign> <foreign xml:lang="grc">γαστὴρ τῆς παρθένου οὐκ ἐστενοχώρησε</foreign> ho. Procli in <ref cRef="Cyr." loc="120.">Cyr. f. 120.</ref> </sense> <sense n="2" xml:id="D2"> <cit type="translation" xml:lang="la">
                                <quote> in angustias compellere </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> premere </quote>
                            </cit>: <foreign xml:lang="gez">ሶበ፡ ጸበበቶ፡ ግብር፡</foreign> <ref cRef="Sx. Masc." loc="3.">Sx. Masc. 3.</ref> </sense> </sense>
               
