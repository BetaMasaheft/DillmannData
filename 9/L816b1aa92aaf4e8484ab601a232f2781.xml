            <entry xml:id="L816b1aa92aaf4e8484ab601a232f2781" n="4228">
               <form>
                  <foreign xml:lang="gez">መትልው</foreign>
               </form>
               <sense xml:lang="la" source="#dillmann" xml:id="Dmain"> et <foreign xml:lang="gez">መትሎ፡</foreign> (pro <foreign xml:lang="gez">መትለው፡</foreign>) <gramGrp> <pos expand="Substantivum">subst.</pos> </gramGrp> <sense n="1" xml:id="D1"> <cit type="translation" xml:lang="la">
                                <quote> id quod sequitur </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> sequentia </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> caetera </quote>
                            </cit>: <foreign xml:lang="gez">ስማዕ፡ መትልወ፡ ዝንቱ፡</foreign> <foreign xml:lang="grc">ἄκουσον τὰ ἑξῆς</foreign> <ref cRef="Chrys. ho." loc="3">Chrys. ho. 3</ref>; <ref cRef="F.N." loc=" 42,13">F.N.  42,13</ref>; <ref cRef="Herm." loc="36 ">Herm. p. 36 </ref>(<foreign xml:lang="grc">τὰ ἀκόλουθα τούτων</foreign>); (<foreign xml:lang="gez">ዘንተ፡</foreign>) <foreign xml:lang="gez">አናጕንስጢስ፡ ያንብብ፡ ወዘመትልው፡ ቀሲስ፡</foreign> (<cit type="translation" xml:lang="la">
                                <quote> et sequentia presbyter </quote>
                            </cit> <lbl expand="scilicet">sc.</lbl> legat) <ref cRef="Kid." loc="12">Kid. f. 12</ref>; <foreign xml:lang="gez">ወመትልወ፡ ዝንቱ፡</foreign> <cit type="translation" xml:lang="la">
                                <quote> et caetera </quote>
                            </cit> <ref cRef="Chrys. ho." loc="1.">Chrys. ho. 1.</ref> Adverbii vel praepositionis loco: <cit type="translation" xml:lang="la">
                                <quote> deinceps </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> porro </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> proxime post </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> post </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> ut </quote>
                            </cit>: <foreign xml:lang="gez">ጽድቀ፡ አቡሃ፡ እዜኑ፡ መትልወ፡</foreign> <ref cRef="Sx. Tachs." loc="5 Enc.">Sx. Tachs. 5 Enc.</ref>; <foreign xml:lang="gez">ወመትሎሁ፡ ይበል፡</foreign> <cit type="translation" xml:lang="la">
                                <quote> et deinceps dicat </quote>
                            </cit> <ref cRef="Kid. " loc="11">Kid.  f. 11</ref>; <foreign xml:lang="gez">ወመትልው፡ ይበሉ፡ አሜን፡</foreign> <ref cRef="Kid." loc="12">Kid. f. 12</ref>; <foreign xml:lang="gez">ለዝ፡ መዋኢ፡ እንዘ፡ ይኔጽር፡ አክሊሎ፤ በዕለተ፡ ፈለሰ፡ አዕረፈ፡ መትሎ፡</foreign> <cit type="translation" xml:lang="la">
                                <quote> hujus victoris coronam adspiciens eodem die </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> quo </quote>
                            </cit> (ille) <cit type="translation" xml:lang="la">
                                <quote> obiit </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> requievit deinceps </quote>
                            </cit> (Lud.: <foreign xml:lang="gez">መትልዎ፡</foreign> <cit type="translation" xml:lang="la">
                                <quote> deinceps post cum </quote>
                            </cit>) <ref cRef="Sx. Haml." loc="17 Enc.">Sx. Haml. 17 Enc.</ref>; Stephane <foreign xml:lang="gez">ዘበመትልወ፡ በዓለ፡ እግዚእከ፡</foreign> (i.e. proxime post natalem Christi) <foreign xml:lang="gez">ኮነ፡ በዓልከ፡</foreign> <ref cRef="Mavâs." loc=" n. 11">Mavâs.  n. 11</ref>; <ref cRef="Deg." loc="">Deg. </ref>; <foreign xml:lang="gez">መትልወ፡ ዝንቱ፡</foreign> <cit type="translation" xml:lang="la">
                                <quote> post haec </quote>
                            </cit>,<bibl>
                                <ptr target="bm:Ludolf1691Commentarius"/>
                            </bibl> p. 500 <gramGrp> <pos expand="nomen">n.</pos> </gramGrp> 27; <foreign xml:lang="gez">መትልወ፡ ሐጋይ፡</foreign> <cit type="translation" xml:lang="la">
                                <quote> post autumnum </quote>
                            </cit> Lud. ex Enc.</sense> <sense n="2" xml:id="D2"> continua sequela vel consecutio, <cit type="translation" xml:lang="la">
                                <quote> ordo </quote>
                            </cit>, <cit type="translation" xml:lang="la">
                                <quote> tenor </quote>
                            </cit>: <foreign xml:lang="gez">በበመትልው፡</foreign> <foreign xml:lang="grc">καθεξῆς</foreign> <ref cRef="Luc. " loc="1,3">Luc.  1,3</ref>; <ref cRef="Clem." loc="164">Clem. f. 164</ref>; <foreign xml:lang="gez">ሥዩማን፡ በበ፡ መትሎሆሙ፡</foreign> <cit type="translation" xml:lang="la">
                                <quote> constituti unusquisque </quote>
                            </cit> <cit type="translation" xml:lang="la">
                                <quote> secundum ordinem suum </quote>
                            </cit> Lud. e Syn.; <foreign xml:lang="gez">በመትልዊሁ፡</foreign> <cit type="translation" xml:lang="la">
                                <quote> secundum ordinem </quote>
                            </cit> <ref cRef="Clem." loc="227,">Clem. f. 227,</ref> vel <foreign xml:lang="gez">በመትሎሁ፡</foreign> <ref cRef="Clem." loc="225">Clem. f. 225</ref>; <foreign xml:lang="gez">ሰላም፡ ለአብርሃም፡ ወለይስሐቅ፡ በመትሎ፤ ወለሣልሳይ፡ ያዕቆብ፡</foreign> <ref cRef="Sx. Mij." loc="28 Enc.">Sx. Mij. 28 Enc.</ref>
                        </sense> </sense>
               
