               <sense xml:lang="la" source="#dillmann" xml:id="Dmain"> <subc>III,2</subc> <cit type="translation" xml:lang="la">
                     <quote>aequalem </quote>
                  </cit>vel <cit type="translation" xml:lang="la">
                     <quote>parem fieri </quote>
                  </cit>vel<cit type="translation" xml:lang="la">
                     <quote> haberi</quote>
                  </cit>, <cit type="translation" xml:lang="la">
                     <quote>exaequari</quote>
                  </cit>, <cit type="translation" xml:lang="la">
                     <quote>aequiparari</quote>
                  </cit>: <foreign xml:lang="gez">በይእቲ፡ ዕለት፡ ይትዔረይ፡ መዓልት፡ ምስለ፡ ሌሊት፡</foreign>
                  Hen.72,20; <foreign xml:lang="gez">እምይእዜ፡ ተዐረይነ፡ </foreign>
                  <ref cRef="Hen." loc="102,7">Hen. 102,7</ref>; <foreign xml:lang="gez">ተዐርዮ፡</foreign>
                  <ref cRef="Kuf." loc="64">Kuf. p. 64</ref>; <lbl expand="construitur cum">c.c.</lbl>
                  <foreign xml:lang="gez">ምስለ፡</foreign>
                  <ref cRef="Sap." loc="18,11">Sap. 18,11</ref>; <lbl expand="cum">c.</lbl>
                  <foreign xml:lang="gez">በ፡</foreign>, ut: <foreign xml:lang="gez">ሊተ፡</foreign>
                  (i.e. <cit type="translation" xml:lang="la">
                     <quote>meum est</quote>
                  </cit>) <foreign xml:lang="gez">ተዐርዮ፡ በእመ፡ እሉ፡ ውሉድ፡</foreign>
                  <ref cRef="Jsp." loc="366">Jsp. p. 366</ref>; <lbl expand="cum">c.</lbl>
                  <foreign xml:lang="gez">ለ፡ </foreign> Lud. ex <ref cRef="Sx. Teq." loc="6 ">Sx.
                     Teq. 6 </ref>Enc. (at in Sx. Tubing. c. <gramGrp>
                     <case value="accusativus">Acc.</case>
                  </gramGrp>); <lbl expand="cum">c.</lbl>
                  <foreign xml:lang="gez">ከመ፡</foreign>
                  <ref cRef="Chrys. Ta." loc="28 ">Chrys. Ta. 28 </ref>(<lbl expand="videas">vid.
                     sub</lbl>
                  <foreign xml:lang="gez">ንጥበት፡</foreign>); <lbl expand="cum">c.</lbl>
                  <gramGrp>
                     <case value="accusativus">Acc.</case>
                  </gramGrp>: <ref cRef="Ps." loc="88,7">Ps. 88,7</ref>; <foreign xml:lang="gez">ኢይትዔረዮ፡ </foreign>
                  <cb n="0967" xml:id="c0967"/>
                  <foreign xml:lang="gez">ቄጵርስስ፡</foreign>
                  <ref cRef="Hez." loc="31,8">Hez. 31,8</ref>
                  <lbl expand="varia lectio">var.</lbl>; <ref cRef="Job" loc="28,19">Job
                  28,19</ref>; <foreign xml:lang="gez">አልቦ፡ ዘይትዔረዮ፡ ለቅንአት፡</foreign>
                  <ref cRef="Prov." loc="27,4">Prov. 27,4</ref>; <foreign xml:lang="gez">ኢድልው፡
                     ይትዐረዮሙ፡</foreign> (non dignus qui iis aequiparetur) <ref cRef="Jsp. " loc="332.">Jsp. p. 332.</ref>
               </sense>
               
