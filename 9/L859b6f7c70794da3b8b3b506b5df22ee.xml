            <entry xml:id="L859b6f7c70794da3b8b3b506b5df22ee" n="6125">
               <form>
                  <foreign xml:lang="gez">ከረየ</foreign>
               <rs type="root"/>
                    </form>
               <sense xml:lang="la" source="#dillmann" xml:id="Dmain"> <subc>I,1</subc> {<ref target="#c1432"/> et <subc>I,2</subc>} [<foreign xml:lang="he">כָּרָה כְּרָא</foreign> <foreign xml:lang="ar">كَرَا</foreign>, <foreign xml:lang="ar">كَارَ</foreign>, al.; <lbl expand="videas">vid.</lbl> Ges. thes. p. 671] <gramGrp> <pos expand="Subjunctivus">Subj.</pos> </gramGrp> <foreign xml:lang="gez">ይክሪ፡</foreign> (raro <foreign xml:lang="gez">ይክረይ፡</foreign> Lud. ex Hist. VII Puer.; Imperat. <foreign xml:lang="gez">ክረይ፡</foreign> <ref cRef="Clem." loc="223">Clem. f. 223</ref>), <cit type="translation" xml:lang="la">
                            <quote> fodere </quote>
                        </cit>, <sense n="1" xml:id="D1"> sensu proprio (<cit type="translation" xml:lang="de">
                                <quote>graben</quote>
                            </cit>): <sense n="a" xml:id="D1a"> <cit type="translation" xml:lang="la">
                                    <quote> fodere </quote>
                                </cit>, <cit type="translation" xml:lang="la">
                                    <quote> effodere </quote>
                                </cit>, <cit type="translation" xml:lang="la">
                                    <quote> defodere </quote>
                                </cit> <sense n="α" xml:id="D1aα"> rei in terra occultae quaerendae causa: <gramGrp> <pos expand="absolute">abs.</pos> </gramGrp> ut: <foreign xml:lang="gez">በህየ፡ ከረየ፡</foreign> <ref cRef="Clem." loc="225">Clem. f. 225</ref>; <foreign xml:lang="gez">ከረይኩ፡ ወነሣእኩ፡</foreign> <ref cRef="Jer." loc="13,7">Jer. 13,7</ref>; (<ref cRef="Matth." loc="6,19">Matth. 6,19</ref>), vel <lbl expand="cum">c.</lbl> <gramGrp> <case value="accusativus">Acc.</case> </gramGrp> rei, sive: <cit type="translation" xml:lang="la">
                                        <quote> fodiendo invenire </quote>
                                    </cit>: <foreign xml:lang="gez">ከረዩ፡ ንዋየ፡ ልሕኵት፡</foreign> <ref cRef="Sx. Jac." loc="30,">Sx. Jac. 30,</ref> sive <cit type="translation" xml:lang="la">
                                        <quote> fodiendo elicere </quote>
                                    </cit>: <foreign xml:lang="gez">ይከሪ፡ ለርእሱ፡ እከየ፡</foreign> <ref cRef="Prov." loc="16,27">Prov. 16,27</ref>; <foreign xml:lang="gez">ብእሲ፡ መዐትም፡ ይከሪ፡ ጋእዘ፡</foreign> <ref cRef="Prov." loc="29,22">Prov. 29,22</ref>.</sense> <sense n="β" xml:id="D1aβ"> foveam, puteum, sepulcrum al.; <foreign xml:lang="gez">ከረየ፡ ምድረ፡</foreign> <ref cRef="Matth." loc="25,18">Matth. 25,18</ref>; <foreign xml:lang="gez">ዘይከሪ፡ ግበ፡</foreign> <ref cRef="Prov. " loc="26,27">Prov.  26,27</ref>; <ref cRef="Ps." loc="7,16">Ps. 7,16</ref>; <ref cRef="Koh." loc="10,8">Koh. 10,8</ref>; <ref cRef="Sir." loc="27,26">Sir. 27,26</ref>; <ref cRef="Zach." loc="3,9">Zach. 3,9</ref>; <foreign xml:lang="gez">አዕመቀ፡ ከሪየ፡ ግበ፡</foreign> <ref cRef="Jer." loc="30,8">Jer. 30,8</ref> <lbl expand="varia lectio">var.</lbl>; <foreign xml:lang="gez">ከረየ፡ ዐዘቅተ፡</foreign> <ref cRef="Kuf." loc="24">Kuf. 24</ref>; <ref cRef="Gen." loc="21,30">Gen. 21,30</ref>; <ref cRef="Gen." loc="26,15">Gen. 26,15</ref> <lbl expand="sequens">seq.</lbl>; <ref cRef="Jes." loc="51,1">Jes. 51,1</ref>; <ref cRef="Jer. " loc="2,13">Jer.  2,13</ref>; <ref cRef="Ex." loc="7,24">Ex. 7,24</ref> (omisso <gramGrp> <case value="accusativus">Acc.</case> </gramGrp>); <foreign xml:lang="gez">ከረየ፡ መቃብረ፡</foreign> <ref cRef="Tob." loc="8,9">Tob. 8,9</ref>; <foreign xml:lang="gez">ከረይኩ፡</foreign> (omisso <gramGrp> <case value="accusativus">Acc.</case> </gramGrp>) <foreign xml:lang="gez">ወቀበርክዎ፡</foreign> <ref cRef="Tob." loc="2,7">Tob. 2,7</ref>; <foreign xml:lang="gez">መቃብር፡ ዘከረየ፡ ለርእሱ፡</foreign> <ref cRef="Kuf." loc="45">Kuf. 45</ref>; <ref cRef="Jsp." loc="368">Jsp. p. 368</ref>; <foreign xml:lang="gez">ከረየ፡ ምክያደ፡</foreign> <ref cRef="Marc." loc="12,1">Marc. 12,1</ref>.</sense> </sense> <sense n="b" xml:id="D1b"> <cit type="translation" xml:lang="la">
                                    <quote> perfodere </quote>
                                </cit> parietes, muros al.: <foreign xml:lang="gez">ይቤለኒ፡ ክሪ፡ ውስተ፡ አረፍት፡ ወከረይኩ፡</foreign> <ref cRef="Hez." loc="8,8">Hez. 8,8</ref>; <lbl expand="cum">c.</lbl> <gramGrp> <case value="accusativus">Acc.</case> </gramGrp>: <foreign xml:lang="gez">ክሪ፡ አረፍተ፡</foreign> <ref cRef="Hez." loc="12,5">Hez. 12,5</ref>; <foreign xml:lang="gez">ይከሪ፡ አብያተ፡ በጽልመት፡</foreign> <ref cRef="Job" loc="24,16">Job 24,16</ref>; <ref cRef="Luc." loc="12,39">Luc. 12,39</ref>; <foreign xml:lang="gez">እለ፡ ይከርዩ፡ አብያተ፡</foreign> (fures) <ref cRef="F.M." loc=" 17,7">F.M.  17,7</ref>; omisso <gramGrp> <case value="accusativus">Acc.</case> </gramGrp> (<cit type="translation" xml:lang="de">
                                    <quote>einbrechen</quote>
                                </cit>): <foreign xml:lang="gez">ከረየ፡ ወሰረቀ፡ ቤተ፡ ነዳይ፡</foreign> <ref cRef="Clem." loc="226">Clem. f. 226</ref>; <ref cRef="Matth." loc="6,19">Matth. 6,19</ref>; <foreign xml:lang="gez">ከሪዮ፡</foreign> <ref cRef="Ex." loc="22,1">Ex. 22,1</ref>.</sense> <sense n="c" xml:id="D1c"> <cit type="translation" xml:lang="la">
                                    <quote> pastinare </quote>
                                </cit>, <cit type="translation" xml:lang="la">
                                    <quote> repastinare </quote>
                                </cit>: <foreign xml:lang="gez">ኢይከርዮ፡</foreign> <ref cRef="Jes." loc="5,6">Jes. 5,6</ref>; <foreign xml:lang="gez">ከረየ፡ ታሕቴሁ፡</foreign> <ref cRef="Herm. " loc="53.">Herm.  p. 53.</ref> </sense> <sense n="d" xml:id="D1d"> <cit type="translation" xml:lang="la">
                                    <quote> suffodere </quote>
                                </cit>, diruendi causa <ref cRef="Hen." loc="89,66">Hen. 89,66</ref>.</sense> <sense n="e" xml:id="D1e"> <cit type="translation" xml:lang="la">
                                    <quote> incisiones facere </quote>
                                </cit>: <foreign xml:lang="gez">ኢይላሕውዎሙ፡ ወኢይከርዮ፡ ሎሙ፡</foreign> (<cit type="translation" xml:lang="de">
                                    <quote>für sie</quote>
                                </cit>) <ref cRef="Jer." loc="16,6">Jer. 16,6</ref>.</sense> </sense> <sense n="2" xml:id="D2"> <cit type="translation" xml:lang="la">
                                <quote> fodicare </quote>
                            </cit> (<cit type="translation" xml:lang="de">
                                <quote>stechen</quote>
                            </cit>), <sense n="a" xml:id="D2a"> <cit type="translation" xml:lang="la">
                                    <quote> fodere </quote>
                                </cit>, <cit type="translation" xml:lang="la">
                                    <quote> effodere </quote>
                                </cit> oculos <ref cRef="Hen." loc="90,2">Hen. 90,2</ref> {<ref target="#c1432"/> stirps <subc>I,2</subc>}. <cb n="0841" xml:id="c0841"/> </sense> <sense n="b" xml:id="D2b"> <lbl expand="translate">transl.</lbl>: <cit type="translation" xml:lang="la">
                                    <quote> fodere </quote>
                                </cit> i.e. <cit type="translation" xml:lang="la">
                                    <quote> incitare </quote>
                                </cit> aliquem <cit type="translation" xml:lang="la">
                                    <quote> ad lasciviam </quote>
                                </cit>, <cit type="translation" xml:lang="la">
                                    <quote> ferociam </quote>
                                </cit>: <foreign xml:lang="gez">ፈሐል፡ ኢይክርየኒ፡</foreign> <ref cRef="Sir." loc="23,6">Sir. 23,6</ref>; <foreign xml:lang="gez">በእኪት፡ ዕለት፡ ይከሪ፡ ጽጋብ፡</foreign> <ref cRef="Sir." loc="11,27">Sir. 11,27</ref>; <foreign xml:lang="gez">ከረያ፡ ጽጋብ፡</foreign> <ref cRef="Hez. " loc="16,49">Hez.  16,49</ref>; <ref cRef="Deut." loc="32,15">Deut. 32,15</ref>. <cit type="translation" xml:lang="la">
                                    <quote> – Voc. Ae.: </quote>
                                </cit> <foreign xml:lang="gez">ከረየ፡ ወደኀየ፡ ዘ፡ መሐሰ፡</foreign> <cit type="translation" xml:lang="la">
                                    <quote> aliud </quote>
                                </cit>: <foreign xml:lang="gez">ከረየ፡ ዘ፡ መሐሰ፤ ደኀየ፡ ዘ፡ አጐደጐደ።</foreign> </sense> </sense> </sense>
               
            </entry>
         </div>
