               <sense xml:id="Dmain" xml:lang="la" source="#dillmann"> et <foreign xml:lang="gez">ሐጸ፡</foreign>, etiam per <foreign xml:lang="gez">ፀ</foreign>, <subc>I,1</subc> [significatio radicis <foreign xml:lang="he">חָצַץ</foreign> prima est <cit type="translation" xml:lang="la"><quote> secuit </quote></cit>, <cit type="translation" xml:lang="la"><quote> abscidit </quote></cit>, <cit type="translation" xml:lang="la"><quote> discidit </quote></cit> <lbl expand="videas">vid.</lbl> Ges. thes. p. 510; Aethiopibus notio <cit type="translation" xml:lang="la"><quote> concidendi </quote></cit> in notionem <cit type="translation" xml:lang="la"><quote> imminuendi </quote></cit> et intransit. <cit type="translation" xml:lang="la"><quote> decrescendi </quote></cit> transiit; <lbl expand="vicinus">vic.</lbl> <foreign xml:lang="gez">ኀጠጠ፡</foreign>, <foreign xml:lang="gez">ቀጢን፡</foreign> q.v.] <gramGrp> <pos expand="Subjunctivus">Subj.</pos> </gramGrp> <foreign xml:lang="gez">ይሕጽጽ፡</foreign> <sense n="1" xml:id="D1"> perraro transit. <cit type="translation" xml:lang="la"><quote> imminuere </quote></cit>, <cit type="translation" xml:lang="la"><quote> deminuere </quote></cit>, ut: <foreign xml:lang="gez">ዘይጸልእ፡ ነቢበ፡ ብዙኀ፡ የሐጽጻ፡ ለኀጢአቱ፡</foreign> <foreign xml:lang="grc">ἐλαττονοῦται κακίᾳ</foreign> (si non <foreign xml:lang="gez">ያሐጽጻ፡</foreign> legere malueris) <ref cRef="Sir." loc="19,6">Sir. 19,6</ref>; <foreign xml:lang="gez">ኢሐጸነ፡ ወኢምንተ፡</foreign> (var.) <cit type="translation" xml:lang="la"><quote> nobis debita nihilo minus deminuit </quote></cit> <ref cRef="Kuf." loc="127 ">Kuf. p. 127 </ref> <lbl expand="annotatio">ann.</lbl> 1. 
</sense> <sense n="2" xml:id="D2"> plerumque intransit. <cit type="translation" xml:lang="la"><quote> minui </quote></cit>, <cit type="translation" xml:lang="la"><quote> imminui </quote></cit>, <cit type="translation" xml:lang="la"><quote> deficere </quote></cit>, <cit type="translation" xml:lang="la"><quote> decrescere </quote></cit>: <foreign xml:lang="gez">ሐጸ፡ ማይ፡</foreign> <ref cRef="Gen." loc="8,3">Gen. 8,3</ref>; <ref cRef="Gen." loc="8,5">Gen. 8,5</ref>; de luna <ref cRef="Hen." loc="78,8">Hen. 78,8</ref>; <foreign xml:lang="gez">ብርሃን፡ ዘየኀልቅ፡ እንዘ፡ የሐጽጽ፡</foreign> <ref cRef="Sir. " loc="43,7">Sir.  43,7</ref>; de ingenio <foreign xml:lang="gez">የሐጽጽ፡ ልቡ፡ ከመ፡ ወርኅ፡</foreign> <ref cRef="Sir." loc="27,11">Sir. 27,11</ref>; (<lbl expand="oppositum, -o, -nitur">opp.</lbl> <foreign xml:lang="gez">ተወሰከ፡</foreign>) <ref cRef="Sir." loc="42,21">Sir. 42,21</ref> 
</sense> <sense n="3" xml:id="D3"> comparative <cit type="translation" xml:lang="la"><quote> minorem </quote></cit>, <cit type="translation" xml:lang="la"><quote> inferiorem </quote></cit>, <cit type="translation" xml:lang="la"><quote> minimum esse </quote></cit>: <foreign xml:lang="gez">ኢይሕጽጽ፡ እክልከ፡</foreign> <ref cRef="Kuf." loc="113">Kuf. p. 113</ref>; <lbl expand="cum">c.</lbl> <gramGrp> <case value="accusativus">Acc.</case> </gramGrp> <lbl expand="persona">pers.</lbl> (sensu Dativi): <foreign xml:lang="gez">እለ፡ የሐጾሙ፡ አእምሮ፡</foreign> <foreign xml:lang="grc">οἱ ἐνδεεῖς φρενῶν</foreign> <ref cRef="Prov." loc="9,4">Prov. 9,4</ref>; <ref cRef="Prov." loc="9,16">Prov. 9,16</ref>; <lbl expand="cum">c.</lbl> <gramGrp> <case value="accusativus">Acc.</case> </gramGrp> rei: <foreign xml:lang="gez">የሐጹ፡ ኀይለ፡</foreign> <cit type="translation" xml:lang="la"><quote> minores sunt viribus </quote></cit> <ref cRef="4 Esr." loc="3,65">4 Esr. 3,65</ref>; <foreign xml:lang="gez">ኢፈድፈደ፡</foreign>–<foreign xml:lang="gez">ወኢሐጸጸ፡</foreign> <ref cRef="Ex." loc="16,18">Ex. 16,18</ref>; (<cit type="translation" xml:lang="la"><quote> tribus filiis suis regnum divisit </quote></cit>) <foreign xml:lang="gez">ከመ፡ ኢይኩን፡ ፩እምኔሆሙ፡ ዘይፈደፍድ፡ ወየሐጽጽ፡ በምንትኒ፡</foreign> <ref cRef="Jsp." loc="332">Jsp. p. 332</ref>; <foreign xml:lang="gez">ትእዛዝ፡ እንተ፡ ተሐጽጽ፡</foreign> <cit type="translation" xml:lang="la"><quote> mandatum minimum </quote></cit> <ref cRef="Matth." loc="5,19">Matth. 5,19</ref>; <foreign xml:lang="gez">ኢየሐጽጽ፡ መዐተ፡ ንጉሥ፡ እመዐተ፡ አንበሳ፡</foreign> <ref cRef="Prov. " loc="20,2">Prov.  20,2</ref>; Lit. Epiph.; <ref cRef="Matth." loc="2,6">Matth. 2,6</ref>. 
</sense> <sense n="4" xml:id="D4"> <cit type="translation" xml:lang="la"><quote> detrahi </quote></cit>, <cit type="translation" xml:lang="la"><quote> deduci </quote></cit> (etiam: reliquum efficere, in arithm.): <foreign xml:lang="gez">እምጥብዖትነ፡ ኢየሐጸነ፡ ግንፋል፡</foreign> <cit type="translation" xml:lang="la"><quote> de penso ne unus quidem later nobis detrahitur </quote></cit> <ref cRef="Ex." loc="5,19">Ex. 5,19</ref>; <foreign xml:lang="gez">፶ ዓመት፡ እንዘ፡ የሐጽጽ፡ ወርኅ፡</foreign> <cit type="translation" xml:lang="la"><quote> quinquaginta anni detracto mense uno </quote></cit> <ref cRef="Abush." loc="50">Abush. 50</ref>; (<foreign xml:lang="gez">በበ፭፡ ዓመት፡</foreign>) <foreign xml:lang="gez">የሐጽጽ፡ ወርኅ፡ እምፀሐይ፡ ፴መዋዕለ፡</foreign> <cit type="translation" xml:lang="la"><quote> per singula quinquennia cursus lunae cum cursu solis comparatus reliquum efficit triginta dierum </quote></cit> <ref cRef="Hen." loc="74,1">Hen. 74,1</ref>-16. 
</sense> </sense>
               
            <sense xml:id="Tmain" xml:lang="en" source="#traces"> <cit type="transcription" xml:lang="gez"><quote>ḥaṣaṣa</quote></cit>, <cit type="transcription" xml:lang="gez"><quote>ḥaṣṣa</quote></cit> <sense n="G" xml:id="TG"> <foreign xml:lang="gez">ሐጸጸ፡</foreign> <cit type="transcription" xml:lang="gez"><quote>ḥaṣaṣa</quote></cit> et <foreign xml:lang="gez">ሐጸ፡</foreign> <cit type="transcription" xml:lang="gez"><quote>ḥaṣṣa</quote></cit> <subc>I,1</subc> <foreign xml:lang="gez">የሐጽጽ፡</foreign>, <foreign xml:lang="gez">ይሕጽጽ፡</foreign>«<cit type="translation" xml:lang="fr"><quote>diminuer</quote></cit>, <cit type="translation" xml:lang="fr"><quote>être moindre</quote></cit>, <cit type="translation" xml:lang="fr"><quote>devenir peu, petit, manquant</quote></cit>» ― <foreign xml:lang="am">ጐደለ፡</foreign> Ms. <ref type="BM" target="BNFabb217">BNFabb217</ref>, fol. 30v. <lb/>
 <bibl><ptr target="bm:Grebaut1952SuppLexicon"/><citedRange unit="page"> 67</citedRange></bibl> 
</sense> <sense n="L" xml:id="TL"> <cit type="transcription" xml:lang="gez"><quote>ḥṣṣ</quote></cit> <foreign xml:lang="gez">ሐጸጸ</foreign> <cit type="transcription" xml:lang="gez"><quote>ḥaṣaṣa</quote></cit>, <foreign xml:lang="gez">ሐጸ</foreign> <cit type="transcription" xml:lang="gez"><quote>ḥaṣṣa</quote></cit> (yǝḥṣǝṣ) <cit type="translation" xml:lang="en"><quote>be deficient</quote></cit>, <cit type="translation" xml:lang="en"><quote>subside</quote></cit>, <cit type="translation" xml:lang="en"><quote>wane</quote></cit>, <cit type="translation" xml:lang="en"><quote>decrease</quote></cit> (intr.), <cit type="translation" xml:lang="en"><quote>be wanting</quote></cit>, <cit type="translation" xml:lang="en"><quote>be less</quote></cit>, <cit type="translation" xml:lang="en"><quote>be subtracted</quote></cit>, <cit type="translation" xml:lang="en"><quote>diminish</quote></cit> (intr.), <cit type="translation" xml:lang="en"><quote>be inferior</quote></cit> (in quality), <cit type="translation" xml:lang="en"><quote>be short of</quote></cit>, <cit type="translation" xml:lang="en"><quote>be the least</quote></cit>, <cit type="translation" xml:lang="en"><quote>be little</quote></cit>, <cit type="translation" xml:lang="en"><quote>be imperfect</quote></cit> <bibl><ptr target="bm:Leslau1987dic"/><citedRange unit="page"> 247b</citedRange></bibl> 
</sense> </sense></entry>
